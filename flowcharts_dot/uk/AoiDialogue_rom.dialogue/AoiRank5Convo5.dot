		j3819782916 -> j432410442;
	j3309623469 [label="але більшість наших розмов зводилися до «привіт, люба» і «привіт», коли ми проходили повз один одного на кухні. вони швидко перекушували та поверталися до своїх офісів, щоби працювати допізна",shape=box,color=blue];
		j3309623469 -> j432410442;
