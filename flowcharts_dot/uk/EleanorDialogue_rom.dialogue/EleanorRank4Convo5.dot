digraph {
	"EleanorRank4Convo5 begins." [color=orange];
		"EleanorRank4Convo5 begins." -> j1488896712;
	j1488896712 [label="Королівство, яке колись було твоїм. Дувірі. Це ж не вигадка, правда?",shape=box,color=blue];
		j1488896712 -> j3631044038;
		j1488896712 -> j3752866699;
	j3631044038 [label="Можливо так, можливо ні. Це місце і персонажі прийшли з моєї улюбленої дитячої книжки. «Дувірські казки». Вони були втілені мною через Порожнечу. ",shape=box,color=red];
		j3631044038 -> j1526217917;
	j3752866699 [label="Мені так набридло говорити про Дувірі…",shape=box,color=red];
		j3752866699 -> j2741049345;
	j1526217917 [label="Як тобі це вдалося?",shape=box,color=blue];
		j1526217917 -> j93548087;
		j1526217917 -> j899474385;
	j2741049345 [label="+5 Chemistry",color=orange];
		j2741049345 -> j3790404507;
	j93548087 [label="Я був зовсім один. Гадаю, інші діти загинули або були врятовані. Я трохи гублюся в деталях.",shape=box,color=red];
		j93548087 -> j3906727327;
	j899474385 [label="Це було не зовсім навмисно. Все, що я пам’ятаю…",shape=box,color=red];
		j899474385 -> j3906727327;
	j3790404507 [label="Йой. Моя помилка.",shape=box,color=blue];
		j3790404507 -> j694340859;
	j3906727327 [label="Янголи наближалися, прориваючись крізь стіни, і мені нікуди було тікати. У мене була книга та мені спало на думку: «Ось де я можу бути в безпеці».",shape=box,color=red];
		j3906727327 -> j3321923121;
	j694340859 [label="Це було необачно з мого боку, чи не так? Це місце було не зовсім схоже на табір для відпочинку.",shape=box,color=blue];
		j694340859 -> j3623447140;
	j3321923121 [label="Янголи? У наш час янголи вважаються доброзичливими істотами. Іноді страшні, але на боці добра. Якщо тільки ти не маєш на увазі полеглих.",shape=box,color=blue];
		j3321923121 -> j3218112163;
	j3623447140 [label="Мені дуже шкода. Відтепер я буду тримати свою цікавість під контролем.",shape=box,color=blue];
		j3623447140 -> "Chat ends.";
	j3218112163 [label="За моїх часів це практично не змінилося. Янголи — це крилаті, сяючі створіння зі старих казок, які допомагають добрим людям. Я говорю про янголів Порожнечі. Це зовсім інші істоти.",shape=box,color=red];
		j3218112163 -> j958622884;
	"Chat ends." [color=orange];
	j958622884 [label="Зрозуміло. Я не можу собі уявити, що хтось під назвою «янгол Порожнечі» налітає та благословляє новонароджену дитину.",shape=box,color=blue];
		j958622884 -> j959950833;
	j959950833 [label="Наскільки я можу зрозуміти, це були люди. Коли пісня янголів доходить до них, вони починають змінюватися. Коли вони більше не можуть цьому опиратися, вони самі стають янголами.",shape=box,color=red];
		j959950833 -> j4093547259;
	j4093547259 [label="Майбутнє, як ти його описуєш, звучить до біса жахливо.",shape=box,color=blue];
		j4093547259 -> j3058640341;
	j3058640341 [label="Я не можу звинувачувати тебе у бажанні сховатися від цих янголів Порожнечі. Особливо, якщо вони були…",shape=box,color=blue];
		j3058640341 -> j128780019;
	j128780019 [label="Ох. У мене з’явилося відчуття. Вони не були твоїми батьками, чи не так?",shape=box,color=blue];
		j128780019 -> j2956229649;
		j128780019 -> j2943810634;
		j128780019 -> j1706095554;
	j2956229649 [label="Я маю впевненість, що мої батьки померли.",shape=box,color=red];
		j2956229649 -> j3275081520;
	j2943810634 [label="Вони могли бути. Інші діти чули, як батьки кликали їх.",shape=box,color=red];
		j2943810634 -> j3275081520;
	j1706095554 [label="Думаю, вони там були. Квін попередив мене не шукати знайомих голосів. Мабуть, саме це він мав на увазі.",shape=box,color=red];
		j1706095554 -> j3275081520;
	j3275081520 [label="Мені так шкода. Я рада, що знайшлася безпечна гавань, навіть якщо вона не залишилася такою.",shape=box,color=blue];
		j3275081520 -> j2811084546;
	j2811084546 [label="Довгий час там було безпечно. Досить довго, щоби забутися. Гадаю, це те, що я хотів зробити.",shape=box,color=red];
		j2811084546 -> j1307072072;
	j1307072072 [label="Але як це тобі вдалося? Ти був ще дитиною. Що дало тобі силу створити ціле королівство з нічого?",shape=box,color=blue];
		j1307072072 -> j2285126530;
		j1307072072 -> j2971272081;
		j1307072072 -> j1784254609;
	j2285126530 [label="Діти цікаві тим, що вони ще не знають, чого не зможуть. Я не знав, що не зможу. Тому в мене вийшло.",shape=box,color=red];
		j2285126530 -> j3239145910;
	j2971272081 [label="У мене було стільки емоцій, одразу, що я… просто… щось з ними зробив. Це важко пояснити.",shape=box,color=red];
		j2971272081 -> j2424412262;
	j1784254609 [label="Порожнеча - це не «ніщо». Я знаю, що це звучить так, ніби так і має бути, але це не так. Альбрехт це довів.",shape=box,color=red];
		j1784254609 -> j2424412262;
	j3239145910 [label="Коли мені було п’ять, я хотіла бути відьмою. Я закричала «Абракадабра» і стрибнула з верхніх сходів. Я не знала, що не вмію літати. Але швидко дізналася.",shape=box,color=blue];
		j3239145910 -> j351106027;
	j2424412262 [label="Ми знаємо про неї дуже мало, навіть зараз. Але навіть орокіни зрозуміли одну річ: Порожнеча реагує на людські емоції. ",shape=box,color=red];
		j2424412262 -> j2639933193;
	j351106027 [label="Сила для створення Дувірі, мабуть, прийшла не від тебе, а від когось іншого. Ти скористався нею, щоби зробити щось неймовірне, але вона не була твоєю.",shape=box,color=blue];
		j351106027 -> j459599265;
	j2639933193 [label="Я ЗРОЗУМІЛА. Отже, якщо зазирнути в Порожнечу з… хм, скажімо, «голодом»… то можна створити бродячого напівголодного вовка. Або, можливо, королівський бенкет. Або буханець хліба зі скреготом вовчих зубів. ",shape=box,color=blue];
		j2639933193 -> j3032563114;
	j459599265 [label="Принаймні, спочатку. Але зараз воно в тобі. Дика енергія. Петлі часу. Боги, ти диво.",shape=box,color=blue];
		j459599265 -> j1834839757;
	j3032563114 [label="Яка жахлива, хвилююча думка.",shape=box,color=blue];
		j3032563114 -> j1834839757;
	j1834839757 [label="Чому орокіни вирішили, що це гарна ідея — запустити в Порожнечу зореліт, повний дітей? Хіба це не була емоційна бомба уповільненої дії, що чекала на вибух? ",shape=box,color=blue];
		j1834839757 -> j3101651305;
	j3101651305 [label="Саме для цього і була створена ця книга історій. На перший погляд, мова йшла про персонажів при дворі божевільного короля, який щодня змінював свій настрій. ",shape=box,color=red];
		j3101651305 -> j2533870670;
	j2533870670 [label="Але головна ідея цих історій полягала в тому, як керувати своїми емоціями. Ми повинні були винести з цього важливий урок, щоби подорож була безпечною.",shape=box,color=red];
		j2533870670 -> j3736096979;
	j3736096979 [label="Тьху. Вибач, але ти мені все зіпсував. ",shape=box,color=blue];
		j3736096979 -> j3759371587;
	j3759371587 [label="Чому історії не можуть бути просто історіями? Я ЗНЕНАВИДІЛА це, коли дізналася, що Рудольфо, король леопардів, який героїчно загинув у битві зі Змієм, мав бути Солом з його сонячними плямами, який страждає на зоряному дереві. ",shape=box,color=blue];
		j3759371587 -> j2867788180;
	j2867788180 [label="Запихати релігію в дитячі книжки — це все одно, що ховати ліки в цукерках. ПРОСТО СКАЖИ ЇМ.",shape=box,color=blue];
		j2867788180 -> j2526353997;
		j2867788180 -> j2835065681;
	j2526353997 [label="Я не знаю історію про Рудольфо, короля леопардів.",shape=box,color=red];
		j2526353997 -> j1517927800;
	j2835065681 [label="Я хочу якось почути історію про Рудольфо, короля леопардів.",shape=box,color=red];
		j2835065681 -> j293375443;
	j1517927800 [label="Я можу померти щасливою жінкою, знаючи, що про ці книги забули. Ура майбутньому!",shape=box,color=blue];
		j1517927800 -> j745259283;
	j293375443 [label="А-а-а. Будь ласка, ні. Це не справжня легенда, а жахлива серія дитячих книжок.",shape=box,color=blue];
		j293375443 -> j990383739;
	j745259283 [label="У вас же є історії, чи не так?",shape=box,color=blue];
		j745259283 -> j1044695123;
	j990383739 [label="Якщо хочеш, я можу розповісти тобі кілька наших справжніх легенд.",shape=box,color=blue];
		j990383739 -> j204914837;
		j990383739 -> j385694852;
	j1044695123 [label="Багато. Навіть тенно та ворфрейми мають свої історії. ",shape=box,color=red];
		j1044695123 -> j2749882427;
	j204914837 [label="Залюбки.",shape=box,color=red];
		j204914837 -> j1099042918;
	j385694852 [label="Не зараз. [Кінець.]",shape=box,color=red];
		j385694852 -> "Chat ends.";
	j2749882427 [label="Я такий хлопець, Друз Леверій. Він як куратор музею. Він веде записи всіх легенд про ворфрейми.",shape=box,color=red];
		j2749882427 -> j1632394808;
	j1099042918 [label="Поглянемо. Звісно, є Артур. Старий добрий Артур. Оригінал. Найкращий в Британії.",shape=box,color=blue];
		j1099042918 -> j2372339414;
	j1632394808 [label="Що ж, це збило мене з пантелику. Ти з далекого майбутнього, але за стільки років перебування в Дувірі не в курсі того, що події твого життя встигли перетворитися на міфи?",shape=box,color=blue];
		j1632394808 -> j3318081872;
	j2372339414 [label="А ще є Юна Локслі. Любила її. Хотіла бути нею. Вона була взірцем для певного типу молодих жінок.",shape=box,color=blue];
		j2372339414 -> j3507241302;
