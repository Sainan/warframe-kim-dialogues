		j2926133025 -> j4246821056;
	j144476518 [label="Тоді розкажи мені про свої міфи.",shape=box,color=red];
		j144476518 -> j1948679846;
	j149818744 [label="Гаразд. Кімнатна версія: вона була дочкою дворянина в ті давні часи, коли країна страждала під владою жорстокого короля.",shape=box,color=blue];
		j149818744 -> j3993549764;
	j4246821056 [label="Ти стаєш персонажем дитячої книжки, чи не так?",shape=box,color=blue];
		j4246821056 -> j2120644333;
	j1948679846 [label="Дідько. Ми будемо тут цілий рік, якщо я почну. ",shape=box,color=blue];
		j1948679846 -> j1099042918;
	j3993549764 [label="Її брат загинув на чужині, але звістка про це ще не дійшла додому, тож коли бідний пригноблений народ потребував героя, вона вирішила стати ним.",shape=box,color=blue];
		j3993549764 -> j2215480408;
	j2120644333 [label="Потрапляєш у пастку чарівного королівства, де час тече по-іншому, прикликаний туди, де найбільше потрібен…",shape=box,color=blue];
