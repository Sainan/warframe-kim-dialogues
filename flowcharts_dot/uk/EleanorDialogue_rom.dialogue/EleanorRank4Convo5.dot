		j2586034101 -> j2307597628;
	j4205093462 [label="Потвори звикли бути самотніми.",shape=box,color=blue];
		j4205093462 -> j238887900;
	j488837689 [label="Здається, вона сама по собі викликала б повагу. Навіщо їй потрібно було прикидатися своїм братом?",shape=box,color=red];
		j488837689 -> j1232627748;
	j2307597628 [label="Потворність в очах того, хто дивиться, мій друже.",shape=box,color=blue];
		j2307597628 -> j3403430618;
	j238887900 [label="Ми вижили.",shape=box,color=blue];
		j238887900 -> "Chat ends.";
	j1232627748 [label="Для тебе це вже історія. У ті часи чоловіки нібито воювали, тож вони й керували. Люди очікували виконувати накази від чоловіків. Їй потрібно було спиратися на репутацію брата, щоби зробити те, що вона мала зробити. Або так, або піти на дно.",shape=box,color=blue];
		j1232627748 -> j1081216600;
		j1232627748 -> j328264501;
		j1232627748 -> j281902048;
	j3403430618 [label="Якщо тебе щиро приваблюють якості, які ти вважаєш «потворними» в інших людях, тоді тобі краще переконатися, що вони поділяють твій вибір, і насолоджуватися цим. Інакше ти здійсниш найгрубішу фетишизацію.",shape=box,color=blue];
		j3403430618 -> j3155006143;
	j1081216600 [label="Що з рештою з нею сталося?",shape=box,color=red];
		j1081216600 -> j3586290089;
	j328264501 [label="Мене радує, що ми виправили це в майбутньому.",shape=box,color=red];
		j328264501 -> j3592327807;
	j281902048 [label="Не можу сказати, що ми *повністю* виправили це в майбутньому…",shape=box,color=red];
		j281902048 -> j614047630;
	j3155006143 [label="Наприклад, чи назвала б я себе потворою? Так. Із захопленням. Я приймаю титул, за який інші засудили б мене, і радію цьому. На випадок, якщо тобі цікаво.",shape=box,color=blue];
