		j650479346 -> j2586034101;
	j318810618 [label="І це нормально, чесно кажучи.",shape=box,color=blue];
		j318810618 -> j4205093462;
	j2098480081 [label="Вона чудово володіла луком і стрілами. Вона та її команда стали улюбленою бандою розбійників, які грабували королівських збирачів податків і роздавали гроші тим, хто їх найбільше потребував.",shape=box,color=blue];
		j2098480081 -> j488837689;
