		j3001683821 -> j1263656440;
	j2571165 [label="Мені завжди були цікавіші потвори. Герої нудні. Передбачувані. …Буденні.",shape=box,color=red];
		j2571165 -> j650479346;
	j575069234 [label="М-м, не думаю, що потвори — це мій тип.",shape=box,color=red];
		j575069234 -> j318810618;
	j995163684 [label="Як я вже казала. Вона була дуже схожа на свого брата, тому переодяглася в його старий одяг, видала себе за нього і пішла очолювати революцію. Якщо хтось підходив надто близько, то міг помітити, що вона дівчина, тому вона носила капюшон. Це стало її візитною карткою.",shape=box,color=blue];
		j995163684 -> j2098480081;
	j1263656440 [label="Boolean EleanorHeroVirus is now true.",color=orange];
		j1263656440 -> j2098480081;
