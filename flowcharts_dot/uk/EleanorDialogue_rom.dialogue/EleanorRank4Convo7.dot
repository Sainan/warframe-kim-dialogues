	j1315397168 [label="Я так і бачу, як тато ввічливо запитує: «То це завжди була смерть через заколювання, чи вони намагалися її трохи урізноманітнити?»",shape=box,color=blue];
		j1315397168 -> j2231996441;
	j875933747 [label="… Я повинна відпустити цю мрію, перш ніж намагатися втілити її в життя. Ми тут, зараз, і війна все ще триває.",shape=box,color=blue];
