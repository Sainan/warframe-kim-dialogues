		j639529953 -> j1654650119;
	j3710422287 [label="Безнадійними. З таким же успіхом мене могли б виховати дикі куброу.",shape=box,color=red];
		j3710422287 -> j272453784;
	j54988905 [label="Я… не можу про це говорити. Вибач. [Кінець.]",shape=box,color=red];
		j54988905 -> j533005134;
	j538181298 [label="Мені шкода. Я нерозумно припустила, що оскільки твої спогади про «Заримен» були такими яскравими, ти зможеш згадати і своїх батьків.",shape=box,color=blue];
		j538181298 -> j164771724;
	j285565683 [label="Я рада. Якщо ти не проти, що я так кажу, це помітно.",shape=box,color=blue];
		j285565683 -> j2552837988;
	j1654650119 [label="Не думала, що у тебе було м’яке виховання. Гадаю, колоністи повинні бути жорсткими.",shape=box,color=blue];
		j1654650119 -> j3569650898;
	j272453784 [label="О, боже. Що ж, хай там як, з тебе вийшла до біса пристойна людина.",shape=box,color=blue];
