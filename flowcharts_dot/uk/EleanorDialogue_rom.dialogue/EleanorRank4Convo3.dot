	"Chat ends." [color=orange];
	j3775446229 [label="The other me. The one who became a nun. I’ve been trying to contact her.",shape=box,color=blue];
		j3775446229 -> j2419358342;
	j3875065428 [label="Ever since you appeared, a question has been pestering me. A question I believe I might've just answered. And dear Sol I don't know what I've done.",shape=box,color=blue];
		j3875065428 -> j4034963426;
	j2419358342 [label="I just did it.",shape=box,color=blue];
		j2419358342 -> j1501838920;
	j4034963426 [label="Start at the beginning. I don't know what you're talking about. What question?",shape=box,color=red];
		j4034963426 -> j2432787418;
	j1501838920 [label="Of course it was never going to work so long as I stayed in this world. I’m already the ‘me’ of this world. I had to go through the roaring whiteness. The blackstar fugue. The leechbone gulf. That’s how you get to the other sides. The maybes and the could-have-beens.",shape=box,color=blue];
		j1501838920 -> j3666206409;
	j2432787418 [label="You! The question of bloody You. You exist not once but twice.",shape=box,color=blue];
		j2432787418 -> j292719556;
	j3666206409 [label="The Void! I saw the Void!",shape=box,color=blue];
		j3666206409 -> j2589678009;
		j3666206409 -> j2122412294;
	j292719556 [label="And if you could exist in another world, as another version of yourself, why couldn't I?",shape=box,color=blue];
		j292719556 -> j1791396303;
	j2589678009 [label="Are you... sure you didn't get into Lettie's special stash?",shape=box,color=red];
		j2589678009 -> j288227447;
	j2122412294 [label="That shouldn’t be possible, Eleanor. Nobody enters the Void until Albrecht Entrati does it. That’s a long way into the future from where we are now.",shape=box,color=red];
		j2122412294 -> j751342468;
	j1791396303 [label="Somewhere out there in the now-confirmed-to-exist vast possible universes was that version of myself that I always wondered if I should become.",shape=box,color=blue];
		j1791396303 -> j798105011;
	j288227447 [label="Drifter dear, I admire your ability to use humour as a coping mechanism, but right now I am entirely serious.",shape=box,color=blue];
		j288227447 -> j3720984414;
	j751342468 [label="I went in with my mind, not my body. That’s the difference.",shape=box,color=blue];
		j751342468 -> j3071118112;
	j798105011 [label="The one who turned away from the world to become a nun.",shape=box,color=blue];
		j798105011 -> j4177926717;
	j3720984414 [label="I did what I set out to do.",shape=box,color=blue];
		j3720984414 -> j2780442477;
	j3071118112 [label="Ow headache ow ow ow",shape=box,color=blue];
		j3071118112 -> j1367355142;
	j4177926717 [label="An apostle of Lua sat on a mountaintop somewhere, incense clouds flowering around her.",shape=box,color=blue];
		j4177926717 -> j1108764199;
	j2780442477 [label="I not only saw the Void, I entered it.",shape=box,color=blue];
		j2780442477 -> j3071118112;
	j1367355142 [label="Check boolean EleanorIndifference2",color=orange];
		j1367355142 -> j4025349813 [label=true,color=green];
		j1367355142 -> j2442738282 [label=true,color=green];
		j1367355142 -> j2818796866 [label=false,color=brown];
		j1367355142 -> j2442738282 [label=false,color=brown];
	j1108764199 [label="I've been trying to contact her. Trying to reach through the Void to find her.",shape=box,color=blue];
		j1108764199 -> j112974167;
	j4025349813 [label="If you really did enter the Void, I cannot tell you how much danger you were putting yourself in. You know what’s out there, and you still just dove into it?",shape=box,color=red];
		j4025349813 -> j1462404902;
	j2442738282 [label="Are you insane?!",shape=box,color=red];
		j2442738282 -> j1250995061;
	j2818796866 [label="What did you see? What happened?",shape=box,color=red];
		j2818796866 -> j1016817224;
	j112974167 [label="Boolean EleanorSister is now true.",color=orange];
		j112974167 -> j329438708;
		j112974167 -> j113151257;
	j1462404902 [label="That’s how I got into the Void in the first place. Through that memory. The horror was just smoke and through the smoke was a GATE. If I hadn’t been so frightened I would have seen it clearly the first time around.",shape=box,color=blue];
		j1462404902 -> j797616394;
	j1250995061 [label="Not even a little bit.",shape=box,color=blue];
		j1250995061 -> j1626467498;
	j1016817224 [label="There’s a feeling there isn’t a word for. You look out on the world and you just KNOW, that all the ordinariness - buildings, cars, people, jobs, work, routine - is just a thin crust over a bottomless horror, and if people ever realized it they’d start screaming and never stop.",shape=box,color=blue];
		j1016817224 -> j3207210976;
	j329438708 [label="Oh no.",shape=box,color=red];
		j329438708 -> j2419358342;
	j113151257 [label="And...?",shape=box,color=red];
		j113151257 -> j2419358342;
	j797616394 [label="This was a mistake. It took Rusalka. It came damn close to taking Albrecht. It could easily take you too.",shape=box,color=red];
		j797616394 -> j4262300442;
	j1626467498 [label="Just listen. Please. And try to understand.",shape=box,color=blue];
		j1626467498 -> j3249542286;
		j1626467498 -> j526110617;
	j3207210976 [label="I fell through the crust.",shape=box,color=blue];
		j3207210976 -> j2229640217;
	j4262300442 [label="It tried. I felt it. I knew it was curious, reaching for me.",shape=box,color=blue];
		j4262300442 -> j3249542286;
		j4262300442 -> j526110617;
	j3249542286 [label="So out of infinities of possible worlds, you just happened to home in on the right one?",shape=box,color=red];
		j3249542286 -> j4059836242;
	j526110617 [label="Eleanor, the Indifference can look like whatever it wants. It looked like Albrecht when he first encountered it.",shape=box,color=red];
		j526110617 -> j3958470634;
	j2229640217 [label="It wasn’t emptiness. I could have borne that. It went on and on forever, but it wasn’t just cold space. There was something in there with me.",shape=box,color=blue];
		j2229640217 -> j299681524;
	j4059836242 [label="It’s okay that you’re not convinced. I know this sounds insane. Just hear me out, okay?",shape=box,color=blue];
		j4059836242 -> j9376899;
	j3958470634 [label="I think maybe it sensed what you wanted and took on the form you were hoping to see.",shape=box,color=red];
		j3958470634 -> j9376899;
	j299681524 [label="Boolean EleanorIndifference2 is now true.",color=orange];
		j299681524 -> j797616394;
	j9376899 [label="I’ll make a deal with you.",shape=box,color=blue];
		j9376899 -> j1084063187;
	j1084063187 [label="First you let me finish telling you what happened.",shape=box,color=blue];
		j1084063187 -> j2646854017;
	j2646854017 [label="Once I’m done, you can make your own mind up about whether it was the Indifference messing with me or not. I promise I’ll take you seriously.",shape=box,color=blue];
		j2646854017 -> j340355965;
	j340355965 [label="Deal?",shape=box,color=blue];
		j340355965 -> j3940372900;
		j340355965 -> j2696790149;
	j3940372900 [label="Deal.",shape=box,color=red];
		j3940372900 -> j3757943919;
	j2696790149 [label="I can't believe you did this. Never do that again, Eleanor! I CAN'T LOSE YOU TO THAT THING!",shape=box,color=red];
		j2696790149 -> j754664860;
	j3757943919 [label="So like I was saying. I found her.",shape=box,color=blue];
		j3757943919 -> j1199887624;
	j754664860 [label="Excuse me?",shape=box,color=blue];
		j754664860 -> j528793918;
	j1199887624 [label="I could feel the Indifference watching. I could feel it make a face like one of those dried-out desert mummies as it reached out for me. I reached out for her, and she pulled me through.",shape=box,color=blue];
		j1199887624 -> j2396921858;
	j528793918 [label="I'm not sure what to say. I'm a bit concerned, because you sound so upset, but I didn't realise you cared quite so much.",shape=box,color=blue];
		j528793918 -> j850799147;
	j2396921858 [label="She was sitting in the Lotus position in some kind of open-air shrine, next to a pit full of embers. It was nighttime. The moon was dim, ghosting through scrolling clouds.",shape=box,color=blue];
		j2396921858 -> j2007292108;
	j850799147 [label="I'm all right. Please don't worry about me.",shape=box,color=blue];
		j850799147 -> j3178522693;
	j2007292108 [label="She put a finger to her lips.",shape=box,color=blue];
		j2007292108 -> j2120616232;
	j3178522693 [label="+5 Chemistry",color=orange];
		j3178522693 -> j3982619922;
	j2120616232 [label="For a second I could feel that giant hand still groping for me. Then it recoiled, like a guillotine had come down on one of its fingers.",shape=box,color=blue];
		j2120616232 -> j731340924;
	j3982619922 [label="I can't promise anything, but I'll try not to scare you again.",shape=box,color=blue];
		j3982619922 -> j1287280685;
		j3982619922 -> j8286478;
	j731340924 [label="Grass whispered. Crickets sang. We were alone.",shape=box,color=blue];
		j731340924 -> j3125460765;
	j1287280685 [label="Fine... just... Tell me what happened.",shape=box,color=red];
		j1287280685 -> j3757943919;
	j8286478 [label="I can't be a party to this. [End.]",shape=box,color=red];
		j8286478 -> "Chat ends.";
	j3125460765 [label="This is all sounding horribly familiar. And then?",shape=box,color=red];
		j3125460765 -> j468238241;
	j468238241 [label="We sat across the fire-pit from each other. She didn’t speak, but she let me look into her mind, soft and sisterly.",shape=box,color=blue];
		j468238241 -> j2638504898;
	j2638504898 [label="She was so happy, so perfectly content. A woman in the place that was right for her. Callused feet, clear eyes. Breathing the fire and drinking the moon.",shape=box,color=blue];
		j2638504898 -> j351112305;
		j2638504898 -> j4121741213;
	j351112305 [label="So you felt that WOULD have been the right choice for you?",shape=box,color=red];
		j351112305 -> j8187515;
	j4121741213 [label="Did you feel tempted to stay?",shape=box,color=red];
		j4121741213 -> j8187515;
	j8187515 [label="No! Just the opposite!",shape=box,color=blue];
		j8187515 -> j4031601977;
	j4031601977 [label="I didn’t need to take that path, because she already had. Don’t you see? I’m free now. I never need to wonder ‘what if’ because there already IS a Sister Eleanor. She’s got my back. Am I making sense?",shape=box,color=blue];
		j4031601977 -> j816209962;
	j816209962 [label="Kind of? All I want to know is, did it help?",shape=box,color=red];
		j816209962 -> j1318257813;
	j1318257813 [label="Oh yeah. I’ve never been more certain of what to do next.",shape=box,color=blue];
		j1318257813 -> j2409000933;
	j2409000933 [label="+15 Chemistry",color=orange];
		j2409000933 -> j1009235455;
	j1009235455 [label="I’m going to take the life she freed me up to live, and live it to the hilt.",shape=box,color=blue];
		j1009235455 -> "Chat ends.";
}