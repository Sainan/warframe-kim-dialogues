digraph {
	"EleanorRank5Convo6 begins." [color=orange];
		"EleanorRank5Convo6 begins." -> j1202098568;
	j1202098568 [label="Можливо, ти помітив, що сьогодні вранці торговельний центр виглядає трохи інакше.",shape=box,color=blue];
		j1202098568 -> j1015146143;
	j1015146143 [label="Нічого особливого та очевидного. Ще кілька посмішок на кількох обличчях. Сміх то тут, то там. Бризки щастя в повітрі.",shape=box,color=blue];
		j1015146143 -> j3829730747;
		j1015146143 -> j3334883684;
	j3829730747 [label="Це залежить від тебе, чи не так?",shape=box,color=red];
		j3829730747 -> j3436634692;
	j3334883684 [label="ЗАРАЗБУДУ [Кінець.]",shape=box,color=red];
		j3334883684 -> "Chat ends.";
	j3436634692 [label="Просто мій спосіб віддячити. Як відомо, телепат може надсилати, так само як і отримувати. Це не обов’язково має бути струмінь зі шланга для однієї людини. Це може бути схожим на розпилення туману на території.",shape=box,color=blue];
		j3436634692 -> j2290836502;
	"Chat ends." [color=orange];
	j2290836502 [label="Чи є у вас, звідки ти родом, зрошувачі для газону?",shape=box,color=blue];
		j2290836502 -> j2698898371;
		j2290836502 -> j2210406823;
	j2698898371 [label="У нас є НДРРД і такі ж дрони. Вони доглядали за рослинами на «Заримені». Це те, що ти маєш на увазі?",shape=box,color=red];
		j2698898371 -> j935036007;
	j2210406823 [label="Зро…шувачі?",shape=box,color=red];
		j2210406823 -> j1927022728;
	j935036007 [label="Саме так.",shape=box,color=blue];
		j935036007 -> j3753879496;
	j1927022728 [label="Так, сонце, зрошувачі. Розпилювачі води для підтримання газону у вологому стані.",shape=box,color=blue];
		j1927022728 -> j81040540;
	j3753879496 [label="Коли я була маленькою дівчинкою, я обожнювала зрошувачі для газонів. Вони були чарівні. Коли сонце було найяскравішим, у них можна було зловити веселку. Ти пробігаєш крізь них і відчуваєш приємний холодок на своєму тілі.",shape=box,color=blue];
		j3753879496 -> j2997770964;
	j81040540 [label="Також ідеально підходить для того, щоби притискати обличчя свого брата, поки він не закричить.",shape=box,color=blue];
		j81040540 -> j2997770964;
	j2997770964 [label="«Щастя — це зрошувачі для газону». Напиши це на вітальній листівці.",shape=box,color=blue];
		j2997770964 -> j242096650;
	j242096650 [label="Здається, у тебе сьогодні гарний настрій. Приємно бачити тебе такою.",shape=box,color=red];
		j242096650 -> j22197767;
	j22197767 [label="Так і є. І я не розумію, чому я маю тримати це в собі. Тож я наповнюю ним весь торговельний центр.",shape=box,color=blue];
		j22197767 -> j1971029942;
		j22197767 -> j3558822563;
		j22197767 -> j3141803063;
	j1971029942 [label="Хіба це не схоже на наркотики?",shape=box,color=red];
		j1971029942 -> j3347675880;
	j3558822563 [label="Транслюєш позитивний настрій? Чому б ні. Я не бачу, чим це відрізняється від вмикання музики.",shape=box,color=red];
		j3558822563 -> j1658334949;
	j3141803063 [label="То сьогодні я можу побачити посмішку Артура?",shape=box,color=red];
		j3141803063 -> j1231021199;
	j3347675880 [label="… чудово. Я тут намагаюся бути таємною феєю щастя, що розбризкує радість, а натомість перетворююся на злого наркотичного гобліна. Мій веселий настрій стрімко впав.",shape=box,color=blue];
		j3347675880 -> j3984127867;
	j1658334949 [label="О! Гарна аналогія!",shape=box,color=blue];
		j1658334949 -> j3659510173;
	j1231021199 [label="Можливо. Я на це сподіваюся. Йому довелося стільки всього пережити.",shape=box,color=blue];
		j1231021199 -> j3170080075;
	j3984127867 [label="Добре. Я перестану.",shape=box,color=blue];
		j3984127867 -> "Chat ends.";
	j3659510173 [label="Якщо Аой може впливати на настрій за допомогою музики, то я можу робити це за допомогою вібрацій, чи не так?",shape=box,color=blue];
		j3659510173 -> j619261880;
	j3170080075 [label="Так багато змінилося відтоді, як ти тут. Я маю на увазі не лише нас із тобою. Навіть торговельний центр.",shape=box,color=blue];
		j3170080075 -> j619261880;
	j619261880 [label="Я рада, що Аой переконала мене залишитися.",shape=box,color=blue];
		j619261880 -> j3745965944;
		j619261880 -> j4024079285;
	j3745965944 [label="Зажди. Що вона зробила?",shape=box,color=red];
		j3745965944 -> j2543577115;
	j4024079285 [label="Назад. Щось пройшло повз мене.",shape=box,color=red];
		j4024079285 -> j2543577115;
	j2543577115 [label="Я тобі не казала?",shape=box,color=blue];
		j2543577115 -> j3915623479;
	j3915623479 [label="Ох. Це було ще до твого прибуття. Ми цілий день намагалися знищити лігво техногнилі. В повітрі висіла вбивча напруга. Бійки зазвичай вгамовують нашу злість і залишають нас занадто виснаженими, щоб кидатися один на одного, але цього дня все було інакше.",shape=box,color=blue];
		j3915623479 -> j960981186;
	j960981186 [label="Пам’ятаю, Артур сказав щось на кшталт «ми повторимо все це завтра», і це вдарило по всіх нас, як кувалдою. Дорогою назад до торговельного центру ніхто не промовив жодного слова.",shape=box,color=blue];
		j960981186 -> j293518523;
		j960981186 -> j2576069278;
		j960981186 -> j4031978036;
	j293518523 [label="Ого. Гарний спосіб підняти командний дух, Артуре.",shape=box,color=red];
		j293518523 -> j3259236590;
	j2576069278 [label="Він може бути таким чарівником…",shape=box,color=red];
		j2576069278 -> j3259236590;
	j4031978036 [label="Багато чого на його плечах. Це дорого коштує.",shape=box,color=red];
		j4031978036 -> j650889500;
	j3259236590 [label="Думаю, він просто намагався змусити нас зосередитися на роботі. Але я відчувала себе розбитою. Артур і Аой не розмовляли, Амір так сильно діяв на нерви Квінсі, що я почала побоюватися за його життя, а Летті косилася на мене при кожній нагоді.",shape=box,color=blue];
		j3259236590 -> j1708735503;
	j650889500 [label="Так. Зазвичай йому вдається приховати це, навіть коли він уже на межі. Але, мабуть, тоді він не впорався.",shape=box,color=blue];
		j650889500 -> j1893139440;
	j1708735503 [label="Тоді я провела багато часу дивлячись у дзеркало. Я продовжувала уявляти, як моє відображення хапає мене за горло та шипить: «ТИ — проблема. Якби не ти, нічого б цього не було».",shape=box,color=blue];
		j1708735503 -> j2666955248;
		j1708735503 -> j1948625473;
	j1893139440 [label="Він перегорів, намагаючись утримати команду разом.",shape=box,color=blue];
		j1893139440 -> j3259236590;
	j2666955248 [label="Звучить так, ніби всі інші для різноманітності влізли у ТВОЮ голову.",shape=box,color=red];
		j2666955248 -> j3867194347;
	j1948625473 [label="Це схоже на те лайно, яке видає Летті.",shape=box,color=red];
		j1948625473 -> j375952720;
	j3867194347 [label="О, це були не вони. Це була я. Того дня вирвалися назовні всі затаєні сумніви та ненависть до себе, які я стримувала в собі.",shape=box,color=blue];
		j3867194347 -> j3764787662;
	j375952720 [label="Дякую. Я знаю, що ти на моєму боці, і це багато для мене значить.",shape=box,color=blue];
		j375952720 -> j3761746768;
	j3764787662 [label="Я подумала: цій команді потрібен цап-відбувайло.",shape=box,color=blue];
		j3764787662 -> j2498885163;
	j3761746768 [label="Я стільки разів чула цю фразу від Летті, що врешті-решт подумала: можливо, вона має рацію. Залишаючись тут, я наражаю всіх на небезпеку.",shape=box,color=blue];
		j3761746768 -> j1919616498;
	j2498885163 [label="Монстр. Хтось винний.",shape=box,color=blue];
		j2498885163 -> j1919616498;
	j1919616498 [label="Я розробила цілий план. Я вислизала вночі, ховалася в місті, де Артур ніколи б мене не знайшов, і — цим я пишалася найбільше —продовжувала допомагати на відстані.",shape=box,color=blue];
		j1919616498 -> j2214771217;
		j1919616498 -> j805882588;
	j2214771217 [label="Думаю, він би тебе знайшов. Він досить рішучий.",shape=box,color=red];
		j2214771217 -> j171183243;
	j805882588 [label="Найімовірніший сценарій — він би загинув, намагаючись знайти тебе.",shape=box,color=red];
		j805882588 -> j652259178;
	j171183243 [label="Як? Амір не може відстежувати телепатію.",shape=box,color=blue];
		j171183243 -> j4093888631;
	j652259178 [label="Намагаєшся звинуватити мене?",shape=box,color=blue];
		j652259178 -> j3426350063;
	j4093888631 [label="Я збиралася стати шляхетним вигнанцем. Я би присідала на дахах, з обвітреним обличчям і рішучим поглядом. Я би могла доповідати про накопичення техногнилі, повідомляти «Прокльон», де «Скальдра» встановлює блокпости, навіть виступала в ролі аварійного ретранслятора. Щоразу рятуючи дупу Летті, я би змушувала її відчувати це по-справжньому. Дріб’язково? Безумовно.",shape=box,color=blue];
		j4093888631 -> j3758011634;
	j3426350063 [label="Не турбуйся. Я і так почуваюся жахливо.",shape=box,color=blue];
		j3426350063 -> "Chat ends.";
	j3758011634 [label="Я обрала момент. Я дочекалася, поки Амір засне. Потім я одягнула довге пальто й попрямувала до гаража так тихо, як тільки могла.",shape=box,color=blue];
		j3758011634 -> j2970953351;
	j2970953351 [label="Захисна решітка опустилася сама собою. Я обернулася і побачила Аой.",shape=box,color=blue];
		j2970953351 -> j3182242387;
	j3182242387 [label="Ми проговорили всю ніч і плакали в обіймах одне одного. Їй теж було боляче. Глибоко. Коли зійшло сонце, я вирішила залишитися.",shape=box,color=blue];
		j3182242387 -> j2426152181;
		j3182242387 -> j1078086938;
		j3182242387 -> j3894130690;
	j2426152181 [label="У тебе ще є час передумати.",shape=box,color=red];
		j2426152181 -> j3725544854;
	j1078086938 [label="Аой чудова. Я радію, що вона була поруч з тобою.",shape=box,color=red];
		j1078086938 -> j1057043623;
	j3894130690 [label="Це була удача.",shape=box,color=red];
		j3894130690 -> j1057043623;
	j3725544854 [label="Який би гротескний імпульс не спонукав тебе написати це, я сподіваюся, що ти ним захлинешся.",shape=box,color=blue];
		j3725544854 -> "Chat ends.";
	j1057043623 [label="+20 Chemistry",color=orange];
		j1057043623 -> j2111024966;
	j2111024966 [label="Так. Думаю, нам усім шістьом судилося бути тут у новорічну ніч. І тобі, звісно. Незнайомець у дверях, що приносить удачу.",shape=box,color=blue];
		j2111024966 -> "Chat ends.";
}