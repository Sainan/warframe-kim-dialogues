digraph {
	"EleanorRank5Convo7 begins." [color=orange];
		"EleanorRank5Convo7 begins." -> j3860788600;
	j3860788600 [label="«Байдужість» є тим, чим вона Є, чи тим, чого вона ХОЧЕ?",shape=box,color=blue];
		j3860788600 -> j3756840881;
		j3860788600 -> j1554902574;
		j3860788600 -> j3180000748;
	j3756840881 [label="Ви з Летті обидві так робите, знаєш. Ви забуваєте казати «Привіт.»",shape=box,color=red];
		j3756840881 -> j1862760581;
	j1554902574 [label="Більшість людей почала б із «Привіт.»",shape=box,color=red];
		j1554902574 -> j1912805382;
	j3180000748 [label="[Ігнорувати.]",shape=box,color=red];
		j3180000748 -> "Chat ends.";
	j1862760581 [label="Ти знайшов те, що нас об’єднує, вітаю. І вибач. Це важливіше.",shape=box,color=blue];
		j1862760581 -> j637512606;
	j1912805382 [label="Вибач. Це важливіше.",shape=box,color=blue];
		j1912805382 -> j637512606;
	"Chat ends." [color=orange];
	j637512606 [label="Я думаю, що «Велика Байдужість» спробує якось стати між нами.",shape=box,color=blue];
		j637512606 -> j2067050927;
	j2067050927 [label="Не смійся. Я відчуваю це, глибоко в кістковому мозку. Те, що було в Русалці, хотіло б бачити, як ми розриваємо один одного на шматки.",shape=box,color=blue];
		j2067050927 -> j230507794;
	j230507794 [label="Я не знаю чому. Просто знаю.",shape=box,color=blue];
		j230507794 -> j274406816;
		j230507794 -> j2191778798;
	j274406816 [label="Так. Думаю, ти маєш рацію.",shape=box,color=red];
		j274406816 -> j3533576991;
	j2191778798 [label="Можливо, саме це сталося з родиною Ентраті. Вони роками жили прямо над його лабораторією, навіть не підозрюючи про це. Коли дитя знайшло їх, вони були відчуженими, зануреними в себе.",shape=box,color=red];
		j2191778798 -> j3533576991;
	j3533576991 [label="Я бачила це в твоїй голові. Альбрехт і Лойд. Матір і Батько. Ти і твої батьки. Русалка і Віктор, наскільки нам відомо. І шестеро нас, звісно.",shape=box,color=blue];
		j3533576991 -> j598832195;
	j598832195 [label="Ця штука наганяє холоду. Збочення. Але десь за цим стоїть мотивація.",shape=box,color=blue];
		j598832195 -> j2335832059;
		j598832195 -> j374595998;
	j2335832059 [label="Думаю, ти маєш рацію. На «Заримені» батьки були першими, хто з’їхав з глузду. Можливо, в «Паланці» протрималися так довго тому, що ніхто з них не мав дітей.",shape=box,color=red];
		j2335832059 -> j2731185555;
	j374595998 [label="Я не люблю претендувати на розуміння речей, які, по суті, є *богами*. Ця штука намагається зрозуміти емоції. Вона просто погано грається зі своїми іграшками.",shape=box,color=red];
		j374595998 -> j3476329361;
	j2731185555 [label="Нехай тільки спробує стати між нами. І допоможе мені Сол, я покінчу з цим. Я знайду спосіб.",shape=box,color=blue];
		j2731185555 -> j2640176646;
	j3476329361 [label="Чудова теорія, але ми цього не ЗНАЄМО, чи не так?",shape=box,color=blue];
		j3476329361 -> j3287144236;
	j2640176646 [label="У Всесвіті є сили, проти яких ця штука не може встояти. Я обираю вірити в це кожною часткою себе.",shape=box,color=blue];
		j2640176646 -> j2565281231;
	j3287144236 [label="Якщо ти маєш рацію, то вона бездоганна. Майже по-дитячому. Це не відповідає тому, що ми знаємо. Вона зловмисна.",shape=box,color=blue];
		j3287144236 -> "Chat ends.";
	j2565281231 [label="Якщо вона проковтне мене цілком, я застрягну, як кістка в горлі, та задушу її.",shape=box,color=blue];
		j2565281231 -> j3707393628;
		j2565281231 -> j575574539;
	j3707393628 [label="Це чудовий спосіб померти.",shape=box,color=red];
		j3707393628 -> j2820582961;
	j575574539 [label="Я ще не знаю, як ми це зробимо, але я з тобою. Ми повинні перемогти.",shape=box,color=red];
		j575574539 -> j4100159136;
	j2820582961 [label="З таким сценарієм я все одно мертва.",shape=box,color=blue];
		j2820582961 -> j2399752572;
	j4100159136 [label="Якби це була казка, можна було б придумати який хитрий трюк. Відколупати кам’яне серце велетня і розбивали його лебединим пером, або обманом змусити кинутися зі скелі.",shape=box,color=blue];
		j4100159136 -> j777784921;
	j2399752572 [label="Але принаймні я можу плюнути їй в очі.",shape=box,color=blue];
		j2399752572 -> "Chat ends.";
	j777784921 [label="Ти казала, що була «журналістом». Це не ті історії, які ти звикла розповідати, чи не так? Ти казала правду, якою б потворною вона не була.",shape=box,color=red];
		j777784921 -> j1273673450;
	j1273673450 [label="Якби ці старі казки не говорили правди, люди не продовжували б їх розповідати.",shape=box,color=blue];
		j1273673450 -> j3329169431;
	j3329169431 [label="Я не маю на увазі, що велетні існують. Я просто маю на увазі, що старі казки розповідають іншу правду.",shape=box,color=blue];
		j3329169431 -> j3959263735;
		j3329169431 -> j4000871894;
	j3959263735 [label="Щоб пройти через це, нам знадобиться щось більше, ніж казки, якими б надихаючими вони не були.",shape=box,color=red];
		j3959263735 -> j3694259583;
	j4000871894 [label="Моє життя було в королівстві з казок. Я знаю, наскільки сильними вони можуть бути.",shape=box,color=red];
		j4000871894 -> j426897207;
	j3694259583 [label="Я знаю. Хотілося б мати щось більше, ніж інстинкт, щоб йти далі.",shape=box,color=blue];
		j3694259583 -> j424319263;
	j426897207 [label="Я думаю, що всі ці казки про вбивць-велетнів об’єднує одне: вони про те, як вивчити правила і з’ясувати, як поводитися з ними розумно.",shape=box,color=blue];
		j426897207 -> j3592217248;
	j424319263 [label="Ми стикаємося з чимось величезним, жахливим і непідвладним нашому розумінню. Зазвичай це роблять лише діти. Саме для цього існують казки.",shape=box,color=blue];
		j424319263 -> j4140312819;
		j424319263 -> j2496585928;
	j3592217248 [label="Деякі монстри дурні. Деякі дуже примітивні. У деяких є секрети. Але ти не зможеш перемогти, поки не дізнаєшся про правила, якими керується монстр.",shape=box,color=blue];
		j3592217248 -> j4140312819;
		j3592217248 -> j2496585928;
	j4140312819 [label="Воно застрягло в стіні. Я навіть не знаю, що являє собою «стіна».",shape=box,color=red];
		j4140312819 -> j1366803624;
	j2496585928 [label="Я не думаю, що «Байдужість» пов’язана якимись правилами.",shape=box,color=red];
		j2496585928 -> j277267946;
	j1366803624 [label="Не має значення. Якщо воно дійсно застрягло, то ми можемо зробити два висновки, чи не так?",shape=box,color=blue];
		j1366803624 -> j1754263085;
	j277267946 [label="Вони мають бути! Навіть Боги мають правила! Якою б могутньою не здавалася «Байдужість», вона не є Богом.",shape=box,color=blue];
		j277267946 -> j3770091006;
	j1754263085 [label="По-перше, воно може потрапити в пастку. Можливо, навіть бути ув’язненим.",shape=box,color=blue];
		j1754263085 -> j4164822029;
	j3770091006 [label="Ти здолав Тракса. Неважливо, скільки разів ти зазнавав невдач і опинявся на сраці. Ти ПЕРЕМІГ. Чому б тобі не здолати Людину в стіні?",shape=box,color=blue];
		j3770091006 -> j3109393797;
	j4164822029 [label="+15 Chemistry",color=orange];
		j4164822029 -> j1387983665;
	j3109393797 [label="Коли ти перестала цього боятися, Елеоноро?",shape=box,color=red];
		j3109393797 -> j2799283430;
	j1387983665 [label="А по-друге, воно чогось ХОЧЕ. Воно хоче вирватися назовні. Якщо ми знаємо, чого воно хоче, ми вже на крок попереду.",shape=box,color=blue];
		j1387983665 -> "Chat ends.";
	j2799283430 [label="Я не перестала.",shape=box,color=blue];
		j2799283430 -> j3478105437;
	j3478105437 [label="+20 Chemistry",color=orange];
		j3478105437 -> j805209799;
	j805209799 [label="Просто тепер я більше боюся тебе втратити, ясно?",shape=box,color=blue];
		j805209799 -> "Chat ends.";
