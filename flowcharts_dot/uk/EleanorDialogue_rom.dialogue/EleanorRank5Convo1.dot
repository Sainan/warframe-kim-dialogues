	j1204850669 [label="Це буде одна з тих розмов «влаштовуйся зручніше, мандрівниче, це займе хвилинку», чи не так?",shape=box,color=red];
		j1204850669 -> j1864617733;
	j4125508160 [label="Друже. Все, що нам дав Ентраті — владу, силу, броньовані дупи… я б обміняла все це на крихту впевненості, що смерть — це не кінець.",shape=box,color=blue];
