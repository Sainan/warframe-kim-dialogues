		j2845558772 -> j4151347695;
	j260510224 [label="Вона — Смерть. Також одна з форм Луа, залежно від того, яких звичаїв ти дотримуєшся. Вона приходить і вкладає тебе в ліжко, коли закінчується шалений день Життя, цілує на ніч і співає тобі перед сном.",shape=box,color=blue];
