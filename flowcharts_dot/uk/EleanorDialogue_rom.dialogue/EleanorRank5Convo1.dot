	j1864617733 [label="… так і мало бути.",shape=box,color=blue];
		j1864617733 -> j1586043400;
	j2792277562 [label="Я намагалася. Мені неприємно в цьому зізнаватися, але іноді, коли ми виходимо на патрулювання і йде бій, я заглядаю в думки вмираючих і намагаюся пережити цей момент разом із ними.",shape=box,color=blue];
		j2792277562 -> j808498134;
		j2792277562 -> j1756385230;
	j1586043400 [label="Забудь. Я впевнена, що у тебе є важливіші справи.",shape=box,color=blue];
		j1586043400 -> "Chat ends.";
	j808498134 [label="Елеоноро, це огидно. Ти не маєш права.",shape=box,color=red];
		j808498134 -> j479936520;
	j1756385230 [label="Ти намагаєшся їх заспокоїти?",shape=box,color=red];
		j1756385230 -> j475426671;
	j479936520 [label="Я знаю. Я ЗНАЮ.",shape=box,color=blue];
		j479936520 -> j1633998356;
	j475426671 [label="Одного разу. Що тут поганого, правда? Вони все одно помирають. Це була молода десантниця «Скальдри». Вона йшла на мене, і я вистрілила їй у груди.",shape=box,color=blue];
		j475426671 -> j3931723141;
	j1633998356 [label="Я кажу собі, що намагаюся допомогти, але правда в тому, що я просто хотіла подивитися. Твоя правда. Це було огидно.",shape=box,color=blue];
		j1633998356 -> "Chat ends.";
	j3931723141 [label="Я опустилася на коліна біля неї, коли вона захлиналася кров’ю, і намагалася сказати їй, що все гаразд, що біль закінчиться, і скоро вона буде зі своєю сім’єю, яка пішла раніше, і всі ті заспокійливі речі, про які ти думаєш, коли маєш справити панахиду на смертному одрі.",shape=box,color=blue];
		j3931723141 -> j2041236445;
	j2041236445 [label="Вона була нажахана. Те, що залишилося від її свідомості, шкреблося, як пацюк у баку з водою. Коли вона відчула, що я подумки з нею, вона намагалася зачепитися за мене. Вона благала мене про допомогу. Під усім цим було це… я навіть не знаю, як це назвати…",shape=box,color=blue];
		j2041236445 -> j4069464081;
		j2041236445 -> j2688724650;
		j2041236445 -> j3903433569;
	j4069464081 [label="Надія?",shape=box,color=red];
