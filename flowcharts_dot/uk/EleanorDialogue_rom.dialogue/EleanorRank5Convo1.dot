		j2688724650 -> j107966579;
	j3903433569 [label="Заперечення?",shape=box,color=red];
		j3903433569 -> j107966579;
	j107966579 [label="Заперечення. Вона не вірила, що може померти. І навіщо їй це робити? Вона завжди була там раніше. Як смерть могла забрати її зараз?",shape=box,color=blue];
		j107966579 -> j358903602;
	j358903602 [label="Кожного дня свого життя ти сприймаєш СЕБЕ як належне. Ти там, коли прокидаєшся, і ти там, коли спиш. Ти живеш усе своє життя в домі, зробленому з Тебе. Ти жодного разу не виходиш назовні, бо не можеш.",shape=box,color=blue];
		j358903602 -> j2214167354;
	j2214167354 [label="А потім одного дня ти чуєш, як будинок руйнують.",shape=box,color=blue];
		j2214167354 -> j234431678;
		j2214167354 -> j1940603593;
	j234431678 [label="Чому ти її не допомогла?",shape=box,color=red];
		j234431678 -> j2311239313;
	j1940603593 [label="Ти залишилася з нею до кінця?",shape=box,color=red];
		j1940603593 -> j2764306629;
	j2311239313 [label="Що на твою думку, я мала робити? Покликати Летті? Залікувати зяючу рану на грудях? Усе тому, що я бачила, через що вона пройшла, зблизька?",shape=box,color=blue];
		j2311239313 -> j3708994247;
	j2764306629 [label="Так. Я була налякана до всирачки. Коли вона відчула, що ковзає по краю, я була впевнена, що мене потягне разом із нею. Я відчувала, як мене тягне на дно.",shape=box,color=blue];
		j2764306629 -> j2818083285;
	j3708994247 [label="Зі «Скальдрою» це — вбити або бути вбитим. Вони не пропонують нам розкіш милосердя.",shape=box,color=blue];
		j3708994247 -> "Chat ends.";
	j2818083285 [label="Але я мусила залишитися. Я не могла залишити її помирати на самоті.",shape=box,color=blue];
		j2818083285 -> j105770823;
	j105770823 [label="Хоча, гадаю, зрештою, так і було. Я не могла зробити цей останній крок разом із нею.",shape=box,color=blue];
		j105770823 -> j3337709251;
		j105770823 -> j4038760248;
