		j3975687166 -> j2921043337;
	j4294204866 [label="Ні.",shape=box,color=blue];
		j4294204866 -> j2921043337;
	j2921043337 [label="Але вона це зробила. Вона побачила обличчя своєї матері, дітей, з якими ходила до школи, свого кота, двоюрідного брата, який потонув. Але вони не були примарами чи духами. Лише спогади, виштовхнуті на поверхню вмираючим мозком, який відчайдушно шукає, за що б зачепитися.",shape=box,color=blue];
		j2921043337 -> j3289444062;
	j3289444062 [label="Потім дім її індивідуальності почав розвалюватися навколо неї. Перед смертю вона широко розплющила очі. Я думаю, в ту секунду, вперше в житті, вона по-справжньому зрозуміла, що таке смерть.",shape=box,color=blue];
		j3289444062 -> j4050530498;
		j3289444062 -> j434276419;
	j4050530498 [label="Помираюча людина розуміє, що таке смерть. Яка проникливість, Елеоноро.",shape=box,color=red];
		j4050530498 -> j2932493445;
	j434276419 [label="Що ти маєш на увазі? Всі знають, що таке смерть.",shape=box,color=red];
		j434276419 -> j2993977066;
	j2932493445 [label="Слухай. Наступного разу, коли ти відчуєш потребу викрити мене в маячні, ти не маєш зобов’язання вислуховувати її до кінця.",shape=box,color=blue];
		j2932493445 -> j3015795888;
	j2993977066 [label="Ні. Ми думаємо, що ми знаємо, але весь наш досвід поверховий.",shape=box,color=blue];
		j2993977066 -> j1076954167;
	j3015795888 [label="Ніхто не змушує тебе говорити зі мною. Бувай.",shape=box,color=blue];
		j3015795888 -> "Chat ends.";
	j1076954167 [label="Усе своє життя вона була схожа на жінку, яка живе в будинку без світла, і все, що у неї було, — це ліхтарик. Вона проливала світло своєї обізнаності на те чи інше місце. Спогад, завдання, взаємодія.",shape=box,color=blue];
		j1076954167 -> j1505693925;
	j1505693925 [label="Вона переходила з кімнати в кімнату, досліджувала підвали уві сні, але завжди це був замкнений, кінцевий процес. Ти мене розумієш?",shape=box,color=blue];
		j1505693925 -> j883790387;
		j1505693925 -> j1795805934;
	j883790387 [label="Не зовсім, але продовжуй.",shape=box,color=red];
		j883790387 -> j2992487467;
	j1795805934 [label="Так.",shape=box,color=red];
		j1795805934 -> j2992487467;
	j2992487467 [label="Потім, коли будинок був зруйнований, а її особистість розпалася на шматки навколо, раптом ліхтарик не світив на стіну, на книгу, на диван. Він світив у безмежне небо. Її свідомість спалахнула в нескінченність, не маючи перешкоди у вигляді неї самої.",shape=box,color=blue];
		j2992487467 -> j2822997559;
		j2992487467 -> j4039713053;
		j2992487467 -> j2151131561;
	j2822997559 [label="Але усвідомлення помирає, коли помирає тіло. Те, що ти відчула, було лише спазмом в її вмираючому мозку.",shape=box,color=red];
		j2822997559 -> j582363629;
	j4039713053 [label="Ти думаєш, що «усвідомлення» — це те, ким ми є насправді, а все інше — лише оболонка?",shape=box,color=red];
		j4039713053 -> j382336593;
	j2151131561 [label="Ого. Так глибоко. Ти застрелила людину, а потім порушила її приватне життя.",shape=box,color=red];
		j2151131561 -> j2932493445;
	j582363629 [label="Я намагалася сказати собі це багато разів.",shape=box,color=blue];
		j582363629 -> j2699731034;
	j382336593 [label="+5 Chemistry",color=orange];
		j382336593 -> j3920032206;
	j2699731034 [label="Я не можу повірити, що це було не що інше, як відключення мозку. Відчуття було таке, ніби душа виривається на волю.",shape=box,color=blue];
		j2699731034 -> "Chat ends.";
	j3920032206 [label="Я знаю, що це не має жодного сенсу. Це здається таким парадоксальним, думка про те, що ми — насамперед точка зору, а особистість — у другу чергу.",shape=box,color=blue];
		j3920032206 -> j1778510452;
	j1778510452 [label="Як ти ставишся до всього цього?",shape=box,color=red];
		j1778510452 -> j913539020;
	j913539020 [label="Заспокійливо? Мені подобається думка, що хтось із нас виживає. Мені подобається думати, що смерть — це не видалення з всесвіту, а видалення всього, що відокремлює тебе від всесвіту.",shape=box,color=blue];
		j913539020 -> j370842404;
	j370842404 [label="Але те, що робить нас НАМИ — це не просто сира свідомість. Це пам’ять, емоції, думки, різні нашарування. Коли ти втрачаєш все це, ти втрачаєш саму особистість, навіть якщо ця іскра якимось чином зберігається.",shape=box,color=red];
		j370842404 -> j3033539085;
	j3033539085 [label="Я знаю.",shape=box,color=blue];
		j3033539085 -> j3441746341;
	j3441746341 [label="Чесно кажучи, саме тому я хотіла поговорити з тобою про привидів Порожнечі, цефалонів і всі ті речі з твого часу. Я хочу знати, чи зможемо ми коли-небудь поспілкуватися з тими, кого ми втратили.",shape=box,color=blue];
		j3441746341 -> j2294670063;
	j2294670063 [label="Справжнє спілкування, а не якесь голограмне лайно. Таке, що дозволяє тобі точно знати, що думає людина.",shape=box,color=blue];
		j2294670063 -> j1773521237;
		j2294670063 -> j1946345166;
	j1773521237 [label="Ні, не зовсім. Навіть в майбутньому люди, яких ми втратили… на щастя чи на біду? Просто… пішли.",shape=box,color=red];
		j1773521237 -> j2661410535;
	j1946345166 [label="Елеоноро, ближче до справи. Кого ти втратила?",shape=box,color=red];
		j1946345166 -> j2530737967;
	j2661410535 [label="Я зрозуміла. Можливо, є двері, які ми ніколи не зможемо відчинити. Може, це й на краще.",shape=box,color=blue];
		j2661410535 -> "Chat ends.";
