		j79551130 -> j64361787;
	j1813877187 [label="Я розслідувала спалах так званого «синдрому Тестудо». Тестудо означає «черепаха», і у людей, які страждали на нього, з’являлися жорсткі плями на шкірі, що є ранньою ознакою. Те саме, що згодом перетворюється на броню для нас, щасливчиків.",shape=box,color=blue];
