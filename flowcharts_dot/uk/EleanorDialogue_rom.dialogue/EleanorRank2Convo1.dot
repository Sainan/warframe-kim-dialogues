		j1554112057 -> j571538828 [label=false,color=brown];
	j716682357 [label="Я бачила, на що здатні люди. Ми не особливі. Насправді, судячи з того, що я бачила у твоїй голові, ми стаємо навіть гіршими з часом.",shape=box,color=blue];
		j716682357 -> j2257438252;
	j2290503467 [label="Вона сидить там усміхаючись зі своїми ампулами та зіллями, пляшечками з кров’ю та шматками шкіри, в оточені армії друзяк пацюків, і має нахабство називати МЕНЕ відьмою.",shape=box,color=blue];
		j2290503467 -> j154393762;
