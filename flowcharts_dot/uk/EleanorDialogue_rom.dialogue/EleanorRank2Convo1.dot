		j2290503467 -> j102255651;
	j312438667 [label="Зрозумій мене правильно. Я БИ посцяла на неї, якби вона горіла.",shape=box,color=blue];
		j312438667 -> j3497666753;
	j4171894744 [label="Гадаю, ти можеш знайти відповідь самотужки.",shape=box,color=blue];
		j4171894744 -> "Chat ends.";
	j2174754517 [label="Я знаю. Іноді я сама так думаю. Мені постійно сниться кошмар, де я вириваю їй горлянку, коли вона всаджує мені кулю в серце.",shape=box,color=blue];
		j2174754517 -> j4119316671;
	j4218901690 [label="Порожнеча є доказом того, що духовне так само реально, як і матеріальне. Все, що твої люди зробили, окрім того, що змінили назву, це розробили надійний спосіб дістатися туди й назад.",shape=box,color=blue];
		j4218901690 -> j571538828;
	j571538828 [label="Послухай. Те, що я визнаю існування демонічних сил, не означає, що згодна з тим, що мене можна списати.",shape=box,color=blue];
