		j2733706041 -> j1461985963;
	j1557159070 [label="Замість того, щоби слідувати програмі маленької покрученої подвійної спіралі нуклеотидів? Тобто, це те, що люди роблять з того часу, як почали ходити прямо?",shape=box,color=blue];
		j1557159070 -> j3921384913;
	j1099777495 [label="Такі, як Віктор Водяник — і яким саме прізвиськом назвати ОТЕ вибирати тобі — виправдовують свої відсталі погляди саме тим, що вони «природні». Біологічний фундаменталізм. Людина як дзеркало Божественного і таке інше.",shape=box,color=blue];
		j1099777495 -> j3515903914;
	j1967750147 [label="Але Амір милий та забиває баки, чи не так? Амір не має зубів, якими можна розгризти бляшанку з пайком. Ніхто не сприймає Аміра як бомбу повільної дії.",shape=box,color=blue];
		j1967750147 -> j132037985;
	j1088509911 [label="Зрозумій мене правильно. Я БИ посцяла на неї, якби вона горіла.",shape=box,color=blue];
		j1088509911 -> j32005091;
	j2920074954 [label="+10 Chemistry",color=orange];
		j2920074954 -> j2174754517;
	j1461985963 [label="Деякі вважають, що людяність священна. Люди, як Віктор. В його очах я — абсолютне богохульство. Заражена, безсоромна та жінка.",shape=box,color=blue];
		j1461985963 -> j1744524172;
		j1461985963 -> j3523430373;
	j3921384913 [label="«Я не виконую забаганки мікроскопічного тирана! Я людина. Я особлива! Знайти собі пару. Розмножуватися. Захищати територію. Тьху.»",shape=box,color=blue];
		j3921384913 -> j1744524172;
		j3921384913 -> j3523430373;
	j3515903914 [label="Але ми перестали бути природними з того моменту, як почали бити каміннями одне одного. Мова — це вірус, друже. Бути людиною — це чистий аркуш, на якому можна писати, а не первісний Едем, з якого нас вигнали.",shape=box,color=blue];
		j3515903914 -> j1744524172;
		j3515903914 -> j3523430373;
	j132037985 [label="Хоча. Окрім Квінсі.",shape=box,color=blue];
		j132037985 -> "Chat ends.";
	j32005091 [label="Але я б не поспішала.",shape=box,color=blue];
		j32005091 -> "Chat ends.";
	j1744524172 [label="Техногниль могла б поглинути твій розум, і ти б цього не дізналася.",shape=box,color=red];
		j1744524172 -> j1483485705;
	j3523430373 [label="Я знаю, як воно поглинає цілі МІСЯЦІ, Елеоноро!",shape=box,color=red];
		j3523430373 -> j1346512176;
	j1483485705 [label="Тобто… так. Ти маєш рацію. Існують паразити, які так роблять.",shape=box,color=blue];
		j1483485705 -> j3164400179;
	j1346512176 [label="І що, в біса, такого чудового в тому, щоби не бути поглиненим місяцем? Чому заражена версія не є покращенням?",shape=box,color=blue];
		j1346512176 -> j2567496242;
	j3164400179 [label="Але якщо це не паразит? Якщо це симбіонт?",shape=box,color=blue];
		j3164400179 -> j2373046995;
		j3164400179 -> j3463760967;
	j2567496242 [label="О ні! Шматок байдужого космічного каменю, який міг би безцільно дрейфувати ще кілька тисячоліть, раптом став живим та цікавим! Яка катастрофа!",shape=box,color=blue];
		j2567496242 -> j1301432541;
	j2373046995 [label="Ти не знаєш, про що ти говориш. Коли хтось губиться в зараженні, він ГУБИТЬСЯ назавжди. До невпізнаваності.",shape=box,color=red];
		j2373046995 -> j1718462665;
	j3463760967 [label="Я розумію, що тобі потрібно спробувати прийняти те, ким ти стала. Ти зіткнулася з неможливим викликом.",shape=box,color=red];
		j3463760967 -> j1936718736;
	j1301432541 [label="Це та сама маячня, з якою виступає команда Віктора. Сентиментальне чіпляння за те, чим щось колись БУЛО.",shape=box,color=blue];
		j1301432541 -> j754129788;
	j1718462665 [label="Я не знаю, чи ти намагаєшся мене спровокувати, але мені доводилося боротися за життя з цією заразою. Іноді можна розрізнити шматки, які колись були людьми. Голови та ноги просто стирчать із маси.",shape=box,color=red];
		j1718462665 -> j541444678;
	j1936718736 [label="Або ненавидь те, чим ти стала, або прийми це. Та щось підказує мені, що ти вже давно навчилася не ненавидіти себе.",shape=box,color=red];
		j1936718736 -> j3794567274;
	j754129788 [label="Наче бути первісним, це те саме, що бути досконалим.",shape=box,color=blue];
		j754129788 -> j2373046995;
		j754129788 -> j3463760967;
	j541444678 [label="Це не симбіонт. Йому БАЙДУЖЕ, що ти хочеш. Воно не для того, щоби допомогти. Це агресивний рак однієї з форм життя, який лише хоче робити все для себе.",shape=box,color=red];
		j541444678 -> j2725080159;
	j3794567274 [label="Але ці демони у твоїй крові НЕ твої друзі. Найкраще, що ти можеш зробити, це тримати їх на короткому повідцю, Елеоноро. Повір мені.",shape=box,color=red];
		j3794567274 -> j2742250069;
	j2725080159 [label="Нагадай мені, з чого зроблені ваші ворфрейми?",shape=box,color=blue];
		j2725080159 -> j2378405023;
	j2742250069 [label="Ти думаєш, я не знаю?",shape=box,color=blue];
		j2742250069 -> j518083137;
	j2378405023 [label="Із заражених людських істот.",shape=box,color=red];
		j2378405023 -> j2234278251;
	j518083137 [label="Ти хоч уявляєш, що потрібно, аби тримати лють під контролем? Знаєш, яка ціна цього зовнішнього спокою?",shape=box,color=blue];
		j518083137 -> j3406301282;
	j2234278251 [label="Але їхнє зараження було сформоване та контрольоване, чи не так?",shape=box,color=blue];
		j2234278251 -> j1680499603;
	j3406301282 [label="Якщо ти бачиш, що я медитую, я не спілкуюся з великими силами космосу. Я охолоджую ядро реактора.",shape=box,color=blue];
		j3406301282 -> j118009208;
	j1680499603 [label="Так само як зараження в мені.",shape=box,color=blue];
		j1680499603 -> j4027665624;
	j118009208 [label="Є так БАГАТО чого, що я могла би зробити, але НЕ роблю. Місця, куди я могла би взяти ці… речі… які тобі навіть не спадали на думку.",shape=box,color=blue];
		j118009208 -> j2526009383;
	j4027665624 [label="Це означає, що хтось мав сталеві яйця та перестав думати про зараження як безмізку почвару і почав шукати, чим воно може стати.",shape=box,color=blue];
		j4027665624 -> j3391771580;
		j4027665624 -> j2001179717;
	j2526009383 [label="Я могла би зайти в їхні СНИ. І вони би навіть не здогадалися, що я там була.",shape=box,color=blue];
		j2526009383 -> j3328511485;
	j3391771580 [label="О, ти щойно описала Балласа, як володаря «сталевих яєць».",shape=box,color=red];
		j3391771580 -> j3290027520;
	j2001179717 [label="Тож… ти вважаєш, що станеш більш схожою на себе, а не навпаки?",shape=box,color=red];
		j2001179717 -> j2328227231;
	j3328511485 [label="+20 Chemistry",color=orange];
