	j3290027520 [label="+10 Chemistry",color=orange];
		j3290027520 -> j4131228133;
	j2328227231 [label="+20 Chemistry",color=orange];
		j2328227231 -> j859183839;
	j3888193776 [label="Що ж. Це був цікавий, хоч і незграбний, обмін думками. Я чудово провела час. Давай якось повторимо.",shape=box,color=blue];
		j3888193776 -> "Chat ends.";
	j4131228133 [label="Знаєш, ти починаєш реагувати досить емоційно. Давай продовжимо розмову іншим разом.",shape=box,color=blue];
		j4131228133 -> "Chat ends.";
