		j1149705900 -> j364143358;
	j3974444588 [label="Ти знаєш, як вона мене називає?",shape=box,color=blue];
		j3974444588 -> j360135565;
		j3974444588 -> j719360360;
	j26774906 [label="Цікаво, як це мати таке романтичне бачення світу. Здавалося б, медик має справу з вимірювальними шкалами. Десять кубиків цього, визначають мікробів того.",shape=box,color=blue];
		j26774906 -> j2337812897;
		j26774906 -> j1938138243;
	j364143358 [label="Я відчуваю, як техноцити плавають у моїй крові. Якщо я заплющу очі, я побачу блискітки, наче чарівний пил.",shape=box,color=blue];
		j364143358 -> j3246754180;
	j360135565 [label="Чесно кажучи, ні.",shape=box,color=red];
		j360135565 -> j1032873779;
	j719360360 [label="«La Bruja Metiche». Набридлива відьма.",shape=box,color=red];
		j719360360 -> j1672606706;
	j2337812897 [label="Дуже науково з твого боку. Здавалося, ти віриш у більш духовні речі?",shape=box,color=red];
		j2337812897 -> j2067929997;
	j1938138243 [label="Здається, ти вважаєш частину про «втрату людяності» перебільшеною.",shape=box,color=red];
		j1938138243 -> j3861881629;
	j3246754180 [label="Це добре для мене. Ти це розумієш?",shape=box,color=blue];
		j3246754180 -> j2331416279;
	j1032873779 [label="«La Bruja Metiche». Набридлива відьма. Гадаю, вона могла б назвати набагато гірше, враховуючи її словниковий запас і хист до креативної нецензурщини.",shape=box,color=blue];
		j1032873779 -> j4231053012;
	j1672606706 [label="Так. Вона сидить там усміхаючись зі своїми ампулами та зіллями, пляшечками з кров’ю та шматками шкіри, в оточені армії друзяк пацюків, і має нахабство називати МЕНЕ відьмою.",shape=box,color=blue];
		j1672606706 -> j154393762;
		j1672606706 -> j3162483180;
		j1672606706 -> j102255651;
	j2067929997 [label="Ну, так. Живучи нашим життям, як можна не вірити в духовний світ?",shape=box,color=blue];
		j2067929997 -> j1554112057;
	j3861881629 [label="Я все ще Елеонора Найтінґейл. Це більш важливо для мене, ніж бути «людиною».",shape=box,color=blue];
		j3861881629 -> j716682357;
	j2331416279 [label="Все, що воно робить, робить зі щирим бажанням полагодити мене і зробити краще. Якщо я швидка та смертоносна, і мене важко вбити, я буду хорошим носієм. Тож воно виконує свою частину симбіотичної угоди, даючи мені акулячу шкіру та вовчі ікла разом із язиком-батогом.",shape=box,color=blue];
		j2331416279 -> j2337812897;
		j2331416279 -> j1938138243;
	j4231053012 [label="Я чула, як вона казала Квінсі зробити досить винахідливі речі з приладдям для дитячого майданчика та частинами його анатомії, що змусило мене деякий час сміятися, візуалізуючи це.",shape=box,color=blue];
		j4231053012 -> j2290503467;
	j154393762 [label="Я бачу, ви не любите одна одну.",shape=box,color=red];
		j154393762 -> j312438667;
