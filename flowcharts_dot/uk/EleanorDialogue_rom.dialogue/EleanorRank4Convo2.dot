		j3867219364 -> j3119933164;
	j1704187435 [label="Здається, ти знаєш яка я, краще за мене. Я протофрейм. Я — частково трансформована бойова машина, і мені здається, з кожним днем ця частковість стає все меншою.",shape=box,color=blue];
		j1704187435 -> j1997032668;
