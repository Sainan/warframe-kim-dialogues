		j2913027990 -> j3951054858;
	j523328151 [label="Ті двоє на Деймосі, батько й мати. Вони одружена пара. Вони можуть бути велетенськими зараженими залишками орокінів, але вони дуже явно й болісно ОДРУЖЕНІ, так само як мої мама й тато.",shape=box,color=blue];
		j523328151 -> j3868767241;
	j889986176 [label="З тих уривків, які почерпнула з твоєї свідомості, я все ще не бачу стандартні пари людей. Людей, які дуже явно й болісно ОДРУЖЕНІ, так само як мої мама й тато.",shape=box,color=blue];
