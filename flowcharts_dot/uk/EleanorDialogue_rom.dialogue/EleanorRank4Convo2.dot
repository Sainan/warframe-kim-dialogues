		j3719839000 -> j3934322830;
	j2827834982 [label="Чи нормально там, звідки ти родом, коли люди об’єднуються з партнерами, які набагато могутніші та небезпечніші за них? Як у казці «Гамбл і драконові шкарпетки», коли ельфійський принц одружився з велеткою?",shape=box,color=blue];
