		j3166541644 -> j3589231434;
	j3589231434 [label="Як працюють стосунки у вашому часі? Я маю на увазі, це все ще звичайні «двоє або більше дорослих збираються разом з різним ступенем урочистості»? Чи є якісь правила щодо того, хто з ким може бути?",shape=box,color=blue];
