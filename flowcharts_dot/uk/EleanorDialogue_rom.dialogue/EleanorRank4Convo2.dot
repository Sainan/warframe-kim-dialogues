		j2356777469 -> "Chat ends.";
	j3482271532 [label="Гаразд. Тож звідки ти родом, чи нормально для людини мати стосунки, скажімо, з цефалоном?",shape=box,color=blue];
		j3482271532 -> j1540779103;
		j3482271532 -> j4291955350;
