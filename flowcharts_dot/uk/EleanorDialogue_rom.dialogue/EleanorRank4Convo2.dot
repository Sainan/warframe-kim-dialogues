		j1915671247 -> j523328151 [label=true,color=green];
		j1915671247 -> j889986176 [label=false,color=brown];
	j3838573181 [label="Я знала про твоє ізолювання в Дувірі, але не думала, що воно було НАСТІЛЬКИ.",shape=box,color=blue];
