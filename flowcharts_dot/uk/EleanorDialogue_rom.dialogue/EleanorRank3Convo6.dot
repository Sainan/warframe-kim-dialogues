		j3960551223 -> j3800678570;
	j3419368569 [label="Я впевнена, ти можеш здогадатися. Техногниль так багато зруйнувала. Лазні не могли залишатися відкритими після спалахів епідемії.",shape=box,color=blue];
		j3419368569 -> j2073459317;
	j1086439154 [label="Дивно.",shape=box,color=blue];
		j1086439154 -> "Chat ends.";
	j3800678570 [label="Хотілося б, щоб дехто з інших був трохи менш напруженим. На жаль, лазні не могли залишатися відкритими після спалахів епідемії.",shape=box,color=blue];
		j3800678570 -> j2073459317;
	j2073459317 [label="З того, що я бачила, деякі з них зараз по суті є нерестовищами. Чесно кажучи, це розбиває мені серце. Я пам’ятаю, як знайшла тисячолітню мармурову та мозаїчну підлогу, вкриту килимом повзучої плоті.",shape=box,color=blue];
