	j534358421 [label="А якщо це просто, я не знаю, клон Порожнечі чи щось таке? Чи могло таке статися? Порожнеча бере ОСНОВУ людини та надає їй форму. І тоді ця форма ВВАЖАЄ себе тією ж людиною, що померла.",shape=box,color=blue];
		j534358421 -> j971429817;
		j534358421 -> j3180375730;
	j2075738504 [label="Милостива Луа, я їх чую. Це як… коли кондиціонер працює в кімнаті так довго, що ти вже звикаєш до його шуму, а потім раптом помічаєш це",shape=box,color=blue];
		j2075738504 -> j811269145;
	j2801755229 [label="Ось чому вони доклали стільки зусиль, або зробити мене великою та сильною.",shape=box,color=blue];
		j2801755229 -> j3576870337;
	j811269145 [label="Чому я почала розпитувати тебе про це, ЧОРТ ЗАБИРАЙ",shape=box,color=blue];
		j811269145 -> j868921101;
	j3576870337 [label="Це змушує мене замислитися над тим, що було б, якби мене просто вбили. Техногниль може оживити труп і змінити його призначення. Я бачила таке. Як колонія термітів, що працює разом для керування роботом.",shape=box,color=blue];
		j3576870337 -> j275995265;
	j868921101 [label="Так. Так. Гаразд. Я зараз зосереджуся. Будь ласка, залишайся зі мною. Я будую стіну зі своїх думок. Вона підіймається між мною та ангелами.",shape=box,color=blue];
		j868921101 -> j1544980397;
	j275995265 [label="Тож… чи стала б я таким собі техно-зомбі? Якби воно мене оживило, чи залишилася б я собою? Або я була б лише частиною розмноження техногнилі з доступом до моїх спогадів і ДУМАЛА, що це я?",shape=box,color=blue];
		j275995265 -> j935814070;
	j1544980397 [label="Стана, стіна, стіна, чудова, чудова стіна",shape=box,color=blue];
		j1544980397 -> j2692747351;
	j935814070 [label="Я не думаю, що для тебе корисно зациклюватися на цьому.",shape=box,color=red];
		j935814070 -> j933862123;
	j2692747351 [label="Отак. Стіна на місці. Тепер все тихо. Мабуть, мені варто відпустити тебе, чи не так.",shape=box,color=blue];
		j2692747351 -> j1470630329;
	j933862123 [label="Для мене вже немає нічого корисного. Ось так.",shape=box,color=blue];
		j933862123 -> j1707953813;
	j1707953813 [label="Але…",shape=box,color=blue];
		j1707953813 -> j975383331;
	j975383331 [label="Я ціную, що тобі не байдуже, говорячи це.",shape=box,color=blue];
		j975383331 -> j277951993;
	j277951993 [label="Летті перевіряє, що робить моє тіло, тому що Артур хоче, щоб я була в повній бойовій готовності. Я не ненавиджу її за це. Це її робота.",shape=box,color=blue];
		j277951993 -> j523223521;
	j523223521 [label="Але ніхто не перевіряє МЕНЕ.",shape=box,color=blue];
		j523223521 -> j2055682256;
	j2055682256 [label="Принаймні, до цього часу.",shape=box,color=blue];
		j2055682256 -> j136573435;
	j136573435 [label="Божечки, все, що ти робиш — це доброта, і ми так давно не були просто ДОБРИМИ один до одного, що я створю величну пісню і танок про це.",shape=box,color=blue];
