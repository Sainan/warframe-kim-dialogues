		j4207325278 -> j3290375651;
	j1553038810 [label="А, зрозуміло. Ти — обраний посланець Короля Громовержця, щоби залучити прекрасну Елеонору. Не сумніваюся, що на мене чекають усілякі шахрайства. Валяння дурня, тощо.",shape=box,color=blue];
