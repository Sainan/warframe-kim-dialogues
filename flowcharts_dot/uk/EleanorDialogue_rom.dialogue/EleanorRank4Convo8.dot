		j3726668015 -> j4207325278;
	j3290375651 [label="Чесно кажучи, ввімкнути дурнуватий голос і придурюватися кілька годин звучить ідеально. Я з вами.",shape=box,color=blue];
		j3290375651 -> j3926386010;
	j120115830 [label="Я думаю, що буде весело, якщо чесно. І, здається, це багато для нього значить.",shape=box,color=red];
		j120115830 -> j3290375651;
