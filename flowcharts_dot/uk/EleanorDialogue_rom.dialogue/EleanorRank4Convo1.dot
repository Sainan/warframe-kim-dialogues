		j3098240974 -> j1226123619;
	j2600776050 [label="На жаль, це не позбавляє нас від нашої дилеми, тому що це означає, що я ніколи не зможу передумати.",shape=box,color=blue];
		j2600776050 -> j3925961955;
	j490661605 [label="Дякую. Ти для мене багато в чому загадка, але тепер я знаю, що ти без вагань пустиш мені кулю в голову, а такі речі важливі на роботі.",shape=box,color=blue];
