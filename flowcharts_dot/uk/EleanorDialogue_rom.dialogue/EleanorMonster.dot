digraph {
	"EleanorMonster begins." [color=orange];
		"EleanorMonster begins." -> j3460742068;
	j3460742068 [label="Мандрівниче. Те, що я почула від тебе вчора. Щоби залишити монстра в живих. Ти це серйозно? Мені треба знати.",shape=box,color=blue];
		j3460742068 -> j2544183689;
		j3460742068 -> j172331320;
		j3460742068 -> j3180000748;
	j2544183689 [label="Звісно, я серйозно.",shape=box,color=red];
		j2544183689 -> j2551474026;
	j172331320 [label="Чесно? Я трохи подумав над цим. І, ні. Мені доведеться зупинити тебе, якщо ти перетворишся на монстра.",shape=box,color=red];
		j172331320 -> j490661605;
	j3180000748 [label="[Ігнорувати.]",shape=box,color=red];
		j3180000748 -> "Chat ends.";
	j2551474026 [label="Не те, щоб я не була надзвичайно вдячна і, чесно кажучи, трохи шокована, але… чому? ЧОМУ ти дозволиш мені жити? Коли всі інші тут готові пустити колю мені в голову?",shape=box,color=blue];
		j2551474026 -> j1995811881;
		j2551474026 -> j3585043823;
	j490661605 [label="Дякую. Ти для мене багато в чому загадка, але тепер я знаю, що ти без вагань пустиш мені кулю в голову, а такі речі важливі на роботі.",shape=box,color=blue];
		j490661605 -> j2747508858;
	"Chat ends." [color=orange];
	j1995811881 [label="Межа між «героєм» і «монстром» більш розмита, ніж більшість людей хотіли б визнати. Мені довелося воювати проти ґрінерів та Корпусу, а також разом із ними.",shape=box,color=red];
		j1995811881 -> j596819738;
	j3585043823 [label="Тому що я думаю, ти одна з найрозумніших і надзвичайно саркастичних людей у моєму житті. Я навіть не думаю, що техногниль може зробити це з тобою.",shape=box,color=red];
		j3585043823 -> j359154271;
	j2747508858 [label="Аааа я щойно відчула, як техногниль у моїх клітинах тривожно затремтіла.",shape=box,color=blue];
		j2747508858 -> j2370587322;
	j596819738 [label="Я не вірю в слова «Добро» і «Зло». Тільки люди. Чому ти маєш менше прав на існування, ніж я? Можливо, в мене немає язика техногнилі, але я така сама потвора від природи.",shape=box,color=red];
		j596819738 -> j2900980587;
	j359154271 [label="Як… чудово наївно. І напрочуд солодко, в якомусь трагічному сенсі.",shape=box,color=blue];
		j359154271 -> j754596254;
	j2370587322 [label="Думаю, відтепер віна буде поводитися трохи краще. Немає нічого кращого, ніж погроза господаря, щоби нагнати на паразита страху перед Солом, чи не так?",shape=box,color=blue];
		j2370587322 -> j2864742878;
	j2900980587 [label="Але ось ти тут, б’єшся зі «Скальдрою» та техногниллю, намагаючись знайти спосіб зупинити Байдужість. Ти явно віриш, що є якась межа, яку треба десь провести.",shape=box,color=blue];
		j2900980587 -> j2878357830;
		j2900980587 -> j3869913548;
	j754596254 [label="Те, що ти віриш, що я вийду з того світу якоюсь королевою вулика з неушкодженим «я», якимось химерним образом зараження, з яким можна було б подружитися або, можливо, навіть обожнювати.",shape=box,color=blue];
		j754596254 -> j4092781325;
	j2864742878 [label="Boolean EleanorNoDate is now true.",color=orange];
		j2864742878 -> "Chat ends.";
	j2878357830 [label="Байдужість була звільнена Ентраті. Людиною. У цьому була винна людська гординя.",shape=box,color=red];
		j2878357830 -> j1477917204;
	j3869913548 [label="Захищати людей від смерті — це не те саме, що вважати, що щось має бути знищене або не має права на існування.",shape=box,color=red];
		j3869913548 -> j961179536;
	j4092781325 [label="Я б дорікнула тобі, що світ не функціонує, як книжка казок, але… згадую, хто ти та звідки.",shape=box,color=blue];
		j4092781325 -> j2275354235;
	j1477917204 [label="Але техногниль — це не Байдужість. Як що до цього? Щодо більш біологічної, наземної загрози?",shape=box,color=blue];
		j1477917204 -> j2418883553;
		j1477917204 -> j338872128;
	j961179536 [label="Переконатися, щоб отару овець не з’їли вовки, — це не те саме, що вийти та вбити всіх вовків за те, що вони злі.",shape=box,color=red];
		j961179536 -> j2892532340;
	j2275354235 [label="Тож, я гадаю, що не повинна дивуватися.",shape=box,color=blue];
		j2275354235 -> j815743246;
	j2418883553 [label="Ми насправді не знаємо, звідки з’явилася техногниль. Вона може бути створена людиною. Це може бути від чогось зовсім іншого.",shape=box,color=red];
		j2418883553 -> j985119417;
	j338872128 [label="Річ у тому, що ти не хочеш, щоб я тебе вбив. Тому я не стану. Я дам тобі піти.",shape=box,color=red];
		j338872128 -> j480131929;
	j2892532340 [label="А якщо від Елеонори нічого не залишилося? Що робити, якщо ти не побачиш нічого, крім шиплячого звіра, який гарчить і плюється?",shape=box,color=blue];
		j2892532340 -> j3052300130;
		j2892532340 -> j338872128;
	j815743246 [label="У будь-якому випадку я вдячна, що ти мене не вб’єш. Хоч щось. Принаймні.",shape=box,color=blue];
		j815743246 -> "Chat ends.";
	j985119417 [label="А якщо техногниль виявиться черговим відхиленням, створеним людиною, на кшталт вашої «Людини в стіні»? Ти вистежиш мене і в’єш?",shape=box,color=blue];
		j985119417 -> j536537101;
		j985119417 -> j3041189119;
	j480131929 [label="Хоча я вдячна за виконання бажання пані-потвори, але, мабуть, я сподівалася на щось більш… виважене.",shape=box,color=blue];
		j480131929 -> j3894709342;
	j3052300130 [label="Звідки я знаю, що від Елеонори нічого не залишилося? Ти говориш про те, як мислить техногниль. Як вона говорить до тебе. Вона явно розумна.",shape=box,color=red];
		j3052300130 -> j573041455;
	j536537101 [label="Ні. Я не зможу.",shape=box,color=red];
		j536537101 -> j4281755406;
	j3041189119 [label="Так. Якщо техногниль справді неприродна? Тоді… треба знищити.",shape=box,color=red];
		j3041189119 -> j490661605;
	j3894709342 [label="+5 Chemistry",color=orange];
		j3894709342 -> j815743246;
	j573041455 [label="Це схоже на розум вулика. Можливо, Елеонора все ще була би там, але не в одному місці, а в мільярді. Хто я, щоби казати, що це «неправильно»?",shape=box,color=red];
		j573041455 -> j1699787699;
	j4281755406 [label="Ти суперечиш собі. Якщо ти хочеш зупинити одне техногенне відхилення, чому б не зупинити друге?",shape=box,color=blue];
		j4281755406 -> j2433897295;
		j4281755406 -> j236152163;
	j1699787699 [label="Саме тоді, коли я думаю, що не можу обожнювати тебе ще більше, я знову сиджу тут і плачу від щастя",shape=box,color=blue];
