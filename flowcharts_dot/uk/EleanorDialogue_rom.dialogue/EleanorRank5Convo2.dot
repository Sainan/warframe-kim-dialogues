	j4075786907 [label="Отже, ви з Крістофером…",shape=box,color=red];
		j4075786907 -> j811365355;
	j811365355 [label="Ага. Він був добрий. Розкішний. Сильний. Легко заводився, що було великим плюсом. Але дуже хотів піти в армію. ",shape=box,color=blue];
		j811365355 -> j1950243618;
	j1950243618 [label="У нас були різні погляди на це. Для нього це було покликанням. Він допомагав людям, які цього потребували, змінюючи світ на краще. Він купився на всю цю тему про мужніх героїв, як і Артур. ",shape=box,color=blue];
