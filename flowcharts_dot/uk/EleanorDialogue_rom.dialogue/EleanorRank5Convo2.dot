		j4084516169 -> j3367192196;
	j3367192196 [label="Я сказала йому, що вагітна. Він запропонував мені одружитися. Дурний красивий ідіот.",shape=box,color=blue];
		j3367192196 -> j2292038121;
	j2688450061 [label="Мені шкода. Я не зможу пройти через це без сліз, тож якщо я не поспішатиму з відповіддю, ти принаймні знатимеш, чому.",shape=box,color=blue];
		j2688450061 -> j3367192196;
	j2292038121 [label="Справді?",shape=box,color=red];
		j2292038121 -> j1764783901;
	j1764783901 [label="Ні. Але я думала, що я була. Я не намагалася його обдурити, якщо ти про це питаєш.",shape=box,color=blue];
		j1764783901 -> j1928451818;
	j1928451818 [label="Артур був на сьомому небі від щастя. Хлопець, якого він вважав братом, мав стати його зятем.",shape=box,color=blue];
		j1928451818 -> j3212374747;
	j3212374747 [label="Потім все переходить у режим перемотування. Щось про превентивний удар. Кордони і стримування, ризик ескалації, моральний бісовий мандат. Крістофер у бойовій готовності, Артур вже записується, всі хлопці пішли в Багряну Варту, а моя мама все ще намагається організувати розсадження гостей на весіллі.",shape=box,color=blue];
		j3212374747 -> j1823643357;
		j3212374747 -> j442242446;
	j1823643357 [label="То що ж сталося?",shape=box,color=red];
		j1823643357 -> j1480476248;
	j442242446 [label="Ти в порядку, щоби продовжувати?",shape=box,color=red];
		j442242446 -> j1480476248;
	j1480476248 [label="Вони вбили його. Люди, яким ми нібито мали допомогти. Вони підірвали його машину міною, зробленою з добрив і будівельних цвяхів.",shape=box,color=blue];
		j1480476248 -> j3038706462;
	j3038706462 [label="Я кажу собі: «Принаймні, це було швидко». «Принаймні, він не страждав». Але наскільки я знаю, він помер, кричачи, з цвяхами в очах.",shape=box,color=blue];
		j3038706462 -> j3252386629;
	j3252386629 [label="Трясця",shape=box,color=blue];
		j3252386629 -> j3745424309;
	j3745424309 [label="Бісове лайно",shape=box,color=blue];
		j3745424309 -> j3971301467;
		j3745424309 -> j1345509908;
	j3971301467 [label="Принаймні, він загинув, роблячи те, що вважав правильним…",shape=box,color=red];
		j3971301467 -> j3485141286;
	j1345509908 [label="Мені так шкода…",shape=box,color=red];
		j1345509908 -> j479097511;
	j3485141286 [label="Мені здається, що ти не звернув уваги на ОДНУ РІЧ, яку я тобі тут говорила.",shape=box,color=blue];
		j3485141286 -> j2912917067;
	j479097511 [label="Звичайно, я плакала, але весь час думала: «Чого ти очікувала, ДУРНА дівко, ти ж ЗНАЛА, на що йдеш, ти ЗНАЛА, що це станеться».",shape=box,color=blue];
		j479097511 -> j3241460002;
	j2912917067 [label="Мене не цікавить «принаймні…» Він помер ДІРНИМ способом і з ДІРНИЧОЇ причини.",shape=box,color=blue];
		j2912917067 -> j1270441115;
	j3241460002 [label="Розповідаючи тобі про це ось так, я просто відчуваю, що це банальне народне пісенне лайно.",shape=box,color=blue];
		j3241460002 -> j155138098;
