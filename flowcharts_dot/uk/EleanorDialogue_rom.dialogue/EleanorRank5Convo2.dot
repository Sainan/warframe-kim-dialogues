		j2708303378 -> j365567009;
	j3789397786 [label="Раптом одного разу я зайшла на кухню, побачила перед собою цього приголомшливого молодого чоловіка у формі, і вдруге в житті подумала: «Фррра!».",shape=box,color=blue];
		j3789397786 -> j712053862;
