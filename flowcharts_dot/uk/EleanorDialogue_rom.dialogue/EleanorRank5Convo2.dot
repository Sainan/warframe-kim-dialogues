		j3550626957 -> j3367192196;
	j2265301911 [label="Я потрапила в ту саму павутину, яку намагався сплести.",shape=box,color=blue];
		j2265301911 -> j3367192196;
	j866535889 [label="Правильно. На той час, звісно, було вже запізно. Я пішла на це, думаючи, що зачарую його, але правда в тому, що він зачарував мене.",shape=box,color=blue];
		j866535889 -> j2688450061;
