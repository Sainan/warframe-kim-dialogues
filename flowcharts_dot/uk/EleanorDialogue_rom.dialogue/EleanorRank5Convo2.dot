	j1769558545 [label="«Фррра»?",shape=box,color=blue];
		j1769558545 -> j4232988350;
	j1708257932 [label="Менді Джелліко. Моя закоханість. Вона кинула школу, пофарбувала волосся, зробила пірсинг, вешталася містом у шкіряній куртці, палила цигарки та витріщалася на кожного, хто підходив надто близько. Я дивилася на неї і танула.",shape=box,color=blue];
		j1708257932 -> j2377780871;
	j4232988350 [label="Це традиційний британський вираз захоплення. Його слід вимовляти з таким собі низьким гарчанням, надуваючи щоки.",shape=box,color=blue];
		j4232988350 -> j2377780871;
	j2377780871 [label="Хай там як. ВЖЕ НЕ АКТУАЛЬНО.",shape=box,color=blue];
