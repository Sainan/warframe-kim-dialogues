		j1270441115 -> j785233004 [label=false,color=brown];
	j155138098 [label="Коханий мій пішов на війну. О ні, тепер він загинув. Я буду блукати в самотніх долинах і ніколи його не знайду.",shape=box,color=blue];
		j155138098 -> j2296734552;
	j3035152930 [label="І якщо ти не слухаєш мене, коли я говорю про це, то що ще ти не чуєш? Як би чарівно все це не було між нами, я… ні. Це була помилка.",shape=box,color=blue];
