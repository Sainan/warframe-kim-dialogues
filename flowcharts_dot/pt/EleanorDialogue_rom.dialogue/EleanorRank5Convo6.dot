	"Chat ends." [color=orange];
	j610907602 [label="De onde você vem, existem irrigadores de gramado?",shape=box,color=blue];
		j610907602 -> j860664895;
		j610907602 -> j1381745723;
	j860664895 [label="Temos a VERD-IE e drones como ela. Eles cuidavam das plantas da Zariman. É esse o tipo de coisa que você quer dizer?",shape=box,color=red];
		j860664895 -> j3374978785;
	j1381745723 [label="Irriga… dores?",shape=box,color=red];
		j1381745723 -> j955688101;
	j3374978785 [label="Algo assim ",shape=box,color=blue];
		j3374978785 -> j1156987187;
	j955688101 [label="Sim, meu amor, irrigadores. Borrifadores de água para manter seu gramado agradável e úmido ",shape=box,color=blue];
		j955688101 -> j2002941041;
	j1156987187 [label="Eu adorava os irrigadores de gramado quando era pequena. Eles eram mágicos. Quando o sol estava mais forte, era possível ver arco-íris neles. Você corria por eles e sentia um frio delicioso por toda parte ",shape=box,color=blue];
		j1156987187 -> j4256905607;
	j2002941041 [label="Também são ideais para segurar o rosto de seu irmão contra até ele gritar ",shape=box,color=blue];
		j2002941041 -> j4256905607;
	j4256905607 [label="\"A felicidade é um irrigador de gramado\". Uma mensagem perfeita para um cartão de felicitações ",shape=box,color=blue];
		j4256905607 -> j1237807927;
	j1237807927 [label="Você parece que tá de bom humor hoje. Fico feliz com isso ",shape=box,color=red];
		j1237807927 -> j1432570778;
	j1432570778 [label="Estou. E não vejo por que deveria guardar isso para mim. Então, estou espalhando o sentimento por todo o shopping ",shape=box,color=blue];
		j1432570778 -> j3046959270;
		j1432570778 -> j2803627068;
		j1432570778 -> j1614899012;
	j3046959270 [label="Isso não é tipo… drogar as pessoas?",shape=box,color=red];
		j3046959270 -> j1241167312;
	j2803627068 [label="Transmitir um sentimento positivo… por que não? Não consigo ver como isso seria diferente de tocar uma música ",shape=box,color=red];
		j2803627068 -> j4004017033;
	j1614899012 [label="Então, hoje é o dia que vou ver o Arthur sorrindo?",shape=box,color=red];
		j1614899012 -> j1117983966;
	j1241167312 [label="Ah… que ótimo… aqui estou eu, tentando ser a fada secreta da felicidade, espalhando alegria, e acabo sendo taxada como o duende maligno das drogas. Meu humor acabou de despencar ",shape=box,color=blue];
		j1241167312 -> j2935460750;
	j4004017033 [label="Ah! Uma ótima analogia!",shape=box,color=blue];
		j4004017033 -> j3333085465;
	j1117983966 [label="Talvez. Espero que sim. Ele teve que suportar tanta coisa ",shape=box,color=blue];
		j1117983966 -> j2942785217;
	j2935460750 [label="Tudo bem. Vou parar ",shape=box,color=blue];
		j2935460750 -> "Chat ends.";
	j3333085465 [label="Se a Aoi pode influenciar o ânimo por meio da música, eu posso fazer isso por meio das emoções, certo?",shape=box,color=blue];
		j3333085465 -> j4003581349;
	j2942785217 [label="Muita coisa mudou desde que você chegou. Não me refiro apenas a você e a mim. Me refiro a tudo. Até mesmo o shopping ",shape=box,color=blue];
		j2942785217 -> j4003581349;
	j4003581349 [label="Estou feliz que a Aoi me convenceu a ficar. Que o Sol a abençoe ",shape=box,color=blue];
		j4003581349 -> j1899160708;
		j4003581349 -> j3449631854;
	j1899160708 [label="Peraí… ela fez o quê?",shape=box,color=red];
		j1899160708 -> j4181222157;
	j3449631854 [label="Peraí… acho que perdi uma parte da história",shape=box,color=red];
		j3449631854 -> j4181222157;
	j4181222157 [label="Eu não te contei?",shape=box,color=blue];
		j4181222157 -> j1765175162;
	j1765175162 [label="Ah… isso foi pouco tempo antes de você chegar. Estivemos fora o dia todo tentando acabar com uma colmeia de Tecnovírus. Tinha um climão no ar. Normalmente, o combate acaba com a raiva e nos deixa exaustos demais para atacar uns aos outros, mas esse dia foi diferente ",shape=box,color=blue];
		j1765175162 -> j4051207026;
	j4051207026 [label="Me lembro do Arthur falar algo como \"faremos tudo isso de novo amanhã\" e isso nos atingiu como uma marreta. Ninguém disse uma palavra no caminho de volta ao shopping ",shape=box,color=blue];
		j4051207026 -> j2428806698;
		j4051207026 -> j3616815516;
		j4051207026 -> j3147504848;
	j2428806698 [label="Uau… bela maneira de elevar o moral da equipe, Arthur ",shape=box,color=red];
		j2428806698 -> j1247141359;
	j3616815516 [label="Ele é tão encantador…",shape=box,color=red];
		j3616815516 -> j1247141359;
	j3147504848 [label="Ele carrega muita responsabilidade nos ombros… é complicado",shape=box,color=red];
		j3147504848 -> j1530501312;
	j1247141359 [label="Acho que ele estava apenas tentando fazer com que nos concentrássemos no trabalho. Mas eu me sentia acabada. O Arthur e a Aoi não se falavam, o Amir estava irritando tanto o Quincy que eu estava começando a temer pela vida dele, e a Lê estava me observando com aquele olhar de desaprovação sempre que podia ",shape=box,color=blue];
		j1247141359 -> j4068560723;
	j1530501312 [label="Sim. Normalmente, ele consegue esconder isso, mesmo quando está quase sem energia. Mas acho que ele não tinha mais nada ",shape=box,color=blue];
		j1530501312 -> j3764386480;
	j4068560723 [label="Passei muito tempo olhando em espelhos naquele dia. Fiquei imaginando meu reflexo me estrangulando e falando \"VOCÊ é o problema. Se não fosse por você, nada disso estaria acontecendo\"",shape=box,color=blue];
		j4068560723 -> j2937246818;
		j4068560723 -> j1437629878;
	j3764386480 [label="Ele se esgotou tentando manter a equipe unida",shape=box,color=blue];
		j3764386480 -> j1247141359;
	j2937246818 [label="Pra variar… parece que todo mundo mexeu com a SUA cabeça",shape=box,color=red];
		j2937246818 -> j2034726820;
	j1437629878 [label="Isso parece ser o tipo de besteira que a Lê falaria ",shape=box,color=red];
		j1437629878 -> j575657220;
	j2034726820 [label="Ah, não eram eles. Era eu. Todo sentimento reprimido de insegurança e ódio por mim mesma que eu estava guardando veio à tona naquele dia ",shape=box,color=blue];
		j2034726820 -> j9815368;
	j575657220 [label="Obrigada. Sei que você está me apoiando e isso significa muito para mim ",shape=box,color=blue];
		j575657220 -> j1890536330;
	j9815368 [label="Pensei para mim mesma: o que esse grupo precisa é de um bode expiatório ",shape=box,color=blue];
		j9815368 -> j3914555144;
	j1890536330 [label="Já tinha escutado essa frase da Lê tantas vezes que acabei pensando comigo mesma: talvez ela tenha razão. Estou colocando todos em perigo ao ficar aqui ",shape=box,color=blue];
		j1890536330 -> j2247789419;
	j3914555144 [label="Um monstro. Alguém para culpar ",shape=box,color=blue];
		j3914555144 -> j2247789419;
	j2247789419 [label="Elaborei todo um plano. Fugiria durante a noite, me esconderia na cidade em algum lugar onde o Arthur nunca iria me encontrar e - essa é a parte de que mais me orgulho - continuaria ajudando à distância ",shape=box,color=blue];
		j2247789419 -> j1655538259;
		j2247789419 -> j4271500685;
	j1655538259 [label="Acho que ele teria te encontrado. Ele é bem determinado ",shape=box,color=red];
		j1655538259 -> j3001094144;
	j4271500685 [label="O cenário mais plausível é que ele teria morrido tentando te encontrar ",shape=box,color=red];
		j4271500685 -> j2341156781;
	j3001094144 [label="Como? O Amir não consegue rastrear telepatia ",shape=box,color=blue];
		j3001094144 -> j122008402;
	j2341156781 [label="Tentando me fazer sentir culpada, é?",shape=box,color=blue];
		j2341156781 -> j4114886764;
	j122008402 [label="Eu seria uma humilde exilada. Eu me agacharia nos telhados, com uma aparência esvoaçante e determinada. Eu poderia informar sobre o acúmulo da Tecnovírus, avisar aos Hexa onde a Scaldra estava montando bloqueios na estrada e até mesmo agir como uma transmissora de comunicações de emergência. Toda vez que eu salvasse a pele de Lê, eu realmente faria ela sentir isso. Mesquinha? Sem dúvida ",shape=box,color=blue];
		j122008402 -> j1857587581;
	j4114886764 [label="Nem se dê o trabalho. Já estou me sentindo muito mal ",shape=box,color=blue];
		j4114886764 -> "Chat ends.";
	j1857587581 [label="Escolhi o momento. Esperei até que o Amir estivesse dormindo. Depois, vesti um casaco longo e fui para a garagem o mais silenciosamente possível ",shape=box,color=blue];
		j1857587581 -> j4100237612;
	j4100237612 [label="A grade de segurança ligou sozinha. Eu me virei e lá estava ela, a Aoi ",shape=box,color=blue];
		j4100237612 -> j41295598;
	j41295598 [label="Conversamos a noite toda e choramos nos braços uma da outra. Ela também estava sofrendo. Profundamente. Quando o sol nasceu, eu decidi ficar ",shape=box,color=blue];
		j41295598 -> j2432623152;
		j41295598 -> j3703153953;
		j41295598 -> j3045007979;
	j2432623152 [label="Ainda dá tempo de mudar de ideia ",shape=box,color=red];
		j2432623152 -> j2551066230;
	j3703153953 [label="A Aoi é maravilhosa. Fico feliz que ela tenha te ajudado ",shape=box,color=red];
		j3703153953 -> j2821256496;
	j3045007979 [label="Que sorte ",shape=box,color=red];
		j3045007979 -> j2821256496;
	j2551066230 [label="Seja qual for o impulso grotesco que te levou a escrever isso, espero que se engasgue nele ",shape=box,color=blue];
		j2551066230 -> "Chat ends.";
