	j362203499 [label="Talvez… outras crianças ouviram seus pais chamando ",shape=box,color=red];
		j362203499 -> j2323835509;
	j2539768110 [label="Acho que sim… o Quinn me avisou pra não procurar por vozes familiares. Deve ter sido isso que ele quis dizer ",shape=box,color=red];
		j2539768110 -> j2323835509;
	j2323835509 [label="Sinto muito. Fico feliz que você tenha encontrado um refúgio seguro, mesmo que não tenha permanecido assim ",shape=box,color=blue];
		j2323835509 -> j4160150982;
	j4160150982 [label="Foi seguro por um bom tempo. Tempo suficiente pra eu esquecer de tudo. Acho que era isso que eu queria fazer ",shape=box,color=red];
		j4160150982 -> j3828314941;
	j3828314941 [label="Mas como você fez isso? Você era apenas uma criança. O que te deu o poder para conjurar um reino inteiro do nada?",shape=box,color=blue];
		j3828314941 -> j2459879245;
		j3828314941 -> j656503808;
		j3828314941 -> j3657767956;
	j2459879245 [label="As crianças são fascinantes pelo fato de ainda não saberem o que não podem fazer. Eu não sabia que não podia… mas fiz ",shape=box,color=red];
		j2459879245 -> j645119824;
	j656503808 [label="Eu tinha tantas emoções, todas ao mesmo tempo, que eu… simplesmente… fiz algo com elas. É difícil de explicar ",shape=box,color=red];
		j656503808 -> j827720613;
	j3657767956 [label="O Void não é \"nada\". Eu sei que parece que deveria ser, mas não é. O Albrecht provou isso ",shape=box,color=red];
		j3657767956 -> j827720613;
	j645119824 [label="Quando tinha 5 anos, eu queria ser uma bruxa. Gritei Abracadabra e pulei do alto da escada. Eu não sabia que não podia voar. Mas descobri bem rápido ",shape=box,color=blue];
		j645119824 -> j3093244146;
	j827720613 [label="Sabemos muito pouco sobre isso, mesmo agora. Mas uma coisa que até mesmo os Orokin descobriram é: o Void responde às emoções humanas",shape=box,color=red];
		j827720613 -> j513232853;
	j3093244146 [label="O poder de criar Duviri deve ter vindo de outro lugar além de você. Você usou para fazer algo incrível, mas ele não era seu ",shape=box,color=blue];
		j3093244146 -> j2259808951;
	j513232853 [label="ENTENDI. Então, se você olhasse para o Void com… hmm, digamos, \"fome\"… você talvez criasse um lobo faminto rondando. Ou um banquete real, talvez. Ou um pedaço de pão com dentes de lobo rangendo nele",shape=box,color=blue];
		j513232853 -> j3189736441;
	j2259808951 [label="Não no começo, pelo menos. Mas agora está em você. A energia selvagem. Os ciclos no tempo. Deuses, você é uma maravilha ",shape=box,color=blue];
