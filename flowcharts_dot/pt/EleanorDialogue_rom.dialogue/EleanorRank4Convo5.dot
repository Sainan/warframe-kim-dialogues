		j2709970812 -> j723110520;
	j2973599870 [label="+5 Chemistry",color=orange];
		j2973599870 -> j3530342354;
	j3641225884 [label="Eu tava sem ninguém. Acho que as outras crianças morreram ou foram resgatadas. Não sei bem os detalhes ",shape=box,color=red];
		j3641225884 -> j2535975088;
	j723110520 [label="Não foi exatamente intencional… tudo o que me lembro é…",shape=box,color=red];
