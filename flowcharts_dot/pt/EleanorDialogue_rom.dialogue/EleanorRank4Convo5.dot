		j2319631072 -> j1632909141;
	j1632909141 [label="Era pra isso que esse livro de histórias deveria servir. Na superfície, era tudo sobre esses personagens exagerados na corte de um rei louco que mudava de humor todos os dias",shape=box,color=red];
		j1632909141 -> j2418868701;
	j2418868701 [label="Mas a verdadeira lição dessas histórias era sobre como controlar suas emoções. A gente deveria aprender uma lição importante com isso, pra que a viagem fosse segura ",shape=box,color=red];
		j2418868701 -> j3416560834;
	j3416560834 [label="Aff… desculpa, mas agora você estragou tudo para mim",shape=box,color=blue];
		j3416560834 -> j1757925362;
