	"EleanorRank4Convo5 begins." [color=orange];
		"EleanorRank4Convo5 begins." -> j793514307;
	j793514307 [label="O reino que você costumava governar, Duviri, você inventou ele?",shape=box,color=blue];
		j793514307 -> j1571991820;
		j793514307 -> j3325289521;
	j1571991820 [label="Sim e não. O lugar e os personagens vieram de um livro infantil que eu adorava. \"Contos de Duviri\". Eu o trouxe à vida por meio do Void",shape=box,color=red];
		j1571991820 -> j2709970812;
