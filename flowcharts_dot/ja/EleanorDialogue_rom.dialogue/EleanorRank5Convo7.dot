	j3683375032 [label="[無視する]",shape=box,color=red];
		j3683375032 -> "Chat ends.";
	j2492741082 [label="私たちの共通点を一つ見つけたのね、おめでとう。そしてごめんなさい。でも今からする話は重要なの。",shape=box,color=blue];
