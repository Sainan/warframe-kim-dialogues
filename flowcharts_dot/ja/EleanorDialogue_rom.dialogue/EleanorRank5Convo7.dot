		j3419415290 -> j2827364002;
	j2108928431 [label="もしそうなら、アレがすることは罪のない行為よ。まるで子供。私たちの知るそれは違う。悪意に満ちている。",shape=box,color=blue];
		j2108928431 -> "Chat ends.";
	j2827364002 [label="丸呑みされたら、喉を突き刺して窒息させるわ。",shape=box,color=blue];
