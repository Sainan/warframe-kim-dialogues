	j2474597268 [label="«Безразличие» — это то, ЧТО оно есть, или то, ЧЕГО оно хочет?",shape=box,color=blue];
		j2474597268 -> j1620898366;
		j2474597268 -> j3329423738;
		j2474597268 -> j3432236701;
	j1620898366 [label="Знаешь, ты и Лэтти обе так делаете. Забываете сказать «Привет».",shape=box,color=red];
		j1620898366 -> j4074441706;
	j3329423738 [label="Большинство людей начали бы со слова «привет».",shape=box,color=red];
		j3329423738 -> j1843231172;
	j3432236701 [label="[Игнорировать]",shape=box,color=red];
		j3432236701 -> "Chat ends.";
	j4074441706 [label="Поздравляю, ты нашла то единственное, что нас объединяет. И прости. Это важнее.",shape=box,color=blue];
		j4074441706 -> j214018291;
	j1843231172 [label="Извини. Это важнее.",shape=box,color=blue];
		j1843231172 -> j214018291;
	"Chat ends." [color=orange];
	j214018291 [label="Я думаю, что «Великое Безразличие» попытается каким-то образом встать между нами.",shape=box,color=blue];
		j214018291 -> j1104139767;
	j1104139767 [label="Не смейся. Я чувствую это, прямо до глубины костного мозга. Та штука, что была в Русалке, просто обожает смотреть, как мы рвём друг друга на куски.",shape=box,color=blue];
		j1104139767 -> j3932203125;
	j3932203125 [label="Я не знаю почему. Я просто знаю.",shape=box,color=blue];
		j3932203125 -> j1844459066;
		j3932203125 -> j1341961093;
	j1844459066 [label="Да, я думаю, ты права.",shape=box,color=red];
		j1844459066 -> j699696121;
	j1341961093 [label="Может, это и случилось с семьёй Энтрати. Они годами жили прямо над его лабораторией, даже не зная об этом. А когда ребёнок нашёл их, они уже были чужими друг другу, утопая в злости и обидах.",shape=box,color=red];
		j1341961093 -> j699696121;
	j699696121 [label="Я видела это у тебя в голове. Альбрехт и Лойд. Мать и Отец. Ты и твои родители. Русалка и Виктор, насколько мы знаем. И, конечно, мы шестеро.",shape=box,color=blue];
		j699696121 -> j3145683542;
	j3145683542 [label="То, что делает эта штука, — это холодно. Извращённо. Но где-то за этим кроется мотив.",shape=box,color=blue];
		j3145683542 -> j473136515;
		j3145683542 -> j2205856936;
	j473136515 [label="Думаю, ты права. На Заримане первыми сходили с ума родители. Может, Упорные держались так долго, потому что у них не было детей.",shape=box,color=red];
		j473136515 -> j1820881545;
	j2205856936 [label="Я не люблю притворяться, что понимаю штуки, которые по сути *боги*. Эта штука пытается разобраться в эмоциях. Просто у неё плохо получается играть с игрушками.",shape=box,color=red];
		j2205856936 -> j210758373;
	j1820881545 [label="Только пусть попробует влезть между нами. Клянусь Сол, я его прикончу. Найду способ.",shape=box,color=blue];
		j1820881545 -> j3594094737;
	j210758373 [label="Теория-то похвальная, но мы же этого не *знаем*, правда?",shape=box,color=blue];
		j210758373 -> j1771189264;
	j3594094737 [label="Во Вселенной есть силы, против которых эта штука не устоит. Я выбираю верить в это каждой частицей себя.",shape=box,color=blue];
		j3594094737 -> j3268759876;
	j1771189264 [label="Если правда на твоей стороне, то это невинно. Детски, почти. Это не вяжется с тем, что нам известно. Это злонамеренно.",shape=box,color=blue];
		j1771189264 -> "Chat ends.";
	j3268759876 [label="Если оно проглотит меня целиком, я застряну у него в глотке и задушу его.",shape=box,color=blue];
		j3268759876 -> j2500637527;
		j3268759876 -> j1139320376;
	j2500637527 [label="Это отличный способ умереть.",shape=box,color=red];
		j2500637527 -> j1642007036;
	j1139320376 [label="Я пока не знаю, как мы это сделаем, но я с тобой. Мы должны победить.",shape=box,color=red];
		j1139320376 -> j2023799361;
	j1642007036 [label="В этом случае я в любом случае мертва.",shape=box,color=blue];
		j1642007036 -> j125666083;
	j2023799361 [label="Если бы это была сказка, тебе пришлось бы придумать бы какой-нибудь хитрый трюк. Найти каменное сердце великана и разбить его лебединым пером или обмануть, заставив его броситься с обрыва.",shape=box,color=blue];
		j2023799361 -> j2521704693;
	j125666083 [label="Но, по крайней мере, я могу плюнуть ему в глаз.",shape=box,color=blue];
		j125666083 -> "Chat ends.";
	j2521704693 [label="Ты говорила, что ты «журналистка». Это ведь не те истории, что ты привыкла рассказывать, да? Ты рассказывала правду, какой бы уродливой она ни была.",shape=box,color=red];
		j2521704693 -> j329148752;
	j329148752 [label="Если бы те старые истории не говорили правду, люди бы не продолжали их рассказывать.",shape=box,color=blue];
		j329148752 -> j46393083;
	j46393083 [label="Я не про то, что великаны настоящие. Я про то, что старые истории говорят о другом виде правды.",shape=box,color=blue];
		j46393083 -> j2375746189;
		j46393083 -> j1642360988;
	j2375746189 [label="Чтобы пережить это, нам понадобится нечто большее, чем сказки, какими бы вдохновляющими они ни были.",shape=box,color=red];
		j2375746189 -> j3355346054;
	j1642360988 [label="Моя жизнь прошла в королевстве, созданном из историй. Я знаю, насколько они могут быть сильны.",shape=box,color=red];
		j1642360988 -> j3718072211;
	j3355346054 [label="Я знаю. Хотелось бы иметь что-то большее, чем инстинкт, чтобы продолжать.",shape=box,color=blue];
		j3355346054 -> j568595996;
	j3718072211 [label="Думаю, все эти истории про победу над великанами объединяет одно: они учат разбираться в правилах и использовать их с умом.",shape=box,color=blue];
		j3718072211 -> j25104665;
	j568595996 [label="Мы сталкиваемся с чем-то огромным, ужасающим и находящимся за пределами нашего понимания. Обычно это доступно только детям. Для этого и существуют сказки.",shape=box,color=blue];
		j568595996 -> j32867937;
		j568595996 -> j2792518939;
	j25104665 [label="Некоторые монстры глупы. Некоторые вынуждены понимать всё буквально. У некоторых есть тайны. Но ты не победишь, пока не узнаешь правила, которые держат монстра в узде.",shape=box,color=blue];
		j25104665 -> j32867937;
		j25104665 -> j2792518939;
	j32867937 [label="Он застрял в Стене. У меня даже нет уверенности, что это за Стена.",shape=box,color=red];
		j32867937 -> j2446587576;
	j2792518939 [label="Мне кажется, «Безразличие» не связано никакими правилами.",shape=box,color=red];
		j2792518939 -> j1269338599;
	j2446587576 [label="Неважно. Если оно действительно застряло, то мы можем сделать два вывода, не так ли?",shape=box,color=blue];
		j2446587576 -> j718252683;
	j1269338599 [label="Должны быть! Даже у богов есть правила! Как бы могущественно «Безразличие» ни казалось, оно не бог.",shape=box,color=blue];
		j1269338599 -> j39284120;
	j718252683 [label="Во-первых, его можно поймать. Может, даже заточить.",shape=box,color=blue];
		j718252683 -> j2879774464;
	j39284120 [label="Тебе удалось победить Тракса. Неважно, сколько раз пришлось упасть и оказаться на земле. ПОБЕДА осталась за тобой. Так почему бы тебе не одолеть и Человека в Стене?",shape=box,color=blue];
		j39284120 -> j639387712;
	j2879774464 [label="+15 Chemistry",color=orange];
		j2879774464 -> j2694948370;
	j639387712 [label="Когда ты перестала его бояться, Элеонора?",shape=box,color=red];
		j639387712 -> j4077026299;
	j2694948370 [label="А во-вторых, он чего-то ХОЧЕТ. Хочет выбраться. Если мы поймём, чего он хочет, мы уже будем на шаг впереди.",shape=box,color=blue];
		j2694948370 -> "Chat ends.";
	j4077026299 [label="Я не перестала.",shape=box,color=blue];
