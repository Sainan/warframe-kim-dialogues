		j3479803741 -> j3841161856;
	j1334139200 [label="Странно.",shape=box,color=blue];
		j1334139200 -> "Chat ends.";
	j695324098 [label="Жаль, что некоторые из остальных такие зажатые. К сожалению, после вспышек болезни бани не могли оставаться открытыми.",shape=box,color=blue];
		j695324098 -> j3841161856;
	j3841161856 [label="Судя по тому, что я видела, некоторые из них теперь фактически стали рассадниками инфекции. Честно говоря, это разрывает мне сердце. Помню, как видела мраморные и мозаичные полы тысячелетней давности, покрытые ползущей плотью.",shape=box,color=blue];
