digraph {
	"EleanorRank5Convo6 begins." [color=orange];
		"EleanorRank5Convo6 begins." -> j2934125270;
	j2934125270 [label="Возможно, ты замечаешь, что сегодня утром торговый центр выглядит несколько иначе.",shape=box,color=blue];
		j2934125270 -> j1034821157;
	j1034821157 [label="Ничего большого и очевидного. Просто немного больше улыбок на лицах. Пара смешков тут и там. Щепотка счастья в воздухе.",shape=box,color=blue];
		j1034821157 -> j4263470169;
		j1034821157 -> j2569848176;
	j4263470169 [label="Это зависит от тебя, не правда ли?",shape=box,color=red];
		j4263470169 -> j1100911919;
	j2569848176 [label="Сейчас вернусь [Завершить]",shape=box,color=red];
		j2569848176 -> "Chat ends.";
	j1100911919 [label="Это мой способ отдать что-то взамен. Как ты знаешь, телепат может не только принимать, но и отправлять. И необязательно бить струёй в одного человека — можно распылять лёгкий туман на целую область.",shape=box,color=blue];
		j1100911919 -> j2156916717;
	"Chat ends." [color=orange];
	j2156916717 [label="У вас там, откуда ты, есть эти газонные разбрызгиватели?",shape=box,color=blue];
		j2156916717 -> j2788375569;
		j2156916717 -> j3044922320;
	j2788375569 [label="У нас были ДРРО-НЭ и подобные дроны. Они ухаживали за растениями на Заримане. Ты об этом?",shape=box,color=red];
		j2788375569 -> j3268307477;
	j3044922320 [label="Разбрызгиватели?",shape=box,color=red];
		j3044922320 -> j1518791240;
	j3268307477 [label="Вот именно.",shape=box,color=blue];
		j3268307477 -> j1883329488;
	j1518791240 [label="Да, дорогуша, разбрызгиватели. Распылители воды для поддержания чистоты и влажности вашего газона.",shape=box,color=blue];
		j1518791240 -> j3083882744;
	j1883329488 [label="Я обожала газонные разбрызгиватели, когда была маленькой. Это было как волшебство. На самом ярком солнце в них можно было поймать радугу. Ты бегаешь сквозь них — и по всему телу пробегает такой приятный холодок.",shape=box,color=blue];
		j1883329488 -> j3616912121;
	j3083882744 [label="Также идеально подходит для того, чтобы брызгать в лицо брата до тех пор, пока он не закричит.",shape=box,color=blue];
		j3083882744 -> j3616912121;
	j3616912121 [label="«Счастье — это разбрызгиватель для газонов». Напиши это на поздравительной открытке.",shape=box,color=blue];
		j3616912121 -> j2497086960;
	j2497086960 [label="У тебя сегодня хорошее настроение. Радостно это видеть.",shape=box,color=red];
		j2497086960 -> j4285710066;
	j4285710066 [label="Да. И я не вижу, почему я должна держать это в себе. Поэтому я заливаю этим весь торговый центр.",shape=box,color=blue];
		j4285710066 -> j1090282690;
		j4285710066 -> j923828594;
		j4285710066 -> j4216603915;
	j1090282690 [label="Разве это не похоже на подмешивание наркотиков людям?",shape=box,color=red];
		j1090282690 -> j4175089117;
	j923828594 [label="Распространять позитивное настроение? А почему бы и нет. По мне, это не сильно отличается от того, чтобы включить музыку.",shape=box,color=red];
		j923828594 -> j2313601407;
	j4216603915 [label="Значит, сегодня тот самый день, когда я, возможно, увижу улыбку Артура?",shape=box,color=red];
		j4216603915 -> j2011633943;
	j4175089117 [label="... Отлично. Тут я, значит, пытаюсь быть тайной феей счастья, разбрасывающей радость, а в итоге превращаюсь в какого-то злого гоблина-наркодилера. Настроение улетело в пике.",shape=box,color=blue];
		j4175089117 -> j4077717902;
	j2313601407 [label="О! Хорошая аналогия!",shape=box,color=blue];
		j2313601407 -> j2749751467;
	j2011633943 [label="Может быть. Надеюсь. Ему пришлось столько всего вытерпеть.",shape=box,color=blue];
		j2011633943 -> j2642998457;
	j4077717902 [label="Ладно, я остановлюсь.",shape=box,color=blue];
		j4077717902 -> "Chat ends.";
	j2749751467 [label="Если Аои может влиять на настроение через музыку, то и я могу делать это через вибрации, верно?",shape=box,color=blue];
		j2749751467 -> j3059360085;
	j2642998457 [label="С момента твоего появления, так много всего изменилось. Я имею в виду не только тебя и меня. Я имею в виду всё. Даже торговый центр.",shape=box,color=blue];
		j2642998457 -> j3059360085;
	j3059360085 [label="Я рада, что Аои убедила меня остаться. Благослови её Бог.",shape=box,color=blue];
		j3059360085 -> j312123915;
		j3059360085 -> j1293442416;
	j312123915 [label="Подожди. Что она сделала?",shape=box,color=red];
		j312123915 -> j2737564095;
	j1293442416 [label="Отмотай назад. Кажется эта часть прошла мимо меня.",shape=box,color=red];
		j1293442416 -> j2737564095;
	j2737564095 [label="Я тебе не говорила?",shape=box,color=blue];
		j2737564095 -> j779742272;
	j779742272 [label="А, это было незадолго до твоего появления. Мы весь день возились с уничтожением улья Техрота. В воздухе прямо витало убийственное напряжение. Обычно сражения выжигают из нас злость, оставляя только усталость, чтобы сцепляться друг с другом уже не было сил. Но этот день оказался другим.",shape=box,color=blue];
		j779742272 -> j3337293722;
	j3337293722 [label="Я помню, как Артур сказал что-то вроде «завтра мы сделаем это снова», и это ударило нас всех, как кувалдой. Никто не произнёс ни слова по дороге обратно в торговый центр.",shape=box,color=blue];
		j3337293722 -> j3132163270;
		j3337293722 -> j2386777135;
		j3337293722 -> j442303416;
	j3132163270 [label="Ого. Отличный способ поднять боевой дух команды, Артур.",shape=box,color=red];
		j3132163270 -> j3468529240;
	j2386777135 [label="Он может быть таким обаятельным...",shape=box,color=red];
		j2386777135 -> j3468529240;
	j442303416 [label="На его плечах лежит большая ответственность. Это сказывается.",shape=box,color=red];
		j442303416 -> j3586499851;
	j3468529240 [label="Думаю, он просто хотел, чтобы мы сосредоточились на задаче. Но я чувствовала себя сломанной. Артур и Аои не разговаривали, Амир так доставал Куинси, что я начала бояться за его жизнь, а Лэтти косилась на меня при каждом удобном случае.",shape=box,color=blue];
		j3468529240 -> j105408554;
	j3586499851 [label="Да. Обычно ему удаётся это скрыть, даже когда он на пределе. Но, думаю, у него просто ничего не осталось.",shape=box,color=blue];
		j3586499851 -> j2718334435;
	j105408554 [label="В тот день я много времени провела, глядя в зеркало. Всё представляла, как моё отражение хватает меня за горло и шипит: «Ты — проблема. Если бы не ты, ничего этого бы не случилось».",shape=box,color=blue];
		j105408554 -> j3044776639;
		j105408554 -> j3867754212;
	j2718334435 [label="Он выгорел, пытаясь сохранить команду.",shape=box,color=blue];
		j2718334435 -> j3468529240;
	j3044776639 [label="Похоже, все остальные для разнообразия залезли в ТВОЮ голову.",shape=box,color=red];
		j3044776639 -> j1931803177;
	j3867754212 [label="Звучит как та фигня, что Лэтти обычно выдаёт.",shape=box,color=red];
		j3867754212 -> j1252663141;
	j1931803177 [label="О, это была не она. Это я. Вся накопившаяся неуверенность и самоненависть, которую я так долго загоняла внутрь, вырвалась наружу в тот день.",shape=box,color=blue];
		j1931803177 -> j3342842488;
	j1252663141 [label="Спасибо. Я знаю, что ты на моей стороне, и для меня это очень важно.",shape=box,color=blue];
		j1252663141 -> j3938339656;
	j3342842488 [label="Я подумала: этой группе нужен козёл отпущения.",shape=box,color=blue];
		j3342842488 -> j2729173998;
	j3938339656 [label="Я столько раз слышала эту фразу от Лэтти, что в итоге подумала: может, она права. Я подвергаю всех опасности, оставаясь здесь.",shape=box,color=blue];
		j3938339656 -> j4038706766;
	j2729173998 [label="Я монстр. Кто-то виноват во всём.",shape=box,color=blue];
		j2729173998 -> j4038706766;
	j4038706766 [label="Я придумала целый план. Ускользнуть ночью, спрятаться где-нибудь в городе, где Артур никогда меня не найдёт, и — вот этим я особенно гордилась — продолжать помогать издалека.",shape=box,color=blue];
		j4038706766 -> j2218961379;
		j4038706766 -> j1290623350;
	j2218961379 [label="Я думаю, он бы тебя нашёл. Он настроен решительно.",shape=box,color=red];
		j2218961379 -> j3505252772;
	j1290623350 [label="Более вероятным сценарием является то, что он бы погиб, пытаясь найти тебя.",shape=box,color=red];
		j1290623350 -> j3864068339;
	j3505252772 [label="Как? Амир не может отслеживать телепатию.",shape=box,color=blue];
		j3505252772 -> j2747766773;
	j3864068339 [label="Пытаешься вызвать у меня чувство вины?",shape=box,color=blue];
		j3864068339 -> j4054057276;
	j2747766773 [label="Я собиралась стать благородной изгнанницей. Залезать на крыши, выглядеть такой вся из себя суровой и решительной. Могла бы докладывать о скоплениях Техрота, предупреждать Гекс, где Скальдра устраивают блокпосты, даже быть экстренным реле для связи. И каждый раз, спасая Лэтти её задницу, заставляла бы её это прочувствовать. Мелочно? Ещё как.",shape=box,color=blue];
		j2747766773 -> j2834734008;
	j4054057276 [label="Не старайся. Я и так чувствую себя ужасно.",shape=box,color=blue];
		j4054057276 -> "Chat ends.";
	j2834734008 [label="Я выбрала момент. Дождалась, пока Амир заснёт. Затем накинула длинное пальто и как можно тише направилась в гараж.",shape=box,color=blue];
		j2834734008 -> j2400888259;
	j2400888259 [label="Решётка безопасности опустилась сама собой. Я обернулся и увидел Аои.",shape=box,color=blue];
		j2400888259 -> j3509660670;
	j3509660670 [label="Мы проговорили всю ночь и рыдали, обнявшись. Она тоже страдала. Глубоко. К тому моменту, как взошло солнце, я решила остаться.",shape=box,color=blue];
		j3509660670 -> j3416563612;
		j3509660670 -> j2393075364;
		j3509660670 -> j782724009;
	j3416563612 [label="Ещё есть время изменить своё мнение.",shape=box,color=red];
		j3416563612 -> j1271880230;
	j2393075364 [label="Аои замечательная. Я рада, что она была рядом с тобой.",shape=box,color=red];
		j2393075364 -> j3333361902;
	j782724009 [label="Это была удача.",shape=box,color=red];
		j782724009 -> j3333361902;
	j1271880230 [label="Какой бы мерзкий импульс ни заставил тебя это сказать, надеюсь, он станет тебе поперёк горла.",shape=box,color=blue];
		j1271880230 -> "Chat ends.";
	j3333361902 [label="+20 Chemistry",color=orange];
		j3333361902 -> j2779546238;
	j2779546238 [label="Да. Все шестеро должны были быть здесь в канун Нового года, как мне кажется. И ты, конечно. Незнакомка на пороге, приносящая удачу.",shape=box,color=blue];
		j2779546238 -> "Chat ends.";
}