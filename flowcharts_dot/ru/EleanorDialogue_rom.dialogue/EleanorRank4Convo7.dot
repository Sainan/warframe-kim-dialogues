		j622078137 -> j3186354975;
	j1865771731 [label="Извини. Я по глупости предположила, что раз твои воспоминания о Заримане столь ярки, ты сможешь вспомнить и своих родителей.",shape=box,color=blue];
		j1865771731 -> j2575598165;
	j2882537085 [label="Я рада и это заметно. Если ты не против того, что я так говорю.",shape=box,color=blue];
