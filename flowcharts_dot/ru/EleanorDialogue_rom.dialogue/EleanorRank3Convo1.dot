		j1684039121 -> j1025090141;
	j1025090141 [label="Ты знаешь это чувство, когда споришь с кем-то, и когда всё уже закончилось, ты думаешь о том, что именно нужно было сказать?",shape=box,color=blue];
		j1025090141 -> j223710217;
	j223710217 [label="У меня этого больше нет. У меня есть магическая кнопка «Я выигрываю». Всё, что мне нужно сделать, — это заглянуть в их разум, схватить то, чего они больше всего стыдятся, и ткнуть это им в лицо.",shape=box,color=blue];
		j223710217 -> j4047213814;
	j4047213814 [label="И я НЕ МОГУ так поступить, сколько бы я ни хотела. Потому что я должна быть «хорошим человеком».",shape=box,color=blue];
		j4047213814 -> j1716598737;
		j4047213814 -> j3178327245;
	j1716598737 [label="Дело не в попытке быть святым, Элеонора. Даже не в том, чтобы быть героем. Это о том, чтобы не быть подлым придурком.",shape=box,color=red];
		j1716598737 -> j3974501619;
