		j2624178496 -> j3397450325;
	j1389914310 [label="Да, я сделала это. Я извинилась, пообещала, что больше никогда не сделаю ничего подобного, но до сих пор чувствую себя дерьмово из-за этого.",shape=box,color=blue];
		j1389914310 -> j3604130884;
	j2674296156 [label="Я рассказываю тебе об этом, потому что туда он вернулся, в своей голове. Он вспомнил, что чувствовал себя в безопасности, когда мог плакать рядом со мной, если ему это действительно НУЖНО было. А теперь?",shape=box,color=blue];
