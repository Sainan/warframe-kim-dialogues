		j4028813067 -> j869190384 [label=false,color=brown];
	j3823696637 [label="Мальчишки смеялись и говорили, что Артур должен быть счастлив. Его игрушка отправилась в Зал Героев на зимний праздник, как воины раньше. Я хотела их убить. Маленькие ублюдки.",shape=box,color=blue];
