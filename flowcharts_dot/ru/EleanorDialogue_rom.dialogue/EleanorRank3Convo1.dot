		j1258900690 -> j3907433739;
	j385320917 [label="Читай дальше на свой страх и риск.",shape=box,color=blue];
		j385320917 -> j3907433739;
	j3907433739 [label="Недолго после того, как нам всем сделали прививки Энтрати, я подумала, что у меня вторичная инфекция мочевого пузыря. Дружище, я вскакивала с дивана и садилась обратно, как йо-йо. Даже Лэтти охотилась за клюквенным соком и минеральной водой для меня.",shape=box,color=blue];
		j3907433739 -> j291271366;
	j291271366 [label="Оказалось, я просто чувствовала естественные биологические позывы шести человек. Моя нервная система автоматически решила, что они мои.",shape=box,color=blue];
		j291271366 -> j1154651799;
		j291271366 -> j5732229;
	j1154651799 [label="Что было хуже? Узнать, что ты не катастрофически больна, или...",shape=box,color=red];
		j1154651799 -> j2719117531;
	j5732229 [label="Должно быть, было облегчением узнать правду.",shape=box,color=red];
		j5732229 -> j963396434;
	j2719117531 [label="Ну, я КАТАСТРОФИЧЕСКИ больна. Давайте уж называть вещи своими именами. Но я лучше буду справляться с телепатией, чем с бесконечным циститом.",shape=box,color=blue];
		j2719117531 -> j2860120443;
	j963396434 [label="После этого случая я научилась строить стены вокруг своего разума. Иногда Лэтти находит лекарства, которые могут отключить всю телепатию во мне на несколько часов. У меня есть запас таких, но я их принимаю только тогда, когда мне действительно, очень нужно поспать.",shape=box,color=blue];
		j963396434 -> j2860120443;
	j2860120443 [label="Это такой полезный талант, в этом проблема. Артур считает его стратегическим активом.",shape=box,color=blue];
		j2860120443 -> j3836296683;
		j2860120443 -> j1229817118;
	j3836296683 [label="А как насчёт частной жизни людей? Это же плохо для морального духа?",shape=box,color=red];
		j3836296683 -> j168677965;
	j1229817118 [label="А если кто-то *не* хочет, чтобы его разум исследовали?",shape=box,color=red];
		j1229817118 -> j473650074;
	j168677965 [label="Это чертовски плохо для морального духа.",shape=box,color=blue];
		j168677965 -> j4058442446;
	j473650074 [label="Даже если я уверяю, что могу это контролировать, ты думаешь, они мне поверят?",shape=box,color=blue];
		j473650074 -> j4058442446;
	j4058442446 [label="Никто из нас не мягкий. Мы привыкли к суровым условиям. Это место — пятизвёздочный отель по сравнению с обычными британскими армейскими казармами.",shape=box,color=blue];
		j4058442446 -> j3642750017;
	j3642750017 [label="Ты вместе принимаешь душ, вместе ешь и так далее. Привыкаешь друг к другу. Это необходимо.",shape=box,color=blue];
		j3642750017 -> j968568574;
	j968568574 [label="Но, как сказал бы Артур, есть чёртова граница.",shape=box,color=blue];
		j968568574 -> j3425811617;
	j3425811617 [label="Так как ты с этим справляешься?",shape=box,color=red];
		j3425811617 -> j704434821;
	j704434821 [label="Я просто не даю им знать, насколько много их внутренней жизни я слышу, вижу и чувствую. Я не лезу без приглашения, что бы там Лэтти ни думала.",shape=box,color=blue];
		j704434821 -> j1492074547;
	j1492074547 [label="Иногда кто-то из них думает что-то и сразу же думает: «Она слышала, как я это подумал?» Добрее дать им сомневаться. То, чего они не знают, не может им навредить.",shape=box,color=blue];
		j1492074547 -> j3387136104;
		j1492074547 -> j3571576064;
