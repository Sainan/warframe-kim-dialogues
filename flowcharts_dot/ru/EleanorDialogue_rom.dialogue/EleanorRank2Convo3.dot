	j1528560124 [label="Детская невинность ничто по сравнению с сознанием крысы.",shape=box,color=blue];
		j1528560124 -> j148884498;
		j1528560124 -> j1877309991;
	j148884498 [label="Звучит приятно, судя по тому, как ты это описываешь.",shape=box,color=red];
		j148884498 -> j1998653922;
	j1877309991 [label="Ты завидуешь крысе?",shape=box,color=red];
		j1877309991 -> j708700490;
	j1998653922 [label="О, это так и было. Я уверена, у крысы есть свои тёмные стороны, вроде необходимости съесть своего мёртвого брата, чтобы хищники не учуяли его. Но тот мимолётный момент простоты был почти как религиозное озарение.",shape=box,color=blue];
		j1998653922 -> j1434173477;
	j708700490 [label="В некотором роде.",shape=box,color=blue];
		j708700490 -> j1631770808;
	j1434173477 [label="Думаю, быть крысой — это чем-то похоже на быть монахом. Жить за счёт объедков, скромно продвигаться по миру, не владеть ни машиной, ни домом.",shape=box,color=blue];
		j1434173477 -> j2419297974;
