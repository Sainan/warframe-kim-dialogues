		j3919631715 -> j1327907364;
	j2012390356 [label="Верно. Возможно, там, откуда ты, их действительно нет.",shape=box,color=blue];
		j2012390356 -> j1327907364;
	j1327907364 [label="Что-то вроде посвящённой Луне. Выбритая голова, белые или чёрные одежды в зависимости от Аспекта, очень строгая жизнь. От тебя ожидается сосредоточенность на святых вещах в ущерб всему остальному. Ежедневная молитва и медитация. Простая диета.",shape=box,color=blue];
		j1327907364 -> j2331158725;
		j1327907364 -> j405183575;
		j1327907364 -> j1249501998;
		j1327907364 -> j4062097892;
	j2331158725 [label="Звучит ужасно.",shape=box,color=red];
