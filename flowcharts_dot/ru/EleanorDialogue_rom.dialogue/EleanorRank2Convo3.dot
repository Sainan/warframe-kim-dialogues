digraph {
	"EleanorRank2Convo3 begins." [color=orange];
		"EleanorRank2Convo3 begins." -> j4004962415;
	j4004962415 [label="Хорошо, мы знакомы уже какое-то время, и ты из будущего, и я думаю, что могу доверять тебе, чтобы быть честной со мной в этом, так что я должна задать самый важный вопрос.",shape=box,color=blue];
		j4004962415 -> j2629934427;
	j2629934427 [label="Мы одни? Или мы наконец-то вышли на контакт с кем-то другим?",shape=box,color=blue];
		j2629934427 -> j2533654519;
		j2629934427 -> j603873629;
		j2629934427 -> j2918999437;
	j2533654519 [label="Ты имеешь в виду инопланетную жизнь? Пока нет, но, может быть, однажды.",shape=box,color=red];
		j2533654519 -> j774226246;
	j603873629 [label="К сожалению, ближе всего мы приблизились к Безразличию.",shape=box,color=red];
		j603873629 -> j678538277;
	j2918999437 [label="Да, мы одни. Только люди и то, что создано людьми.",shape=box,color=red];
		j2918999437 -> j3136977236;
	j774226246 [label="Чёрт возьми.",shape=box,color=blue];
		j774226246 -> j3547448149;
	j678538277 [label="Что ТАКОЕ «Безразличие»? Я видела его образы в твоём сознании, но не могу понять. Оно заставляет меня хотеть убежать и спрятаться где-то далеко за очень толстыми стенами.",shape=box,color=blue];
		j678538277 -> j1142369165;
		j678538277 -> j1765099199;
	j3136977236 [label="Самый мрачный из возможных ответов.",shape=box,color=blue];
		j3136977236 -> j2820519408;
	j3547448149 [label="Я действительно надеялась, что там есть что-то, с чем мы могли бы поговорить. Что-то с действительно внешним взглядом на наш вид.",shape=box,color=blue];
		j3547448149 -> j2165046474;
	j1142369165 [label="Мы до сих пор не уверены, что это. Или чего оно ХОЧЕТ. Мы даже не знаем, является ли оно древней сущностью, как считает Красная Вуаль, или Альбрехт как-то привёл его в существование.",shape=box,color=red];
		j1142369165 -> j2348588541;
	j1765099199 [label="Это то, что забрало Майора Русалка.",shape=box,color=red];
		j1765099199 -> j2149256653;
	j2820519408 [label="Я не уверена почему. Интеллект есть интеллект, в конце концов. Не должно иметь значения, открыли мы его или создали.",shape=box,color=blue];
		j2820519408 -> j2790746893;
	j2165046474 [label="Говорить с ним и… другие вещи, конечно.",shape=box,color=blue];
		j2165046474 -> j2816983586;
		j2165046474 -> j1765791478;
	j2348588541 [label="Если я скажу тебе, что это то, что вселилось в майора Русалку, это поможет?",shape=box,color=red];
		j2348588541 -> j2149256653;
	j2149256653 [label="Ох.",shape=box,color=blue];
		j2149256653 -> j2304472814;
	j2790746893 [label="Думаю, я просто хотела уверенности, что Вселенная не сложила все яйца в одну корзину. Потому что если люди — это всё, что есть, то ситуация фиговая.",shape=box,color=blue];
		j2790746893 -> j809813565;
	j2816983586 [label="Ищешь друга-монстра с щупальцами?",shape=box,color=red];
		j2816983586 -> j596168512;
	j1765791478 [label="Подожди. Что именно ты имела в виду под взаимодействием?",shape=box,color=red];
		j1765791478 -> j4081896825;
	j2304472814 [label="О, нет.",shape=box,color=blue];
		j2304472814 -> j2251406790;
	j809813565 [label="Ну, что ж.",shape=box,color=blue];
		j809813565 -> "Chat ends.";
	j596168512 [label="+5 Chemistry",color=orange];
		j596168512 -> j3635540004;
	j4081896825 [label="Что-то вроде общения, полагаю. Не обязательно что-то физическое. Я просто хочу соприкоснуться разумом с чем-то, что не только не человеческое, но даже не земное.",shape=box,color=blue];
		j4081896825 -> j874747127;
	j2251406790 [label="О, ЧЁРТ.",shape=box,color=blue];
		j2251406790 -> j3356596266;
	"Chat ends." [color=orange];
	j3635540004 [label="Прошу прощения. Я не «монстр с щупальцами». Я монстр, у которого ЕСТЬ ЩУПАЛЬЦЕ. Одно. Пока что.",shape=box,color=blue];
		j3635540004 -> j1934250706;
	j874747127 [label="Я однажды пыталась соприкоснуться разумом с одной из крыс Лэтти, просто, чтобы посмотреть, что произойдёт.",shape=box,color=blue];
		j874747127 -> j1541755851;
		j874747127 -> j840915512;
	j3356596266 [label="ЭТО? Это то же самое, что твоё «Безразличие»?",shape=box,color=blue];
		j3356596266 -> j1819886019;
	j1934250706 [label="Если бы я только хотела поговорить с существами с щупальцами, я бы зашла в Зоологический сад принцессы Люсины и подружилась с осьминогом.",shape=box,color=blue];
		j1934250706 -> j2737416971;
	j1541755851 [label="Бедное создание не пострадало?",shape=box,color=red];
		j1541755851 -> j4227512782;
	j840915512 [label="Хорошо, я должна спросить, как это прошло.",shape=box,color=red];
		j840915512 -> j4227512782;
	j1819886019 [label="Я коснулась его. Я коснулась этой вещи краешком своего разума и заблокировала память.",shape=box,color=blue];
		j1819886019 -> j2270093554;
	j2737416971 [label="... и теперь я жалею, что не сделала этого. Осьминоги очень умны. Мы, вероятно, уже сочиняли бы стихи вместе. Интересно, где сейчас этот осьминог?",shape=box,color=blue];
		j2737416971 -> j127413906;
	j4227512782 [label="Для него это были ужасные три секунды.",shape=box,color=blue];
		j4227512782 -> j1197928259;
	j2270093554 [label="Я даже не помню, когда это случилось. Она читала одну из своих речей по громкоговорителю, и я подумала: «Я никогда не исследовал её разум, интересно, что там внутри, вероятно, кожа и иглы», и я прикоснулся к ее разуму и…",shape=box,color=blue];
		j2270093554 -> j3017629488;
	j127413906 [label="Кто-то, вероятно, съел его. Обычно это происходит с животными в зоопарке в карантинных городах, когда запасы заканчиваются.",shape=box,color=blue];
		j127413906 -> j3851427850;
	j1197928259 [label="Бедное создание. Я не подумала, что для НЕГО может означать соприкосновение с МОИМ разумом, понимаешь? Представь ребёнка, потерявшегося в бесконечном универмаге, который орёт во всё горло, зовя мать, снова и снова. Всё было огромным, непостижимым, и имело ЗВУКИ и ФОРМЫ вместо ЗАПАХОВ.",shape=box,color=blue];
		j1197928259 -> j2958852670;
	j3017629488 [label="чёрт-чёрт-чёрт",shape=box,color=blue];
		j3017629488 -> j696018008;
	j3851427850 [label="А всё, что не съели, теперь, вероятно, заражено Техротом.",shape=box,color=blue];
		j3851427850 -> j2266246722;
	j2958852670 [label="Я разорвала связь так быстро, как смогла. Он немного покружился с головокружением час или около того, а потом всё прошло. Сомневаюсь, что он вообще помнит, что с ним случилось.",shape=box,color=blue];
		j2958852670 -> j1284010502;
		j2958852670 -> j3821169255;
	j696018008 [label="Я пока не могу об этом говорить. Прости.",shape=box,color=blue];
		j696018008 -> j1609974498;
	j2266246722 [label="… именно так это было во «сне», да? Только это вообще не сон. Это произошло. Или произойдёт. Или произойдёт какая-то версия этого. Что-то случается в этом зоопарке.",shape=box,color=blue];
		j2266246722 -> j2690501334;
	j1284010502 [label="А с твоей стороны? Каково это было — быть крысой?",shape=box,color=red];
		j1284010502 -> j838950068;
	j3821169255 [label="Это всего лишь крыса, да ладно.",shape=box,color=red];
		j3821169255 -> j602690189;
	j1609974498 [label="+20 Chemistry",color=orange];
		j1609974498 -> j238837693;
	j2690501334 [label="Я постараюсь запомнить всё, что смогу.",shape=box,color=blue];
		j2690501334 -> j2069459616;
	j838950068 [label="Не знаю, был ли у тебя когда-нибудь такой спад, когда всё, чего ты хочешь, — это лежать в постели, дышать и просто существовать, не делая больше ничего. Это было очень похоже.",shape=box,color=blue];
		j838950068 -> j1209929547;
	j602690189 [label="Не дай Лэтти услышать, как ты это говоришь. Если, конечно, тебе нравятся твои органы на их местах.",shape=box,color=blue];
