		j1249501998 -> j2487968450;
	j4062097892 [label="Звучит… так, будто это не для меня.",shape=box,color=red];
		j4062097892 -> j4240281419;
	j1498612014 [label="С виду да. Единственная разница между монастырской жизнью и тюрьмой в том, что ты делаешь это добровольно.",shape=box,color=blue];
		j1498612014 -> j1745465873;
	j2654617630 [label="Это кажется потрясающим большинству молодых послушников, пока они действительно не попробуют монастырскую жизнь. Думаю, процент ухода составляет около восьмидесяти.",shape=box,color=blue];
		j2654617630 -> j1745465873;
	j2487968450 [label="О, нет. Вначале, да, нужно избавиться от всего, чему общество научило тебя нуждаться. Эта фаза, вероятно, самая сложная.",shape=box,color=blue];
		j2487968450 -> j466644830;
	j4240281419 [label="+10 Chemistry",color=orange];
		j4240281419 -> j1851433320;
	j1745465873 [label="Оказалось, это не для меня. Я слишком люблю жить в этом мире.",shape=box,color=blue];
		j1745465873 -> j230854815;
	j466644830 [label="Но когда ты уже устроишься, какой вызов останется? Искушение? Ты его никогда не увидишь. Смысл в том, чтобы отрезать себя от всего, что может отвлечь тебя от святого.",shape=box,color=blue];
		j466644830 -> j1745465873;
	j1851433320 [label="Это оказалось не подходящим и для меня. Оказалось, я слишком люблю жить в этом мире.",shape=box,color=blue];
		j1851433320 -> j230854815;
	j230854815 [label="Но Сол… это как будто существует альтернативная версия меня, которая сидит где-то на вершине горы, окружённая облаками благовоний, перебирая бусы из чёрного агата и лунного камня, и смотрит на меня с откровенным сожалением.",shape=box,color=blue];
		j230854815 -> j1022722371;
	j1022722371 [label="Я знаю, что это просто фантазия. Такая же фантазия, как эти рыцари в доспехах из сказок Артура.",shape=box,color=blue];
		j1022722371 -> j2243748303;
		j1022722371 -> j2627603811;
	j2243748303 [label="Мы все фантазируем о том, что однажды у нас всё будет под контролем.",shape=box,color=red];
		j2243748303 -> j511216562;
	j2627603811 [label="Я разговариваю с альтернативной версией себя всё время. Мы даже ели вместе. Это не так безумно, как тебе кажется.",shape=box,color=red];
		j2627603811 -> j473302666;
	j511216562 [label="Спасибо. Почему-то это невероятно утешительно слышать.",shape=box,color=blue];
		j511216562 -> j4100117117;
	j473302666 [label="Да. Я всё время забываю, что то, через что тебе пришлось пройти, — это чистое безумие по любым нормальным стандартам.",shape=box,color=blue];
		j473302666 -> j4100117117;
	j4100117117 [label="Знаешь… я никогда раньше этого не делала, но думаю, попробую дотянуться до неё, вдруг она существует. Да. Может быть, я смогу связаться с «Сестрой Элеонорой» телепатически.",shape=box,color=blue];
		j4100117117 -> j3514973598;
	j3514973598 [label="Что самое худшее, что может случиться?",shape=box,color=blue];
