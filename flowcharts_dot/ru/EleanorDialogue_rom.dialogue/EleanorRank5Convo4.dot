		j4194722692 -> j726537946;
	j726537946 [label="Когда стало совсем плохо, она лежала в постели и визжала. Мы уходили утром в школу, а когда я возвращалась к нашему дому, у меня сердце сжималось, потому что я слышала её крики за десять домов до нашего, всё те же, спустя восемь часов.",shape=box,color=blue];
		j726537946 -> j515718943;
	j515718943 [label="Наш отец иногда говорил о том, чтобы положить ей подушку на лицо. Мы думали, он шутит, ведь, конечно, шутит — это же его мать.",shape=box,color=blue];
