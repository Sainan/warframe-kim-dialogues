		j3432236701 -> "Chat ends.";
	j3278198886 [label="Не то чтобы я не была чрезвычайно благодарна и, честно говоря, немного шокирована, но... почему? ПОЧЕМУ ты позволяешь мне жить? Когда все остальные здесь с радостью пустили бы пулю мне в голову?",shape=box,color=blue];
