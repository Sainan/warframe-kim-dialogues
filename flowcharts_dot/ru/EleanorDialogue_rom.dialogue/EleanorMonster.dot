		j3278198886 -> j4099980693;
	j584290441 [label="Спасибо. Ты для меня во многом загадка, но теперь я знаю, что ты можешь пустить пулю мне в голову, а такие вещи важны на рабочем месте.",shape=box,color=blue];
		j584290441 -> j517294908;
	"Chat ends." [color=orange];
	j993744247 [label="Грань между «героем» и «монстром» гораздо размытее, чем большинство готово признать. Я сражаюсь против Гринир и Корпуса, но приходилось сражаться и бок о бок с ними.",shape=box,color=red];
		j993744247 -> j3132565186;
	j4099980693 [label="Потому что я считаю тебя одной из самых яростно умных, удивительно саркастичных людей, которых мне пришлось повидать. Мне кажется, даже Техрот не смог бы отнять у тебя это.",shape=box,color=red];
		j4099980693 -> j2183741860;
	j517294908 [label="Иииии я только что почувствовала, как Техрот в моих клетках тревожно задрожал.",shape=box,color=blue];
		j517294908 -> j1712341488;
	j3132565186 [label="Я не верю в слова «Добро» и «Зло». Есть только люди. Почему ты имеешь меньшее право на существование, чем я? У меня может и нет языка, поражённого Техротом, но природа меня так же изуродовала, как и тебя.",shape=box,color=red];
		j3132565186 -> j2832059066;
	j2183741860 [label="Как... прекрасно наивно. И чудесно мило, в трагическом смысле.",shape=box,color=blue];
		j2183741860 -> j3771767955;
	j1712341488 [label="Думаю, теперь он будет вести себя немного лучше. Ничто так не наводит страх на паразита, как угроза его хозяину, верно?",shape=box,color=blue];
		j1712341488 -> j2864742878;
	j2832059066 [label="Но вот ты здесь, сражаешься со Скальдрой и Техротом, пытаешься найти способ остановить Безразличие. Очевидно, ты веришь, что где-то всё же должна быть проведена грань.",shape=box,color=blue];
		j2832059066 -> j2989148798;
		j2832059066 -> j216063780;
	j3771767955 [label="То, что ты веришь, будто я выйду с другой стороны этой ситуации в роли какой-то королевы улья, сохранив свою сущность, каким-то странным голосом заражения, с которым ты сумеешь подружиться или, возможно, даже будешь восхищаться.",shape=box,color=blue];
		j3771767955 -> j2939530622;
	j2864742878 [label="Boolean EleanorNoDate is now true.",color=orange];
		j2864742878 -> "Chat ends.";
	j2989148798 [label="Безразличие было выпущено на свободу Энтрати. Человеком. Виной тому была человеческая гордыня.",shape=box,color=red];
		j2989148798 -> j801314337;
	j216063780 [label="Защищать людей от смерти — это не то же самое, что верить, что что-то должно быть уничтожено или не имеет права на существование.",shape=box,color=red];
		j216063780 -> j1889802906;
	j2939530622 [label="Я бы пожурила тебя за то, что мир не устроен как в книге сказок, но... Я вспомнила, кто ты и откуда ты родом.",shape=box,color=blue];
		j2939530622 -> j3190909077;
	j801314337 [label="Но Техрот — это не Безразличие. Что из этого? Что из более биологической, земной угрозы?",shape=box,color=blue];
		j801314337 -> j563190382;
		j801314337 -> j1352973238;
	j1889802906 [label="Защищать стадо овец от волков — это не то же самое, что выходить и убивать всех волков за то, что они «злые».",shape=box,color=red];
		j1889802906 -> j825182730;
	j3190909077 [label="Так что, полагаю, мне не стоит удивляться.",shape=box,color=blue];
		j3190909077 -> j3220887743;
	j563190382 [label="Мы на самом деле не знаем, откуда появился Техрот. Он мог быть создан человеком. А может произошло и от чего-то совершенно другого.",shape=box,color=red];
		j563190382 -> j2481725286;
	j1352973238 [label="Простой факт состоит в том, что ты не хочешь быть мною убитой. Поэтому я и не хочу этого делать. Я бы тебя отпустил.",shape=box,color=red];
		j1352973238 -> j2232785753;
	j825182730 [label="А что, если от Элеоноры ничего не осталось? Что, если ты не увидишь ничего, кроме шипящего, рычащего, плюющегося зверя?",shape=box,color=blue];
		j825182730 -> j4255315371;
		j825182730 -> j1352973238;
	j3220887743 [label="В любом случае, я думаю, я благодарна, что ты меня не убьёшь. По крайней мере.",shape=box,color=blue];
		j3220887743 -> "Chat ends.";
	j2481725286 [label="А если Техрот окажется ещё одним человеком, выпущенным на свободу, как ваш Человек в Стене? Что тогда? Вы выследите меня и убьёте?",shape=box,color=blue];
		j2481725286 -> j2159725486;
		j2481725286 -> j2534560412;
	j2232785753 [label="Хотя я благодарна, что ты проявляешь готовность исполнить желание чудовищной леди, я, пожалуй, надеялась на что-то более... обдуманное.",shape=box,color=blue];
		j2232785753 -> j1790079250;
	j4255315371 [label="Откуда у меня появится уверенность, что в Элеоноре ничего не осталось? Ты говоришь о том, как Техрот думает. Как он с тобой разговаривает. Это явно разумное существо.",shape=box,color=red];
		j4255315371 -> j3123031627;
	j2159725486 [label="Нет, я не смогу.",shape=box,color=red];
		j2159725486 -> j3990981831;
