digraph {
	"EleanorRank4Convo5 begins." [color=orange];
		"EleanorRank4Convo5 begins." -> j1523531885;
	j1523531885 [label="Королевство, которое было твоим. Дувири. Это ведь не вымысел, правда?",shape=box,color=blue];
		j1523531885 -> j3843339168;
		j1523531885 -> j3439647684;
	j3843339168 [label="И так, и не совсем так. Место и персонажи появились из моей любимой детской книги. «Сказки о Дувири». Она была воплощена в жизнь с помощью Бездны.",shape=box,color=red];
		j3843339168 -> j2104835016;
	j3439647684 [label="Мне так надоело говорить о Дувири...",shape=box,color=red];
		j3439647684 -> j3114209103;
	j2104835016 [label="Тебе удалось этого добиться?",shape=box,color=blue];
		j2104835016 -> j1018764154;
		j2104835016 -> j814269272;
	j3114209103 [label="+5 Chemistry",color=orange];
		j3114209103 -> j1062401306;
	j1018764154 [label="Думаю, остальные дети, кроме меня, погибли или были спасены. Подробности мне неясны.",shape=box,color=red];
		j1018764154 -> j2584999467;
	j814269272 [label="Это было не совсем намеренно. Всё, что я помню, это…",shape=box,color=red];
		j814269272 -> j2584999467;
	j1062401306 [label="Упс. Моя ошибка.",shape=box,color=blue];
		j1062401306 -> j283463543;
	j2584999467 [label="Ангелы наступали, пробивая стены, и мне некуда было бежать. У меня была книга, и я помню, как пришла в голову мысль: «Здесь я могу быть в безопасности.»",shape=box,color=red];
		j2584999467 -> j437480640;
	j283463543 [label="Это было безрассудно с моей стороны, не так ли? Это место не было для тебя лагерем отдыха.",shape=box,color=blue];
		j283463543 -> j1939058192;
	j437480640 [label="Ангелы? В это время ангелы считались доброжелательными существами. Иногда ужасающими, но на стороне добра. Если только не имелись в виду падшие ангелы.",shape=box,color=blue];
		j437480640 -> j319127675;
	j1939058192 [label="Мне искренне жаль. Отныне я буду держать своё любопытство под контролем.",shape=box,color=blue];
		j1939058192 -> "Chat ends.";
	j319127675 [label="За моё время это не особо изменилось. Ангелы — это крылатые, сияющие существа из старых историй, которые помогают добрым людям. Я говорю об Ангелах Бездны. Это совсем другие создания.",shape=box,color=red];
		j319127675 -> j515284859;
	"Chat ends." [color=orange];
	j515284859 [label="Понятно. Я не могу представить, чтобы нечто под названием «Ангел Бездны» спустилось и благословило новорожденного.",shape=box,color=blue];
		j515284859 -> j1554018714;
	j1554018714 [label="Насколько я могу судить, они были людьми. Когда песня Ангелов достигает их, они начинают меняться. Когда они больше не могут ей сопротивляться, они сами становятся Ангелами.",shape=box,color=red];
		j1554018714 -> j419058010;
	j419058010 [label="Будущее, как ты его описываешь, звучит чертовски ужасно.",shape=box,color=blue];
		j419058010 -> j4211379876;
	j4211379876 [label="Я не могу винить тебя за желание спрятаться от этих Ангелов Бездны. Особенно если они были…",shape=box,color=blue];
		j4211379876 -> j3961835449;
	j3961835449 [label="О. У меня просто возникла мысль. Это ведь не твои родители, да?",shape=box,color=blue];
		j3961835449 -> j2988463339;
		j3961835449 -> j1688690440;
		j3961835449 -> j26178340;
	j2988463339 [label="Я верю в то, что мои родители мертвы.",shape=box,color=red];
		j2988463339 -> j1189142550;
	j1688690440 [label="Может и они. Другие дети слышали, как их родители звали.",shape=box,color=red];
		j1688690440 -> j1189142550;
	j26178340 [label="Думаю, да. Куинн предупреждал меня не искать знакомые голоса. Наверное, он это имел в виду.",shape=box,color=red];
		j26178340 -> j1189142550;
	j1189142550 [label="Мне очень жаль. Я рада, что тебе удалось найти безопасную гавань, даже если она не осталась такой.",shape=box,color=blue];
		j1189142550 -> j1878320752;
	j1878320752 [label="Это было безопасно долгое время. Достаточно долго, чтобы наступило забытье. Думаю, это то, чего мне хотелось больше всего.",shape=box,color=red];
		j1878320752 -> j1356287073;
	j1356287073 [label="Но как, черт возьми, тебе это удалось? Ты всего лишь ребёнок. Что дало тебе силу создать целое королевство из ничего?",shape=box,color=blue];
		j1356287073 -> j3141933706;
		j1356287073 -> j1574778851;
		j1356287073 -> j991030132;
	j3141933706 [label="Дети интересны тем, что они ещё не знают, чего они не могут сделать. У меня не было знания, что я не могу. Поэтому всё получилось.",shape=box,color=red];
		j3141933706 -> j2682153613;
	j1574778851 [label="У меня было так много эмоций, все сразу, что я... просто... они преобразовались. Это трудно объяснить.",shape=box,color=red];
		j1574778851 -> j1896473584;
	j991030132 [label="Бездна — это не «ничто». Я знаю, это звучит так, как будто это должно быть так, но это не так. Альбрехт это доказал.",shape=box,color=red];
		j991030132 -> j1896473584;
	j2682153613 [label="Когда мне было пять, я хотела стать ведьмой. Я кричала «Абракадабра» и прыгала с лестницы. Я не знала, что не умею летать. Но я быстро поняла.",shape=box,color=blue];
		j2682153613 -> j3275355120;
	j1896473584 [label="Мы знаем об этом очень мало, даже сейчас. Но даже Орокин поняли одну вещь: Бездна реагирует на человеческие эмоции.",shape=box,color=red];
		j1896473584 -> j1251930332;
	j3275355120 [label="Сила, создавшая Дувири, должна была прийти откуда-то извне. Она была использована тобой, чтобы сделать что-то невероятное, но она не была твоей.",shape=box,color=blue];
		j3275355120 -> j1244035351;
	j1251930332 [label="ПОНЯТНО. Так что если ты посмотришь в Бездну с… хм, скажем, чувством голода… ты можешь создать рыскающего полуголодного волка. Или королевский пир, может быть. Или буханку хлеба со скрежещущими в ней волчьими зубами.",shape=box,color=blue];
		j1251930332 -> j2893320131;
	j1244035351 [label="По крайней мере, не поначалу. Но сейчас это в тебе. Дикая энергия. Петли во времени. Боги, ты чудо.",shape=box,color=blue];
		j1244035351 -> j1072596354;
	j2893320131 [label="Какая ужасающая и воодушевляющая мысль.",shape=box,color=blue];
		j2893320131 -> j1072596354;
	j1072596354 [label="Почему Орокин посчитали хорошей идеей запустить корабль, полный детей, в Бездну? Разве это не была эмоциональная бомба замедленного действия, готовая взорваться?",shape=box,color=blue];
		j1072596354 -> j2378823517;
	j2378823517 [label="Вот для чего предназначалась эта книга рассказов. На первый взгляд, она была о вычурных персонажах при дворе безумного короля, чьё настроение менялось каждый день.",shape=box,color=red];
		j2378823517 -> j2850852440;
	j2850852440 [label="Но настоящий урок этой книги был о том, как управлять своими эмоциями. Мы должны были извлечь из этого важный урок, чтобы путешествие было безопасным.",shape=box,color=red];
		j2850852440 -> j911935242;
	j911935242 [label="Тьфу. Извини, но тебе удалось всё испортить.",shape=box,color=blue];
		j911935242 -> j3844530337;
	j3844530337 [label="Почему бы не просто оставить истории как истории? Я НЕНАВИДЕЛА, когда узнала, что Рудольфо, Львиный Король, который героически погиб в битве с Зодиакальным Змеем, должен был быть Сол с солнечными пятнами, страдающим на звёздном дереве.",shape=box,color=blue];
		j3844530337 -> j10455533;
	j10455533 [label="Прятать религию в детских книгах — это как прятать лекарства в конфетах. ПРОСТО РАССКАЗЫВАЙТЕ ИСТОРИИ.",shape=box,color=blue];
		j10455533 -> j3694378229;
		j10455533 -> j3759804857;
	j3694378229 [label="Не имею понятия о Рудольфо — короле леопардов.",shape=box,color=red];
		j3694378229 -> j924593689;
	j3759804857 [label="Я хочу когда-нибудь услышать историю о Рудольфо — короле леопардов.",shape=box,color=red];
		j3759804857 -> j3490187571;
	j924593689 [label="Я могу умереть счастливой женщиной, зная, что об этих книгах забудут. Ура будущему!",shape=box,color=blue];
		j924593689 -> j2498720162;
	j3490187571 [label="Фу. Пожалуйста, нет. Это не легенда, это ужасная серия детских книг.",shape=box,color=blue];
		j3490187571 -> j794191593;
	j2498720162 [label="Но ведь у вас же есть истории, не так ли?",shape=box,color=blue];
		j2498720162 -> j3406140989;
	j794191593 [label="Но если настаиваешь, я могу рассказать тебе некоторые из наших настоящих легенд.",shape=box,color=blue];
		j794191593 -> j1379266957;
		j794191593 -> j4037888610;
	j3406140989 [label="Множество. Даже у Тэнно и варфреймов есть истории.",shape=box,color=red];
		j3406140989 -> j1841764872;
	j1379266957 [label="Мне бы понравилось.",shape=box,color=red];
		j1379266957 -> j4290288449;
	j4037888610 [label="Не сейчас [Завершить]",shape=box,color=red];
		j4037888610 -> "Chat ends.";
	j1841764872 [label="Есть такой парень, Драсус Левериан. Он как музейный куратор. Он хранит записи всех легенд варфреймов.",shape=box,color=red];
		j1841764872 -> j4097659761;
	j4290288449 [label="Посмотрим. Конечно, есть Артур. Старый добрый Артур. Оригинал. Лучший в Британии.",shape=box,color=blue];
		j4290288449 -> j2359144410;
	j4097659761 [label="Ну, это меня ошарашило. Ты из далёкого будущего, но ты в это же время в стороне от событий в Дувири так много лет, что события, которые произошли при твоей жизни, успели стать мифами?",shape=box,color=blue];
		j4097659761 -> j2080761280;
	j2359144410 [label="А потом была Локсли Янг. Я её любила. Хотела быть ею. Она была образцом определённого типа молодой женщины.",shape=box,color=blue];
		j2359144410 -> j2332496741;
	j2080761280 [label="У меня начинается головокружение.",shape=box,color=blue];
		j2080761280 -> j2500441248;
		j2080761280 -> j910428759;
	j2332496741 [label="Какова её история?",shape=box,color=red];
		j2332496741 -> j3355069241;
	j2500441248 [label="Поверь мне. Я знаю, что ты чувствуешь.",shape=box,color=red];
		j2500441248 -> j293500199;
	j910428759 [label="Тогда расскажи мне о ваших мифах.",shape=box,color=red];
		j910428759 -> j1014600652;
	j3355069241 [label="Хорошо. Краткая версия: она была дочерью знатного человека в стародавние времена, когда земля страдала под жестоким королём.",shape=box,color=blue];
		j3355069241 -> j1236287;
	j293500199 [label="Вы ведь тоже сказочный персонаж, не так ли?",shape=box,color=blue];
		j293500199 -> j3613757941;
	j1014600652 [label="Вот это да. Если я начну, мы тут весь год просидим.",shape=box,color=blue];
		j1014600652 -> j4290288449;
	j1236287 [label="Её брат был убит в сражении в чужих землях, но весть об этом ещё не дошла до дома, так что, когда угнетённым людям понадобился герой, она начала борьбу.",shape=box,color=blue];
		j1236287 -> j2215480408;
	j3613757941 [label="Попадаешь в ловушку волшебного королевства, где время течёт по-другому, призываешься туда, где это больше всего нужно…",shape=box,color=blue];
		j3613757941 -> j2196753736;
	j2215480408 [label="Check boolean EleanorHeroVirus",color=orange];
		j2215480408 -> j1516164440 [label=true,color=green];
		j2215480408 -> j3232039733 [label=false,color=brown];
	j2196753736 [label="Всё, что нам нужно, чтобы завершить историю, — это любовная интрига или слюнявое, бесчеловечное чудовище.",shape=box,color=blue];
		j2196753736 -> j2216486085;
	j1516164440 [label="Кажется, ты ненавидишь героев.",shape=box,color=red];
		j1516164440 -> j1179703830;
	j3232039733 [label="Теперь я не выношу героические фигуры. Они токсичны. Заразны, и по этой причине опасны.",shape=box,color=blue];
		j3232039733 -> j2500171515;
	j2216486085 [label="(Или слюнявая, бесчеловечная любовная интрига, в зависимости от ваших вкусов.)",shape=box,color=blue];
		j2216486085 -> j1756382627;
		j2216486085 -> j3300945979;
	j1179703830 [label="Ей можно. Заткнись.",shape=box,color=blue];
		j1179703830 -> j3627558732;
	j2500171515 [label="Локсли Янг другая.",shape=box,color=blue];
		j2500171515 -> j3947692357;
	j1756382627 [label="Мне всегда монстры казались интереснее, в любом случае. Герои скучные. Предсказуемые…Обыденные.",shape=box,color=red];
		j1756382627 -> j4013290146;
	j3300945979 [label="М-м-м. Не думаю, что монстры — это действительно мой тип.",shape=box,color=red];
		j3300945979 -> j1931563064;
	j3627558732 [label="Как я уже говорила. Она была очень похожа на своего брата, поэтому переоделась в его старую одежду, выдала себя за него и пошла возглавлять революцию. Если кто-то подходил слишком близко, мог заметить, что она девушка, поэтому она носила капюшон. Это стало её визитной карточкой.",shape=box,color=blue];
		j3627558732 -> j2917597936;
	j3947692357 [label="Boolean EleanorHeroVirus is now true.",color=orange];
		j3947692357 -> j2917597936;
	j4013290146 [label="Boolean EleanorFlirt is now true.",color=orange];
		j4013290146 -> j1932510322;
	j1931563064 [label="И это нормально, честно говоря.",shape=box,color=blue];
		j1931563064 -> j1489370857;
	j2917597936 [label="Она отлично владела луком и стрелами. Она и её команда стали известной бандой разбойников, грабивших королевских сборщиков налогов и раздавали деньги тем, кто в них больше нуждался.",shape=box,color=blue];
		j2917597936 -> j1352917714;
	j1932510322 [label="+15 Chemistry",color=orange];
		j1932510322 -> j577934128;
	j1489370857 [label="Монстры привыкли к одиночеству.",shape=box,color=blue];
		j1489370857 -> j1558375501;
	j1352917714 [label="Похоже, она бы заслужила уважение сама по себе. Зачем ей было притворяться своим братом?",shape=box,color=red];
		j1352917714 -> j2116030067;
	j577934128 [label="Чудовищность — она в глазах смотрящего...",shape=box,color=blue];
		j577934128 -> j25387714;
	j1558375501 [label="Мы - те, кто выжил.",shape=box,color=blue];
		j1558375501 -> "Chat ends.";
	j2116030067 [label="Вот такая история. В те времена считалось, что мужчины занимались войной, а значит, и правлением тоже. Люди привыкли подчиняться мужчинам. Ей нужно было использовать репутацию своего брата, чтобы сделать то, что она должна была. Или так, или погибнуть.",shape=box,color=blue];
		j2116030067 -> j167038383;
		j2116030067 -> j889286584;
		j2116030067 -> j1787031646;
	j25387714 [label="Если вас искренне тянет к качествам, которые вы считаете «чудовищными» в другом человеке, то вам лучше убедиться, что этот человек разделяет ваш выбор ярлыков и наслаждается этим. В противном случае вы совершаете грубейшую фетишизацию.",shape=box,color=blue];
		j25387714 -> j2076979344;
	j167038383 [label="Что с ней случилось в конце концов?",shape=box,color=red];
		j167038383 -> j3141168450;
	j889286584 [label="Хорошо, что мы исправили это в будущем.",shape=box,color=red];
		j889286584 -> j3034463237;
	j1787031646 [label="У меня нет уверенности, что мы *полностью* исправим это в будущем...",shape=box,color=red];
		j1787031646 -> j21823785;
	j2076979344 [label="Назвала бы я себя монстром, например? Да. С удовольствием. Я принимаю титул, которым меня награждают другие, и радуюсь этому. На случай, если тебе интересно.",shape=box,color=blue];
		j2076979344 -> "Chat ends.";
	j3141168450 [label="+20 Chemistry",color=orange];
		j3141168450 -> j1421482722;
	j3034463237 [label="+20 Chemistry",color=orange];
		j3034463237 -> j2559086039;
	j21823785 [label="+20 Chemistry",color=orange];
		j21823785 -> j2101313965;
	j1421482722 [label="Это зависит от того, какую версию истории ты читаешь. Либо её сразу вызвали в Зал Героев, либо она вышла замуж за своенравную дочь короля, либо её заточили в дубе, где она спит до сих пор, ожидая часа, когда она понадобится Британии.",shape=box,color=blue];
		j1421482722 -> j2173898719;
	j2559086039 [label="Я тоже так думаю.",shape=box,color=blue];
		j2559086039 -> j2969027735;
	j2101313965 [label="Нет, если верить персонажу Балласа, вы этого не сделали.",shape=box,color=blue];
		j2101313965 -> j2227971490;
	j2173898719 [label="Отсюда и поговорка у нас дома. Если дела идут плохо, как во время войны, принято пожимать плечами и говорить: «Ну, не может быть, чтобы всё было ТАК плохо. Локсли Янг ещё не объявилась».",shape=box,color=blue];
		j2173898719 -> "Chat ends.";
	j2969027735 [label="Боги, я бы отдала всё, чтобы увидеть твою эпоху своими глазами. Может, ты смогла бы пронести для меня одну из тех ледяных гробниц, и я просто бы спааааала в ней несколько тысячелетий.",shape=box,color=blue];
		j2969027735 -> "Chat ends.";
	j2227971490 [label="Сколько, чёрт побери, ещё нужно ждать?",shape=box,color=blue];
		j2227971490 -> "Chat ends.";
}