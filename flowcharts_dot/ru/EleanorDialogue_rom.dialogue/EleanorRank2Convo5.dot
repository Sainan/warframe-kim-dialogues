		j2990402986 -> j4269202954;
	j2057291539 [label="Я расследовала вспышку того, что называлось синдромом Тестудо. Тестудо — потому что это значит черепаха, и люди, у которых он проявлялся, обнаруживали на своей коже плотные пятна как ранний признак. То же самое, что в конечном итоге превращается в броню у нас, счастливчиков.",shape=box,color=blue];
