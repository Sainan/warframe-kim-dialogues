	j1640465027 [label="Не знаю. Может экзотический танец?",shape=box,color=blue];
		j1640465027 -> j843908070;
	j2001821707 [label="Провести вечер с Амиром, слушая, как он с воодушевлением рассказывает о какой-нибудь серии комиксов или ретро-игре, на которой он сейчас зациклен.",shape=box,color=blue];
