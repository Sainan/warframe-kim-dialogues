	j843908070 [label="Описание смерти Балласа во всех мелких деталях?",shape=box,color=blue];
		j843908070 -> j2544057473;
	j2717030916 [label="Окружу себя кристаллами и буду мысленно распевать мантры, пока Лэтти не вызовет экзорциста.",shape=box,color=blue];
		j2717030916 -> j1816034122;
	j2544057473 [label="Очень любезно с твоей стороны предложить это, но я думаю, что эту меланхолию придётся просто перетерпеть.",shape=box,color=blue];
		j2544057473 -> j1521509679;
	j1816034122 [label="Это совсем другая грусть. Она тянет меня вниз, как мокрое одеяло.",shape=box,color=blue];
		j1816034122 -> j3158661757;
		j1816034122 -> j2747318865;
	j1521509679 [label="Может быть, я буду рисовать.",shape=box,color=blue];
		j1521509679 -> j227430165;
	j3158661757 [label="1999 год в Хёльвании — это жёстко! Скальдра, Техрот, Бездна знает, что ещё. Будущее действительно выглядит настолько хуже?",shape=box,color=red];
		j3158661757 -> j3001805905;
	j2747318865 [label="Ой, да ладно... ты не из робкого десятка. Ты справишься. Здесь столько всего крутого.",shape=box,color=red];
		j2747318865 -> j1841820167;
	j227430165 [label="+5 Chemistry",color=orange];
		j227430165 -> "Chat ends.";
	j3001805905 [label="Слушай, я никогда особо не увлекалась научной фантастикой. У Артура был такой период, но он всегда больше любил суровые военные книги. Настоящие мужики-герои, правильные «истории для мальчиков», куча подтекста — ты, думаю, понимаешь, о чём я.",shape=box,color=blue];
		j3001805905 -> j1778296908;
	j1841820167 [label="Хорошо. Возьми меня за руку и проведи по улицам Хёльвании.",shape=box,color=blue];
		j1841820167 -> j1066858851;
	j1778296908 [label="Но я знала достаточно о научной фантастике, чтобы понимать: в далёком-далёком будущем мы должны были бы всё уладить. Всё должно быть блестящее и стерильное. Роботы выполняют всю тяжёлую работу, болезней нет, деньги никому не нужны, вокруг серебряные башни, голубые небеса и... сексуальные ящерицы.",shape=box,color=blue];
		j1778296908 -> j2076049788;
		j1778296908 -> j752818932;
	j1066858851 [label="Я знаю одно место, где можно взять пирожки из тёртой репы и посмотреть, как солнце садится над руинами школы.",shape=box,color=blue];
		j1066858851 -> j1931343542;
	j2076049788 [label="У нас есть роботы. Они выполняют большую часть тяжёлой работы.",shape=box,color=red];
		j2076049788 -> j1620463058;
	j752818932 [label="Но то, о чем ты говоришь, это просто вымысел, верно? Так почему ты ожидаешь, что реальность будет соответствовать ему?",shape=box,color=red];
		j752818932 -> j2951430995;
	j1931343542 [label="Позже мы сможем потягивать коктейли при свете горящего танка.",shape=box,color=blue];
		j1931343542 -> "Chat ends.";
	j1620463058 [label="Ну, ладно. За это ты получаешь очко.",shape=box,color=blue];
		j1620463058 -> j2060694290;
	j2951430995 [label="Фу. Я не… это сложно. Позвольте мне объяснить.",shape=box,color=blue];
		j2951430995 -> j4086185170;
	j2060694290 [label="Но облегчили ли эти роботы жизнь простым людям?",shape=box,color=blue];
		j2060694290 -> j3481864522;
	j4086185170 [label="Я не говорю, что ожидаю, что будущее будет похоже на детский комикс. Я просто имею в виду, что с ним был связан определённый оптимизм.",shape=box,color=blue];
		j4086185170 -> j2800226977;
	j3481864522 [label="Или они просто стали чем-то само собой разумеющимся?",shape=box,color=blue];
		j3481864522 -> j2971033471;
		j3481864522 -> j3701111782;
	j2800226977 [label="Я думаю, мы все просто принимали как должное, что каким бы дерьмовым ни было положение дел сейчас, мы в конце концов всё наладим. Наша цивилизация должна развиваться. Мы начали с того, что колотили друг друга дубинками по голове, а спустя несколько тысяч лет научились решать разногласия без этого.",shape=box,color=blue];
		j2800226977 -> j3322560297;
		j2800226977 -> j835902309;
	j2971033471 [label="Ты привыкла видеть во всем худшее, не правда ли?",shape=box,color=red];
		j2971033471 -> j3066249059;
	j3701111782 [label="Ладно. Мы не избавились от эксплуатации, просто создав роботов.",shape=box,color=red];
		j3701111782 -> j2388546311;
	j3322560297 [label="Люди остаются людьми, как бы ни менялись технологии.",shape=box,color=red];
		j3322560297 -> j2596896102;
	j835902309 [label="Извини, мы тебя подвели?",shape=box,color=red];
		j835902309 -> j1473535444;
	j3066249059 [label="Точно.",shape=box,color=blue];
		j3066249059 -> j3700747987;
	j2388546311 [label="Точно. Вот это и разочаровывает.",shape=box,color=blue];
		j2388546311 -> j1447436749;
	j2596896102 [label="Точно. Ты прямо в точку — именно это я нахожу таким удручающим во всей этой ситуации.",shape=box,color=blue];
		j2596896102 -> j3239118255;
	j1473535444 [label="Именно так.",shape=box,color=blue];
		j1473535444 -> j3239118255;
	j3700747987 [label="Так ты понимаешь, что нужно изменить.",shape=box,color=blue];
		j3700747987 -> "Chat ends.";
	j1447436749 [label="Вы продвинутые, но вы должны были стать ещё и ПРОСВЕТЛЁННЫМИ.",shape=box,color=blue];
		j1447436749 -> j1307524108;
	j3239118255 [label="У вас под рукой была вся эта невероятная, почти магическая технология, и всё, что вы с ней сделали, — нашли новые способы эксплуатации друг друга. Наука сделала нас менее равными, а не более.",shape=box,color=blue];
		j3239118255 -> j2879495797;
		j3239118255 -> j2323959370;
	j1307524108 [label="Спасибо, что терпишь меня. Я знаю, это много.",shape=box,color=blue];
		j1307524108 -> j4072819434;
	j2879495797 [label="Эм, напомню вежливо, что моего участия НИ В ЧЁМ из этого не было.",shape=box,color=red];
