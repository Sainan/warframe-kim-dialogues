	j2002832925 [label="+10 Chemistry",color=orange];
		j2002832925 -> j2447627955;
	j602253605 [label="Куинси и Артур пытались приготовить тюремный самогон из фруктовых пакетиков с рационом, но те из нас, у кого ещё оставалось хоть немного достоинства, отказались. Редкий момент солидарности с Лэтти, надо сказать.",shape=box,color=blue];
