		j2447627955 -> "Chat ends.";
	j2422326122 [label="Другая вещь, гораздо более значительная, — это будущее.",shape=box,color=blue];
		j2422326122 -> j1077397165;
	j1077397165 [label="Это не твоя вина, но... теперь, когда я увидела будущее твоими глазами, я чувствую себя так ужасно, что привычные способы справиться с этим больше не помогают.",shape=box,color=blue];
