		j3446263145 -> j2240937609;
	j2231056648 [label="+5 Chemistry",color=orange];
		j2231056648 -> j2760576817;
	j373781597 [label="Не совсем.",shape=box,color=blue];
		j373781597 -> j2240937609;
	j1643950009 [label="+5 Chemistry",color=orange];
		j1643950009 -> j3022911971;
	j2240937609 [label="Она — Смерть. Также одна из Сущностей Луа, в зависимости от того, какой традиции ты придерживаешься. Она та, кто приходит, укрывает тебя одеялом после шумного дня Жизни, целует на ночь и поёт колыбельную, чтобы ты уснула.",shape=box,color=blue];
		j2240937609 -> j907594912;
		j2240937609 -> j161437883;
	j2760576817 [label="Ха. Если говорить о нашей дорогой Лэтти, то я понимаю. Да.",shape=box,color=blue];
		j2760576817 -> j2240937609;
	j3022911971 [label="Вообще-то да.",shape=box,color=blue];
		j3022911971 -> j2240937609;
	j907594912 [label="Ты ведь не шутила, когда говорила о меланхолии?",shape=box,color=red];
		j907594912 -> j1512133985;
	j161437883 [label="Это будет один из тех разговоров в духе «устраивайся поудобнее, скиталец, это займёт какое-то время», не так ли?",shape=box,color=red];
		j161437883 -> j3948597853;
	j1512133985 [label="Понимаешь… Всё, что дал нам Энтрати, власть, способности, бронированные задницы… Я бы отдала всё это за крошку уверенности в том, что смерть — это не конец.",shape=box,color=blue];
		j1512133985 -> j1906609388;
	j3948597853 [label="...так и должно было быть.",shape=box,color=blue];
		j3948597853 -> j998413597;
	j1906609388 [label="Я пыталась. Мне не нравится это признавать, но иногда, когда мы патрулируем и происходит драка, я заглядываю в сознание умирающих и пытаюсь пройти через этот процесс вместе с ними. Если есть хоть какой-то свет в конце туннеля, я просто хочу его увидеть.",shape=box,color=blue];
		j1906609388 -> j1165664362;
		j1906609388 -> j4013708506;
	j998413597 [label="Забудь об этом. Я уверена, у тебя есть дела поважнее.",shape=box,color=blue];
		j998413597 -> "Chat ends.";
	j1165664362 [label="Элеонора, это так мрачно. У тебя нет на это права.",shape=box,color=red];
		j1165664362 -> j4035857516;
	j4013708506 [label="Пытаешься ли ты их хоть как-то утешить?",shape=box,color=red];
		j4013708506 -> j3587875490;
	j4035857516 [label="Я знаю. Я ЗНАЮ.",shape=box,color=blue];
		j4035857516 -> j955300104;
	j3587875490 [label="Да, однажды я это сделала. Какая разница, правда? Они всё равно умирают. Это была молодая солдатка Скальдры. Она шла на меня, и я выстрелила ей в грудь.",shape=box,color=blue];
		j3587875490 -> j2579843045;
	j955300104 [label="Я говорю себе, что пыталась помочь, но правда в том, что мне просто хотелось посмотреть. Ты права. Это мрачно.",shape=box,color=blue];
		j955300104 -> "Chat ends.";
	j2579843045 [label="Я опустилась на колени рядом с ней, пока она захлёбывалась собственной кровью, и попыталась сказать, что всё в порядке, боль скоро закончится, что вскоре она будет со своей семьёй, которая ушла раньше. Все эти спокойные, успокаивающие слова, которые приходят в голову, когда приходится проводить кого-то на смертном одре.",shape=box,color=blue];
		j2579843045 -> j3813921348;
	j3813921348 [label="Она была в ужасе. Остатки её сознания метались, как крыса в водяном баке. Когда она почувствовала, что я рядом, в её сознании, она попыталась зацепиться за меня. Она умоляла помочь. А под всем этим было... даже не знаю, как это назвать...",shape=box,color=blue];
		j3813921348 -> j888621007;
		j3813921348 -> j989176065;
		j3813921348 -> j414270704;
	j888621007 [label="Надежда?",shape=box,color=red];
		j888621007 -> j814817563;
	j989176065 [label="Страх?",shape=box,color=red];
		j989176065 -> j814817563;
	j414270704 [label="Отрицание?",shape=box,color=red];
		j414270704 -> j814817563;
	j814817563 [label="Отрицание. Она не верила, что может умереть. И почему она должна была это сделать? Она всегда была там раньше. Как смерть могла забрать её сейчас?",shape=box,color=blue];
		j814817563 -> j637765526;
	j637765526 [label="Каждый день своей жизни ты принимаешь СЕБЯ как должное. Ты есть, когда просыпаешься, и ты есть, когда засыпаешь. Ты проживаешь всю свою жизнь в доме, построенном из тебя самой. Ты никогда не выходишь наружу, потому что не можешь.",shape=box,color=blue];
		j637765526 -> j4290917214;
	j4290917214 [label="И вот однажды вы видишь, как рушится дом.",shape=box,color=blue];
		j4290917214 -> j67955407;
		j4290917214 -> j3354279525;
	j67955407 [label="Почему ты ей не помогла?",shape=box,color=red];
		j67955407 -> j2428278026;
	j3354279525 [label="Ты оставалась с ней до конца?",shape=box,color=red];
		j3354279525 -> j467378817;
	j2428278026 [label="Что мне надо было делать? Позвала Лэтти? Исцелила зияющую дыру в груди? Только потому, что я увидела приближение смерти так близко?",shape=box,color=blue];
		j2428278026 -> j626026553;
	j467378817 [label="Да. Я была чертовски напугана. Когда она почувствовала, как уходит, я была уверена, что она и меня потянет за собой. Я чувствовала это течение, тянущее меня.",shape=box,color=blue];
		j467378817 -> j1733557348;
	j626026553 [label="Со Скальдрой отношения такие — убить или быть убитой. Они не предлагают нам роскошь милосердия.",shape=box,color=blue];
		j626026553 -> "Chat ends.";
	j1733557348 [label="Но мне пришлось остаться. Я не могла бросить её умирать в одиночестве.",shape=box,color=blue];
		j1733557348 -> j1617024322;
	j1617024322 [label="Хотя, думаю, она так и сделала, в конце концов. Я не смогла сделать с ней этот последний шаг.",shape=box,color=blue];
		j1617024322 -> j1237757219;
		j1617024322 -> j1259211803;
	j1237757219 [label="Чему вы научилась из этого опыта?",shape=box,color=red];
		j1237757219 -> j2213926031;
	j1259211803 [label="Итак, ты увидела то, что хотела увидеть?",shape=box,color=red];
		j1259211803 -> j475512734;
	j2213926031 [label="Я ничему не научилась.",shape=box,color=blue];
		j2213926031 -> j2681801762;
	j475512734 [label="Нет.",shape=box,color=blue];
		j475512734 -> j2681801762;
	j2681801762 [label="Но она видела. Она видела лицо своей матери, детей, с которыми ходила в школу, свою кошку, какого-то утонувшего кузена. Но это были не присутствия или духи. Просто воспоминания, вытолкнутые на поверхность умирающим мозгом, отчаянно ищущим, за что бы зацепиться.",shape=box,color=blue];
		j2681801762 -> j3121588234;
	j3121588234 [label="Тогда дом её личности начал рушиться вокруг неё. Прежде чем она умерла, она широко открыла глаза. Я думаю, в тот момент, впервые в своей жизни, она действительно поняла, что такое смерть.",shape=box,color=blue];
		j3121588234 -> j3002038467;
		j3121588234 -> j4112417245;
	j3002038467 [label="Умирающий понимает, что такое смерть. Какое прозрение, Элеонора.",shape=box,color=red];
		j3002038467 -> j1513179635;
	j4112417245 [label="Что ты имеешь в виду? Все знают, что такое смерть.",shape=box,color=red];
		j4112417245 -> j1568852394;
	j1513179635 [label="Слушай. В следующий раз, когда почувствуешь необходимость остановить мой поток чепухи, не думай, что тебе нужно сначала это выслушать.",shape=box,color=blue];
		j1513179635 -> j3611861883;
	j1568852394 [label="Нет. Мы думаем, что знаем, что это такое, но весь наш опыт получен извне.",shape=box,color=blue];
		j1568852394 -> j1719442175;
	j3611861883 [label="Никто тебя не заставляет со мной разговаривать. Пока.",shape=box,color=blue];
		j3611861883 -> "Chat ends.";
	j1719442175 [label="Всю свою жизнь она была как женщина, живущая в доме без света, и всё, что у неё было, это фонарик. Она освещала светом своего сознания то одно, то другое место. Воспоминание, задача, взаимодействие.",shape=box,color=blue];
		j1719442175 -> j3648246263;
	j3648246263 [label="Она переходила из комнаты в комнату, исследовала подвалы во сне, но всегда это был замкнутый, конечный процесс. Понимаешь о чём я?",shape=box,color=blue];
		j3648246263 -> j4081124589;
		j3648246263 -> j1616368095;
	j4081124589 [label="Не совсем, но продолжай.",shape=box,color=red];
		j4081124589 -> j2376084053;
	j1616368095 [label="Да.",shape=box,color=red];
		j1616368095 -> j2376084053;
	j2376084053 [label="Затем, когда дом был разрушен, и её личность рассыпалась на куски вокруг неё, внезапно фонарик не светил на стену, книгу или диван. Он светил в бесконечное небо. Её осознание вспыхнуло в бесконечности без препятствия в виде неё самой, стоящей на пути.",shape=box,color=blue];
		j2376084053 -> j1830340820;
		j2376084053 -> j3836511065;
		j2376084053 -> j1512650425;
	j1830340820 [label="Но осознание умирает, когда умирает тело. То, что ты испытала, было всего лишь спазмом в её умирающем мозге.",shape=box,color=red];
		j1830340820 -> j891672977;
	j3836511065 [label="Ты думаешь, что «осознание» — это то, кем мы на самом деле являемся, а всё остальное — лишь оболочка?",shape=box,color=red];
		j3836511065 -> j1386948632;
	j1512650425 [label="Ого. Так глубоко. Ты кого-то застрелила, а потом ещё и нарушила её частную жизнь.",shape=box,color=red];
		j1512650425 -> j1513179635;
	j891672977 [label="Я много раз пытался убедить себя в этом.",shape=box,color=blue];
		j891672977 -> j2018558398;
	j1386948632 [label="+5 Chemistry",color=orange];
		j1386948632 -> j2924497154;
	j2018558398 [label="Я не могу поверить, что это было не что иное, как отключение мозга. Это было похоже на то, как будто душа вырвалась на свободу.",shape=box,color=blue];
		j2018558398 -> "Chat ends.";
	j2924497154 [label="Я знаю, что это не имеет никакого смысла. Это ощущается так нелогично — мысль о том, что мы сначала являемся точкой зрения, а уже потом личностью. Всё, что я знаю, это то, как это чувствовалось — быть рядом с человеческой душой, когда она переходила от жизни к смерти.",shape=box,color=blue];
		j2924497154 -> j485461049;
	j485461049 [label="Что ты обо всем этом думаешь?",shape=box,color=red];
		j485461049 -> j1795633483;
	j1795633483 [label="Утешение? Мне нравится мысль, что что-то из нас выживает. Я люблю думать, что смерть — это не исчезновение из вселенной, а уничтожение всего, что отделяет нас от вселенной.",shape=box,color=blue];
		j1795633483 -> j1068058878;
	j1068058878 [label="Но то, что делает нас НАМИ, — это не только чистое осознание. Это память, эмоции, мнение, всякие слои. Когда ты теряешь всё это, ты теряешь настоящего человека, даже если эта искорка как-то выживает.",shape=box,color=red];
		j1068058878 -> j3724638505;
	j3724638505 [label="Я знаю.",shape=box,color=blue];
		j3724638505 -> j2208714491;
	j2208714491 [label="Честно говоря, вот почему я хотела поговорить с тобой о привидениях Бездны, цефалонах и всех этих вещах из твоего времени. Мне нужно знать, будет ли у нас когда-нибудь шанс общаться с людьми, которых мы потеряли.",shape=box,color=blue];
		j2208714491 -> j1711032456;
	j1711032456 [label="Реальное общение, а не какая-то голографическая чушь. Такое, которое позволяет точно узнать, что думал человек.",shape=box,color=blue];
		j1711032456 -> j3951665145;
		j1711032456 -> j2222141808;
	j3951665145 [label="Нет, не совсем. Даже в будущем, люди, которых мы потеряли… к лучшему или к худшему. просто… ушли.",shape=box,color=red];
		j3951665145 -> j1929979069;
	j2222141808 [label="Элеонора, ближе к делу. Кого ты потеряла?",shape=box,color=red];
		j2222141808 -> j4028289786;
	j1929979069 [label="Понятно. Есть двери, которые мы никогда не сможем открыть. Может быть, это и к лучшему.",shape=box,color=blue];
		j1929979069 -> "Chat ends.";
	j4028289786 [label="Его звали Кристофер. Он был лучшим другом Артура.",shape=box,color=blue];
		j4028289786 -> j533947267;
	j533947267 [label="Мы были помолвлены. Я думала, что влюблена в него.",shape=box,color=blue];
		j533947267 -> j3583038178;
	j3583038178 [label="Boolean EleanorChristopher1 is now true.",color=orange];
		j3583038178 -> j4008898742;
	j4008898742 [label="+10 Chemistry",color=orange];
		j4008898742 -> j1220876272;
	j1220876272 [label="Извини. Я не могу этого сделать. Я поговорю с тобой в другой раз.",shape=box,color=blue];
		j1220876272 -> "Chat ends.";
}