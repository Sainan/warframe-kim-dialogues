	j1980503073 [label="И когда он вонзает лезвие мне между рёбер, я кричу: «Элеонора ПРЯМО ПЕРЕД ТОБОЙ, тупой ублюдок!», но он меня больше не слышит.",shape=box,color=blue];
		j1980503073 -> j4048966758;
	j3039860407 [label="Я реально зациклилась на этом. Мне снились кошмары, где я просыпалась на своём диване, а Артур стоял надо мной с мечом в руке и слезами на глазах. Он сквозь зубы говорил что-то вроде: «Элеонора была моей сестрой, но теперь Элеоноры больше нет, и я должен это сделать, даже если это убивает меня», — бу-гу-гу.",shape=box,color=blue];
