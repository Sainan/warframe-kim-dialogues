	j4150630115 [label="Конечно.",shape=box,color=red];
		j4150630115 -> j3819237296;
	j2068900743 [label="Никогда.",shape=box,color=red];
		j2068900743 -> j4183856265;
	j3387371374 [label="Я не буду отвечать на этот вопрос.",shape=box,color=red];
		j3387371374 -> j385103787;
	j3652232845 [label="Хм. Неплохое начало. Ты переводишь разговор на то, чего хочу я, и не используешь мою жизнь для демонстрации своих моральных принципов. Ты даёшь мне понять, что дело здесь не в тебе.",shape=box,color=blue];
		j3652232845 -> j3077503260;
	j3819237296 [label="Ого. Ладно.",shape=box,color=blue];
		j3819237296 -> j551349545;
	j4183856265 [label="Ладно. Эм.",shape=box,color=blue];
		j4183856265 -> j3601935863;
	j385103787 [label="Как хочешь.",shape=box,color=blue];
		j385103787 -> j1987909028;
	j3077503260 [label="Это редкость.",shape=box,color=blue];
		j3077503260 -> j4052063318;
	j551349545 [label="Никаких колебаний, никакой борьбы, простое «да».",shape=box,color=blue];
		j551349545 -> j584290441;
	j3601935863 [label="Извини. Я немного ошеломлена. Не ожидала, что ты скажешь это так открыто.",shape=box,color=blue];
		j3601935863 -> j2729596613;
	j1987909028 [label="Я больше не буду спрашивать.",shape=box,color=blue];
		j1987909028 -> j4195730623;
	j4052063318 [label="К сожалению, это не решает нашу дилемму, потому что значит, что я никогда не смогу передумать.",shape=box,color=blue];
		j4052063318 -> j3889931609;
	j584290441 [label="Спасибо. Ты для меня во многом загадка, но теперь я знаю, что ты можешь пустить пулю мне в голову, а такие вещи важны на рабочем месте.",shape=box,color=blue];
		j584290441 -> j517294908;
	j2729596613 [label="Ты сможешь позволить монстру жить?",shape=box,color=blue];
		j2729596613 -> j4026973447;
	j4195730623 [label="Полагаю, нам придётся дождаться момента, когда это случится, чтобы увидеть, как ты отреагируешь, верно?",shape=box,color=blue];
		j4195730623 -> j1891221386;
	j3889931609 [label="Допустим, я скажу «да», я бы хотела, чтобы ты покончил со мной быстро и чисто, а не позволил мне жить дальше как безумное чудовище. Допустим, это то, чего Элеонора хочет СЕЙЧАС.",shape=box,color=blue];
		j3889931609 -> j2011467393;
	j517294908 [label="Иииии я только что почувствовала, как Техрот в моих клетках тревожно задрожал.",shape=box,color=blue];
		j517294908 -> j1712341488;
	j4026973447 [label="Извини, извини, просто я живу каждый день, слушая, как Лэтти твердит мне в ухо, что я не заслуживаю быть здесь, а Артур смотрит на меня, как на безнадёжный случай, считая дни до того момента, когда ему придётся это сделать. Я просто не ожидала, что ты так скажешь, вот и всё.",shape=box,color=blue];
		j4026973447 -> j538242623;
	j1891221386 [label="Ты — смертельная маленькая загадка.",shape=box,color=blue];
		j1891221386 -> j1044824984;
	j2011467393 [label="А что если это безумное чудовище — это всё ещё я, только в большей степени? Что если, когда моя человечность сгорит в горниле Техрота, я больше НЕ захочу умирать?",shape=box,color=blue];
		j2011467393 -> j2794245231;
	j1712341488 [label="Думаю, теперь он будет вести себя немного лучше. Ничто так не наводит страх на паразита, как угроза его хозяину, верно?",shape=box,color=blue];
		j1712341488 -> j2864742878;
	j538242623 [label="+20 Chemistry",color=orange];
		j538242623 -> j386301673;
	j1044824984 [label="И я тоже.",shape=box,color=blue];
		j1044824984 -> j2864742878;
