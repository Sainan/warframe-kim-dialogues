	j4114757405 [label="Хорошо. Значит, мы приносим счастливую, сильную Элеонору в жертву Элеоноре, какой она была раньше?",shape=box,color=blue];
		j4114757405 -> j623778670;
	j1823728976 [label="Ах, то есть всё, что мне нужно, — это хлопнуть своими большими красивыми глазами, и вся твоя решимость рухнет?",shape=box,color=blue];
		j1823728976 -> j2638539617;
	j704941950 [label="Ах, понятно. В этом сценарии я — бесчеловечное чудовище, которое, предположительно, убивает людей налево и направо. Ты можешь отнести меня к категории «хищных животных», вроде бешеных собак и голодных медведей, а это значит, что никаких моральных дилемм не возникает.",shape=box,color=blue];
		j704941950 -> j1855269965;
	j1883155114 [label="… Не знаю. В некоторые дни мне так кажется. Я чувствую себя, как типичный персонаж из мелодрамы.",shape=box,color=blue];
		j1883155114 -> j409919811;
	j623778670 [label="Я могу изменить своё мнение и решить жить, но ты не можешь изменить своё?",shape=box,color=blue];
