		j3744845632 -> j1179370094;
	j1055873958 [label="Спасибо. Ты для меня во многом загадка, но теперь я знаю, что ты можешь пустить пулю мне в голову, а такие вещи важны на рабочем месте.",shape=box,color=blue];
