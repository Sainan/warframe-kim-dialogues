digraph {
	"EleanorRank4Convo1 begins." [color=orange];
		"EleanorRank4Convo1 begins." -> j148263926;
	j148263926 [label="Я должна тебя кое о чём спросить, и, думаю, тебе это не понравится. Кажется, ты где-то в глубине души знаешь, что этот разговор однажды придётся завести, учитывая моё состояние.",shape=box,color=blue];
		j148263926 -> j3258346910;
	j3258346910 [label="Я доверяю тебе, и ты мне нравишься, поэтому я просто выскажусь, ладно?",shape=box,color=blue];
		j3258346910 -> j1733878844;
	j1733878844 [label="На самом деле я не имею права спрашивать, но мне нужно знать.",shape=box,color=blue];
		j1733878844 -> j4145840813;
	j4145840813 [label="Если бы я полностью изменилась — если бы техрот поглотил меня, и я больше не была той Элеонорой, которую ты знаешь, — ты убьёшь меня?",shape=box,color=blue];
		j4145840813 -> j1398559663;
		j4145840813 -> j4150630115;
		j4145840813 -> j2068900743;
		j4145840813 -> j3387371374;
	j1398559663 [label="Ты бы этого хотела?",shape=box,color=red];
