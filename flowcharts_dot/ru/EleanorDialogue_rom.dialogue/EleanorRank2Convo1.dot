	j1879041333 [label="Ты так больна, как, по-видимому, думает Лэтти?",shape=box,color=red];
		j1879041333 -> j241490794;
	j4065451762 [label="Как у тебя дела с личным заражением?",shape=box,color=red];
		j4065451762 -> j269820530;
	j3879883449 [label="Что происходит между тобой и Лэтти?",shape=box,color=red];
		j3879883449 -> j2369566595;
	j241490794 [label="Давай проясним. Лэтти не думает, что я больна. Лэтти думает, что я демон.",shape=box,color=blue];
		j241490794 -> j989082302;
	j269820530 [label="В этом нет ничего неприятного, чего нельзя сказать о большинстве случаев заражения паразитами.",shape=box,color=blue];
		j269820530 -> j4187409459;
	j2369566595 [label="Ты знаешь, как она меня называет?",shape=box,color=blue];
		j2369566595 -> j1195267114;
		j2369566595 -> j1319440389;
	j989082302 [label="Интересно, каково это — иметь столь романтический взгляд на мир. Казалось бы, медик должен заниматься измеримыми фактами. Десять кубиков этого, идентификация микробов того.",shape=box,color=blue];
		j989082302 -> j2127614630;
		j989082302 -> j211662664;
	j4187409459 [label="Я чувствую Техноциты, плавающие в моей крови. Если я закрою глаза, я могу видеть искорки, как сказочную пыль.",shape=box,color=blue];
		j4187409459 -> j2338845791;
	j1195267114 [label="Честно говоря, нет.",shape=box,color=red];
		j1195267114 -> j2763387599;
	j1319440389 [label="«La Bruja Metiche». Назойливая ведьма.",shape=box,color=red];
		j1319440389 -> j1379693909;
	j2127614630 [label="Очень научно с твоей стороны. Я думала, ты веришь в более духовные вещи?",shape=box,color=red];
		j2127614630 -> j2928526237;
	j211662664 [label="Звучит так, будто ты считаешь, что часть о «потере человечности» преувеличена.",shape=box,color=red];
		j211662664 -> j2697612006;
	j2338845791 [label="Она желает мне добра. Ты можешь это понять?",shape=box,color=blue];
		j2338845791 -> j2709956590;
	j2763387599 [label="«La Bruja Metiche». Назойливая ведьма. Думаю, она могла бы придумать что-то похуже, учитывая её словарный запас и склонность к творческим ругательствам.",shape=box,color=blue];
		j2763387599 -> j1630192662;
	j1379693909 [label="Ага. Она сидит там, ухмыляется с её флаконами и зельями, бутылками крови и кусочками кожи, армией облезлых фамильяров, шныряющих повсюду, и имеет наглость называть ВЕДЬМОЙ меня.",shape=box,color=blue];
		j1379693909 -> j2248545727;
		j1379693909 -> j1767211558;
		j1379693909 -> j3641576054;
	j2928526237 [label="Ну, да. Живя так, как мы, как можно не верить в духовный мир?",shape=box,color=blue];
		j2928526237 -> j1554112057;
	j2697612006 [label="Я всё ещё Элеонора Найтингейл. Для меня это важнее, чем быть «человечной».",shape=box,color=blue];
		j2697612006 -> j2355917953;
	j2709956590 [label="Всё, что оно делает, оно делает из искреннего желания исправить меня и сделать лучше. Если я быстрая, смертоносная и трудная для убийства, я буду хорошим носителем. Поэтому оно выполняет свою часть нашего симбиотического соглашения, давая мне кожу акулы и клыки волка, вместе с языком, как кожаный кнут.",shape=box,color=blue];
		j2709956590 -> j2127614630;
		j2709956590 -> j211662664;
	j1630192662 [label="Я слышала, как она говорила Куинси сделать кое-что довольно изобретательное с игровыми конструкциями и частями его тела, и я честно некоторое время хихикала, представляя это.",shape=box,color=blue];
		j1630192662 -> j2511955167;
	j2248545727 [label="Я понимаю, вы не нравитесь друг другу.",shape=box,color=red];
		j2248545727 -> j147747079;
	j1767211558 [label="Это всегда было так между вами?",shape=box,color=red];
		j1767211558 -> j643403730;
	j3641576054 [label="Она считает, что ты потеряешь контроль.",shape=box,color=red];
		j3641576054 -> j3754121377;
	j1554112057 [label="Check boolean EleanorVoidLove",color=orange];
		j1554112057 -> j2473068781 [label=true,color=green];
		j1554112057 -> j32566653 [label=false,color=brown];
	j2355917953 [label="Я видел, на что способны люди. Мы не являемся чем-то особенным. Судя по тому, что я видел в твоей голове, со временем нам станет ещё хуже.",shape=box,color=blue];
		j2355917953 -> j2257438252;
	j2511955167 [label="Она сидит там, ухмыляется с её флаконами и зельями, бутылками крови и кусочками кожи, армией облезлых фамильяров, шныряющих повсюду, и имеет наглость называть ВЕДЬМОЙ меня.",shape=box,color=blue];
		j2511955167 -> j2248545727;
		j2511955167 -> j1767211558;
		j2511955167 -> j3641576054;
	j147747079 [label="Не пойми меня неправильно. Я БЫ обмочилась на неё, если бы она горела.",shape=box,color=blue];
		j147747079 -> j1600793895;
	j643403730 [label="Думаю, ты можешь догадаться, каков будет ответ.",shape=box,color=blue];
		j643403730 -> "Chat ends.";
	j3754121377 [label="Я знаю. Иногда у меня тоже так бывает. Мне снится повторяющийся кошмар, где я разрываю ей горло, а она в этот момент стреляет мне в сердце.",shape=box,color=blue];
		j3754121377 -> j1319581378;
	j2473068781 [label="Бездна — это убедительное доказательство того, что духовное так же реально, как и материальное. Всё, что ваши люди сделали, кроме как назвали это по-другому, — это разработали надёжный способ туда попасть и вернуться обратно.",shape=box,color=blue];
		j2473068781 -> j32566653;
	j32566653 [label="Слушай. То, что я признаю существование демонических сил, не значит, что я согласна, чтобы меня записали в их ряды.",shape=box,color=blue];
		j32566653 -> j2630893323;
	j2257438252 [label="Check boolean EleanorEntrati",color=orange];
		j2257438252 -> j1187821441 [label=true,color=green];
		j2257438252 -> j2742156564 [label=true,color=green];
