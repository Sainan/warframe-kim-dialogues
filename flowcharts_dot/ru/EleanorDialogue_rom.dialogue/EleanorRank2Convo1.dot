		j2257438252 -> j2742156564 [label=false,color=brown];
		j2257438252 -> j3078972947 [label=false,color=brown];
	j1600793895 [label="Но я бы не торопился с этим.",shape=box,color=blue];
		j1600793895 -> "Chat ends.";
	"Chat ends." [color=orange];
	j1319581378 [label="Но бояться чего-то — не значит, что это реально. До сих пор я не устроила НИ ОДНОЙ кровавой бойни. НИ ОДНОЙ.",shape=box,color=blue];
		j1319581378 -> j1557278168;
	j2630893323 [label="То, что происходит в моей системе, — это результат весьма обыденных атомов и молекул, танцующих вместе неожиданными и удивительными способами. Любое духовное значение этому придам Я. А не Лэтти.",shape=box,color=blue];
		j2630893323 -> j360246200;
		j2630893323 -> j2681035185;
		j2630893323 -> j1109184965;
	j1187821441 [label="Ты хочешь просто сдаться? Ты звучишь как Сын.",shape=box,color=red];
		j1187821441 -> j1820926181;
	j2742156564 [label="Если бы ты следовала его замыслу, ты бы вообще это поняла?",shape=box,color=red];
		j2742156564 -> j1131154811;
	j3078972947 [label="По крайней мере, человеческий геном естественен.",shape=box,color=red];
		j3078972947 -> j3851346378;
	j1557278168 [label="Амир причинил больше чертовых травм, чем я.",shape=box,color=blue];
		j1557278168 -> j106320131;
	j360246200 [label="Я понимаю, вы друг друга недолюбливаете.",shape=box,color=red];
		j360246200 -> j2943816861;
	j2681035185 [label="Так всегда было между вами?",shape=box,color=red];
		j2681035185 -> j643403730;
	j1109184965 [label="Она думает, ты потеряешь контроль.",shape=box,color=red];
		j1109184965 -> j356932524;
	j1820926181 [label="Я такого не говорила.",shape=box,color=blue];
		j1820926181 -> j312454336;
	j1131154811 [label="В отличие от следования замыслу хитроумной двойной спирали нуклеотидов? Ты знаешь, то, чем люди занимались с тех пор, как встали на две ноги?",shape=box,color=blue];
		j1131154811 -> j75328039;
	j3851346378 [label="Всякие типы вроде Виктора Водяного — и что это за псевдоним такой, чтобы выбирать для себя — оправдывают свои устаревшие взгляды, утверждая, что они «естественны». Биологический фундаментализм. Человек как отражение Божественного и всё такое.",shape=box,color=blue];
		j3851346378 -> j2495505918;
	j106320131 [label="Но Амир милый и немного простоватый, не так ли? У Амира нет зубов, способных разгрызть консервную банку. Поэтому никто не видит в Амире бомбу с часовым механизмом.",shape=box,color=blue];
		j106320131 -> j3940389659;
	j2943816861 [label="Не пойми меня неправильно. Я БЫ обмочилась на неё, если бы она горела.",shape=box,color=blue];
		j2943816861 -> j476137831;
	j356932524 [label="+10 Chemistry",color=orange];
		j356932524 -> j3754121377;
	j312454336 [label="Некоторые люди считают человечество священным. Такие, как Виктор. Я для него — высшее кощунство. Заражённая, бесстыжая и женщина.",shape=box,color=blue];
		j312454336 -> j2116943627;
		j312454336 -> j2752672223;
	j75328039 [label="«Я не подчиняюсь микроскопическому тирану! Я человек. Я особенный! Найти пару. Размножиться. Защитить территорию. Уфф.»",shape=box,color=blue];
		j75328039 -> j2116943627;
		j75328039 -> j2752672223;
	j2495505918 [label="Но мы стали неестественными с того самого момента, как начали бить камни друг о друга. Язык — это вирус, дружище. Быть человеком — это чистый лист, на котором мы можем писать, а не первобытный Эдем, из которого нас изгнали.",shape=box,color=blue];
		j2495505918 -> j2116943627;
		j2495505918 -> j2752672223;
	j3940389659 [label="Ну, кроме Куинси.",shape=box,color=blue];
		j3940389659 -> "Chat ends.";
	j476137831 [label="Но я бы не стала торопиться с этим.",shape=box,color=blue];
		j476137831 -> "Chat ends.";
	j2116943627 [label="Техрот мог бы захватить твой разум, и ты бы даже этого не поняла.",shape=box,color=red];
		j2116943627 -> j4234099271;
	j2752672223 [label="Я видела, как он поглощал целые ЛУНЫ, Элеонора!",shape=box,color=red];
		j2752672223 -> j1539756600;
	j4234099271 [label="Я имею в виду... да. Ты права. Есть паразиты, которые так делают.",shape=box,color=blue];
		j4234099271 -> j4014493254;
	j1539756600 [label="И что такого чудесного в непоглощённой луне? Почему версия с Заражением хуже?",shape=box,color=blue];
		j1539756600 -> j2590811908;
	j4014493254 [label="Но что, если это не паразит? Что, если это симбиоз?",shape=box,color=blue];
		j4014493254 -> j3713569001;
		j4014493254 -> j3488290019;
	j2590811908 [label="О нет! Кусок безразличной космической породы, который мог бы ещё несколько тысячелетий дрейфовать без цели, вдруг становится живым и интересным! Какая катастрофа!",shape=box,color=blue];
		j2590811908 -> j2467295695;
	j3713569001 [label="Ты не знаешь, о чём говоришь. Когда кто-то теряется в Заражении, он ПОТЕРЯН. Полностью неузнаваем.",shape=box,color=red];
		j3713569001 -> j215736345;
	j3488290019 [label="Я понимаю, тебе нужно попытаться принять то, чем ты стала. Ты сталкиваешься с невозможной задачей.",shape=box,color=red];
		j3488290019 -> j1458542865;
	j2467295695 [label="Это точно такая же чушь, что несут последователи Виктора. Сентиментальная привязанность к тому, чем что-то БЫЛО раньше.",shape=box,color=blue];
		j2467295695 -> j1268528329;
	j215736345 [label="Я не знаю, пытаешься ли ты меня спровоцировать, но мне приходилось сражаться за свою жизнь против этих созданий. Иногда ты можешь различить части, которые раньше были человеческими. Головы и ноги, просто торчащие из массы.",shape=box,color=red];
		j215736345 -> j2814005489;
	j1458542865 [label="Или ненавидь то, чем ты стала, или прими это. И что-то мне подсказывает, что ты давно научилась переставать ненавидеть себя.",shape=box,color=red];
		j1458542865 -> j1097299443;
	j1268528329 [label="Как будто быть первобытным — это то же самое, что быть совершенным.",shape=box,color=blue];
		j1268528329 -> j3713569001;
		j1268528329 -> j3488290019;
	j2814005489 [label="Это не симбиоз. Ему ПЛЕВАТЬ, чего ты хочешь. Он здесь не для того, чтобы помочь. Это агрессивный рак жизни, который хочет только превратить всё остальное в самого себя.",shape=box,color=red];
		j2814005489 -> j3539438524;
	j1097299443 [label="Но эти демоны в твоей крови НЕ твои друзья. Всё, что ты можешь сделать, — это держать их на коротком поводке, Элеонора. Поверь мне.",shape=box,color=red];
		j1097299443 -> j3734925379;
	j3539438524 [label="Напомни мне, из чего снова сделаны ваши варфреймы?",shape=box,color=blue];
		j3539438524 -> j2553840047;
	j3734925379 [label="Ты думаешь, я не знаю?",shape=box,color=blue];
		j3734925379 -> j3980622228;
	j2553840047 [label="Заражённые люди.",shape=box,color=red];
		j2553840047 -> j1193516044;
	j3980622228 [label="Ты вообще представляешь, чего стоит сдерживать ярость? Ты знаешь, какая цена этой внешней невозмутимости?",shape=box,color=blue];
		j3980622228 -> j2859969065;
	j1193516044 [label="Но их Заражение было сформировано и контролировалось, не так ли?",shape=box,color=blue];
		j1193516044 -> j1138394343;
	j2859969065 [label="Если ты видишь, как я медитирую, я не общаюсь с великим космическим капустным духом. Я остужаю чёртов реактор.",shape=box,color=blue];
		j2859969065 -> j3724376867;
	j1138394343 [label="Прямо как Заражение, которое сейчас во мне.",shape=box,color=blue];
		j1138394343 -> j62878454;
	j3724376867 [label="Есть СТОЛЬКО вещей, которые я могла бы сделать, но НЕ ДЕЛАЮ. Места, куда я мог бы это отнести... вещи... о которых ты даже не знаешь.",shape=box,color=blue];
		j3724376867 -> j662886240;
	j62878454 [label="Это значит, что кто-то, где-то набрался смелости перестать считать Заражение безмозглым монстром и начал смотреть на то, чем оно могло бы стать.",shape=box,color=blue];
		j62878454 -> j387999433;
		j62878454 -> j298437099;
	j662886240 [label="Я могла бы проникнуть в их СНЫ. И они даже не поняли бы, что я там была.",shape=box,color=blue];
		j662886240 -> j3923513676;
	j387999433 [label="О, ты НЕ только что описала Баллас как «имеющих смелость».",shape=box,color=red];
		j387999433 -> j4214390407;
	j298437099 [label="Так… ты думаешь, ты становишься больше собой, а не меньше?",shape=box,color=red];
		j298437099 -> j1038569208;
	j3923513676 [label="+20 Chemistry",color=orange];
		j3923513676 -> j2820340288;
	j4214390407 [label="+10 Chemistry",color=orange];
		j4214390407 -> j1331908934;
	j1038569208 [label="+20 Chemistry",color=orange];
		j1038569208 -> j1483962313;
	j2820340288 [label="Ну, это был интересный, хоть и немного грубоватый обмен мнениями. Я прекрасно провела время. Давай повторим.",shape=box,color=blue];
		j2820340288 -> "Chat ends.";
	j1331908934 [label="Знаешь, ты начинаешь слишком эмоционально реагировать. Нам стоит закончить этот разговор в другой раз.",shape=box,color=blue];
		j1331908934 -> "Chat ends.";
	j1483962313 [label="В прошлый раз, когда меня накрыло биохимическим молотом, который оставил меня растянутой на диване на полдня, а люди шептались за моей спиной, что я превращаюсь в монстра, это называлось \"пубертат\".",shape=box,color=blue];
