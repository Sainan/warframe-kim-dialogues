		j3120566514 -> j1832810552;
	j160890126 [label="Да, то, что наши пути пересеклись, было редким благословением в моей жизни.",shape=box,color=blue];
		j160890126 -> j2697574152;
	j2829827228 [label="В общем, лето за летом пролетали. Мы становились подростками. Всё было очень неловко, воняло, и гормоны шалили. Были вечеринки и экзамены. Кристофер уехал в колледж. Собака умерла, завели новую. Вставь монтаж здесь.",shape=box,color=blue];
		j2829827228 -> j2682657082;
	j1832810552 [label="Но я хотела рассказать тебе о тяжёлой утрате.",shape=box,color=blue];
