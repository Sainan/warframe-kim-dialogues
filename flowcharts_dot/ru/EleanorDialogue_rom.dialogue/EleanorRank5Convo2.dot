		j2267352566 -> j1163685787;
	j4029688813 [label="Да. Я ненавижу, что оно сделало с Кристофером, что оно до сих пор делает с Артуром, что оно делает с такими, как они.",shape=box,color=blue];
		j4029688813 -> j1344285856;
