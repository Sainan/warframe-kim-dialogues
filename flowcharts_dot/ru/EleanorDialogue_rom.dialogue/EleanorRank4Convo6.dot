digraph {
	"EleanorRank4Convo6 begins." [color=orange];
		"EleanorRank4Convo6 begins." -> j2124580806;
		"EleanorRank4Convo6 begins." -> j906594870;
	j2124580806 [label="Итак… почему кристаллы?",shape=box,color=red];
		j2124580806 -> j2992443001;
	j906594870 [label="Итак. Ты и Куинси. Вы когда-нибудь… ну, понимаешь?",shape=box,color=red];
		j906594870 -> j1755381163;
	j2992443001 [label="О, я всегда их обожала. Когда я была маленькой, папа отвёл нас в Музей инноваций в Людстоне, и мы с Артуром получили наборы для выращивания кристаллов. У Артура получилось нечто ужасное — буквально комковатая каменная какашка. Кристалл сразу выкинули. А у меня вырос прекрасный голубой кристалл, как алмаз. С того дня я как-то почувствовала, что кристаллы на моей стороне.",shape=box,color=blue];
		j2992443001 -> j1701819741;
	j1755381163 [label="Ладно, сегодня я чувствую себя доброй, поэтому дам тебе это бесплатно.",shape=box,color=blue];
		j1755381163 -> j1157271921;
	j1701819741 [label="Так они просто для украшения?",shape=box,color=red];
		j1701819741 -> j2016487058;
	j1157271921 [label="Мы никогда. Даже не чуть-чуть.",shape=box,color=blue];
		j1157271921 -> j174881802;
	j2016487058 [label="А. Нет.",shape=box,color=blue];
		j2016487058 -> j3706625272;
	j174881802 [label="Не могу избавиться от мысли, что любимая позиция Куинси была бы «партнёр сверху с зеркалом, чтобы он мог представить, что занимается сексом с самим собой».",shape=box,color=blue];
		j174881802 -> j3383241418;
	j3706625272 [label="Я знаю, что о кристаллах написано много ерунды. Не моё. Я не верю, что они очищают мою ауру или освещают мои чакры.",shape=box,color=blue];
		j3706625272 -> j871481112;
	j3383241418 [label="Нет, спасибо. Мне достаточно любоваться им издалека.",shape=box,color=blue];
		j3383241418 -> "Chat ends.";
	j871481112 [label="Но у них есть сущность.",shape=box,color=blue];
		j871481112 -> j1233681502;
	"Chat ends." [color=orange];
	j1233681502 [label="Извини. Я знаю, что звучу как полная идиотка.",shape=box,color=blue];
		j1233681502 -> j3544414592;
		j1233681502 -> j2794004595;
		j1233681502 -> j3713331295;
	j3544414592 [label="Хорошо, что ты сказала это, а не я. Это просто красивые камешки.",shape=box,color=red];
		j3544414592 -> j2352665548;
	j2794004595 [label="У меня есть друзья, которые являются Цефалонами. Они буквально представляют собой разум в форме стеклянных кубов. С чего ты взяла, что я буду смеяться над тобой за то, что ты думаешь, будто у кристаллов есть некая сущность?",shape=box,color=red];
		j2794004595 -> j1707138825;
	j3713331295 [label="Я провожу слишком много времени, выкапывая кристаллы из земли, чтобы они не имели какого-то более глубокого значения. Ты бы удивилась, сколько варфреймов были созданы частично благодаря кристаллам одного или нескольких видов.",shape=box,color=red];
		j3713331295 -> j1556435932;
	j2352665548 [label="О, иногда ты просто СВОДИШЬ С УМА.",shape=box,color=blue];
		j2352665548 -> j4128204480;
	j1707138825 [label="Ну вот, теперь я чувствую себя глупо за то, что почувствовала себя глупо.",shape=box,color=blue];
		j1707138825 -> j3193333793;
	j1556435932 [label="Удивительно и интригующе!",shape=box,color=blue];
		j1556435932 -> j158796688;
	j4128204480 [label="Я бы подумала, что ты, из всех людей, можешь поднять свой взгляд выше сугубо материального.",shape=box,color=blue];
		j4128204480 -> j3023688315;
	j3193333793 [label="Думаю, у вас есть кристаллы, о которых я могу только мечтать.",shape=box,color=blue];
		j3193333793 -> j1199314418;
		j3193333793 -> j2530923122;
		j3193333793 -> j2639832533;
	j158796688 [label="Расскажи мне о кристаллах твоего времени.",shape=box,color=blue];
		j158796688 -> j2530923122;
	j3023688315 [label="Уйди прочь. Оставь меня моим причудливым камням.",shape=box,color=blue];
		j3023688315 -> "Chat ends.";
	j1199314418 [label="Аргон. Его можно найти только в Бездн, и он распадается, как только его достаёшь.",shape=box,color=red];
		j1199314418 -> j1971050371;
	j2530923122 [label="Есть целый варфрейм, сделанный из кристалла. Цитрина.",shape=box,color=red];
		j2530923122 -> j422953270;
	j2639832533 [label="У нас есть пара обречённых влюблённых, превращённых в кристалл.",shape=box,color=red];
		j2639832533 -> j2862105663;
	j1971050371 [label="Ты говоришь: «Его можно найти только в Бездне», как будто люди постоянно туда-сюда из Бездны шастают. Я думала, это безумное измерение, а не курорт у моря.",shape=box,color=blue];
		j1971050371 -> j3633105052;
	j422953270 [label="+10 Chemistry",color=orange];
		j422953270 -> j1168419898;
	j2862105663 [label="+10 Chemistry",color=orange];
		j2862105663 -> j2312833331;
	j3633105052 [label="Думаю, ты ещё не знаешь о Башнях Орокин. Это как крепости в Бездне, возведённые на случай, если Владеющие Разумом попытаются напасть.",shape=box,color=red];
		j3633105052 -> j1404673758;
	j1168419898 [label="Это её имя? Мне кажется, я видела её в твоём воображении!",shape=box,color=blue];
		j1168419898 -> j150023803;
	j2312833331 [label="Как красиво и грустно. Каково это — стать памятником своей любви?",shape=box,color=blue];
		j2312833331 -> j2988047675;
	j1404673758 [label="+20 Chemistry",color=orange];
		j1404673758 -> j2158175954;
	j150023803 [label="Красивая дама-жеода. Полагаю, теперь мы с ней родственницы. Утешительная мысль.",shape=box,color=blue];
		j150023803 -> j2317490178;
	j2988047675 [label="Надеюсь, никто не отломит от них кусочки на память. В моё время это бы точно произошло.",shape=box,color=blue];
		j2988047675 -> j1276616055;
	j2158175954 [label="Это знакомо. Под Людстоном и Манкунией есть сверхсекретные бункеры. Идея в том, что если когда-нибудь начнётся Радиационная Война, толстосумы и шишки смогут спрятаться в своих роскошных убежищах и потягивать коктейли, пока нас всех наверху превращают в пепел.",shape=box,color=blue];
		j2158175954 -> j2808077110;
	j2317490178 [label="Добро пожаловать в семью, я полагаю?",shape=box,color=red];
		j2317490178 -> "Chat ends.";
	j1276616055 [label="Люди пытаются, но мы их бережём. Это меньшее, что мы можем сделать.",shape=box,color=red];
		j1276616055 -> "Chat ends.";
	j2808077110 [label="«Та же старая история», — сказала она со вздохом.",shape=box,color=blue];
		j2808077110 -> j646961111;
	j646961111 [label="Забавно. Когда я росла, все так боялись Радиационной Войны. Помню те общественные объявления, которые шли по телевидению. Суровые советы о том, как запасаться припасами, как подготовить убежище и что обязательно нужно похоронить умерших родственников, даже если не хочется.",shape=box,color=blue];
		j646961111 -> j1048447320;
	j1048447320 [label="Когда я увидела взрывающийся реактор в твоих мыслях, это вернуло меня ко всем тем кошмарам. Я поняла, что мы должны это остановить, чего бы это ни стоило. Я заставила бы остальных поверить тебе.",shape=box,color=blue];
		j1048447320 -> j1457388520;
	j1457388520 [label="Но этой планете грозит не Радиационная Война, верно? Это Техрот. Так или иначе, этот цикл завершится, и когда это случится, остальной мир заразится.",shape=box,color=blue];
		j1457388520 -> "Chat ends.";
}