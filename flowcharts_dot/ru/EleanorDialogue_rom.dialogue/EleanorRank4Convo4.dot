		j2395147699 -> j2326672899;
	j2326672899 [label="Так что отчасти это просто потому, что она боится, что я прочту её подростковый дневник, так сказать. Я загляну в её самые затаённые секреты и посмеюсь над ними до упаду, а она будет унижена навсегда.",shape=box,color=blue];
		j2326672899 -> j593435324;
