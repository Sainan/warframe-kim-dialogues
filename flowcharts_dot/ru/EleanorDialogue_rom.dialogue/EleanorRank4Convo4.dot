	j2312235983 [label="Теперь она ведёт себя так, будто я совсем другой человек. Удобно для неё. Если я больше не Элеонора, то она может быть сколько угодно жестокой со мной, ведь она не ранит того человека, которого знала. По-моему, это трусость.",shape=box,color=blue];
		j2312235983 -> j2910436949;
	j249365149 [label="И если ты вдруг думаешь о том, чтобы попытаться с кем-то из нас...",shape=box,color=blue];
		j249365149 -> j2755432485;
	j4294238995 [label="Если бы я думала, что они знают, что делают, и испытывают искренние чувства к этому парню, я была бы в восторге. Очевидно.",shape=box,color=blue];
