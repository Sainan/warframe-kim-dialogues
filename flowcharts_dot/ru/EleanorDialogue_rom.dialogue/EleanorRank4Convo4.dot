		j3309848793 -> j3333806194;
	j2392124914 [label="Но нельзя же подойти к людям, которые горюют, сказать: «Я же говорила». Надо их поддерживать. Я сделала всё, что могла, чтобы утешить Артура, а Лэтти старалась залечить разбитое сердце Аои.",shape=box,color=blue];
