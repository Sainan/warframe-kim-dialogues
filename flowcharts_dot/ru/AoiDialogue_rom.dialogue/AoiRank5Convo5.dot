digraph {
	"AoiRank5Convo5 begins." [color=orange];
		"AoiRank5Convo5 begins." -> j4291913033;
		"AoiRank5Convo5 begins." -> j1752837284;
	j4291913033 [label="Одна вещь, которой я действительно восхищаюсь в тебе, - это твоя непоколебимая приверженность построению лучшего мира. Откуда это берется?",shape=box,color=red];
		j4291913033 -> j980194247;
	j1752837284 [label="У тебя впечатляющий запас драйва. Что зажигает огонь в тебе?",shape=box,color=red];
		j1752837284 -> j980194247;
	j980194247 [label="ого, спасибо, что сказали это. Эмм... хм. Нет какого-то решающего момента, который определил бы мой жизненный путь или что-то в этом роде.",shape=box,color=blue];
		j980194247 -> j2884518799;
	j2884518799 [label="насколько я могу судить, я унаследовала это от своих родителей. они были практичными людьми, но и мечтателями тоже",shape=box,color=blue];
		j2884518799 -> j4090999949;
	j4090999949 [label="Я имею в виду, что ты не строишь ракеты для исследования космоса, если ты не мечтатель.",shape=box,color=blue];
		j4090999949 -> j1711702382;
		j4090999949 -> j969061129;
		j4090999949 -> j3673851644;
	j1711702382 [label="Подождите. Твои родители строили ракеты?",shape=box,color=red];
		j1711702382 -> j1232768562;
	j969061129 [label="Это чертовски крутой семейный бизнес. Похоже, они установили высокую планку.",shape=box,color=red];
		j969061129 -> j1569129642;
	j3673851644 [label="Это не совсем обычное семейное наследие. Они когда-нибудь говорили о том, что их вдохновляло?",shape=box,color=red];
		j3673851644 -> j3851348837;
	j1232768562 [label="ну, папа знал, и мама знала, как это делать.",shape=box,color=blue];
		j1232768562 -> j2359145181;
	j1569129642 [label="да... и когда я стала солдатом, они ясно дали мне понять, что это не совсем та жизнь, которую они надеялись построить для меня.",shape=box,color=blue];
		j1569129642 -> j2359145181;
	j3851348837 [label="не очень. они не были большими любителями делиться своими мечтами вслух. Я думаю, они считали, что их вклад в мир скажет за них.",shape=box,color=blue];
		j3851348837 -> j2359145181;
	j2359145181 [label="папа был аэрокосмическим инженером. мама преподавала проектирование космических систем в университете, где они познакомились, когда были студентами.",shape=box,color=blue];
		j2359145181 -> j2748315590;
		j2359145181 -> j3445975314;
		j2359145181 -> j3598216781;
	j2748315590 [label="Ого, ваш дом был наполовину аналитическим центром, наполовину стартовой площадкой.",shape=box,color=red];
		j2748315590 -> j2803927439;
	j3445975314 [label="С ними, должно быть, было интересно общаться.",shape=box,color=red];
		j3445975314 -> j2189947568;
	j3598216781 [label="Они звучат так, словно были великолепны. Должно быть, это было невероятно - расти в такой обстановке.",shape=box,color=red];
		j3598216781 -> j2189947568;
	j2803927439 [label="хаха, не совсем. в основном было просто тихо.",shape=box,color=blue];
		j2803927439 -> j1480181042;
	j2189947568 [label="да, я так полагаю",shape=box,color=blue];
		j2189947568 -> j1544505696;
	j1480181042 [label="большинство наших разговоров состояли из \"привет, милая\" и \"привет\" мимоходом. Затем они хватали ужин по пути в офис, чтобы поработать допоздна или что-то в этом роде",shape=box,color=blue];
		j1480181042 -> j1153897266;
	j1544505696 [label="но большинство наших разговоров состояло только из \"эй, милая\" и \"привет\", когда мы проходили мимо друг друга на кухне. Они быстро перекусывали и возвращались в свои офисы, чтобы поработать допоздна",shape=box,color=blue];
		j1544505696 -> j1153897266;
	j1153897266 [label="они были заняты",shape=box,color=blue];
		j1153897266 -> j3982478777;
	j3982478777 [label="пока я не попадала в неприятности, они не особо обо мне беспокоились. И, несмотря на весь огонь, который горел у меня внутри, мне действительно удавалось избегать неприятностей.",shape=box,color=blue];
		j3982478777 -> j3922017725;
		j3982478777 -> j2954854343;
		j3982478777 -> j949490756;
	j3922017725 [label="Я понимаю. Мои родители тоже всегда были заняты. Управление судном Зариман требовало полной занятости для всех. Тем не менее, мы всегда находили время, чтобы хотя бы посидеть вместе за обедом.",shape=box,color=red];
		j3922017725 -> j1828569939;
	j2954854343 [label="Это имеет смысл. Когда работа затягивает тебя так сильно, времени остается не так уж много, но я уверен, что они думали о тебе постоянно.",shape=box,color=red];
		j2954854343 -> j4064546405;
	j949490756 [label="Не могу сказать, что я их виню. Я знаю по себе, что космические путешествия не оставляют много времени для отдыха. Похоже, ты облегчила им жизнь, не высовываясь.",shape=box,color=red];
		j949490756 -> j3427337327;
	j1828569939 [label="+5 Chemistry",color=orange];
		j1828569939 -> j1592406030;
	j4064546405 [label="+5 Chemistry",color=orange];
		j4064546405 -> j3301274086;
	j3427337327 [label="да, я так думаю.",shape=box,color=blue];
		j3427337327 -> j854318503;
	j1592406030 [label="это мило. Я рада, что они это сделали.",shape=box,color=blue];
		j1592406030 -> j3741865469;
	j3301274086 [label="может быть",shape=box,color=blue];
		j3301274086 -> j228559236;
	j854318503 [label="Надеюсь, что да.",shape=box,color=blue];
		j854318503 -> j2651032366;
	j3741865469 [label="Теперь я понимаю, насколько важны такие моменты, но когда я росла, мне не казалось странным, что их у меня было мало.",shape=box,color=blue];
		j3741865469 -> j2651032366;
	j228559236 [label="на самом деле да, я думаю, это правда :)",shape=box,color=blue];
		j228559236 -> j2651032366;
	j2651032366 [label="Я понимаю, почему мои родители были так сосредоточены на своей работе. По-своему, это был их способ проявлять любовь. Они хотели обеспечить меня и создать технологию, которая могла бы помочь человечеству обрести лучшее будущее",shape=box,color=blue];
		j2651032366 -> j3846650356;
	j3846650356 [label="оглядываясь назад, это своего рода прекрасно",shape=box,color=blue];
		j3846650356 -> j1242890699;
		j3846650356 -> j3693539039;
		j3846650356 -> j388398785;
	j1242890699 [label="Может быть, так, но мне все равно не по себе, что их дочь не проводила с ними больше времени. У нас в этой жизни не так уж много времени, понимаешь? Мы должны проводить как можно больше времени с теми, кого любим.",shape=box,color=red];
		j1242890699 -> j1347613729;
	j3693539039 [label="Красиво, конечно, но и практично тоже. Они делали что-то важное, и важнее этого не бывает.",shape=box,color=red];
		j3693539039 -> j2847318608;
	j388398785 [label="Это сильная любовь, даже если она не та, что проявляется за обеденным столом.",shape=box,color=red];
		j388398785 -> j507923886;
	j1347613729 [label="+5 Chemistry",color=orange];
		j1347613729 -> j4030260606;
	j2847318608 [label="и все же я не могу не думать о том, что мы не так уж много времени проводили вместе.",shape=box,color=blue];
		j2847318608 -> j794014142;
	j507923886 [label="+5 Chemistry",color=orange];
		j507923886 -> j2878780787;
	j4030260606 [label="вот эта часть «настолько, насколько мы можем» — сложная, не правда ли? ее трудно измерить",shape=box,color=blue];
		j4030260606 -> j794014142;
	j794014142 [label="если я когда-нибудь увижу их снова, я обязательно наверстаю упущенное время. Я знаю, что они тоже так сделают.",shape=box,color=blue];
		j794014142 -> j671833316;
		j794014142 -> j2913058189;
		j794014142 -> j2154077445;
	j2878780787 [label="точно! не такой, как в кино, но вполне реальный.",shape=box,color=blue];
		j2878780787 -> j794014142;
	j671833316 [label="Справедливо. Просто не трать время на то, чтобы сейчас переживать из-за этого. Они сделали свой выбор, а ты свой. Похоже, ты прекрасно справляешься с тем, что они тебе дали.",shape=box,color=red];
		j671833316 -> j2456497275;
	j2913058189 [label="Знаешь, это твое стремление сделать мир лучше... оно глубже, чем ты себе представляешь. Тебе стоит больше гордиться этим.",shape=box,color=red];
		j2913058189 -> j1480177650;
	j2154077445 [label="Надеюсь, у тебя будет такой шанс. Твое сердце полно надежды и силы. Я думаю, они гордились бы тем, кем ты стала.",shape=box,color=red];
		j2154077445 -> j1096643232;
	j2456497275 [label="Я так думаю",shape=box,color=blue];
