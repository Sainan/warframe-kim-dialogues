		j2456497275 -> j4167985040;
		j2456497275 -> j1596770521;
	j1480177650 [label="+5 Chemistry",color=orange];
		j1480177650 -> j1917364723;
	j1096643232 [label="+5 Chemistry",color=orange];
		j1096643232 -> j4259366394;
	j3391915458 [label="У тебя их мозги и стремление, но ты сделала это своим. Вот что имеет значение.",shape=box,color=red];
		j3391915458 -> j2219474531;
	j4167985040 [label="Мне кажется, ты выросла таким любящим и внимательным человеком, в котором нуждалась, когда была моложе. Этим стоит гордиться.",shape=box,color=red];
		j4167985040 -> j1640069205;
	j1596770521 [label="Ты взяла все, что они дали тебе, и построили на этом — не только с мечтами и стремлением, но и с состраданием. Это нелегко сделать.",shape=box,color=red];
		j1596770521 -> j1640069205;
	j1917364723 [label="о? как так?",shape=box,color=blue];
		j1917364723 -> j3391915458;
		j1917364723 -> j4167985040;
		j1917364723 -> j1596770521;
	j4259366394 [label="это много для меня значит. Я тоже на это надеюсь.",shape=box,color=blue];
		j4259366394 -> j3391915458;
		j4259366394 -> j4167985040;
		j4259366394 -> j1596770521;
	j2219474531 [label="это... на самом деле очень классный способ об этом подумать.",shape=box,color=blue];
