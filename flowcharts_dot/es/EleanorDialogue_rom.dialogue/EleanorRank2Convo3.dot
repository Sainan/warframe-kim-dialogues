		j375736117 -> j335528173;
	j385579298 [label="¿Qué ES \"la indiferencia\"? He visto destellos en tu mente, pero no puedo entenderlos. Me dan ganas de correr y acurrucarme en algún lugar lejano con paredes muy gruesas.",shape=box,color=blue];
