	j1484675833 [label="¿\"La Indiferencia\" es lo que ES o lo que QUIERE?",shape=box,color=blue];
		j1484675833 -> j888037478;
		j1484675833 -> j1846881576;
		j1484675833 -> j984287941;
	j888037478 [label="Tú y Lettie hacen eso, ¿sabes? Se olvidan de decir \"hola\".",shape=box,color=red];
		j888037478 -> j2214019862;
	j1846881576 [label="La mayoría de la gente comenzaría con \"hola\".",shape=box,color=red];
		j1846881576 -> j3809273302;
	j984287941 [label="[Ignorar]",shape=box,color=red];
		j984287941 -> "Chat ends.";
	j2214019862 [label="Has encontrado lo único que compartimos, felicitaciones. Y lo siento. Esto es más importante.",shape=box,color=blue];
		j2214019862 -> j3631604476;
	j3809273302 [label="Lo siento. Esto es más importante.",shape=box,color=blue];
		j3809273302 -> j3631604476;
	"Chat ends." [color=orange];
	j3631604476 [label="Creo que 'La Gran Indiferencia' va a intentar interponerse entre nosotros, de alguna manera.",shape=box,color=blue];
		j3631604476 -> j756000077;
	j756000077 [label="No te rías. Lo puedo sentir, en lo más profundo de mi ser. A la cosa que había en Rusalka le encantaría vernos destrozándonos unos a otros.",shape=box,color=blue];
		j756000077 -> j3574108549;
	j3574108549 [label="No sé por qué. Simplemente lo sé.",shape=box,color=blue];
		j3574108549 -> j549805461;
		j3574108549 -> j3072190536;
	j549805461 [label="Sí, creo que tienes razón.",shape=box,color=red];
		j549805461 -> j3889368859;
	j3072190536 [label="Tal vez eso fue lo que le pasó a la familia de Entrati. Llevaban años viviendo encima de su laboratorio sin saberlo. Cuando el infante los encontró, estaban distanciados y sumidos en su despecho.",shape=box,color=red];
		j3072190536 -> j3889368859;
	j3889368859 [label="Lo he visto en tu cabeza. Albrecht y Loid. Madre y Padre. Tú y tus padres. Rusalka y Viktor, por lo que sabemos. Y nosotros seis, por supuesto.",shape=box,color=blue];
		j3889368859 -> j2647763212;
	j2647763212 [label="Lo que hace esta cosa es frío y perverso, pero hay una motivación detrás, en algún lugar.",shape=box,color=blue];
		j2647763212 -> j3375869388;
		j2647763212 -> j288036721;
	j3375869388 [label="Creo que tienes razón. En la Zariman, los padres fueron los primeros en perder la cabeza. Tal vez Los Inquebrantables resistieron tanto tiempo porque ninguno de ellos tenía hijos.",shape=box,color=red];
		j3375869388 -> j1122039839;
	j288036721 [label="No me gusta pretender que entiendo cosas que son básicamente *dioses*. Esta cosa está intentando comprender las emociones. Pero está jugando mal con sus juguetes.",shape=box,color=red];
		j288036721 -> j1014691546;
	j1122039839 [label="Deja que intente interponerse entre nosotros. Por Sol que acabaré con ella. Encontraré una manera.",shape=box,color=blue];
		j1122039839 -> j142006479;
	j1014691546 [label="Una teoría admirable, pero no lo SABEMOS, ¿verdad?",shape=box,color=blue];
		j1014691546 -> j2491684470;
	j142006479 [label="Hay poderes en el universo contra los cuales esa cosa no puede oponerse. Elijo creer en ello con cada átomo de mi ser.",shape=box,color=blue];
		j142006479 -> j3529934693;
	j2491684470 [label="Si tienes razón, entonces es inocente. Casi infantil. Eso no encaja con lo que sabemos. Es malévolo.",shape=box,color=blue];
		j2491684470 -> "Chat ends.";
	j3529934693 [label="Si me traga entera, me clavaré en su garganta y la ahogaré.",shape=box,color=blue];
		j3529934693 -> j3487826428;
		j3529934693 -> j1158406784;
	j3487826428 [label="Esa es una gran manera de morir.",shape=box,color=red];
		j3487826428 -> j1565785707;
	j1158406784 [label="Aún no sé cómo lo vamos a hacer, pero estoy contigo. Tenemos que ganar.",shape=box,color=red];
		j1158406784 -> j2660061992;
	j1565785707 [label="Estoy muerta de todas formas en este escenario.",shape=box,color=blue];
		j1565785707 -> j2868147892;
	j2660061992 [label="Si esto fuera una historia, se te ocurriría algún truco ingenioso: desenterrarías el corazón de piedra del gigante y lo romperías con una pluma de cisne, o lo engañarías para que se arrojara por un acantilado.",shape=box,color=blue];
		j2660061992 -> j2598158212;
	j2868147892 [label="Pero al menos puedo escupirle en el ojo.",shape=box,color=blue];
		j2868147892 -> "Chat ends.";
	j2598158212 [label="Dijiste que eras \"periodista\". Ese no era el tipo de historias que solías contar, ¿no? Decías la verdad, por muy fea que fuera.",shape=box,color=red];
		j2598158212 -> j366676567;
	j366676567 [label="Si esas viejas historias no dijeran la verdad, la gente no habría seguido contándolas.",shape=box,color=blue];
		j366676567 -> j45217934;
	j45217934 [label="No quiero decir que los gigantes sean reales, sólo que las viejas historias cuentan un tipo de verdad diferente.",shape=box,color=blue];
		j45217934 -> j4156159574;
		j45217934 -> j1424645250;
	j4156159574 [label="Necesitaremos algo más que cuentos de hadas para superar esto, por muy inspiradores que sean.",shape=box,color=red];
		j4156159574 -> j2499849609;
	j1424645250 [label="He vivido en un reino hecho de historias. Sé lo poderosas que pueden ser.",shape=box,color=red];
		j1424645250 -> j2636369921;
	j2499849609 [label="Lo sé. Ojalá tuviera algo más que instinto para seguir adelante.",shape=box,color=blue];
		j2499849609 -> j822757594;
	j2636369921 [label="Creo que lo único que todas esas historias de asesinos de gigantes tienen en común es esto: todas tratan sobre aprender las reglas y descubrir cómo ser astuto con ellas.",shape=box,color=blue];
		j2636369921 -> j185480769;
	j822757594 [label="Nos enfrentamos a algo enorme, aterrador y que escapa a nuestra comprensión. Normalmente, sólo los niños pueden hacerlo. Por eso existen los cuentos de hadas.",shape=box,color=blue];
		j822757594 -> j1910445803;
		j822757594 -> j471467264;
	j185480769 [label="Algunos monstruos son estúpidos. Algunos se ven obligados a ser literales. Algunos tienen secretos. Pero no puedes ganar hasta que conozcas las reglas a las que está sujeto el monstruo.",shape=box,color=blue];
		j185480769 -> j1910445803;
		j185480769 -> j471467264;
	j1910445803 [label="Está atascado en la Pared. Ni siquiera sé qué es la Pared.",shape=box,color=red];
		j1910445803 -> j261747405;
	j471467264 [label="No creo que la Indiferencia esté sujeta a ninguna regla.",shape=box,color=red];
		j471467264 -> j111124914;
	j261747405 [label="No importa. Si realmente está atascado, entonces podemos deducir dos cosas, ¿no?",shape=box,color=blue];
		j261747405 -> j160232464;
	j111124914 [label="¡Tiene que estarlo! ¡Hasta los dioses tienen reglas! No importa cuán poderosa parezca la Indiferencia, no es un Dios.",shape=box,color=blue];
		j111124914 -> j4020649266;
	j160232464 [label="Uno: puede quedar atrapado. Tal vez incluso encarcelado.",shape=box,color=blue];
		j160232464 -> j3895214913;
	j4020649266 [label="Venciste a Thrax. No importa cuántas veces fallaste y terminaste en el suelo. GANASTE. ¿Por qué no deberías vencer también al Hombre en la Pared?",shape=box,color=blue];
		j4020649266 -> j3666271207;
	j3895214913 [label="+15 Chemistry",color=orange];
		j3895214913 -> j1760349650;
