digraph {
	"EleanorRank4Convo5 begins." [color=orange];
		"EleanorRank4Convo5 begins." -> j676717029;
	j676717029 [label="El reino que solías gobernar, Duviri. No lo inventaste, ¿verdad?",shape=box,color=blue];
		j676717029 -> j1997974706;
		j676717029 -> j3473793656;
	j1997974706 [label="Lo hice y no. El lugar y los personajes surgieron de un libro infantil que me encantaba, \"Cuentos de Duviri\". Le di vida a través del Vacío. ",shape=box,color=red];
		j1997974706 -> j1072956414;
	j3473793656 [label="Ya me cansé de hablar de Duviri...",shape=box,color=red];
		j3473793656 -> j2976893056;
	j1072956414 [label="¿Cómo lo lograste?",shape=box,color=blue];
		j1072956414 -> j4018226793;
		j1072956414 -> j1307738190;
	j2976893056 [label="+5 Chemistry",color=orange];
		j2976893056 -> j736296129;
	j4018226793 [label="No tenía a nadie más. Supongo que los otros niños estaban muertos o fueron rescatados. No recuerdo muy bien los detalles.",shape=box,color=red];
		j4018226793 -> j1766775156;
	j1307738190 [label="No fue exactamente intencional. Todo lo que recuerdo es…",shape=box,color=red];
		j1307738190 -> j1766775156;
	j736296129 [label="Ups. Mi error.",shape=box,color=blue];
		j736296129 -> j1179378497;
	j1766775156 [label="Los Ángeles se acercaban, atravesaban las paredes y yo no tenía adónde correr. Tenía el libro y recuerdo que pensé: \"Aquí es donde puedo estar a salvo\".",shape=box,color=red];
		j1766775156 -> j305967021;
	j1179378497 [label="Fue una falta de consideración por mi parte, ¿verdad? Ese lugar no fue exactamente un campamento de vacaciones para ti.",shape=box,color=blue];
		j1179378497 -> j811872515;
	j305967021 [label="¿Ángeles? En esta época, se piensa que los ángeles son criaturas benévolas. Aterradoras, a veces, pero del lado del bien. A menos que te refieras a los caídos.",shape=box,color=blue];
		j305967021 -> j683010984;
	j811872515 [label="Lo siento mucho. A partir de ahora mantendré mi curiosidad bajo control.",shape=box,color=blue];
		j811872515 -> "Chat ends.";
	j683010984 [label="Eso no ha cambiado mucho en mi época. Los ángeles son criaturas aladas y brillantes de las antiguas historias que ayudan a los humanos buenos. Me refiero a los Ángeles del Vacío. Son una bestia completamente diferente.",shape=box,color=red];
		j683010984 -> j946657092;
	"Chat ends." [color=orange];
	j946657092 [label="Ya veo. No puedo imaginarme algo llamado \"Ángel del Vacío\" descendiendo y bendiciendo a un bebé recién nacido.",shape=box,color=blue];
		j946657092 -> j2001252592;
	j2001252592 [label="Por lo que he podido entender, antes eran personas. Cuando la canción de los Ángeles llega a ellos, empiezan a cambiar. Cuando ya no pueden resistirla más, se convierten en Ángeles ellos mismos.",shape=box,color=red];
		j2001252592 -> j2786249427;
	j2786249427 [label="El futuro, tal como lo describes, suena verdaderamente horrible.",shape=box,color=blue];
		j2786249427 -> j864643610;
	j864643610 [label="No puedo culparte por querer esconderte de estos Ángeles del Vacío. Especialmente si eran…",shape=box,color=blue];
		j864643610 -> j1112473532;
	j1112473532 [label="Ah, acabo de tener un presentimiento. No eran tus padres, ¿verdad?",shape=box,color=blue];
		j1112473532 -> j3798549963;
		j1112473532 -> j807404965;
		j1112473532 -> j594602231;
	j3798549963 [label="Tengo la certeza de que mis padres están muertos.",shape=box,color=red];
		j3798549963 -> j288673382;
	j807404965 [label="Podrían haberlo sido. Otros niños oyeron a sus padres llamarlos.",shape=box,color=red];
		j807404965 -> j288673382;
	j594602231 [label="Creo que sí. Quinn me advirtió que no buscara voces conocidas. Eso debe haber sido lo que quiso decir.",shape=box,color=red];
		j594602231 -> j288673382;
	j288673382 [label="Lo siento mucho. Me alegro de que hayas encontrado un lugar seguro, aunque no haya permanecido así.",shape=box,color=blue];
		j288673382 -> j3636487802;
	j3636487802 [label="Fue seguro durante mucho tiempo. Lo suficiente para hacerme olvidar. Supongo que eso era lo que yo quería hacer.",shape=box,color=red];
		j3636487802 -> j1495226990;
	j1495226990 [label="Pero ¿cómo demonios lo hiciste? Eras apenas un niño. ¿Qué te dio el poder de crear un reino entero de la nada?",shape=box,color=blue];
		j1495226990 -> j1427162373;
		j1495226990 -> j2973995424;
		j1495226990 -> j3672876524;
	j1427162373 [label="Los niños son fascinantes porque aún no saben lo que no pueden hacer. Yo no sabía que no podía hacerlo. Así que lo hice.",shape=box,color=red];
		j1427162373 -> j152890268;
	j2973995424 [label="Sentí tantas emociones a la vez que... simplemente... hice algo con ellas. Es difícil de explicar.",shape=box,color=red];
		j2973995424 -> j1969865294;
	j3672876524 [label="El Vacío no es \"nada\". Sé que suena como si así fuera, pero no lo es. Albrecht lo demostró.",shape=box,color=red];
		j3672876524 -> j1969865294;
	j152890268 [label="Cuando tenía cinco años, quería ser una bruja. Grité abracadabra y salté desde lo alto de las escaleras. No sabía que no podía volar, pero pronto lo descubrí.",shape=box,color=blue];
		j152890268 -> j3194258043;
	j1969865294 [label="Sabemos muy poco sobre eso, incluso ahora. Pero hay algo que incluso los Orokin descubrieron: el Vacío responde a las emociones humanas. ",shape=box,color=red];
		j1969865294 -> j830062151;
	j3194258043 [label="El poder para crear a Duviri debe haber venido de algún lugar que no seas tú. Lo usaste para hacer algo increíble, pero no era tuyo.",shape=box,color=blue];
		j3194258043 -> j3431365282;
	j830062151 [label="YA VEO. Entonces, si miraras al Vacío con… hmm, digamos \"hambre\"… podrías crear un lobo medio muerto de hambre al acecho. O un banquete real, tal vez. O una hogaza de pan con dientes de lobo en ella. ",shape=box,color=blue];
		j830062151 -> j2955776989;
	j3431365282 [label="Al principio no, pero ahora está en ti. La energía salvaje. Los bucles en el tiempo. Dioses, eres una maravilla.",shape=box,color=blue];
		j3431365282 -> j2671615679;
	j2955776989 [label="¡Qué pensamiento tan aterrador y estimulante!",shape=box,color=blue];
		j2955776989 -> j2671615679;
	j2671615679 [label="¿Por qué los Orokin pensaron que era una buena idea lanzar una nave llena de niños al Vacío? ¿No era eso una bomba de tiempo emocional a punto de estallar? ",shape=box,color=blue];
		j2671615679 -> j2468390176;
	j2468390176 [label="Ese era el propósito de ese libro de historias. A primera vista, trataba de personajes extravagantes en la corte de un rey loco que cambiaba de humor todos los días. ",shape=box,color=red];
		j2468390176 -> j2508410335;
	j2508410335 [label="Pero la verdadera lección de las historias era cómo controlar las emociones. Debíamos aprender una lección importante de ellas para que el viaje fuera seguro.",shape=box,color=red];
		j2508410335 -> j2417913869;
	j2417913869 [label="Ugh. Lo siento, pero ahora lo arruinaste para mí. ",shape=box,color=blue];
		j2417913869 -> j1830037484;
	j1830037484 [label="¿Por qué las historias no pueden ser simplemente historias? ODIÉ cuando descubrí que Rodolfo, el Rey Leopardo que murió heroicamente en batalla contra la Serpiente del Zodíaco representaba al Sol con sus manchas solares, sufriendo en el árbol estelar. ",shape=box,color=blue];
		j1830037484 -> j1830458253;
	j1830458253 [label="Introducir religión en los libros infantiles es como esconder medicamentos dentro de caramelos. SÓLO DÍGANLO.",shape=box,color=blue];
		j1830458253 -> j926223775;
		j1830458253 -> j1395738638;
	j926223775 [label="Nunca había oído hablar de Rodolfo el Rey Leopardo.",shape=box,color=red];
		j926223775 -> j836643225;
	j1395738638 [label="Quiero escuchar la historia de Rodolfo el Rey Leopardo algún día.",shape=box,color=red];
		j1395738638 -> j2334074441;
	j836643225 [label="Puedo morir feliz sabiendo que esos libros quedarán olvidados. ¡Viva el futuro!",shape=box,color=blue];
		j836643225 -> j4225197015;
	j2334074441 [label="Ugh. Por favor, no. No es una leyenda real, es una serie terrible de libros infantiles.",shape=box,color=blue];
		j2334074441 -> j2278676568;
	j4225197015 [label="Pero tienes historias, ¿no?",shape=box,color=blue];
		j4225197015 -> j2563173414;
	j2278676568 [label="Aunque, si quieres, podría contarte algunas de nuestras verdaderas leyendas.",shape=box,color=blue];
		j2278676568 -> j2589331338;
		j2278676568 -> j4026733602;
	j2563173414 [label="Muchas. Incluso los tenno y los warframes tienen historias. ",shape=box,color=red];
		j2563173414 -> j2843143373;
	j2589331338 [label="Eso me gustaría.",shape=box,color=red];
		j2589331338 -> j3102533549;
	j4026733602 [label="No en este momento. [Fin.]",shape=box,color=red];
		j4026733602 -> "Chat ends.";
	j2843143373 [label="Hay un tipo llamado Drusus Leverian. Es como un curador de museo. Mantiene registros de todas las leyendas de los warframes.",shape=box,color=red];
		j2843143373 -> j4264387141;
	j3102533549 [label="Veamos. Está Arturo, por supuesto. El bueno de Arturo. El original. El mejor de Britannia.",shape=box,color=blue];
		j3102533549 -> j604678438;
	j4264387141 [label="Bueno, eso me voló la cabeza. Eres de un futuro lejano, ¿pero has estado fuera de circulación en Duviri durante tantos años que los acontecimientos que sucedieron durante tu vida han tenido tiempo de convertirse en mitos?",shape=box,color=blue];
		j4264387141 -> j3623550414;
	j604678438 [label="Luego está la joven Loxley. Me encantaba. Quería ser como ella. Era el modelo para cierto tipo de jovencita.",shape=box,color=blue];
		j604678438 -> j4158769848;
	j3623550414 [label="Me está dando vértigo. ",shape=box,color=blue];
		j3623550414 -> j2104277849;
		j3623550414 -> j1842358285;
	j4158769848 [label="¿Cuál es su historia?",shape=box,color=red];
		j4158769848 -> j4073662502;
	j2104277849 [label="Créeme, sé cómo te sientes.",shape=box,color=red];
		j2104277849 -> j2133278166;
	j1842358285 [label="Cuéntame sobre tus mitos entonces.",shape=box,color=red];
		j1842358285 -> j4026249294;
	j4073662502 [label="Bueno. Versión resumida: ella era la hija de un noble en los viejos tiempos, cuando la tierra sufría bajo el dominio de un rey cruel.",shape=box,color=blue];
		j4073662502 -> j956277193;
	j2133278166 [label="Tú mismo eres un personaje de cuento de hadas, ¿no?",shape=box,color=blue];
		j2133278166 -> j2336288256;
	j4026249294 [label="¡Vaya! Si empiezo, estaremos aquí todo el año. ",shape=box,color=blue];
		j4026249294 -> j3102533549;
	j956277193 [label="Su hermano había muerto luchando en tierras extranjeras, pero la noticia aún no había llegado a casa, así que cuando el pobre pueblo oprimido necesitaba un héroe, ella estuvo a la altura de las circunstancias.",shape=box,color=blue];
		j956277193 -> j2215480408;
	j2336288256 [label="Atrapado en un reino mágico donde el tiempo fluye de manera diferente, convocado al lugar donde más te necesitaban...",shape=box,color=blue];
		j2336288256 -> j909380516;
	j2215480408 [label="Check boolean EleanorHeroVirus",color=orange];
		j2215480408 -> j926796626 [label=true,color=green];
		j2215480408 -> j2068740586 [label=false,color=brown];
	j909380516 [label="Todo lo que necesitamos para completar tu historia es un interés amoroso o un monstruo babeante e inhumano.",shape=box,color=blue];
		j909380516 -> j280025013;
	j926796626 [label="Creí que odiabas a los héroes.",shape=box,color=red];
		j926796626 -> j4277183390;
	j2068740586 [label="Normalmente no soporto las figuras heroicas. Son tóxicas, contagiosas y, por eso mismo, peligrosas.",shape=box,color=blue];
		j2068740586 -> j825978883;
	j280025013 [label="(O un interés amoroso babeante e inhumano, dependiendo de tus gustos).",shape=box,color=blue];
		j280025013 -> j3791108746;
		j280025013 -> j3291812932;
	j4277183390 [label="Con ella hago una excepción. Así que cállate.",shape=box,color=blue];
		j4277183390 -> j1745390565;
	j825978883 [label="La joven Loxley es diferente.",shape=box,color=blue];
		j825978883 -> j1665651194;
	j3791108746 [label="Siempre me han parecido más interesantes los monstruos. Los héroes son aburridos, predecibles... mundanos.",shape=box,color=red];
		j3791108746 -> j899297402;
	j3291812932 [label="Mm, no sé si los monstruos son realmente mi tipo.",shape=box,color=red];
		j3291812932 -> j3901462887;
	j1745390565 [label="Como decía, se parecía mucho a su hermano, así que se vistió con su ropa vieja y se hizo pasar por él, y se fue a liderar una revolución. Si alguien se acercaba demasiado, podría darse cuenta de que era una chica, así que empezó a usar una capucha. Eso se convirtió en su distintivo.",shape=box,color=blue];
		j1745390565 -> j1277626074;
	j1665651194 [label="Boolean EleanorHeroVirus is now true.",color=orange];
		j1665651194 -> j1277626074;
	j899297402 [label="Boolean EleanorFlirt is now true.",color=orange];
		j899297402 -> j2197818032;
	j3901462887 [label="Y eso está bien, honestamente.",shape=box,color=blue];
		j3901462887 -> j1778471832;
	j1277626074 [label="Era una experta en el manejo del arco y la flecha. Ella y su equipo se convirtieron en una banda de forajidos muy querida que robaba a los recaudadores de impuestos del rey y entregaba el dinero a quien más lo necesitaba.",shape=box,color=blue];
		j1277626074 -> j3081652686;
	j2197818032 [label="+15 Chemistry",color=orange];
		j2197818032 -> j2383854530;
	j1778471832 [label="Los monstruos están acostumbrados a estar solos.",shape=box,color=blue];
		j1778471832 -> j449556400;
	j3081652686 [label="Parece que ella misma se habría ganado el respeto. ¿Por qué tenía que fingir ser su hermano?",shape=box,color=red];
		j3081652686 -> j4083152689;
	j2383854530 [label="La monstruosidad está en los ojos de quien la mira, camarada.",shape=box,color=blue];
		j2383854530 -> j2497675390;
	j449556400 [label="Somos supervivientes.",shape=box,color=blue];
		j449556400 -> "Chat ends.";
	j4083152689 [label="Eso tiene que ver con la historia. En aquel entonces, se suponía que los hombres eran los que luchaban, así que también gobernaban. Se esperaba que la gente obedeciera las órdenes de los hombres. Ella necesitaba aprovechar la reputación de su hermano para hacer lo que tenía que hacer. Era eso o hundirse.",shape=box,color=blue];
		j4083152689 -> j1792623108;
		j4083152689 -> j2744037128;
		j4083152689 -> j2153541139;
	j2497675390 [label="Si te sientes sinceramente atraído por las cualidades que consideras \"monstruosas\" en otra persona, entonces es mejor que te asegures de que esa persona se sienta cómoda con tu elección de etiquetas. De lo contrario, cometerás la fetichización más burda.",shape=box,color=blue];
		j2497675390 -> j1751616791;
	j1792623108 [label="¿Qué le pasó al final?",shape=box,color=red];
		j1792623108 -> j948806510;
	j2744037128 [label="Me alegro mucho de que hayamos solucionado eso en el futuro.",shape=box,color=red];
		j2744037128 -> j2278498143;
	j2153541139 [label="No tengo la certeza de que hayamos solucionado eso *por completo* en el futuro...",shape=box,color=red];
		j2153541139 -> j1373337819;
	j1751616791 [label="¿Me consideraría un monstruo, por ejemplo? Sí. Con entusiasmo. Acepto el título con el que otros me condenarían y me regocijo por ello. Por si te lo estabas preguntando.",shape=box,color=blue];
		j1751616791 -> "Chat ends.";
	j948806510 [label="+20 Chemistry",color=orange];
		j948806510 -> j1813041442;
	j2278498143 [label="+20 Chemistry",color=orange];
		j2278498143 -> j4048385791;
	j1373337819 [label="+20 Chemistry",color=orange];
		j1373337819 -> j808024799;
	j1813041442 [label="Depende de qué versión de la historia leas. O bien la llamaron directamente al Salón de los Héroes, o se casó con la hija descarriada del Rey, o quedó atrapada en un roble donde duerme hasta el día de hoy esperando que Britannia la necesite.",shape=box,color=blue];
		j1813041442 -> j349239346;
	j4048385791 [label="Yo también.",shape=box,color=blue];
		j4048385791 -> j1785168014;
	j808024799 [label="No, si ese personaje Ballas sirve de referencia, no lo hicieron.",shape=box,color=blue];
		j808024799 -> j2761979242;
	j349239346 [label="De ahí el dicho local: si las cosas van mal, como durante la guerra, es tradición encogerse de hombros y decir: \"Bueno, no puede ser TAN malo. La joven Loxley aún no ha aparecido\".",shape=box,color=blue];
		j349239346 -> "Chat ends.";
	j1785168014 [label="Dioses, me encantaría ver tu era con mis propios ojos. Tal vez podrías traerme de contrabando uno de esos ataúdes con forma de nevera y así podría dormir unos cuantos milenios.",shape=box,color=blue];
		j1785168014 -> "Chat ends.";
	j2761979242 [label="¿Cuánto tiempo más necesitamos? Maldita sea.",shape=box,color=blue];
		j2761979242 -> "Chat ends.";
}