digraph {
	"EleanorRank4Convo5 begins." [color=orange];
		"EleanorRank4Convo5 begins." -> j2933067273;
	j2933067273 [label="Il regno che eri solito governare. Duviri. Non l'hai inventato, vero?",shape=box,color=blue];
		j2933067273 -> j2826596567;
		j2933067273 -> j2743158917;
	j2826596567 [label="Sì e no. Il luogo e i personaggi provenivano da un libro per bambini che amavo. 'Leggende di Duviri'. L'ho portato in vita attraverso il Void. ",shape=box,color=red];
		j2826596567 -> j444007173;
	j2743158917 [label="Sono così stanco di parlare di Duviri...",shape=box,color=red];
		j2743158917 -> j3595372542;
	j444007173 [label="Come hai fatto?",shape=box,color=blue];
		j444007173 -> j3100251902;
		j444007173 -> j4252116900;
	j3595372542 [label="+5 Chemistry",color=orange];
		j3595372542 -> j875625592;
	j3100251902 [label="Ero completamente solo. Immaginavo che gli altri bambini fossero morti, o fossero stati salvati. Sono un po' confuso sui dettagli.",shape=box,color=red];
		j3100251902 -> j591711861;
	j4252116900 [label="Non era proprio intenzionale. Tutto quello che ricordo è…",shape=box,color=red];
		j4252116900 -> j591711861;
	j875625592 [label="Ops. Errore mio.",shape=box,color=blue];
		j875625592 -> j3809398471;
	j591711861 [label="Gli Angeli stavano arrivando, insinuandosi tra le pareti, e io non avevo vie d'uscita. Avevo il libro, ricordo solo di aver pensato \"Questo è dove potrò essere al sicuro\".",shape=box,color=red];
		j591711861 -> j2948524343;
	j3809398471 [label="È stato sconsiderato da parte mia, non è vero? Quel posto non era esattamente un campo-vacanza per te.",shape=box,color=blue];
		j3809398471 -> j3632303292;
	j2948524343 [label="Angeli? In quest'epoca, gli angeli sono considerati creature benevole. Terrificanti, a volte, ma dalla parte del bene. A meno che non intendi quelli caduti.",shape=box,color=blue];
		j2948524343 -> j2298723372;
	j3632303292 [label="Mi dispiace, sinceramente. D'ora in poi terrò sotto controllo la mia curiosità.",shape=box,color=blue];
		j3632303292 -> "Chat ends.";
	j2298723372 [label="Questo non è cambiato molto nel mio tempo. Gli Angeli sono creature alate e splendenti delle vecchie storie che aiutano gli umani buoni. Sto parlando degli Angeli del Void. Sono bestie completamente diverse.",shape=box,color=red];
		j2298723372 -> j3875841579;
	"Chat ends." [color=orange];
	j3875841579 [label="Capisco. Non riesco a immaginare qualcosa chiamato \"Angelo del Void\" che piomba giù e benedice un neonato.",shape=box,color=blue];
		j3875841579 -> j1441207529;
	j1441207529 [label="Per quanto ne so, un tempo erano persone. Quando il canto degli Angeli le raggiunge, cominciano a cambiare. Quando non riescono più a resistergli, diventano Angeli loro stesse.",shape=box,color=red];
		j1441207529 -> j2920665830;
	j2920665830 [label="Il futuro, così come lo descrivi, sembra davvero orribile.",shape=box,color=blue];
		j2920665830 -> j3147184858;
	j3147184858 [label="Non posso biasimarti per volerti nascondere da questi Angeli del Void. Soprattutto se erano…",shape=box,color=blue];
		j3147184858 -> j3450843008;
	j3450843008 [label="Oh. Ho avuto un presentimento. Non erano i tuoi genitori, vero?",shape=box,color=blue];
		j3450843008 -> j4157805692;
		j3450843008 -> j2013098740;
		j3450843008 -> j302091131;
	j4157805692 [label="Ho la certezza che i miei genitori siano morti.",shape=box,color=red];
		j4157805692 -> j4011942393;
	j2013098740 [label="Sarebbero potuti esserlo. Altri bambini hanno sentito i loro genitori chiamare.",shape=box,color=red];
		j2013098740 -> j4011942393;
	j302091131 [label="Penso che lo fossero, sì. Quinn mi aveva avvertito di non cercare voci familiari. Deve essere questo che intendeva.",shape=box,color=red];
		j302091131 -> j4011942393;
	j4011942393 [label="Mi dispiace tanto. Sono felice che tu abbia trovato un porto sicuro, anche se non è rimasto tale.",shape=box,color=blue];
		j4011942393 -> j3468908182;
	j3468908182 [label="È stato sicuro per molto tempo. Abbastanza a lungo per dimenticare. Credo che fosse quello che volevo fare.",shape=box,color=red];
		j3468908182 -> j1138410486;
	j1138410486 [label="Ma come diavolo hai fatto? Eri solo un bambino. Cosa ti ha dato il potere di evocare un intero regno dal nulla?",shape=box,color=blue];
		j1138410486 -> j1762669800;
		j1138410486 -> j3565950546;
		j1138410486 -> j1458971763;
	j1762669800 [label="I bambini sono affascinanti nel fatto che non sanno ancora cosa non possono fare. Io non sapevo di non poterlo fare. Quindi l'ho fatto.",shape=box,color=red];
		j1762669800 -> j984279133;
	j3565950546 [label="Ho vissuto così tante emozioni, tutte insieme, che... ho semplicemente... creato qualcosa con esse. È difficile da spiegare.",shape=box,color=red];
		j3565950546 -> j842185236;
	j1458971763 [label="Il Void non è \"niente\". So che sembra che dovrebbe esserlo, ma non lo è. Albrecht lo ha dimostrato.",shape=box,color=red];
		j1458971763 -> j842185236;
	j984279133 [label="Quando avevo cinque anni, volevo diventare una strega. Ho urlato Abracadabra e sono saltata dalla cima delle scale. Non sapevo di non poter volare. Ma l'ho scoperto subito.",shape=box,color=blue];
		j984279133 -> j2080562951;
	j842185236 [label="Ne sappiamo molto poco, anche adesso. Ma una cosa che persino gli Orokin hanno capito è questa: il Void risponde alle emozioni umane. ",shape=box,color=red];
		j842185236 -> j1683378657;
	j2080562951 [label="Il potere di creare Duviri deve essere venuto da qualche altra parte, non da te. L'hai usato per fare qualcosa di incredibile, ma non era tuo.",shape=box,color=blue];
		j2080562951 -> j2369329576;
	j1683378657 [label="HO CAPITO. Quindi se guardassi nel Void con… hmm, diciamo 'fame'… potresti creare un lupo affamato che si aggira furtivo. O un banchetto reale. O una pagnotta di pane con i denti digrignanti di un lupo. ",shape=box,color=blue];
		j1683378657 -> j3979728620;
	j2369329576 [label="Non all'inizio, in ogni caso. Ora è in te, però. L'energia selvaggia. I loop nel tempo. Per gli dei, sei una meraviglia.",shape=box,color=blue];
		j2369329576 -> j3353190913;
	j3979728620 [label="Che pensiero terrificante ed esaltante.",shape=box,color=blue];
		j3979728620 -> j3353190913;
	j3353190913 [label="Perché mai gli Orokin hanno pensato che fosse una buona idea lanciare una nave piena di bambini nel Void? Non era forse una bomba a orologeria emotiva in attesa di esplodere? ",shape=box,color=blue];
		j3353190913 -> j2671334751;
	j2671334751 [label="Ecco a cosa serviva quel libro di storie. In superficie, parlava solo di questi personaggi esagerati alla corte di un re pazzo che cambiava umore ogni giorno. ",shape=box,color=red];
		j2671334751 -> j2290630893;
	j2290630893 [label="Ma la vera lezione delle storie riguardava come dominare le proprie emozioni. Dovevamo imparare una lezione importante da questo, affinché il viaggio fosse sicuro.",shape=box,color=red];
		j2290630893 -> j3640353690;
	j3640353690 [label="Ugh. Mi dispiace, ma ora mi hai rovinato tutto. ",shape=box,color=blue];
		j3640353690 -> j1447904151;
	j1447904151 [label="Perché le storie non possono essere solo storie? Ho ODIATO quando ho scoperto che Rudolfo il Re Leopardo, morto eroicamente in battaglia con il Serpente dello Zodiaco, era destinato a essere Sol con le sue macchie solari, che soffriva sull'albero delle stelle. ",shape=box,color=blue];
		j1447904151 -> j2864346600;
	j2864346600 [label="Introdurre la religione nei libri per bambini è come nascondere una medicina dentro delle caramelle. DITELO E BASTA.",shape=box,color=blue];
		j2864346600 -> j439147820;
		j2864346600 -> j2890362274;
	j439147820 [label="Non ho mai sentito parlare di Rudolfo, il Re Leopardo.",shape=box,color=red];
		j439147820 -> j1234967321;
	j2890362274 [label="Un giorno vorrei ascoltare la storia di Rudolfo, il Re Leopardo.",shape=box,color=red];
		j2890362274 -> j247474757;
	j1234967321 [label="Posso morire felice sapendo che quei libri verranno dimenticati. Evviva il futuro!",shape=box,color=blue];
		j1234967321 -> j4125230120;
	j247474757 [label="Ugh. Per favore, no. Non è una vera leggenda, è una serie terribile di libri per bambini.",shape=box,color=blue];
		j247474757 -> j1345566953;
	j4125230120 [label="Però hai delle storie da raccontare, no?",shape=box,color=blue];
		j4125230120 -> j3858085378;
	j1345566953 [label="Se vuoi, potrei raccontarti alcune delle nostre vere leggende.",shape=box,color=blue];
		j1345566953 -> j97851702;
		j1345566953 -> j706360026;
	j3858085378 [label="Tante. Anche i Tenno e i Warframe hanno storie. ",shape=box,color=red];
		j3858085378 -> j441087470;
	j97851702 [label="Mi piacerebbe.",shape=box,color=red];
		j97851702 -> j3280068107;
	j706360026 [label="Non adesso. [Fine.]",shape=box,color=red];
		j706360026 -> "Chat ends.";
	j441087470 [label="C'è questo tizio, Drusus Leverian. È tipo un curatore di museo. Tiene traccia di tutte le leggende dei Warframe.",shape=box,color=red];
		j441087470 -> j2866273015;
	j3280068107 [label="Vediamo. C'è Arthur, naturalmente. Il caro vecchio Arthur. L'originale. Il migliore della Britannia.",shape=box,color=blue];
		j3280068107 -> j3350590180;
	j2866273015 [label="Beh, questo mi ha lasciata perplessa. Tu vieni dal lontano futuro, ma sei anche stato fuori dal ciclo di Duviri per così tanti anni che gli eventi accaduti durante la tua vita hanno avuto il tempo di diventare miti?",shape=box,color=blue];
		j2866273015 -> j3743961460;
	j3350590180 [label="Poi c'è la giovane Loxley. La adoravo. Volevo essere lei. Era il modello per un certo tipo di giovane donna.",shape=box,color=blue];
		j3350590180 -> j3887603088;
	j3743961460 [label="Mi stanno venendo le vertigini. ",shape=box,color=blue];
		j3743961460 -> j2076136559;
		j3743961460 -> j3476831949;
	j3887603088 [label="Qual è la sua storia?",shape=box,color=red];
		j3887603088 -> j3015938220;
	j2076136559 [label="Fidati. So come ti senti.",shape=box,color=red];
		j2076136559 -> j685236522;
	j3476831949 [label="Raccontami allora dei tuoi miti.",shape=box,color=red];
		j3476831949 -> j4086800401;
	j3015938220 [label="Ok. Versione riassunta: era la figlia di un nobile dei tempi antichi, quando la terra soffriva sotto un re crudele.",shape=box,color=blue];
		j3015938220 -> j30811422;
	j685236522 [label="Anche tu sei un personaggio di una favola, non è vero?",shape=box,color=blue];
		j685236522 -> j3465534719;
	j4086800401 [label="Cavolo. Se inizio, staremo qui tutto l'anno. ",shape=box,color=blue];
		j4086800401 -> j3280068107;
	j30811422 [label="Suo fratello era stato ucciso in un combattimento in terre straniere, ma la notizia non era ancora arrivata, così quando la povera gente oppressa ebbe bisogno di un eroe, lei si fece avanti.",shape=box,color=blue];
		j30811422 -> j2215480408;
	j3465534719 [label="Intrappolato in un regno magico dove il tempo scorre in modo diverso, convocato nel luogo in cui eri più necessario...",shape=box,color=blue];
		j3465534719 -> j3528673322;
	j2215480408 [label="Check boolean EleanorHeroVirus",color=orange];
		j2215480408 -> j3145055699 [label=true,color=green];
		j2215480408 -> j3659140846 [label=false,color=brown];
	j3528673322 [label="Tutto ciò di cui abbiamo bisogno per concludere la tua storia è un interesse amoroso o un mostro sbavante e disumano.",shape=box,color=blue];
		j3528673322 -> j2806916032;
	j3145055699 [label="Pensavo che odiassi gli eroi.",shape=box,color=red];
		j3145055699 -> j2802796373;
	j3659140846 [label="Ora, di solito non sopporto le figure degli eroi. Sono tossiche. Infettive e, per questo motivo, pericolose.",shape=box,color=blue];
		j3659140846 -> j3786266236;
	j2806916032 [label="(O un interesse amoroso sbavante e disumano, dipende dai tuoi gusti.)",shape=box,color=blue];
		j2806916032 -> j1122016040;
		j2806916032 -> j3181002991;
	j2802796373 [label="Lei ha un lasciapassare. Taci.",shape=box,color=blue];
		j2802796373 -> j2474909835;
	j3786266236 [label="La giovane Loxley è diversa.",shape=box,color=blue];
		j3786266236 -> j2855165626;
	j1122016040 [label="Ho sempre trovato i mostri più interessanti in ogni caso. Gli eroi sono noiosi. Prevedibili. ...Mondani.",shape=box,color=red];
		j1122016040 -> j2482267576;
	j3181002991 [label="Mm, non so se i mostri siano davvero il mio tipo.",shape=box,color=red];
		j3181002991 -> j4180369021;
	j2474909835 [label="Dicevo. Somigliava molto a suo fratello, così si vestì con i suoi vecchi vestiti e fece finta di essere lui, e andò a guidare una rivoluzione. Se qualcuno si fosse avvicinato troppo avrebbe potuto notare che era una ragazza, così iniziò a indossare un cappuccio. Divenne il suo tratto distintivo.",shape=box,color=blue];
		j2474909835 -> j959632907;
	j2855165626 [label="Boolean EleanorHeroVirus is now true.",color=orange];
		j2855165626 -> j959632907;
	j2482267576 [label="Boolean EleanorFlirt is now true.",color=orange];
		j2482267576 -> j1759309534;
	j4180369021 [label="E va bene così, onestamente.",shape=box,color=blue];
		j4180369021 -> j1307918486;
	j959632907 [label="Era una bomba con arco e frecce. Lei e la sua ciurma divennero questa amata banda di fuorilegge, derubando gli esattori delle tasse del re e dando i soldi a chi ne aveva più bisogno.",shape=box,color=blue];
		j959632907 -> j980356304;
	j1759309534 [label="+15 Chemistry",color=orange];
		j1759309534 -> j553342519;
	j1307918486 [label="I mostri sono abituati a stare da soli.",shape=box,color=blue];
		j1307918486 -> j555683705;
	j980356304 [label="Sembra una persona che si sarebbe guadagnata il rispetto di tutti. Perché avrebbe dovuto fingere di essere suo fratello?",shape=box,color=red];
		j980356304 -> j2120915094;
	j553342519 [label="La mostruosità è negli occhi di chi guarda, amico mio.",shape=box,color=blue];
		j553342519 -> j2234733259;
	j555683705 [label="Siamo dei sopravvissuti.",shape=box,color=blue];
		j555683705 -> "Chat ends.";
	j2120915094 [label="Questa è storia per voi. A quei tempi, gli uomini presumibilmente combattevano tutti, quindi comandavano anche. La gente si aspettava di ricevere ordini dagli uomini. Lei aveva bisogno di attingere alla reputazione del fratello per fare ciò che doveva fare. Era quello o la rovina.",shape=box,color=blue];
		j2120915094 -> j3366393623;
		j2120915094 -> j1675899643;
		j2120915094 -> j1704970849;
	j2234733259 [label="Se sei sinceramente attratto dalle qualità che ritieni \"mostruose\" in un altro, allora faresti meglio ad assicurarti che condivida la tua scelta di etichette. Altrimenti, commetti la più grossolana feticizzazione.",shape=box,color=blue];
		j2234733259 -> j98080748;
	j3366393623 [label="Cosa le è successo alla fine?",shape=box,color=red];
		j3366393623 -> j2228466161;
	j1675899643 [label="Sono davvero contento che nel futuro abbiamo risolto quel problema.",shape=box,color=red];
		j1675899643 -> j2625790991;
	j1704970849 [label="Non sono sicuro che nel futuro abbiamo risolto *completamente* questo problema...",shape=box,color=red];
		j1704970849 -> j3391405695;
	j98080748 [label="Mi definirei un mostro, per esempio? Sì. Con entusiasmo. Accetto il titolo con cui altri mi condannerebbero, e ne sono felice. Nel caso te lo stessi chiedendo.",shape=box,color=blue];
		j98080748 -> "Chat ends.";
	j2228466161 [label="+20 Chemistry",color=orange];
