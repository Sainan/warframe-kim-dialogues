		j2625790991 -> j3767063134;
	j3391405695 [label="+20 Chemistry",color=orange];
		j3391405695 -> j2781584318;
	j3492301504 [label="Dipende quale versione della storia leggi. O è stata chiamata direttamente alla Hall of Heroes, o ha sposato la figlia ribelle del Re, o è rimasta intrappolata in una quercia dove dorme ancora oggi in attesa che la Britannia abbia bisogno.",shape=box,color=blue];
		j3492301504 -> j2100601724;
	j3767063134 [label="Anch'io.",shape=box,color=blue];
		j3767063134 -> j3466294591;
	j2781584318 [label="No, se quel personaggio di Ballas è un esempio, non ci siete riusciti.",shape=box,color=blue];
		j2781584318 -> j3862490827;
	j2100601724 [label="Da qui il detto. Se le cose vanno male, come durante la guerra, è tradizione una scrollata di spalle e dire \"Beh, non sta andando COSÌ male. La giovane Loxley non si è ancora fatta viva\".",shape=box,color=blue];
		j2100601724 -> "Chat ends.";
	j3466294591 [label="Per gli dei, mi piacerebbe vedere la tua era con i miei occhi. Forse potresti farmi tornare di nascosto in una di quelle bare ghiacciaie e potrei dormire per qualche millennio.",shape=box,color=blue];
		j3466294591 -> "Chat ends.";
	j3862490827 [label="Quanto ancora ci vorrà? Per l'amor di Sol.",shape=box,color=blue];
		j3862490827 -> "Chat ends.";
