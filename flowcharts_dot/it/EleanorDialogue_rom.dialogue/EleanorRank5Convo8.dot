		j2391583421 -> j3198212800 [label=true,color=green];
		j2391583421 -> j935533696 [label=false,color=brown];
	j4198441335 [label="Lo mantengo legato a me da anni. E così anche Arthur. Ogni anno facciamo un rituale in sua memoria. Ma quest'anno no. Non potevo affrontarlo.",shape=box,color=blue];
		j4198441335 -> j3476846663;
	j3198212800 [label="Per... rendere breve una storia molto lunga e tragica... Christopher è morto a causa di una bomba sul ciglio della strada prima che ci sposassimo.",shape=box,color=blue];
		j3198212800 -> j2719425685;
	j935533696 [label="È ora che ti parli di Christopher.",shape=box,color=blue];
		j935533696 -> j4069560867;
	j3476846663 [label="Non so se i fantasmi esistano, ma potrebbero anche esistere, perché ho lasciato che lui lo diventasse.",shape=box,color=blue];
		j3476846663 -> j1397398611;
	j2719425685 [label="Forse ti racconterò tutta la storia un altro giorno. O forse non lo farò.",shape=box,color=blue];
		j2719425685 -> j1904485863;
	j4069560867 [label="Il migliore amico di Arthur. Eravamo fidanzati. Pensavo di essere innamorata di lui.",shape=box,color=blue];
		j4069560867 -> j3247367802;
	j1397398611 [label="Non essere così dura con te stessa. Eri innamorata di quel ragazzo.",shape=box,color=red];
		j1397398611 -> j3785397084;
	j1904485863 [label="Ti sto dicendo tutto questo perché...",shape=box,color=blue];
		j1904485863 -> j816290729;
	j3247367802 [label="Boolean EleanorChristopher1 is now true.",color=orange];
		j3247367802 -> j3198212800;
	j3785397084 [label="Oh, amico. Ho CREDUTO di esserlo. Gli volevo bene, ma non sapevo nemmeno se ero INNAMORATA di lui o meno.",shape=box,color=blue];
		j3785397084 -> j2345006914;
	j816290729 [label="Boolean EleanorChristopher2 is now true.",color=orange];
		j816290729 -> j4198441335;
	j2345006914 [label="Sembra terribile, vero? Ma è così. All'epoca, non sembrava importante. Poi è morto, e questo è diventato il tema della nostra storia.",shape=box,color=blue];
		j2345006914 -> j341692739;
	j341692739 [label="Tutto ciò che ammiravo di lui è stato oscurato dal modo in cui è morto. Non riesco a pensare a lui senza che un nodo mi stringa lo stomaco.",shape=box,color=blue];
		j341692739 -> j3870074040;
	j3870074040 [label="Non è giusto. PER LUI. Lui merita di meglio.",shape=box,color=blue];
		j3870074040 -> j2083527574;
		j3870074040 -> j478828789;
	j2083527574 [label="Capisco. Assolutamente. Ho scelto di ricordare i miei genitori nel modo in cui meritavano.",shape=box,color=red];
		j2083527574 -> j3646322747;
	j478828789 [label="Quello che è successo è stato terribile. Non c'è modo di evitarlo. Ma non possiamo lasciare che questa sia l'unica cosa a cui aggrapparci.",shape=box,color=red];
		j478828789 -> j3646322747;
	j3646322747 [label="Allora perché mi sento così in colpa?",shape=box,color=blue];
		j3646322747 -> j2095135406;
	j2095135406 [label="Perché ho la sensazione che lasciarlo andare sia la stessa cosa che gettarlo via?",shape=box,color=blue];
		j2095135406 -> j2900433636;
		j2095135406 -> j2790259466;
	j2900433636 [label="Perché l'hai trascinato per così tanto tempo che non sai come fermarti.",shape=box,color=red];
		j2900433636 -> j1944101293;
	j2790259466 [label="Perché hai paura che nessun altro sia in grado di amarti.",shape=box,color=red];
		j2790259466 -> j1415583883;
	j1944101293 [label="Per gli dei. Hai maledettamente ragione, lo sai.",shape=box,color=blue];
		j1944101293 -> j1521805699;
	j1415583883 [label="Check boolean EleanorDating",color=orange];
		j1415583883 -> j1299519343 [label=true,color=green];
		j1415583883 -> j3429855704 [label=false,color=brown];
	j1521805699 [label="Non siamo in grado di gestire questa situazione. Imbottigliamo tutto. Ci facciamo coraggio e fischiettiamo in trincea. Ci permette di andare avanti, proprio come con i nostri genitori. Ma non sappiamo come fermarci.",shape=box,color=blue];
