		j2246039095 -> j1924199693;
	j3784002574 [label="Forse è quello che è successo alla famiglia Entrati. Hanno vissuto per anni proprio sopra il suo laboratorio, senza saperlo. Quando il bambino li ha trovati, tra loro erano distanti, si crogiolavano nel rancore.",shape=box,color=red];
		j3784002574 -> j1924199693;
	j1924199693 [label="L'ho visto nella tua testa. Albrecht e Loid. Madre e Padre. Tu e i tuoi genitori. Rusalka e Viktor, per quanto ne sappiamo. E noi sei, ovviamente.",shape=box,color=blue];
		j1924199693 -> j3814669579;
	j3814669579 [label="Ciò che fa questa cosa è arida. Perversa. Ma c'è una motivazione dietro, da qualche parte.",shape=box,color=blue];
		j3814669579 -> j3784236486;
		j3814669579 -> j3650344698;
	j3784236486 [label="Penso che tu abbia ragione. Sulla Zariman, i genitori sono stati i primi a perdere la testa. Forse gli Holdfast hanno resistito così a lungo perché nessuno di loro aveva figli.",shape=box,color=red];
		j3784236486 -> j2452759049;
	j3650344698 [label="Non mi piace fingere di capire delle creature che sono fondamentalmente *dei*. Questa cosa sta cercando di capire le emozioni. Ma sta giocando malissimo con i suoi giocattoli.",shape=box,color=red];
		j3650344698 -> j4197775850;
	j2452759049 [label="Lascia che provi a mettersi tra noi. Per Sol, la finirò io. Troverò un modo.",shape=box,color=blue];
		j2452759049 -> j661583948;
	j4197775850 [label="Teoria ammirevole, ma non ne siamo CERTI, no?",shape=box,color=blue];
		j4197775850 -> j966586599;
	j661583948 [label="Ci sono poteri nell'Universo contro cui quella cosa non può resistere. Io scelgo di crederci con ogni atomo di me stessa.",shape=box,color=blue];
		j661583948 -> j4128448905;
	j966586599 [label="Se è così, allora è non ha colpe. Anzi, è quasi fanciullesca. Ma questo non rispecchia ciò che sappiamo. È malevole.",shape=box,color=blue];
		j966586599 -> "Chat ends.";
	j4128448905 [label="Se mi ingoiasse per intero, mi infilerei nella sua gola per soffocarla.",shape=box,color=blue];
		j4128448905 -> j205755343;
		j4128448905 -> j3122106933;
	j205755343 [label="Questo è un bel modo di morire.",shape=box,color=red];
