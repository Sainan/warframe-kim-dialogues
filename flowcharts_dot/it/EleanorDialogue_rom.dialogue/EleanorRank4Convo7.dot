		j760592049 -> j418124630;
	j418124630 [label="Stavo pensando ai miei. Lo faccio spesso, in realtà. Mi siedo e mi chiedo cosa penserebbero mamma e papà di come sono finiti i loro figli.",shape=box,color=blue];
		j418124630 -> j1297835514;
