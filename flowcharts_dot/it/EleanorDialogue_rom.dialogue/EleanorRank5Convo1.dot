		j388073601 -> j1899146239;
	j3771460485 [label="+5 Chemistry",color=orange];
		j3771460485 -> j3754511236;
	j1899146239 [label="Lei è la Morte. E anche uno degli Aspetti di Lua, a seconda della tradizione che segui. È quella che viene e ti mette a letto una volta che la giornata frenetica della Vita è finita, ti dà il bacio della buonanotte e canta per farti addormentare.",shape=box,color=blue];
		j1899146239 -> j1449380389;
		j1899146239 -> j3361574372;
	j3671470711 [label="Ah. Stai parlando con la nostra cara Lettie, vedo. Sì.",shape=box,color=blue];
		j3671470711 -> j1899146239;
	j3754511236 [label="Sì.",shape=box,color=blue];
		j3754511236 -> j1899146239;
	j1449380389 [label="Non stavi scherzando quando dicevi di essere malinconica, vero?",shape=box,color=red];
		j1449380389 -> j851245897;
	j3361574372 [label="Questa sarà una di quelle conversazioni \"mettiti comodo, Drifter, ci vorrà un minuto\", non è vero?",shape=box,color=red];
		j3361574372 -> j1930895294;
	j851245897 [label="Amico. Tutto ciò che Entrati ci ha dato, il potere, la forza, i culi corazzati… Lo scambierei per una briciola di rassicurazione che la morte non è la fine.",shape=box,color=blue];
		j851245897 -> j2184076628;
	j1930895294 [label="... doveva essere così.",shape=box,color=blue];
		j1930895294 -> j2359218662;
	j2184076628 [label="Ci ho provato. Non mi sento a mio agio ad ammetterlo, ma a volte quando andavamo in pattuglia e c'erano dei combattimenti, guardo nella mente dei morenti e cerco di attraversare con loro il processo. Se c'è una luce alla fine del tunnel, voglio solo vederla.",shape=box,color=blue];
		j2184076628 -> j3724280596;
		j2184076628 -> j2300661081;
	j2359218662 [label="Lascia perdere. Sono sicura che hai cose più importanti da fare.",shape=box,color=blue];
		j2359218662 -> "Chat ends.";
	j3724280596 [label="Eleanor, questo è macabro. Non ne hai il diritto.",shape=box,color=red];
		j3724280596 -> j1454091578;
	j2300661081 [label="Cerchi in qualche modo di confortarli?",shape=box,color=red];
		j2300661081 -> j3982843678;
	j1454091578 [label="Lo so. LO SO.",shape=box,color=blue];
		j1454091578 -> j2269966137;
	j3982843678 [label="L'ho fatto, una volta. Che male c'è? Stanno morendo comunque. Era questo giovane soldato Scaldra. Stava venendo verso di me e le ho sparato al petto.",shape=box,color=blue];
		j3982843678 -> j3178229762;
	j2269966137 [label="Cerco di convincere che stessa che stavo solo cercando di aiutare, ma la verità è che volevo solo vedere. Hai ragione. Era macabro.",shape=box,color=blue];
		j2269966137 -> "Chat ends.";
	j3178229762 [label="Mi inginocchiai al suo fianco mentre lei soffocava con il suo sangue e cercai di dirle che andava tutto bene, che il dolore stava volgendo al termine, che presto sarebbe stata insieme alla sua famiglia che se n'era andata prima, tutte quelle cose placide e pacificanti a cui pensi quando c'è da celebrare una cerimonia funebre.",shape=box,color=blue];
		j3178229762 -> j1526480485;
	j1526480485 [label="Era terrorizzata. Ciò che restava del suo sé cosciente si stava agitando come un topo in una cisterna d'acqua. Quando mi ha sentita nella sua mente lì con lei, ha cercato di aggrapparsi. Mi ha implorato di aiutarla. Sotto tutta questa c'era… Non so nemmeno come la chiameresti…",shape=box,color=blue];
		j1526480485 -> j1079767837;
		j1526480485 -> j1612609811;
		j1526480485 -> j519280776;
	j1079767837 [label="Speranza?",shape=box,color=red];
		j1079767837 -> j1169438356;
	j1612609811 [label="Paura?",shape=box,color=red];
		j1612609811 -> j1169438356;
	j519280776 [label="Negazione?",shape=box,color=red];
		j519280776 -> j1169438356;
	j1169438356 [label="Negazione. Non credeva di poter morire. E perché avrebbe dovuto? Era sempre stata lì. Come poteva la morte portarla via ora?",shape=box,color=blue];
		j1169438356 -> j3019769634;
	j3019769634 [label="Ogni giorno della tua vita, dai per scontato TE STESSO. Sei lì quando ti svegli e sei lì quando dormi. Vivi tutta la tua vita in una casa fatta di Te. Non esci mai, perché non puoi.",shape=box,color=blue];
		j3019769634 -> j2943859432;
	j2943859432 [label="Poi un giorno senti la casa che viene abbattuta.",shape=box,color=blue];
		j2943859432 -> j1203137715;
		j2943859432 -> j2470643100;
	j1203137715 [label="Perché non l'hai aiutata?",shape=box,color=red];
		j1203137715 -> j994200863;
	j2470643100 [label="Sei rimasta con lei fino alla fine?",shape=box,color=red];
		j2470643100 -> j955066849;
	j994200863 [label="Cosa avresti voluto che facessi? Chiamassi Lettie? Guarissi la ferita? Tutto perché ho visto da vicino cosa stava passando questa?",shape=box,color=blue];
		j994200863 -> j1403591375;
	j955066849 [label="Sì. Ero spaventata a morte. Mentre lei si sentiva scivolare oltre il bordo, ero sicura che sarei stata trascinata giù con lei. Potevo sentire questa risacca che mi tirava.",shape=box,color=blue];
		j955066849 -> j3629707712;
	j1403591375 [label="Con gli Scaldra è uccidere o essere uccisi. Non ci offrono il lusso della pietà.",shape=box,color=blue];
		j1403591375 -> "Chat ends.";
	j3629707712 [label="Ma dovevo restare. Non potevo abbandonarla a morire da sola.",shape=box,color=blue];
		j3629707712 -> j3472279816;
	j3472279816 [label="Anche se immagino che alla fine l'abbia fatto. Non sono riuscita a fare quell'ultimo passo con lei.",shape=box,color=blue];
		j3472279816 -> j1080919556;
		j3472279816 -> j2313012190;
	j1080919556 [label="Cosa hai imparato da questa esperienza?",shape=box,color=red];
		j1080919556 -> j897864561;
	j2313012190 [label="Quindi hai visto quello che speravi di vedere?",shape=box,color=red];
		j2313012190 -> j1053374129;
	j897864561 [label="Non ho imparato nulla.",shape=box,color=blue];
		j897864561 -> j3815364662;
	j1053374129 [label="No.",shape=box,color=blue];
		j1053374129 -> j3815364662;
	j3815364662 [label="Lei sì, però. Vide il volto di sua madre, i bambini con cui andava a scuola, il suo gatto, un cugino che era annegato. Ma non erano presenze, o spiriti. Solo ricordi, forzati a emergere da un cervello morente alla disperata ricerca di qualcosa a cui aggrapparsi.",shape=box,color=blue];
		j3815364662 -> j2407587757;
	j2407587757 [label="Poi la casa della sua identità cominciò a sgretolarsi intorno a lei. Proprio prima di morire, spalancò gli occhi. Penso che in quel secondo, per la prima volta nella sua vita, capì veramente cosa fosse la morte.",shape=box,color=blue];
		j2407587757 -> j722981646;
		j2407587757 -> j1500636653;
	j722981646 [label="La persona morente capisce cos'è la morte. Che intuizione, Eleanor.",shape=box,color=red];
		j722981646 -> j85716200;
	j1500636653 [label="Cosa intendi? Tutti sanno cosa è la morte.",shape=box,color=red];
		j1500636653 -> j812010394;
	j85716200 [label="Guarda. La prossima volta che senti il bisogno di criticarmi per le mie stronzate, non sentirti obbligato a sopportarle prima.",shape=box,color=blue];
		j85716200 -> j1766513583;
	j812010394 [label="No. Pensiamo di sapere di cosa si tratta, ma tutte le nostre esperienze provengono dall'esterno.",shape=box,color=blue];
		j812010394 -> j2667679032;
	j1766513583 [label="Nessuno ti obbliga a parlare con me. Ciao.",shape=box,color=blue];
		j1766513583 -> "Chat ends.";
	j2667679032 [label="Per tutta la vita è stata come una donna che vive in una casa senza luci, e tutto ciò che ha è una torcia elettrica. Illumina la luce della sua consapevolezza in un punto o nell'altro. Un ricordo, un compito, un'interazione.",shape=box,color=blue];
		j2667679032 -> j3776921604;
	j3776921604 [label="Si spostava da una stanza all'altra, esplorava le cantine nel sonno, ma era sempre un processo chiuso e finito. Mi segui?",shape=box,color=blue];
		j3776921604 -> j1538104611;
		j3776921604 -> j1105651894;
	j1538104611 [label="Non proprio, ma continua.",shape=box,color=red];
		j1538104611 -> j2586124162;
	j1105651894 [label="Sì.",shape=box,color=red];
		j1105651894 -> j2586124162;
	j2586124162 [label="Poi, quando la casa fu demolita e la sua persona cadde a pezzi intorno a lei, all'improvviso la torcia non stava più illuminando un muro, un libro o un divano. Stava illuminando il cielo infinito. La sua consapevolezza ardeva nell'infinito senza l'ostacolo di se stessa che la ostacolava.",shape=box,color=blue];
		j2586124162 -> j2345930080;
		j2586124162 -> j3699865479;
		j2586124162 -> j3461847370;
	j2345930080 [label="Ma la consapevolezza muore quando muore il corpo. Ciò che hai sperimentato è stato solo uno spasmo nel suo cervello morente.",shape=box,color=red];
		j2345930080 -> j2677099204;
	j3699865479 [label="Credi che la \"consapevolezza\" sia ciò che siamo veramente e che il resto sia solo un guscio?",shape=box,color=red];
		j3699865479 -> j1412989608;
	j3461847370 [label="Wow. Così profondo. Hai sparato a qualcuno e poi hai violato la sua privacy.",shape=box,color=red];
		j3461847370 -> j85716200;
	j2677099204 [label="Ho provato a ripetermelo molte volte.",shape=box,color=blue];
		j2677099204 -> j2344252242;
	j1412989608 [label="+5 Chemistry",color=orange];
		j1412989608 -> j3647262003;
	j2344252242 [label="Non riesco a credere che non sia stato altro che lo spegnimento del cervello. È stato come se l'anima si fosse liberata.",shape=box,color=blue];
		j2344252242 -> "Chat ends.";
	j3647262003 [label="So che non ha alcun senso. Mi sembra così contraddittorio, l'idea che siamo prima un punto di vista e poi una personalità. So solo che è così che mi sono sentita con un'anima umana mentre passava dalla vita alla morte.",shape=box,color=blue];
		j3647262003 -> j3477201110;
	j3477201110 [label="Cosa ne pensi di tutto questo?",shape=box,color=red];
		j3477201110 -> j3134670368;
	j3134670368 [label="Confortata? Mi piace l'idea che qualcosa di noi sopravviva. Mi piace pensare che la morte non significhi essere cancellati dall'universo, ma che venga cancellato tutto ciò che ci separa dall'universo.",shape=box,color=blue];
		j3134670368 -> j4153605670;
	j4153605670 [label="Ma ciò che ci rende NOI non è solo la consapevolezza grezza. È memoria, emozione, opinione, tutti i tipi di strati. Quando si perde tutto questo, si perde una persona reale, anche se la scintilla in qualche modo sopravvive.",shape=box,color=red];
		j4153605670 -> j3077496351;
	j3077496351 [label="Lo so.",shape=box,color=blue];
		j3077496351 -> j3198160612;
	j3198160612 [label="Onestamente, è per questo che volevo parlarti dei fantasmi del void, dei Cephalon e di tutte quelle cose del tuo tempo. Devo sapere se avremo la possibilità di comunicare con le persone che abbiamo perso.",shape=box,color=blue];
		j3198160612 -> j66811442;
	j66811442 [label="Una comunicazione reale, non un ologramma di scarsa qualità. Il tipo di comunicazione che ti permette di sapere definitivamente cosa pensava la persona.",shape=box,color=blue];
		j66811442 -> j1956095743;
		j66811442 -> j1126862512;
	j1956095743 [label="No, non proprio. Anche nel futuro, le persone che abbiamo perso sono... nel bene e nel male? Semplicemente... scomparse.",shape=box,color=red];
		j1956095743 -> j3009406677;
	j1126862512 [label="Eleanor, vieni al dunque. Chi è che hai perso?",shape=box,color=red];
		j1126862512 -> j3216018410;
	j3009406677 [label="Capisco. Forse ci sono delle porte che non potremo mai aprire. Forse è meglio così.",shape=box,color=blue];
		j3009406677 -> "Chat ends.";
	j3216018410 [label="Il suo nome era Christopher. Era il migliore amico di Arthur.",shape=box,color=blue];
		j3216018410 -> j179335973;
	j179335973 [label="Eravamo fidanzati. Pensavo di essere innamorata di lui.",shape=box,color=blue];
		j179335973 -> j3570759725;
	j3570759725 [label="Boolean EleanorChristopher1 is now true.",color=orange];
		j3570759725 -> j2387536173;
	j2387536173 [label="+10 Chemistry",color=orange];
		j2387536173 -> j2398731232;
	j2398731232 [label="Mi dispiace. Non posso farcela. Ci sentiamo un'altra volta.",shape=box,color=blue];
