		j2249897526 -> j1183862659;
	j347844492 [label="Ah. Capisco. Sei l'araldo inviato dal Re Thunderbolt per arruolare la bella Eleanor. Senza dubbio mi aspetta ogni sorta di furfanteria. Storie fantastiche e così via.",shape=box,color=blue];
