digraph {
	"EleanorRank5Convo6 begins." [color=orange];
		"EleanorRank5Convo6 begins." -> j1771116600;
	j1771116600 [label="Potresti notare che il Centro Commerciale sembra un po' diverso stamattina.",shape=box,color=blue];
		j1771116600 -> j1630481798;
	j1630481798 [label="Niente di troppo assurdo ed evidente. Solo qualche sorriso in più su qualche volto. Una risata qua e là. Un pizzico di felicità nell'aria.",shape=box,color=blue];
		j1630481798 -> j3174668851;
		j1630481798 -> j558201707;
	j3174668851 [label="È merito tuo, giusto?",shape=box,color=red];
		j3174668851 -> j551676434;
	j558201707 [label="A DOPO. [Fine.]",shape=box,color=red];
		j558201707 -> "Chat ends.";
	j551676434 [label="È il mio modo di ricambiare. Come già sai, un telepate può inviare e ricevere. Non deve essere necessariamente un getto d'acqua diretto a una persona. Può essere più simile a una pioggerellina sottile spruzzata su un'area.",shape=box,color=blue];
		j551676434 -> j735825298;
	"Chat ends." [color=orange];
	j735825298 [label="Hai degli irrigatori per il prato dove vivi?",shape=box,color=blue];
		j735825298 -> j4115959484;
		j735825298 -> j1040279096;
	j4115959484 [label="Abbiamo VERD-IE e droni simili. Si prendevano cura delle piante sulla Zariman. È questo il genere di cose che intendi?",shape=box,color=red];
		j4115959484 -> j2558607051;
	j1040279096 [label="Irriga...tori?",shape=box,color=red];
		j1040279096 -> j2682850687;
	j2558607051 [label="Diciamo di sì.",shape=box,color=blue];
		j2558607051 -> j3211164112;
	j2682850687 [label="Sì, mio caro, irrigatori. Nebulizzatori d'acqua per mantenere il prato bello e umido.",shape=box,color=blue];
		j2682850687 -> j1853991920;
	j3211164112 [label="Quando ero bambina adoravo gli irrigatori da giardino. Erano magici. Quando il sole splendeva luminoso, in essi si potevano vedere gli arcobaleni. Ci correvi in ​​mezzo e sentivi un delizioso brivido in tutto il corpo.",shape=box,color=blue];
		j3211164112 -> j2077810961;
	j1853991920 [label="Sono ottimi anche per trattenerci il volto di tuo fratello finché non urla.",shape=box,color=blue];
		j1853991920 -> j2077810961;
	j2077810961 [label="\"La felicità è un irrigatore da giardino\". Scrivilo su un biglietto d'auguri.",shape=box,color=blue];
		j2077810961 -> j734672547;
	j734672547 [label="Sembri di buon umore oggi. Sono felice di ciò.",shape=box,color=red];
		j734672547 -> j2297760853;
	j2297760853 [label="Lo sono. E non vedo perché dovrei tenermelo per me. Quindi sto irradiando questa felicità per tutto il Centro Commerciale.",shape=box,color=blue];
		j2297760853 -> j2572568333;
		j2297760853 -> j3376682793;
		j2297760853 -> j2836416329;
	j2572568333 [label="Non è un po' come drogare la gente?",shape=box,color=red];
		j2572568333 -> j1847301319;
	j3376682793 [label="Trasmettere uno stato d'animo positivo? Perché no. Non vedo come sia diverso dal suonare della musica.",shape=box,color=red];
		j3376682793 -> j1354753209;
	j2836416329 [label="Quindi oggi potrei vedere addirittura Arthur sorridere?",shape=box,color=red];
		j2836416329 -> j2003359935;
	j1847301319 [label="... fantastico. Io sto cercando di essere la fata segreta della felicità che sparge allegria, e invece vengo accusata di essere il folletto malvagio che droga le persone. Il mio umore è appena precipitato.",shape=box,color=blue];
		j1847301319 -> j1479822166;
	j1354753209 [label="Oh! Bella analogia!",shape=box,color=blue];
		j1354753209 -> j1441697507;
	j2003359935 [label="Forse. Spero di sì. Ha dovuto sopportare fin troppo.",shape=box,color=blue];
		j2003359935 -> j3816612647;
	j1479822166 [label="Va bene. La smetto.",shape=box,color=blue];
		j1479822166 -> "Chat ends.";
	j1441697507 [label="Se Aoi riesce a influenzare l'umore attraverso la musica, io posso farlo attraverso le vibrazioni, giusto?",shape=box,color=blue];
		j1441697507 -> j477333674;
	j3816612647 [label="Sono cambiate così tante cose da quando sei arrivato. Non intendo solo tra te e me. Intendo tutto. Anche il Centro Commerciale.",shape=box,color=blue];
		j3816612647 -> j477333674;
	j477333674 [label="Sono felice che Aoi mi abbia convinta a restare. Che Sol la benedica.",shape=box,color=blue];
		j477333674 -> j296662146;
		j477333674 -> j1297662810;
	j296662146 [label="Aspetta. Cosa ha fatto?",shape=box,color=red];
		j296662146 -> j359264977;
	j1297662810 [label="Frena un secondo. Mi è sfuggito qualcosa.",shape=box,color=red];
		j1297662810 -> j359264977;
	j359264977 [label="Non te l'ho detto?",shape=box,color=blue];
		j359264977 -> j542190988;
	j542190988 [label="Ah. Non è successo molto prima che tu arrivassi. Eravamo stati fuori tutto il giorno a cercare di estirpare un alveare Techrot. C'era una tensione omicida nell'aria. Di solito combattere affievolisce la nostra rabbia e siamo troppo esausti per scagliarci l'uno contro l'altro, ma quel giorno è stato diverso.",shape=box,color=blue];
		j542190988 -> j1148902689;
	j1148902689 [label="Ricordo che Arthur disse qualcosa del tipo \"domani dovremo rifare tutto da capo\" e la realizzazione di ciò ci colpì come una mazza. Nessuno proferì parola durante il viaggio di ritorno al Centro Commerciale.",shape=box,color=blue];
		j1148902689 -> j3842328178;
		j1148902689 -> j1712649092;
		j1148902689 -> j4104854384;
	j3842328178 [label="Wow. Ottimo modo per risollevare il morale della squadra, Arthur.",shape=box,color=red];
		j3842328178 -> j1906997833;
	j1712649092 [label="Sa essere davvero affascinante...",shape=box,color=red];
		j1712649092 -> j1906997833;
	j4104854384 [label="Ha un'enorme peso sulle spalle. Talvolta può essere eccessivo.",shape=box,color=red];
		j4104854384 -> j3451745171;
	j1906997833 [label="Penso che stesse solo cercando di farci concentrare sul lavoro. Ma io mi sono sentita devastata. Arthur e Aoi non parlavano, Amir stava innervosendo Quincy così tanto che cominciavo a temere per la sua vita, e Lettie mi lanciava occhiatacce ogni volta che ne aveva la possibilità.",shape=box,color=blue];
		j1906997833 -> j2555556707;
	j3451745171 [label="Sì. Di solito riesce a nasconderlo, anche quando è esausto. Immagino semplicemente non ne potesse più.",shape=box,color=blue];
		j3451745171 -> j513069907;
	j2555556707 [label="Quel giorno ho passato molto tempo a guardarmi allo specchio. Continuavo a immaginare il mio riflesso che mi afferrava per la gola e sibilava \"TU sei il problema. Se non fosse stato per te, niente di tutto questo sarebbe successo\".",shape=box,color=blue];
		j2555556707 -> j2921603218;
		j2555556707 -> j1416970506;
	j513069907 [label="Si era stancato nel tentativo di tenere unita la squadra.",shape=box,color=blue];
		j513069907 -> j1906997833;
	j2921603218 [label="Sembra che una volta tanto fossero tutti gli altri a essere entrati nella TUA testa.",shape=box,color=red];
		j2921603218 -> j3417268425;
	j1416970506 [label="Sembra il genere di sciocchezze che direbbe Lettie.",shape=box,color=red];
		j1416970506 -> j3281057390;
	j3417268425 [label="Oh, non erano loro. Ero io. Tutti i dubbi e l'odio per me stessa che avevo represso vennero a galla quel giorno.",shape=box,color=blue];
		j3417268425 -> j299900681;
	j3281057390 [label="Grazie. So che mi supporti e questo significa molto per me.",shape=box,color=blue];
		j3281057390 -> j3063512760;
	j299900681 [label="Ho pensato tra me e me: ciò di cui questo gruppo ha bisogno è un capro espiatorio.",shape=box,color=blue];
		j299900681 -> j2111850144;
	j3063512760 [label="Avevo sentito questa frase da Lettie così tante volte che ho finito per pensare che forse aveva ragione. Sto mettendo tutti in pericolo restando qui.",shape=box,color=blue];
		j3063512760 -> j929988769;
	j2111850144 [label="Un mostro. Qualcuno da incolpare.",shape=box,color=blue];
		j2111850144 -> j929988769;
	j929988769 [label="Quindi ho elaborato un piano. Mi sarei allontanata di notte, mi sarei nascosta in città in un posto dove Arthur non mi avrebbe mai trovata e, questa è la parte di cui ero più orgogliosa, avrei continuato ad aiutarli a distanza di sicurezza.",shape=box,color=blue];
		j929988769 -> j3311702850;
		j929988769 -> j1480741643;
	j3311702850 [label="Penso che ti avrebbe trovato. È parecchio determinato.",shape=box,color=red];
		j3311702850 -> j114346509;
	j1480741643 [label="Lo scenario più plausibile è che sarebbe morto nel tentativo di trovarti.",shape=box,color=red];
		j1480741643 -> j1556226288;
	j114346509 [label="Come? Amir non riesce a rintracciare la telepatia.",shape=box,color=blue];
		j114346509 -> j3894088041;
	j1556226288 [label="Stai cercando di farmi sentire in colpa?",shape=box,color=blue];
		j1556226288 -> j2554873071;
	j3894088041 [label="Sarei diventata una nobile esiliata. Mi sarei accovacciata sui tetti con aria terrorizzata e determinata. Avrei potuto informarli sull'accumulo di Techrot, indicare agli Hex dove si stavano posizionando gli Scaldra, persino agire come rete di comunicazione d'emergenza. Ogni volta che avrei salvato il culo a Lettie, glielo avrei fatto capire bene. Meschino? Sicuramente.",shape=box,color=blue];
		j3894088041 -> j3002332551;
	j2554873071 [label="Non disturbarti. Mi sento già abbastanza male.",shape=box,color=blue];
		j2554873071 -> "Chat ends.";
	j3002332551 [label="Avevo scelto il momento perfetto. Ho aspettato che Amir si addormentasse. Poi ho indossato un lungo cappotto e mi sono diretta verso il garage il più silenziosamente possibile.",shape=box,color=blue];
		j3002332551 -> j1468549999;
	j1468549999 [label="Ma il cancello di sicurezza si è abbassato da solo. Mi sono girata e c'era Aoi.",shape=box,color=blue];
		j1468549999 -> j3995517118;
	j3995517118 [label="Abbiamo parlato tutta la notte e pianto l'una nelle braccia dell'altra. Anche lei soffriva. Profondamente. Quando è sorto il sole, avevo deciso di restare.",shape=box,color=blue];
		j3995517118 -> j3234870588;
		j3995517118 -> j139603214;
		j3995517118 -> j3298323013;
	j3234870588 [label="Sei ancora in tempo per cambiare idea.",shape=box,color=red];
		j3234870588 -> j600668201;
	j139603214 [label="Aoi è meravigliosa. Sono felice che fosse lì per te.",shape=box,color=red];
		j139603214 -> j1317152988;
	j3298323013 [label="È stata una grande fortuna.",shape=box,color=red];
		j3298323013 -> j1317152988;
	j600668201 [label="Qualunque sia stato l'impulso grottesco che ti ha spinto a scrivere questo, spero che ti soffochi.",shape=box,color=blue];
		j600668201 -> "Chat ends.";
	j1317152988 [label="+20 Chemistry",color=orange];
		j1317152988 -> j224516665;
	j224516665 [label="Sì. Dovevamo essere qui tutti e sei alla vigilia di Capodanno. E tu, naturalmente. Lo sconosciuto alla porta che portava fortuna.",shape=box,color=blue];
		j224516665 -> "Chat ends.";
}