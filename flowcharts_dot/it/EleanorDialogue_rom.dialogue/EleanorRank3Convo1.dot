		j109542782 -> j3150043361;
	j1985551489 [label="Ho imparato a costruire muri attorno alla mia mente dopo quell'incidente. Lettie a volte trova delle medicine che possono spegnere del tutto il mio lato telepatico, solo per qualche ora. Ne ho una scorta, ma non le prendo a meno che non abbia davvero, davvero bisogno di dormire.",shape=box,color=blue];
		j1985551489 -> j3150043361;
