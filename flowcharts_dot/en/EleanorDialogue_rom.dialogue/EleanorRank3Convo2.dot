		j1003779126 -> j2709052952;
	j855131361 [label="There's so much we don't know.",shape=box,color=blue];
		j855131361 -> j1274308272;
	j785712334 [label="Just hold on to that belief. Let me feel it.",shape=box,color=blue];
		j785712334 -> j1145614632;
	j3830109319 [label="Thank you.",shape=box,color=blue];
		j3830109319 -> j783559194;
	j2709052952 [label="I'll keep fighting out of pure bloody stubbornness.",shape=box,color=blue];
		j2709052952 -> j2578349673;
	j1274308272 [label="After what's already happened this year, how are we supposed to make sane plans ever again?",shape=box,color=blue];
		j1274308272 -> j1234124827;
	j1145614632 [label="Please.",shape=box,color=blue];
		j1145614632 -> j595510079;
