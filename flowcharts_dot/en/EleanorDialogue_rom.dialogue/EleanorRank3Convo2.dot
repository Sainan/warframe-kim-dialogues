		j2541827282 -> j94011486;
	j3526897899 [label="Well, shit.",shape=box,color=blue];
		j3526897899 -> j2360395328;
	j94011486 [label="There’s a feeling there isn’t a word for. You look out on the world and you just KNOW, that all the ordinariness - buildings, cars, people, jobs, work, routine - is just a thin crust over a bottomless horror, and if people ever realized it they’d start screaming and never stop.",shape=box,color=blue];
		j94011486 -> j3489308515;
	j2360395328 [label="I'd just about managed to psych myself up to this, and now, what? we're not mentioning the Devil in case it summons him?",shape=box,color=blue];
		j2360395328 -> j123592170;
	j3489308515 [label="I fell through the crust.",shape=box,color=blue];
		j3489308515 -> j1830795358;
	j123592170 [label="If you hear screaming in the night, it's me.",shape=box,color=blue];
		j123592170 -> "Chat ends.";
	j1830795358 [label="It wasn’t emptiness. I could have borne that. It went on and on forever, but it wasn’t just cold space. There was something in there with me.",shape=box,color=blue];
		j1830795358 -> j3889901997;
		j1830795358 -> j2026436148;
		j1830795358 -> j1018714496;
	j3889901997 [label="An eye?",shape=box,color=red];
		j3889901997 -> j4012560882;
	j2026436148 [label="A man trapped in a wall?",shape=box,color=red];
		j2026436148 -> j4012560882;
	j1018714496 [label="What was it?",shape=box,color=red];
		j1018714496 -> j1162168995;
	j4012560882 [label="I don’t know. But it knew I was there. It was AWARE of me. Like an elephant would be aware of a fly crawling across its eye.",shape=box,color=blue];
		j4012560882 -> j3647310400;
	j1162168995 [label="+5 Chemistry",color=orange];
		j1162168995 -> j4012560882;
	j3647310400 [label="I remember thinking ‘nothing that big should know I exist’.",shape=box,color=blue];
		j3647310400 -> j1280225857;
	j1280225857 [label="It felt such utter disregard for me, such monumental indifference that I wanted to stop existing on the spot.",shape=box,color=blue];
		j1280225857 -> j2886702054;
		j1280225857 -> j3944588747;
	j2886702054 [label="It... does that.",shape=box,color=red];
		j2886702054 -> j1114854327;
	j3944588747 [label="What did you do?",shape=box,color=red];
		j3944588747 -> j1114854327;
	j1114854327 [label="I think I could have coped if it had hated me. Then I would have had some value, some significance. But it saw me as meaningless, and I saw what it saw. I was a speck. A nothing.",shape=box,color=blue];
		j1114854327 -> j1777972761;
	j1777972761 [label="Everything I had ever been, every tender moment, every laugh, every morsel of Eleanor Nightingale blew away in that storm.",shape=box,color=blue];
		j1777972761 -> j2636344462;
	j2636344462 [label="It felt my light fading, and I swear, a tiny tremor of satisfaction went through it. As if it was pleased to have crushed me. A flea under its fingernail.",shape=box,color=blue];
		j2636344462 -> j89555289;
	j89555289 [label="But you came back, didn’t you? You’re still here. You proved that thing wrong.",shape=box,color=red];
		j89555289 -> j773926141;
	j773926141 [label="Did I?",shape=box,color=blue];
		j773926141 -> j273882330;
	j273882330 [label="It’s still out there, isn’t it?",shape=box,color=blue];
		j273882330 -> j3463951950;
		j273882330 -> j1884726060;
	j3463951950 [label="Yes, it is. But you’re still here, too.",shape=box,color=red];
		j3463951950 -> j3973086958;
	j1884726060 [label="The stars don’t stop burning just because space is big, cold and dark.",shape=box,color=red];
		j1884726060 -> j3916610303;
	j3973086958 [label="Indifference wins in the end, my friend, no matter how many platitudes we throw into the abyss.",shape=box,color=blue];
		j3973086958 -> j1275980726;
	j3916610303 [label="But they will stop burning. One day. The heat death of the Universe.",shape=box,color=blue];
		j3916610303 -> j3973086958;
	j1275980726 [label="That’s the problem with not going mad. You’re cursed with clarity of thought.",shape=box,color=blue];
		j1275980726 -> j4145751246;
	j4145751246 [label="You know what I’m talking about. You could have shattered like a sparrow’s egg or burnt your brain out with drugs until you felt nothing any more. And it would have hurt less than this… knowing.",shape=box,color=blue];
		j4145751246 -> j1597540321;
	j1597540321 [label="I don’t understand why you haven’t walked away from this conversation yet.",shape=box,color=blue];
		j1597540321 -> j36498319;
		j1597540321 -> j1006319357;
	j36498319 [label="Look. Entropy is real. I get that. Maybe the Great Indifference is the same thing. But whether it wins in the end, you don’t know and I don’t know either.",shape=box,color=red];
		j36498319 -> j4045842822;
	j1006319357 [label="I'm waiting for you to make up your mind, honestly. Do you want to take the easy way out and go insane? Or do you want to stand and fight?",shape=box,color=red];
		j1006319357 -> j479702098;
	j4045842822 [label="What I do know is that I’m not going to let it win NOW.",shape=box,color=red];
		j4045842822 -> j1848125277;
	j479702098 [label="FIGHT HOW",shape=box,color=blue];
		j479702098 -> j2233726485;
	j1848125277 [label="So you’re putting up with my wails of existential despair as a battle tactic? That’s very Arthur of you.",shape=box,color=blue];
		j1848125277 -> j643691202;
	j2233726485 [label="IS THAT ALL YOU AND ARTHUR CAN UNDERSTAND? FIGHTING?",shape=box,color=blue];
		j2233726485 -> j4247204218;
	j643691202 [label="Actually, believe it or not, I care about you.",shape=box,color=red];
		j643691202 -> j557503596;
	j4247204218 [label="YOUR ANSWER TO THE UNDERLYING MEANINGLESSNESS OF EXISTENCE IS 'FIND A WAY TO PUNCH IT?'",shape=box,color=blue];
		j4247204218 -> j1576977395;
	j557503596 [label="I care about you too, mate. We’re squadmates. We've been through a lot.",shape=box,color=blue];
		j557503596 -> j460976799;
