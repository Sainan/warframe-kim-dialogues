		j2450008385 -> j408065276;
	j211499007 [label="This creature, this presence, is what we’re ultimately up against. I know that much now.",shape=box,color=blue];
		j211499007 -> j1764795470;
	j3438890509 [label="Giving it a name feels like I’ve gotten a tiny bit of control back.",shape=box,color=blue];
		j3438890509 -> j741012135;
	j408065276 [label="Anything human about it was like a mockery.",shape=box,color=blue];
		j408065276 -> j1946270621;
	j1764795470 [label="Please tell me the truth. Do you believe there is any hope?",shape=box,color=blue];
		j1764795470 -> j2504956942;
		j1764795470 -> j1727556893;
		j1764795470 -> j2219005603;
	j741012135 [label="I’ve touched that thing with my mind. I thought I’d be probing Rusalka’s mind, but… the outer layers just caved in like wet, spongy plasterboard and then I was immersed in it. Floundering.",shape=box,color=blue];
		j741012135 -> j591709930;
	j1946270621 [label="It was about as human as a store mannequin. In pieces.",shape=box,color=blue];
		j1946270621 -> j724669988;
	j2504956942 [label="At this point? ... No.",shape=box,color=red];
		j2504956942 -> j1515435173;
	j1727556893 [label="Honestly? I'm... not sure.",shape=box,color=red];
		j1727556893 -> j1716326490;
	j2219005603 [label="Yes, I do.",shape=box,color=red];
		j2219005603 -> j2929872840;
	j591709930 [label="Boolean EleanorIndifference is now true.",color=orange];
		j591709930 -> j1764795470;
