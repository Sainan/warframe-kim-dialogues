		j783559194 -> j447404707;
	j2578349673 [label="Hate. Rage. Anything.",shape=box,color=blue];
		j2578349673 -> j2310323068;
	j1234124827 [label="Maybe hope is a delusion. I don't know.",shape=box,color=blue];
		j1234124827 -> j908324754;
	j595510079 [label="I reached out for Rusalka, and it was as if my fingers touched her face and went right through. The mask just collapsed.",shape=box,color=blue];
		j595510079 -> j2055997517;
	j447404707 [label="Boolean EleanorIndifference is now true.",color=orange];
		j447404707 -> j265411798;
