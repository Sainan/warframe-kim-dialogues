	"Chat ends." [color=orange];
	j310220515 [label="The other me. The one who became a nun. I’ve been trying to contact her.",shape=box,color=blue];
		j310220515 -> j3596411623;
	j1546619229 [label="Ever since you appeared, a question has been pestering me. A question I believe I might've just answered. And dear Sol I don't know what I've done.",shape=box,color=blue];
		j1546619229 -> j1087024683;
	j3596411623 [label="I just did it.",shape=box,color=blue];
		j3596411623 -> j2757542933;
	j1087024683 [label="Start at the beginning. I don't know what you're talking about. What question?",shape=box,color=red];
		j1087024683 -> j2826326797;
	j2757542933 [label="Of course it was never going to work so long as I stayed in this world. I’m already the ‘me’ of this world. I had to go through the roaring whiteness. The blackstar fugue. The leechbone gulf. That’s how you get to the other sides. The maybes and the could-have-beens.",shape=box,color=blue];
		j2757542933 -> j1741335716;
	j2826326797 [label="You! The question of bloody You. You exist not once but twice.",shape=box,color=blue];
		j2826326797 -> j3999770098;
	j1741335716 [label="The Void! I saw the Void!",shape=box,color=blue];
		j1741335716 -> j4103098519;
		j1741335716 -> j2842773701;
	j3999770098 [label="And if you could exist in another world, as another version of yourself, why couldn't I?",shape=box,color=blue];
		j3999770098 -> j1667319004;
	j4103098519 [label="Are you... sure you didn't get into Lettie's special stash?",shape=box,color=red];
		j4103098519 -> j1974630145;
	j2842773701 [label="That shouldn’t be possible, Eleanor. Nobody enters the Void until Albrecht Entrati does it. That’s a long way into the future from where we are now.",shape=box,color=red];
		j2842773701 -> j1915035077;
	j1667319004 [label="Somewhere out there in the now-confirmed-to-exist vast possible universes was that version of myself that I always wondered if I should become.",shape=box,color=blue];
		j1667319004 -> j3686890258;
	j1974630145 [label="Drifter dear, I admire your ability to use humour as a coping mechanism, but right now I am entirely serious.",shape=box,color=blue];
		j1974630145 -> j2787638160;
	j1915035077 [label="I went in with my mind, not my body. That’s the difference.",shape=box,color=blue];
		j1915035077 -> j1131717531;
	j3686890258 [label="The one who turned away from the world to become a nun.",shape=box,color=blue];
		j3686890258 -> j61510342;
	j2787638160 [label="I did what I set out to do.",shape=box,color=blue];
		j2787638160 -> j3603219789;
	j1131717531 [label="Ow headache ow ow ow",shape=box,color=blue];
		j1131717531 -> j3014708669;
	j61510342 [label="An apostle of Lua sat on a mountaintop somewhere, incense clouds flowering around her.",shape=box,color=blue];
		j61510342 -> j1003914446;
	j3603219789 [label="I not only saw the Void, I entered it.",shape=box,color=blue];
		j3603219789 -> j1131717531;
	j3014708669 [label="Check boolean EleanorIndifference2",color=orange];
		j3014708669 -> j3474531682 [label=true,color=green];
		j3014708669 -> j2567733963 [label=true,color=green];
		j3014708669 -> j1069464196 [label=false,color=brown];
		j3014708669 -> j2567733963 [label=false,color=brown];
	j1003914446 [label="I've been trying to contact her. Trying to reach through the Void to find her.",shape=box,color=blue];
		j1003914446 -> j3212129121;
	j3474531682 [label="If you really did enter the Void, I cannot tell you how much danger you were putting yourself in. You know what’s out there, and you still just dove into it?",shape=box,color=red];
		j3474531682 -> j2472551231;
	j2567733963 [label="Are you insane?!",shape=box,color=red];
		j2567733963 -> j1033998371;
	j1069464196 [label="What did you see? What happened?",shape=box,color=red];
		j1069464196 -> j3186668364;
	j3212129121 [label="Boolean EleanorSister is now true.",color=orange];
		j3212129121 -> j1948348822;
		j3212129121 -> j1884203746;
	j2472551231 [label="That’s how I got into the Void in the first place. Through that memory. The horror was just smoke and through the smoke was a GATE. If I hadn’t been so frightened I would have seen it clearly the first time around.",shape=box,color=blue];
		j2472551231 -> j594349521;
	j1033998371 [label="Not even a little bit.",shape=box,color=blue];
		j1033998371 -> j3951306673;
	j3186668364 [label="There’s a feeling there isn’t a word for. You look out on the world and you just KNOW, that all the ordinariness - buildings, cars, people, jobs, work, routine - is just a thin crust over a bottomless horror, and if people ever realized it they’d start screaming and never stop.",shape=box,color=blue];
		j3186668364 -> j2643692192;
	j1948348822 [label="Oh no.",shape=box,color=red];
		j1948348822 -> j3596411623;
	j1884203746 [label="And...?",shape=box,color=red];
		j1884203746 -> j3596411623;
	j594349521 [label="This was a mistake. It took Rusalka. It came damn close to taking Albrecht. It could easily take you too.",shape=box,color=red];
		j594349521 -> j2891540314;
	j3951306673 [label="Just listen. Please. And try to understand.",shape=box,color=blue];
		j3951306673 -> j4269642011;
		j3951306673 -> j718146887;
	j2643692192 [label="I fell through the crust.",shape=box,color=blue];
		j2643692192 -> j3723520700;
	j2891540314 [label="It tried. I felt it. I knew it was curious, reaching for me.",shape=box,color=blue];
		j2891540314 -> j4269642011;
		j2891540314 -> j718146887;
	j4269642011 [label="So out of infinities of possible worlds, you just happened to home in on the right one?",shape=box,color=red];
		j4269642011 -> j1147696079;
	j718146887 [label="Eleanor, the Indifference can look like whatever it wants. It looked like Albrecht when he first encountered it.",shape=box,color=red];
		j718146887 -> j2645095451;
	j3723520700 [label="It wasn’t emptiness. I could have borne that. It went on and on forever, but it wasn’t just cold space. There was something in there with me.",shape=box,color=blue];
		j3723520700 -> j639434477;
	j1147696079 [label="It’s okay that you’re not convinced. I know this sounds insane. Just hear me out, okay?",shape=box,color=blue];
		j1147696079 -> j31664789;
	j2645095451 [label="I think maybe it sensed what you wanted and took on the form you were hoping to see.",shape=box,color=red];
		j2645095451 -> j31664789;
	j639434477 [label="Boolean EleanorIndifference2 is now true.",color=orange];
		j639434477 -> j594349521;
	j31664789 [label="I’ll make a deal with you.",shape=box,color=blue];
		j31664789 -> j2054991508;
	j2054991508 [label="First you let me finish telling you what happened.",shape=box,color=blue];
		j2054991508 -> j2743533167;
	j2743533167 [label="Once I’m done, you can make your own mind up about whether it was the Indifference messing with me or not. I promise I’ll take you seriously.",shape=box,color=blue];
		j2743533167 -> j504988741;
	j504988741 [label="Deal?",shape=box,color=blue];
		j504988741 -> j4279338403;
		j504988741 -> j1489393238;
	j4279338403 [label="Deal.",shape=box,color=red];
		j4279338403 -> j2605015932;
	j1489393238 [label="I can't believe you did this. Never do that again, Eleanor! I CAN'T LOSE YOU TO THAT THING!",shape=box,color=red];
		j1489393238 -> j2726655618;
	j2605015932 [label="So like I was saying. I found her.",shape=box,color=blue];
		j2605015932 -> j389500478;
	j2726655618 [label="Excuse me?",shape=box,color=blue];
		j2726655618 -> j4025609458;
	j389500478 [label="I could feel the Indifference watching. I could feel it make a face like one of those dried-out desert mummies as it reached out for me. I reached out for her, and she pulled me through.",shape=box,color=blue];
		j389500478 -> j887290170;
	j4025609458 [label="I'm not sure what to say. I'm a bit concerned, because you sound so upset, but I didn't realise you cared quite so much.",shape=box,color=blue];
		j4025609458 -> j2127839202;
	j887290170 [label="She was sitting in the Lotus position in some kind of open-air shrine, next to a pit full of embers. It was nighttime. The moon was dim, ghosting through scrolling clouds.",shape=box,color=blue];
		j887290170 -> j1117132410;
	j2127839202 [label="I'm all right. Please don't worry about me.",shape=box,color=blue];
		j2127839202 -> j3136398766;
	j1117132410 [label="She put a finger to her lips.",shape=box,color=blue];
		j1117132410 -> j1852168884;
	j3136398766 [label="+5 Chemistry",color=orange];
		j3136398766 -> j914383264;
	j1852168884 [label="For a second I could feel that giant hand still groping for me. Then it recoiled, like a guillotine had come down on one of its fingers.",shape=box,color=blue];
		j1852168884 -> j3825933646;
	j914383264 [label="I can't promise anything, but I'll try not to scare you again.",shape=box,color=blue];
		j914383264 -> j538028019;
		j914383264 -> j2016450503;
	j3825933646 [label="Grass whispered. Crickets sang. We were alone.",shape=box,color=blue];
		j3825933646 -> j667928400;
	j538028019 [label="Fine... just... Tell me what happened.",shape=box,color=red];
		j538028019 -> j2605015932;
	j2016450503 [label="I can't be a party to this. [End.]",shape=box,color=red];
		j2016450503 -> "Chat ends.";
	j667928400 [label="This is all sounding horribly familiar. And then?",shape=box,color=red];
		j667928400 -> j3153525120;
	j3153525120 [label="We sat across the fire-pit from each other. She didn’t speak, but she let me look into her mind, soft and sisterly.",shape=box,color=blue];
		j3153525120 -> j2778286317;
	j2778286317 [label="She was so happy, so perfectly content. A woman in the place that was right for her. Callused feet, clear eyes. Breathing the fire and drinking the moon.",shape=box,color=blue];
		j2778286317 -> j1021888304;
		j2778286317 -> j2290931050;
	j1021888304 [label="So you felt that WOULD have been the right choice for you?",shape=box,color=red];
		j1021888304 -> j681868714;
	j2290931050 [label="Did you feel tempted to stay?",shape=box,color=red];
		j2290931050 -> j681868714;
	j681868714 [label="No! Just the opposite!",shape=box,color=blue];
		j681868714 -> j2353922950;
	j2353922950 [label="I didn’t need to take that path, because she already had. Don’t you see? I’m free now. I never need to wonder ‘what if’ because there already IS a Sister Eleanor. She’s got my back. Am I making sense?",shape=box,color=blue];
		j2353922950 -> j2452088649;
	j2452088649 [label="Kind of? All I want to know is, did it help?",shape=box,color=red];
		j2452088649 -> j713556842;
	j713556842 [label="Oh yeah. I’ve never been more certain of what to do next.",shape=box,color=blue];
		j713556842 -> j929683391;
	j929683391 [label="+15 Chemistry",color=orange];
		j929683391 -> j1368351291;
	j1368351291 [label="I’m going to take the life she freed me up to live, and live it to the hilt.",shape=box,color=blue];
		j1368351291 -> "Chat ends.";
}