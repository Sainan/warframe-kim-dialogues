		j1951407919 -> j2519477241;
	j151419492 [label="或許改天我會告訴你整個故事。或許我不會。",shape=box,color=blue];
		j151419492 -> j3216257777;
	j213616985 [label="亞瑟最好的朋友。我們當時訂了婚。我以為我很愛他。",shape=box,color=blue];
		j213616985 -> j1120041388;
