		j2306563479 -> j2170926026;
	j182874175 [label="莉蒂？",shape=box,color=blue];
		j182874175 -> j1714302326;
	j2867598667 [label="某部分的我覺得這是個蠢主意。他不會看到這封信。他死了。不過……",shape=box,color=blue];
		j2867598667 -> j2577120708;
	j2170926026 [label="是，他有。也許這就是問題的其中一部分。我知道他因為我今年沒有舉行我們的小型紀念儀式而感到難過。",shape=box,color=blue];
		j2170926026 -> j1819785024;
	j1714302326 [label="為什麼是她？",shape=box,color=blue];
		j1714302326 -> j1620959225;
	j2577120708 [label="……我越想，就越覺得這個主意沒那麼蠢。",shape=box,color=blue];
		j2577120708 -> j2088712480;
	j1819785024 [label="我只不過……這次以我的方式跟克里斯多福說再見。從此放下。",shape=box,color=blue];
		j1819785024 -> j686781662;
	j1620959225 [label="她有很多經驗。她對紀念亡者的一切瞭如指掌。",shape=box,color=red];
		j1620959225 -> j3981586451;
	j2088712480 [label="好。我會這樣做。有些話我需要跟他說。他在生的時候我從來沒有說過這些話。也許這樣他就可以安眠了。",shape=box,color=blue];
		j2088712480 -> j1097791731;
	j686781662 [label="對亞瑟來說，他永遠是個一個失落的兄弟，一個陣亡的士兵。這就是他所知道跟理解的哀悼。但這不是我的方式。",shape=box,color=blue];
		j686781662 -> j2396557038;
	j3981586451 [label="我相信如果你向她尋求建議的話，有禮物地，她不會拒絕你的。",shape=box,color=red];
