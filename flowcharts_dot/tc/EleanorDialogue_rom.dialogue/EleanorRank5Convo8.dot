		j2879471209 -> j2644625822;
	j3502923765 [label="噢老友。我以為我是。我愛過他，但我甚至不知道我有沒有愛上他。",shape=box,color=blue];
		j3502923765 -> j3302363026;
	j884945296 [label="Boolean EleanorChristopher2 is now true.",color=orange];
		j884945296 -> j1466589246;
	j3302363026 [label="聽起來很無情，對吧？不過，是真的。在當時，這似乎並不重要。然後他死了，這就是我們全部的故事。",shape=box,color=blue];
		j3302363026 -> j4034375536;
	j4034375536 [label="我喜歡他的一切都被他死亡的方式蒙上陰影。一想到他，我的心就像被石頭壓住。",shape=box,color=blue];
		j4034375536 -> j3322430534;
	j3322430534 [label="這不公平。對他來說。他應該得到更好的下場。",shape=box,color=blue];
		j3322430534 -> j2291203736;
		j3322430534 -> j403682721;
	j2291203736 [label="我明白。完全明白。我選擇了配得上我父母的方式來愐懷他們。",shape=box,color=red];
		j2291203736 -> j1233808156;
	j403682721 [label="曾經發生過事情很可怕。沒有躲避的餘地。但我們不能為了它永遠耿耿於懷。",shape=box,color=red];
		j403682721 -> j1233808156;
	j1233808156 [label="那為什麼我會感到這麼內疚呢？",shape=box,color=blue];
		j1233808156 -> j2130857112;
	j2130857112 [label="為什麼我覺得放下他就等同拋棄他？",shape=box,color=blue];
		j2130857112 -> j333556245;
		j2130857112 -> j188825354;
	j333556245 [label="因為你已經背負他太久，久得你不知道怎樣放下。",shape=box,color=red];
		j333556245 -> j601593721;
	j188825354 [label="因為你擔心沒有其他人能愛你。",shape=box,color=red];
		j188825354 -> j1415583883;
	j601593721 [label="天啊。你說得真對，你知道。",shape=box,color=blue];
		j601593721 -> j2737290654;
	j1415583883 [label="Check boolean EleanorDating",color=orange];
		j1415583883 -> j1880340468 [label=true,color=green];
		j1415583883 -> j2541392813 [label=false,color=brown];
	j2737290654 [label="我們對如何處理這些問題一竅不通，我們這種人。我們把一切都收藏在心底。我們擺出勇敢的姿態在戰壕裡吹著口哨。這樣做讓我們繼續生活，如同我們的父母如何繼續生活一樣。但我們不知道怎樣改變。",shape=box,color=blue];
		j2737290654 -> j4196658499;
	j1880340468 [label="就是這個原因嗎？我有那麼膚淺嗎？",shape=box,color=blue];
		j1880340468 -> j3999863434;
	j2541392813 [label="我懷疑以我目前的景況來看即使是克里斯多福也不會愛我。",shape=box,color=blue];
		j2541392813 -> j2408725565;
	j4196658499 [label="我該怎樣做？我該如何放下？",shape=box,color=blue];
