	j3318239570 [label="你很先進，但你也需接受新的知識",shape=box,color=blue];
		j3318239570 -> j3641898668;
	j743313761 [label="你們擁有所有這些令人難以置信的神奇技術，而你們所做的就是找出剝削彼此的新方法。科學使我們變得更不平等，而不是更平等。",shape=box,color=blue];
