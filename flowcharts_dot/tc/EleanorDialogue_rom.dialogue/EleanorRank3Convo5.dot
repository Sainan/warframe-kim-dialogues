		j769441223 -> j4232339531;
	j1338065289 [label="崑西以及亞瑟嘗試用水果配給袋來製作監獄烈酒，但我們這些還有點尊嚴的人拒絕了。那是莉蒂少有的團結時刻。",shape=box,color=blue];
