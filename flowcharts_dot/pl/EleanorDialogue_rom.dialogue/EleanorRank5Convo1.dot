		j3137874513 -> "Chat ends.";
	j312175424 [label="Wiem, że to nie ma żadnego sensu, że wydaje się sprzeczne z intuicją. Idea, że przede wszystkim jesteśmy punktem widzenia a dopiero potem osobowością. Wiem tylko jakie to uczucie, kiedy ludzka dusza przechodzi z życia w śmierć.",shape=box,color=blue];
