	j2985895595 [label="Nikt cię nie zmusza do rozmowy ze mną. Pa.",shape=box,color=blue];
		j2985895595 -> "Chat ends.";
	j436438756 [label="Całe życie była jak kobieta w domu bez światła, miała tylko latarkę. Musiała świecić nią na konkretne miejsca. Wspomnienie, zadanie, interakcję.",shape=box,color=blue];
		j436438756 -> j1603548094;
