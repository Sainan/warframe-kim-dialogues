		j923292131 -> j836110965;
	j777538551 [label="Kolego. Wszystko, co dał nam Entrati, potęgę, siłę, opancerzone tyłki… Oddałabym to wszystko za pewność, że śmierć nie jest końcem.",shape=box,color=blue];
