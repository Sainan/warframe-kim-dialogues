		j836110965 -> j3212014346;
	j1169698004 [label="Próbowałam. Nie czuję się dobrze, przyznając się do tego, ale kiedy czasem jesteśmy na patrolu i zakończyliśmy walkę, zaglądam do głów umierających i przechodzę ten proces wraz z nimi. Jeśli istnieje światło na końcu tunelu, chcę je zobaczyć.",shape=box,color=blue];
