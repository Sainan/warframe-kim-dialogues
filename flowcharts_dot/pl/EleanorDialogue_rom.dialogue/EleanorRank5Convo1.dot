		j760344607 -> j1260957699;
	j1260957699 [label="Ale ona tak. Zobaczyła twarz matki, dzieci, z którymi chodziła do szkoły, swojego kota, kuzyna, który utonął. Ale to nie były obecności ani duchy, tylko wspomnienia umierającego mózgu, który kurczowo chwytał się czegokolwiek.",shape=box,color=blue];
		j1260957699 -> j1427426480;
