		j1012668557 -> j67205709;
	j441160222 [label="Tak, raz. Co w tym złego, prawda? I tak umierają. Była młodym żołnierzem Scaldry. Pobiegła na mnie, a ja ją zastrzeliłam.",shape=box,color=blue];
		j441160222 -> j2443852387;
