		j1327443414 -> j4108072615;
	j1002835268 [label="Więc może dzisiaj zobaczę uśmiechniętego Artura?",shape=box,color=red];
		j1002835268 -> j1866250943;
	j2296545471 [label="... świetnie. Starałam się podzielić ze wszystkimi dobrym nastrojem, a zamiast tego jestem traktowana jak podły goblin. Cały mój nastrój padł na pysk.",shape=box,color=blue];
		j2296545471 -> j2478442550;
