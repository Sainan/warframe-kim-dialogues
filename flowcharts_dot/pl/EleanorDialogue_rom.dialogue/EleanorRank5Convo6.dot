		j1994865597 -> j3976385059;
	j3976385059 [label="Rozmawiałyśmy całą noc i płakałyśmy razem. Też była głęboko zraniona. A wraz ze wschodem słońca zdecydowałam się zostać.",shape=box,color=blue];
