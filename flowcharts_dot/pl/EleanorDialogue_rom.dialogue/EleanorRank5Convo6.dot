		j1441894643 -> j3041307398;
	j681244236 [label="Och, to nie była ich wina, tylko moja. Każda odrobina wątpliwości i nienawiści do siebie, którą skrywałam, tego dnia wybiła na powierzchnię.",shape=box,color=blue];
		j681244236 -> j3706866007;
	j3041307398 [label="Dziękuję, że mnie wspierasz. To wiele dla mnie znaczy.",shape=box,color=blue];
		j3041307398 -> j97598121;
	j3706866007 [label="Pomyślałam: ta grupa potrzebuje kozła ofiarnego.",shape=box,color=blue];
		j3706866007 -> j3550632368;
	j97598121 [label="Tak wiele razy usłyszałam to od Lettie, że zaczęłam się zastanawiać, czy może rzeczywiście ma rację. Moja obecność stanowi dla wszystkich zagrożenie.",shape=box,color=blue];
		j97598121 -> j1077153932;
	j3550632368 [label="Potwora. Czegoś, na co można zrzucić winę.",shape=box,color=blue];
		j3550632368 -> j1077153932;
	j1077153932 [label="Przygotowałam cały plan. Wymknęłabym się w nocy, ukryłabym się gdzieś w mieście, gdzie Artur nigdy by mnie nie znalazł - i z tej części jestem najbardziej dumna - pomagałabym z oddali.",shape=box,color=blue];
		j1077153932 -> j2031098455;
		j1077153932 -> j3147702908;
	j2031098455 [label="Myślę, że by cię znalazł. Nie poddaje się łatwo.",shape=box,color=red];
		j2031098455 -> j4040456767;
	j3147702908 [label="Bardziej prawdopodobne jest to, że zginąłby, próbując cię odnaleźć.",shape=box,color=red];
		j3147702908 -> j3427768562;
