		j2026952191 -> j861513727;
	j1029079339 [label="Dźwiga wielki ciężar, to daje mu się we znaki.",shape=box,color=red];
		j1029079339 -> j1557635157;
	j861513727 [label="Myślę, że po prostu chciał, żebyśmy skupili się na zadaniu. Ale ja czułam się załamana. Artur i Aoi nie rozmawiali, Amir tak bardzo działał Quincy'emu na nerwy, że zaczęłam się bać o jego życie, a Lettie krzywo na mnie patrzyła przy każdej okazji.",shape=box,color=blue];
		j861513727 -> j2284068258;
	j1557635157 [label="Tak. Zazwyczaj nie daje po sobie poznać, że jeździ na oparach. Ale wtedy chyba nic już nie zostało.",shape=box,color=blue];
		j1557635157 -> j3712811395;
	j2284068258 [label="Tego dnia spędziłam dużo czasu patrząc w lustra. Wciąż widziałam, jak moje odbicie chwyta mnie za gardło i syczy \"to TY jesteś problemem. Gdyby nie ty, nic z tego nie miałoby miejsca\".",shape=box,color=blue];
		j2284068258 -> j3300765091;
		j2284068258 -> j1441894643;
	j3712811395 [label="Wypalił się, próbując utrzymać drużynę w całości.",shape=box,color=blue];
		j3712811395 -> j861513727;
