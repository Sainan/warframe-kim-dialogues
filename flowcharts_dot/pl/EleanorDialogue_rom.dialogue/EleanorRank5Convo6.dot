		j2251646067 -> j3686920889;
	j1924015962 [label="Kochałam zraszacze, kiedy byłam mała. Było w nich coś magicznego. A kiedy słońce świeciło najjaśniej, można było w nich zauważyć tęczę. A potem przebiec przez nią i poczuć przyjemny chłód.",shape=box,color=blue];
		j1924015962 -> j900605099;
