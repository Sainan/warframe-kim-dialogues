		j2801385379 -> j1503092035;
	j2252245867 [label="Wiele się zmieniło, od kiedy tu przybyłeś. Nie chodzi nawet tylko o nas, a o wszystko. Nawet całe centrum handlowe.",shape=box,color=blue];
		j2252245867 -> j1503092035;
