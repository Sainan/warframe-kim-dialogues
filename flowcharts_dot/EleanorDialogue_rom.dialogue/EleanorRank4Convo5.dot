		j3727339474 -> j2789387113;
	j864301661 [label="Hence the saying back home. If things are going badly, like they were during the war, it’s tradition to shrug and say ‘Well, it can’t be THAT bad. Young Loxley’s not turned up yet.’",shape=box,color=blue];
