		j2442220509 -> j899177046;
	j3889254782 [label="There's so much we don't know.",shape=box,color=blue];
		j3889254782 -> j527540089;
	j2351369067 [label="Just hold on to that belief. Let me feel it.",shape=box,color=blue];
		j2351369067 -> j4221304659;
	j2763646056 [label="Thank you.",shape=box,color=blue];
		j2763646056 -> j3634954244;
	j899177046 [label="I'll keep fighting out of pure bloody stubbornness.",shape=box,color=blue];
		j899177046 -> j732409284;
	j527540089 [label="After what's already happened this year, how are we supposed to make sane plans ever again?",shape=box,color=blue];
