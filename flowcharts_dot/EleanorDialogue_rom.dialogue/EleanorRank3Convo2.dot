		j1494925676 -> j2492618980;
	j1196611355 [label="I’ve touched that thing with my mind. I thought I’d be probing Rusalka’s mind, but… the outer layers just caved in like wet, spongy plasterboard and then I was immersed in it. Floundering.",shape=box,color=blue];
