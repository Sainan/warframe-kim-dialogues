		j1186929354 -> j278049215;
	j3238777200 [label="Please tell me the truth. Do you believe there is any hope?",shape=box,color=blue];
		j3238777200 -> j2405859482;
		j3238777200 -> j2483063964;
		j3238777200 -> j1422451222;
	j2583408103 [label="I’ve touched that thing with my mind. I thought I’d be probing Rusalka’s mind, but… the outer layers just caved in like wet, spongy plasterboard and then I was immersed in it. Floundering.",shape=box,color=blue];
		j2583408103 -> j2186854149;
	j278049215 [label="It was about as human as a store mannequin. In pieces.",shape=box,color=blue];
		j278049215 -> j3109756062;
	j2405859482 [label="At this point? ... No.",shape=box,color=red];
		j2405859482 -> j1356403214;
	j2483063964 [label="Honestly? I'm... not sure.",shape=box,color=red];
		j2483063964 -> j1163617270;
	j1422451222 [label="Yes, I do.",shape=box,color=red];
		j1422451222 -> j3782653367;
