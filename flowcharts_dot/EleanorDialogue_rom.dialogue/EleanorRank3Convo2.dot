		j732409284 -> j2033797117;
	j3464309611 [label="Maybe hope is a delusion. I don't know.",shape=box,color=blue];
		j3464309611 -> j3275934544;
	j3610076584 [label="I reached out for Rusalka, and it was as if my fingers touched her face and went right through. The mask just collapsed.",shape=box,color=blue];
		j3610076584 -> j1494925676;
	j832186818 [label="Boolean EleanorIndifference is now true.",color=orange];
		j832186818 -> j1196611355;
