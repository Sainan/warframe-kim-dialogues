	j1510465393 [label="未来居然有这种冷笑话",shape=box,color=blue];
		j1510465393 -> j3025957914;
	"Chat ends." [color=orange];
	j1650897248 [label="我想念葡萄酒。我已经很久没喝过真正的酒了。霍瓦尼亚所有的酒都在疫情爆发几天后被炽蛇军指挥部征用了。",shape=box,color=blue];
		j1650897248 -> j1491253097;
	j3025957914 [label="+10 Chemistry",color=orange];
		j3025957914 -> j1476150290;
	j1491253097 [label="昆西和亚瑟试着用水果口粮包做监狱特酿，但我们这些还有点尊严的人拒绝了。这可是莱蒂难得的团结时刻。",shape=box,color=blue];
		j1491253097 -> j912102776;
	j1476150290 [label="现在开心了。谢谢。",shape=box,color=blue];
		j1476150290 -> "Chat ends.";
	j912102776 [label="另一件事，也是更重要的事情，是未来。",shape=box,color=blue];
		j912102776 -> j2739306491;
	j2739306491 [label="这不是你的错，但是……现在我从你的眼里看到了未来，从某种意义上说我觉得很痛苦，常用的方法都无法改变这种痛苦。",shape=box,color=blue];
		j2739306491 -> j2274611187;
		j2739306491 -> j671842710;
	j2274611187 [label="我要怎么做才能让你高兴起来？",shape=box,color=red];
		j2274611187 -> j526603196;
	j671842710 [label="常用的方法？",shape=box,color=red];
		j671842710 -> j588741574;
	j526603196 [label="我不知道。也许跳一段异国情调的舞蹈？",shape=box,color=blue];
		j526603196 -> j718789375;
	j588741574 [label="和阿米尔一起度过一个晚上，听他热情地谈论他目前着迷的漫画系列或复古游戏。",shape=box,color=blue];
		j588741574 -> j2259051822;
	j718789375 [label="向我详细描述一下 Ballas 的死？",shape=box,color=blue];
		j718789375 -> j3102196581;
	j2259051822 [label="用水晶包围我自己，心里默念，直到莱蒂叫驱魔师来。",shape=box,color=blue];
		j2259051822 -> j3826015263;
	j3102196581 [label="谢谢你的好心，但我觉得这份忧愁是不得不承受的。",shape=box,color=blue];
		j3102196581 -> j1646547541;
	j3826015263 [label="这是一种不同的悲伤。它像一条湿透的毯子一样重重地压着我。",shape=box,color=blue];
		j3826015263 -> j1301764135;
		j3826015263 -> j3965062102;
	j1646547541 [label="也许我会去画画。",shape=box,color=blue];
		j1646547541 -> j227430165;
	j1301764135 [label="1999 年的霍瓦尼亚可真够呛！炽蛇军、科腐者、虚空知道还有什么。未来真的看起来糟糕那么多吗？",shape=box,color=red];
		j1301764135 -> j1842295931;
	j3965062102 [label="哦，来吧……你很坚强。你能应付的。这里有很多很酷的东西。",shape=box,color=red];
		j3965062102 -> j3708322362;
	j227430165 [label="+5 Chemistry",color=orange];
		j227430165 -> "Chat ends.";
	j1842295931 [label="听着，我从来都不喜欢科幻小说。亚瑟有一段时间喜欢，但他总是更喜欢坚韧不拔的军队题材。男子汉成为英雄，积极健康的男孩们的故事，有大量的潜台词，我相信你知道这种东西。",shape=box,color=blue];
		j1842295931 -> j3973457116;
	j3708322362 [label="好吧。拉着我的手，陪我穿过霍瓦尼亚的街道。",shape=box,color=blue];
		j3708322362 -> j3980781807;
	j3973457116 [label="但我对科幻小说的了解足以让我知道，在遥远的未来，我们应该把我们的事情安排得井井有条。一切应该是光鲜亮丽、一腐不染的。机器人做所有艰苦的工作，没有疾病，没有人需要钱，到处都是银色的塔、蓝蓝的天和性感的蜥蜴。",shape=box,color=blue];
		j3973457116 -> j1091228830;
		j3973457116 -> j2398991794;
	j3980781807 [label="我知道一个地方，我们可以在那里吃一些碎萝卜蛋糕，再看看太阳从学校的废墟上空落下。",shape=box,color=blue];
		j3980781807 -> j3032362883;
	j1091228830 [label="我们有机器人。它们确实做了很多艰苦的工作。",shape=box,color=red];
		j1091228830 -> j2894912030;
	j2398991794 [label="但你讲的这些都是小说里的内容，不是吗？你为什么期望现实也会变成这样？",shape=box,color=red];
		j2398991794 -> j1537154168;
	j3032362883 [label="稍后，我们可以在燃烧坦克的光下品尝鸡尾酒。",shape=box,color=blue];
		j3032362883 -> "Chat ends.";
	j2894912030 [label="好吧，这可以给你加一分。",shape=box,color=blue];
		j2894912030 -> j3307959948;
	j1537154168 [label="唉，我不是……这很复杂。让我解释一下。",shape=box,color=blue];
		j1537154168 -> j3792829159;
	j3307959948 [label="但这些机器人真的让普通人的生活变得更轻松了吗？",shape=box,color=blue];
		j3307959948 -> j32806141;
	j3792829159 [label="我并不是说我期待未来看起来像儿童漫画那样美好。我只是说对未来有一种乐观的期待。",shape=box,color=blue];
		j3792829159 -> j1055866256;
	j32806141 [label="还是只是变成了另一种被人们视为理所当然的存在？",shape=box,color=blue];
		j32806141 -> j3080931499;
		j32806141 -> j4289575415;
	j1055866256 [label="我们似乎都理所当然地认为，无论现在有多么困难，终究会有解决的一天。我们的文明注定要向前发展。我们一开始是用棍棒互相砸对方的头，几千年后，我们学会了用更文明的方式来解决分歧，而不是靠暴力。",shape=box,color=blue];
		j1055866256 -> j990101882;
		j1055866256 -> j3175526967;
	j3080931499 [label="你总是喜欢往坏处想，是吧？",shape=box,color=red];
		j3080931499 -> j2644165418;
	j4289575415 [label="好吧。机器人并没有解决剥削问题。",shape=box,color=red];
		j4289575415 -> j345944014;
	j990101882 [label="无论技术发生多大变化，人性始终如一。",shape=box,color=red];
		j990101882 -> j3804086149;
	j3175526967 [label="对不起，我们让你失望了？",shape=box,color=red];
		j3175526967 -> j2701795341;
	j2644165418 [label="没错。",shape=box,color=blue];
		j2644165418 -> j1849894180;
	j345944014 [label="对。这就是令人失望的地方。",shape=box,color=blue];
		j345944014 -> j1006380242;
	j3804086149 [label="一针见血。你所说的正是我对这一切感到沮丧的原因。",shape=box,color=blue];
		j3804086149 -> j2405522989;
	j2701795341 [label="没错。",shape=box,color=blue];
		j2701795341 -> j2405522989;
	j1849894180 [label="只有这样我们才能知道哪里需要改变。",shape=box,color=blue];
		j1849894180 -> "Chat ends.";
	j1006380242 [label="你们很先进，但也应该有更高的觉悟",shape=box,color=blue];
		j1006380242 -> j4181033241;
	j2405522989 [label="你们拥有所有这些不可思议的神奇技术，却只用它来找到新的方式去剥削彼此。科学并没有让我们变得更平等，反而是更加不平等。",shape=box,color=blue];
		j2405522989 -> j3404276790;
		j2405522989 -> j77924230;
	j4181033241 [label="感谢你的耐心，我知道这些说起来可能有点多。",shape=box,color=blue];
		j4181033241 -> j4072819434;
	j3404276790 [label="嗯，礼貌地提醒一下，那些事我一件都没做过？",shape=box,color=red];
		j3404276790 -> j3338846905;
	j77924230 [label="我会把你的投诉转达给 Orokin。",shape=box,color=red];
		j77924230 -> j3338846905;
	j4072819434 [label="+15 Chemistry",color=orange];
		j4072819434 -> "Chat ends.";
	j3338846905 [label="哦，天啊，我不是指你个人。我是说你所在的那个时代。",shape=box,color=blue];
		j3338846905 -> j778785995;
	j778785995 [label="说到变化，你们是在倒退。还神的化身！那么多可以重新创造的东西，你们居然选了这个！",shape=box,color=blue];
		j778785995 -> j1667549721;
	j1667549721 [label="是 Tenno 灭了 Orokin。你忘了这部分吗？",shape=box,color=red];
		j1667549721 -> j202842583;
	j202842583 [label="不。我没忘记那些情感受虐的孤儿们杀死不朽精英的那部分。我特别喜欢那一幕。如果我闭上眼睛，我几乎可以想象到尖叫声。",shape=box,color=blue];
		j202842583 -> j88908286;
		j202842583 -> j799647464;
	j88908286 [label="好吧。未来一团糟。如果你喜欢，你可以关注这一点。",shape=box,color=red];
		j88908286 -> j636443827;
	j799647464 [label="这……是……我不会轻易向人透露的事情。",shape=box,color=red];
		j799647464 -> j2798307616;
	j636443827 [label="我支持那些在努力让未来变得更好的人。",shape=box,color=red];
		j636443827 -> j2895199346;
	j2798307616 [label="……未来连讽刺也过时了吗？",shape=box,color=blue];
		j2798307616 -> "Chat ends.";
	j2895199346 [label="+20 Chemistry",color=orange];
		j2895199346 -> "Chat ends.";
