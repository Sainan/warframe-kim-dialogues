	"Chat ends." [color=orange];
	j360428669 [label="那么，关于克里斯托弗。",shape=box,color=blue];
		j360428669 -> j283970991;
	j2391583421 [label="Check boolean EleanorChristopher1",color=orange];
		j2391583421 -> j2275725330 [label=true,color=green];
		j2391583421 -> j400995641 [label=false,color=brown];
	j283970991 [label="这些年我一直在怀念他。亚瑟也是如此。我们每年都会举行一个仪式来纪念他。但今年我做不到。我无法面对。",shape=box,color=blue];
		j283970991 -> j2158134502;
	j2275725330 [label="长话短说，这是一个悲惨的故事……在我们结婚前，克里斯托弗在一次路边炸弹袭击中丧生。",shape=box,color=blue];
		j2275725330 -> j1925428512;
	j400995641 [label="现在是时候告诉你关于克里斯托弗的事了。",shape=box,color=blue];
		j400995641 -> j3298810787;
	j2158134502 [label="我不知道鬼魂是否存在，但它们或许真的存在，因为我让他变成了一个鬼魂。",shape=box,color=blue];
		j2158134502 -> j1543302944;
	j1925428512 [label="也许我改天再告诉你整个故事。也许我不会。",shape=box,color=blue];
		j1925428512 -> j622498994;
	j3298810787 [label="他是亚瑟最好的朋友。我们订了婚。我以为我很爱他。",shape=box,color=blue];
		j3298810787 -> j26993761;
	j1543302944 [label="别对自己太苛刻了。你爱过那个人。",shape=box,color=red];
		j1543302944 -> j2096570067;
	j622498994 [label="我现在告诉你这些是因为……",shape=box,color=blue];
		j622498994 -> j1517195732;
	j26993761 [label="Boolean EleanorChristopher1 is now true.",color=orange];
		j26993761 -> j2275725330;
	j2096570067 [label="哦，伙计。我以为我爱他。我爱过他，但我甚至不确定我是否真的爱上了他。",shape=box,color=blue];
		j2096570067 -> j2901642585;
	j1517195732 [label="Boolean EleanorChristopher2 is now true.",color=orange];
		j1517195732 -> j283970991;
	j2901642585 [label="这听起来很糟糕，不是吗？但这是真的。当时，这似乎并不重要。后来他死了，这成了我们故事的全部。",shape=box,color=blue];
		j2901642585 -> j1522618805;
	j1522618805 [label="我对他的所有钦佩都被他的去世所掩盖。一想到他，我的胃就会感到一阵绞痛。",shape=box,color=blue];
		j1522618805 -> j1454177671;
	j1454177671 [label="这对他不公平。他值得更好的。",shape=box,color=blue];
		j1454177671 -> j2981491214;
		j1454177671 -> j706347625;
	j2981491214 [label="我完全理解。我选择以我父母应得的方式来纪念他们。",shape=box,color=red];
		j2981491214 -> j831423500;
	j706347625 [label="发生的事情很可怕。这是无法避免的。但我们不能让这成为我们的心结。",shape=box,color=red];
		j706347625 -> j831423500;
	j831423500 [label="那为什么我会感到这么内疚？",shape=box,color=blue];
		j831423500 -> j634782539;
	j634782539 [label="为什么我觉得放手就像是抛弃他一样？",shape=box,color=blue];
		j634782539 -> j1024906653;
		j634782539 -> j935903781;
	j1024906653 [label="因为你心里一直放不下他，久到不知如何让自己放手。",shape=box,color=red];
		j1024906653 -> j841324891;
	j935903781 [label="因为你担心再也没有人能真正爱你了。",shape=box,color=red];
		j935903781 -> j1415583883;
	j841324891 [label="天。你说得太对了，你知道。",shape=box,color=blue];
		j841324891 -> j2530163297;
	j1415583883 [label="Check boolean EleanorDating",color=orange];
		j1415583883 -> j2125173582 [label=true,color=green];
		j1415583883 -> j36887763 [label=false,color=brown];
	j2530163297 [label="我们这帮人，不怎么会面对这种事。我们把一切都藏在心里。装出一副勇敢的样子，在战壕中吹口哨。这让我们撑过来了，就像我们的父母，他们也是这样撑过来的。但我们不知道该如何停止。",shape=box,color=blue];
		j2530163297 -> j3892225907;
	j2125173582 [label="是这个原因吗？我有这么肤浅吗？",shape=box,color=blue];
		j2125173582 -> j3353555288;
	j36887763 [label="我甚至怀疑，以我现在的状态，克里斯托弗是否还会爱我……",shape=box,color=blue];
		j36887763 -> j3476378424;
	j3892225907 [label="我该怎么办？我该如何放手？",shape=box,color=blue];
		j3892225907 -> j4017015120;
	j3353555288 [label="现在我觉得我不配得到你们中的任何一个。",shape=box,color=blue];
		j3353555288 -> j3630608932;
	j3476378424 [label="……不。这不公平。我这样对他太不公平了，不是吗？",shape=box,color=blue];
		j3476378424 -> j2386395939;
	j4017015120 [label="Check boolean LettieDiasdeMuertos",color=orange];
		j4017015120 -> j3245501717 [label=true,color=green];
		j4017015120 -> j177139426 [label=true,color=green];
		j4017015120 -> j1718628084 [label=true,color=green];
		j4017015120 -> j177139426 [label=false,color=brown];
		j4017015120 -> j1718628084 [label=false,color=brown];
	j3630608932 [label="我想……我希望你拥有我的全部。我不能让自己的一部分永远停留在无尽的哀悼中。",shape=box,color=blue];
		j3630608932 -> j4017015120;
	j2386395939 [label="他不会想妨碍我的幸福。他很善良。",shape=box,color=blue];
		j2386395939 -> j4017015120;
	j3245501717 [label="我真的觉得莱蒂可以帮助你。",shape=box,color=red];
		j3245501717 -> j273083968;
	j177139426 [label="你可以给他写一封信。",shape=box,color=red];
		j177139426 -> j793816507;
	j1718628084 [label="也许你可以和亚瑟谈谈。他从一开始就和你一起经历了这段悲伤。",shape=box,color=red];
		j1718628084 -> j351319629;
	j273083968 [label="莱蒂？",shape=box,color=blue];
		j273083968 -> j485244963;
	j793816507 [label="我心里想，这是个愚蠢的想法。他不会读到这些。他已经不在了。但是……",shape=box,color=blue];
		j793816507 -> j2034011632;
	j351319629 [label="是的，他确实有。也许这正是问题的一部分。我知道他很不高兴，因为我今年没有继续我们的小小纪念仪式。",shape=box,color=blue];
		j351319629 -> j677307768;
	j485244963 [label="为什么是她？",shape=box,color=blue];
		j485244963 -> j3421845072;
	j2034011632 [label="……我越想越觉得这似乎并不那么愚蠢。",shape=box,color=blue];
		j2034011632 -> j1427428249;
	j677307768 [label="我只是……这次我必须以我自己的方式和克里斯托弗告别。彻底地。",shape=box,color=blue];
		j677307768 -> j3085963752;
	j3421845072 [label="她有很多经验。她非常了解如何纪念逝者。",shape=box,color=red];
		j3421845072 -> j1722829016;
	j1427428249 [label="好。我会去做的。有些话我需要对他说。那些他在世时我从未说过的话。也许这样他就可以安息了。",shape=box,color=blue];
		j1427428249 -> j364246555;
	j3085963752 [label="对亚瑟来说，他永远是一个死去的兄弟，一位阵亡的士兵。这是他所知道和理解的哀悼方式。但那不是我的方式。",shape=box,color=blue];
		j3085963752 -> j1805557874;
	j1722829016 [label="我相信如果你恭敬地向她寻求建议，她不会拒绝你。",shape=box,color=red];
		j1722829016 -> j3800180548;
	j364246555 [label="谢谢你的倾听和建议。",shape=box,color=blue];
		j364246555 -> j3887668637;
	j1805557874 [label="我不能再用亚瑟的方式了。我想……如果我告诉他这一点，他会理解的。",shape=box,color=blue];
		j1805557874 -> j892555329;
	j3800180548 [label="……",shape=box,color=blue];
		j3800180548 -> j1771660437;
	j3887668637 [label="+20 Chemistry",color=orange];
		j3887668637 -> j1559379119;
	j892555329 [label="+10 Chemistry",color=orange];
		j892555329 -> j147382484;
	j1771660437 [label="是的",shape=box,color=blue];
		j1771660437 -> j3070402459;
	j1559379119 [label="我永远都不会忘记这件事。",shape=box,color=blue];
		j1559379119 -> "Chat ends.";
	j147382484 [label="谢谢你。",shape=box,color=blue];
		j147382484 -> "Chat ends.";
	j3070402459 [label="是时候了。",shape=box,color=blue];
		j3070402459 -> j2611320388;
	j2611320388 [label="+20 Chemistry",color=orange];
		j2611320388 -> j1087893658;
	j1087893658 [label="我想我需要放下很多东西。",shape=box,color=blue];
		j1087893658 -> j1538600485;
	j1538600485 [label="我要和她谈谈。如果她叫我滚蛋，那就这样吧。但至少我尝试过了，对吧？",shape=box,color=blue];
		j1538600485 -> j2453596980;
	j2453596980 [label="这值得一试。莱蒂没有人们想象的那么无情。",shape=box,color=red];
		j2453596980 -> j1783133922;
	j1783133922 [label="我敢打赌。过这样的生活，做着我们每天所做的事，我们都筑起了墙。我们只是忘了在墙上装门而已。",shape=box,color=blue];
		j1783133922 -> j561144184;
