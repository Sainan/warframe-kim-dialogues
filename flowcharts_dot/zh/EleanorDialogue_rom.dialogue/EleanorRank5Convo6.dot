digraph {
	"EleanorRank5Convo6 begins." [color=orange];
		"EleanorRank5Convo6 begins." -> j1866329935;
	j1866329935 [label="你可能会注意到今天早上商场的气氛有点不一样。",shape=box,color=blue];
		j1866329935 -> j3034513249;
	j3034513249 [label="没有什么大的明显区别。只是更多人在微笑。时不时传来一阵笑声。空气中弥漫着一丝幸福。",shape=box,color=blue];
		j3034513249 -> j1151855237;
		j3034513249 -> j3915782939;
	j1151855237 [label="那是你做的，对吧？",shape=box,color=red];
		j1151855237 -> j3239724028;
	j3915782939 [label="马上回来。[结束]",shape=box,color=red];
		j3915782939 -> "Chat ends.";
	j3239724028 [label="这只是我回馈的方式。你知道，一个心灵感应者既能发送信息，也能接收信息。它不一定是对一个人的强力喷射。它可以像细雾一样喷洒在一个区域上。",shape=box,color=blue];
		j3239724028 -> j2436335141;
	"Chat ends." [color=orange];
	j2436335141 [label="你们那里有草坪洒水器吗？",shape=box,color=blue];
		j2436335141 -> j1772527024;
		j2436335141 -> j457073035;
	j1772527024 [label="我们有环管-独立机和像她一样的无人机。它们负责照料扎里曼号上的植物。你是指这种东西吗？",shape=box,color=red];
		j1772527024 -> j1293884719;
	j457073035 [label="洒水……器？",shape=box,color=red];
		j457073035 -> j3465041686;
	j1293884719 [label="可以这么说。",shape=box,color=blue];
		j1293884719 -> j398186323;
	j3465041686 [label="是的，亲爱的，洒水器。洒水器可以让你的草坪保持湿润。",shape=box,color=blue];
		j3465041686 -> j2767563700;
	j398186323 [label="我小时候很喜欢草坪洒水器。它们太神奇了。阳光最灿烂的时候，你可以在它们当中看到彩虹。穿过它们时，浑身都能感到一阵凉爽。",shape=box,color=blue];
		j398186323 -> j44514044;
	j2767563700 [label="也是把你弟弟的脸按上去直到他尖叫的理想工具。",shape=box,color=blue];
		j2767563700 -> j44514044;
	j44514044 [label="“幸福是一个草坪洒水器”。把这句话放在贺卡上吧。",shape=box,color=blue];
		j44514044 -> j1885337825;
	j1885337825 [label="你今天看起来心情不错。看到你这样我很高兴。",shape=box,color=red];
		j1885337825 -> j2797006829;
	j2797006829 [label="是的。我不觉得有必要把这种心情只留给自己。所以我把它扩散到整个商场。",shape=box,color=blue];
		j2797006829 -> j3714724157;
		j2797006829 -> j3652852246;
		j2797006829 -> j846576514;
	j3714724157 [label="这不是有点像给人下药吗？",shape=box,color=red];
		j3714724157 -> j591160838;
	j3652852246 [label="传播积极情绪？为什么不呢？我看不出这和放音乐有什么不同。",shape=box,color=red];
		j3652852246 -> j2191468802;
	j846576514 [label="那么今天我可能会看到亚瑟微笑吗？",shape=box,color=red];
		j846576514 -> j1950316795;
	j591160838 [label="……真好。我本来想做一个散播欢乐的秘密幸福小仙女，结果却变成了邪恶的毒品小妖精。我的好心情一落千丈。",shape=box,color=blue];
		j591160838 -> j1517830240;
	j2191468802 [label="哦！好比喻！",shape=box,color=blue];
		j2191468802 -> j1058949375;
	j1950316795 [label="也许。我希望如此。他忍受了太多。",shape=box,color=blue];
		j1950316795 -> j844295329;
	j1517830240 [label="好吧。我会停下来。",shape=box,color=blue];
		j1517830240 -> "Chat ends.";
	j1058949375 [label="如果碧可以通过音乐来影响心情，那么我也可以通过氛围做到，对吧？",shape=box,color=blue];
		j1058949375 -> j265771445;
	j844295329 [label="自从你来后，很多事情都变了。我指的不只是你和我。我指的是所有的一切。甚至是商场。",shape=box,color=blue];
		j844295329 -> j265771445;
	j265771445 [label="我很高兴碧说服我留下。感谢她。",shape=box,color=blue];
		j265771445 -> j3494588474;
		j265771445 -> j2344573855;
	j3494588474 [label="等等。她做了什么？",shape=box,color=red];
		j3494588474 -> j2835235374;
	j2344573855 [label="倒回去一下。我错过了什么。",shape=box,color=red];
		j2344573855 -> j2835235374;
	j2835235374 [label="我没有告诉你吗？",shape=box,color=blue];
		j2835235374 -> j367911343;
	j367911343 [label="啊。这是你来之前不久的事。那天我们整天都在外面试图清除一个科腐者巢穴。空气中弥漫着一种凶险的紧张气氛。战斗通常能消磨我们的怒气，让我们筋疲力尽，无法对彼此发脾气，但那一天不同。",shape=box,color=blue];
		j367911343 -> j2270963950;
	j2270963950 [label="我记得亚瑟说了类似“我们明天再做一遍”这样的话，这对我们来说就像是一个重拳。在回商场的路上，没有人说一句话。",shape=box,color=blue];
		j2270963950 -> j2107346740;
		j2270963950 -> j3414339863;
		j2270963950 -> j2080255280;
	j2107346740 [label="哇哦。亚瑟，真是个“提升团队士气”的好办法。",shape=box,color=red];
		j2107346740 -> j1858299449;
	j3414339863 [label="他可真会讨人喜欢……",shape=box,color=red];
		j3414339863 -> j1858299449;
	j2080255280 [label="他肩上担负着很多责任。这对他的影响很大。",shape=box,color=red];
		j2080255280 -> j963106878;
	j1858299449 [label="我想他只是想让我们集中精力工作。但我感觉自己快崩溃了。亚瑟和碧不再说话，阿米尔一直在烦昆西，我开始担心他的生命，而莱蒂一有机会就冷眼看我。",shape=box,color=blue];
		j1858299449 -> j1031009550;
	j963106878 [label="是的。通常他都能掩饰得很好，即使是在精疲力竭的时候。但我想他已经没有余力了。",shape=box,color=blue];
		j963106878 -> j2052414418;
	j1031009550 [label="那天我花了很多时间照镜子。我不断幻想镜子里的我抓住我的喉咙并嘶声说“你就是问题所在。如果不是因为你，这一切都不会发生。”",shape=box,color=blue];
		j1031009550 -> j3699268341;
		j1031009550 -> j2273206269;
	j2052414418 [label="为了维持团队的团结，他已经透支了自己。",shape=box,color=blue];
		j2052414418 -> j1858299449;
	j3699268341 [label="听起来这次反而像是别人进入了你的脑海。",shape=box,color=red];
		j3699268341 -> j1364525411;
	j2273206269 [label="这听起来像是莱蒂会说的废话。",shape=box,color=red];
		j2273206269 -> j238745994;
	j1364525411 [label="哦，这不是他们。这是我自己。在一那天，我一直压抑的自我怀疑和自我憎恨全都涌上心头。",shape=box,color=blue];
		j1364525411 -> j42409725;
	j238745994 [label="谢谢你。我知道你会支持我，这对我来说很重要。",shape=box,color=blue];
		j238745994 -> j3881903273;
	j42409725 [label="我心里想：这个团队需要的是一个替罪羊。",shape=box,color=blue];
		j42409725 -> j3429800552;
	j3881903273 [label="这句话我听莱蒂说了无数次了，最后我心想：也许她是对的。我留在这里会让每个人都处于危险之中。",shape=box,color=blue];
		j3881903273 -> j121839397;
	j3429800552 [label="一个怪物。一个可以归咎的人。",shape=box,color=blue];
		j3429800552 -> j121839397;
	j121839397 [label="我想出了一个完整的计划。我会在夜里溜走，躲在城里某个亚瑟永远找不到的地方，然后——这是我最自豪的部分——继续在远处帮助他们。",shape=box,color=blue];
		j121839397 -> j628691674;
		j121839397 -> j3331589072;
	j628691674 [label="我觉得他还是能找到你。他很有决心的。",shape=box,color=red];
		j628691674 -> j2419763945;
	j3331589072 [label="更可能的情况是，他会在找你的过程中死去。",shape=box,color=red];
		j3331589072 -> j474052444;
	j2419763945 [label="怎么找到？阿米尔无法追踪心灵感应。",shape=box,color=blue];
		j2419763945 -> j1876377054;
	j474052444 [label="想让我感到内疚吗？",shape=box,color=blue];
		j474052444 -> j1379998926;
	j1876377054 [label="我将会是一个高尚的流亡者。我会蹲在屋顶上，看起来被风吹得凌乱，却目光坚定。我可以报告科腐者的集结情况，让六人组知道炽蛇军在哪里设置路障，甚至充当紧急通讯中继。每次我救了莱蒂的命，我一定要让她感受到。小气？绝对是。",shape=box,color=blue];
		j1876377054 -> j1509310191;
	j1379998926 [label="别费心了。我已经够难受了。",shape=box,color=blue];
		j1379998926 -> "Chat ends.";
	j1509310191 [label="我选择了合适的时机。我等到阿米尔睡着了。然后穿上一件长外套，悄无声息地走向车库。",shape=box,color=blue];
		j1509310191 -> j4271836739;
	j4271836739 [label="防盗栅栏自动落了下来。我转过身，碧就在那里。",shape=box,color=blue];
		j4271836739 -> j819858884;
	j819858884 [label="我们彻夜长谈，相拥而泣。她也很受伤，非常深。等到太阳升起时，我决定留下。",shape=box,color=blue];
		j819858884 -> j3827092990;
		j819858884 -> j4043328445;
		j819858884 -> j1732641551;
	j3827092990 [label="现在还有时间改变主意。",shape=box,color=red];
		j3827092990 -> j2913549343;
	j4043328445 [label="碧太棒了。我很高兴她在那里陪伴你。",shape=box,color=red];
		j4043328445 -> j619358826;
	j1732641551 [label="真幸运。",shape=box,color=red];
		j1732641551 -> j619358826;
	j2913549343 [label="不管是什么怪诞的冲动让你写下句话，我希望你被它呛到。",shape=box,color=blue];
		j2913549343 -> "Chat ends.";
	j619358826 [label="+20 Chemistry",color=orange];
		j619358826 -> j2151457457;
	j2151457457 [label="是的。我想，在除夕夜，我们六个人都应该在这里。当然还有你。门外带来好运的陌生人。",shape=box,color=blue];
		j2151457457 -> "Chat ends.";
}