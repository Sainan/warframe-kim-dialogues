	j755648197 [label="你真的像莱蒂认为的那样生病了吗？",shape=box,color=red];
		j755648197 -> j1649546071;
	j1607811372 [label="你的个人感染情况怎么样？",shape=box,color=red];
		j1607811372 -> j4279102710;
	j1693036305 [label="你和莱蒂是怎么回事？",shape=box,color=red];
		j1693036305 -> j750155576;
	j1649546071 [label="让我们把事情说清楚。莱蒂并不认为我生病了。莱蒂认为我是个恶魔。",shape=box,color=blue];
		j1649546071 -> j2140268048;
	j4279102710 [label="还算可以，这已经比大多数寄生虫感染要好了。",shape=box,color=blue];
		j4279102710 -> j809662483;
	j750155576 [label="你听见她怎么叫我了吗？",shape=box,color=blue];
		j750155576 -> j3321850161;
		j750155576 -> j3582508440;
	j2140268048 [label="我很好奇拥有这么浪漫的世界观会是什么感觉。你以为医生会依据具体的事实操作，比如打十毫升的药物，识别某种微生物。",shape=box,color=blue];
		j2140268048 -> j776205007;
		j2140268048 -> j2498263165;
	j809662483 [label="我能感觉到科技细胞在我的血液中游动。如果我闭上眼睛，我可以看到像仙尘一样闪闪发光的东西。",shape=box,color=blue];
		j809662483 -> j1450909478;
	j3321850161 [label="老实说，没有。",shape=box,color=red];
		j3321850161 -> j226752752;
	j3582508440 [label="“La Bruja Metiche”。多管闲事的巫婆。",shape=box,color=red];
		j3582508440 -> j1156964339;
	j776205007 [label="你这么说很科学。我还以为你相信更多精神层面的东西呢？",shape=box,color=red];
		j776205007 -> j1901901845;
	j2498263165 [label="听起来你认为“失去人性”的部分有些夸张了。",shape=box,color=red];
		j2498263165 -> j2549176316;
	j1450909478 [label="它对我是善意的。你能理解吗？",shape=box,color=blue];
		j1450909478 -> j1078314183;
	j226752752 [label="“La Bruja Metiche”。多管闲事的巫婆。考虑到她的词汇量和脏话创造天赋，我猜她完全可以说得更难听。",shape=box,color=blue];
		j226752752 -> j1761785546;
	j1156964339 [label="没错。她坐在那里，带着嘲讽的笑容，摆弄她的试剂瓶和药水，还有那些装着血液和皮肤碎片的小瓶子，她那一群像使魔的老鼠到处乱窜，她居然还敢叫我女巫。",shape=box,color=blue];
		j1156964339 -> j1773064016;
		j1156964339 -> j2947879476;
		j1156964339 -> j2740235484;
	j1901901845 [label="是的。以我们的生活方式，怎么可能不相信精神世界的存在呢？",shape=box,color=blue];
		j1901901845 -> j2546009719;
	j2549176316 [label="我依然是埃莉诺·南丁格尔。这对我来说比作为“人”更重要。",shape=box,color=blue];
		j2549176316 -> j565400414;
	j1078314183 [label="它所做的一切，都是出于真诚的愿望，希望修复我，如果我够敏捷、够致命并且难以杀死，我就会成为一个好的宿主。因此，它履行了我们共生协议的一部分，赋予了我鲨鱼的皮肤、狼的牙齿，以及像皮鞭一样的舌头。",shape=box,color=blue];
		j1078314183 -> j776205007;
		j1078314183 -> j2498263165;
	j1761785546 [label="我曾听到她让昆西用操场设施和他自己身体的某些部分做一些相当有创意的事，我必须承认，我想象出来的画面让我笑了很久。",shape=box,color=blue];
		j1761785546 -> j2584215260;
	j1773064016 [label="我明白了，你们不喜欢对方。",shape=box,color=red];
		j1773064016 -> j2290319388;
	j2947879476 [label="你们之间一直都是这样吗？",shape=box,color=red];
		j2947879476 -> j2147928479;
	j2740235484 [label="她觉得你会失控。",shape=box,color=red];
