		j579831873 -> j4186714126;
	j4066234966 [label="她觉得你会失控。",shape=box,color=red];
		j4066234966 -> j2926140080;
	j4008688635 [label="我从未那样说过。",shape=box,color=blue];
		j4008688635 -> j435299503;
	j955664423 [label="而不是遵循那串扭来扭去的核苷酸双螺旋的计划？你知道吗，那是人类自从直立行走以来一直在做的事情。",shape=box,color=blue];
		j955664423 -> j3530826661;
	j1648160435 [label="像维克多·维德诺伊这样的人——还有谁会为自己选择那样的称号——他们通过声称自己是“自然的”来为自己的落后观点辩护。这是生物学上的原教旨主义。人被视为神的镜像之类的。",shape=box,color=blue];
		j1648160435 -> j3212019290;
	j4186714126 [label="但阿米尔很可爱，有点傻乎乎的，不是吗？阿米尔没有能咬开口粮罐头的牙齿。因此没人认为阿米尔是颗定时炸弹。",shape=box,color=blue];
		j4186714126 -> j5739300;
	j2926140080 [label="+10 Chemistry",color=orange];
		j2926140080 -> j1686274760;
	j435299503 [label="有些人认为人类是神圣的。像维克多那样的人。在他眼里，我是最大的亵渎。被感染了、无耻并且还是女性。",shape=box,color=blue];
		j435299503 -> j1261939642;
		j435299503 -> j429299947;
	j3530826661 [label="“我不听从微观暴君！我人类。我特别！寻找伴侣。繁衍后代。保护领土。呃。”",shape=box,color=blue];
		j3530826661 -> j1261939642;
		j3530826661 -> j429299947;
	j3212019290 [label="但从我们开始用石头敲打的那一刻起，我们就已经不自然了。伙计，语言是一种病毒。做人就是在一张白纸上自由涂鸦，而非追求当初我们被逐出伊甸园之前的那个状态。",shape=box,color=blue];
		j3212019290 -> j1261939642;
		j3212019290 -> j429299947;
	j5739300 [label="嗯。除了昆西。",shape=box,color=blue];
		j5739300 -> "Chat ends.";
	j1261939642 [label="科腐者可能会劫持你的思想，而你根本不会意识到。",shape=box,color=red];
		j1261939642 -> j702885552;
	j429299947 [label="埃莉诺，我曾经见过它吞噬整个卫星！",shape=box,color=red];
		j429299947 -> j3157180229;
	j702885552 [label="我的意思是……是的。你说得对。确实有些寄生虫能做到这一点。",shape=box,color=blue];
		j702885552 -> j2408099277;
	j3157180229 [label="那么，未被吞噬的卫星又有什么了不起的呢？被感染的版本为什么不能是一种改进？",shape=box,color=blue];
		j3157180229 -> j2256064235;
	j2408099277 [label="但如果它不是寄生虫呢？如果它是共生体呢？",shape=box,color=blue];
		j2408099277 -> j3922226229;
		j2408099277 -> j3003911921;
	j2256064235 [label="哦不！一块原本可以漫无目的地漂流几千年的普通太空岩石，突然有了生命力！还变得这么有趣！真是一场灾难！",shape=box,color=blue];
		j2256064235 -> j4255752601;
	j3922226229 [label="你根本不知道你在说什么。一旦有人被完全感染，他们就再也回不来了。完全变得面目全非。",shape=box,color=red];
		j3922226229 -> j3742076253;
	j3003911921 [label="我明白你需要努力接受现在的自己。你面对的是一个不可能的挑战。",shape=box,color=red];
		j3003911921 -> j1304510289;
	j4255752601 [label="这和维克多团队的废话没什么两样。因为情感而坚持原来的样子。",shape=box,color=blue];
		j4255752601 -> j3095157109;
	j3742076253 [label="我不知道你是不是想激怒我，但我曾经为了生存不得不与这些东西搏斗。有时你能看到曾经是人类的那些部分。头和脚就挂在那一团东西的外面。",shape=box,color=red];
		j3742076253 -> j1395304700;
	j1304510289 [label="要么讨厌现在的自己，要么接受现在的自己。而我觉得你很早以前就学会不再讨厌自己了。",shape=box,color=red];
		j1304510289 -> j957420281;
	j3095157109 [label="就好像原始的就是完美的一样。",shape=box,color=blue];
		j3095157109 -> j3922226229;
		j3095157109 -> j3003911921;
	j1395304700 [label="这不是共生体。它不在乎你想要什么。它不是来帮助你的。它是一种极具攻击性的癌症生命体，只想把一切都变成它自己。",shape=box,color=red];
		j1395304700 -> j4034410806;
	j957420281 [label="但是你血液中的这些恶魔并不是你的朋友。埃莉诺，你最好能牢牢地控制它们。相信我。",shape=box,color=red];
		j957420281 -> j3581658116;
	j4034410806 [label="提醒我一下，你的战甲是由什么制成的？",shape=box,color=blue];
		j4034410806 -> j854033753;
	j3581658116 [label="你以为我没有吗？",shape=box,color=blue];
		j3581658116 -> j1019436525;
	j854033753 [label="受感染的人类。",shape=box,color=red];
		j854033753 -> j637570726;
	j1019436525 [label="你知道控制这股怒火需要多大的力气吗？你知道维持这种表面的平静要付出多大的代价吗？",shape=box,color=blue];
		j1019436525 -> j2704893094;
	j637570726 [label="但他们受到的感染是被塑造和控制过的，不是吗？",shape=box,color=blue];
		j637570726 -> j889430;
	j2704893094 [label="如果你看到我在冥想，那不是我在与宇宙的伟大进行精神交流，而是我是在冷却那该死的“反应堆核心”。",shape=box,color=blue];
		j2704893094 -> j4162145710;
	j889430 [label="就像现在我体内的感染一样。",shape=box,color=blue];
		j889430 -> j334147439;
	j4162145710 [label="我能做的事情太多了，而我还没有去做。我能把这些……东西……带到你们甚至没想过的地方。",shape=box,color=blue];
		j4162145710 -> j2002140948;
	j334147439 [label="这意味着某个地方的某个人有勇气不再将感染者看作无脑的怪物，而是开始探索它的潜能。",shape=box,color=blue];
