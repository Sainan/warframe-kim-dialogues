digraph {
	"EleanorRank4Convo6 begins." [color=orange];
		"EleanorRank4Convo6 begins." -> j3167926813;
		"EleanorRank4Convo6 begins." -> j305290460;
	j3167926813 [label="所以……为什么是水晶？",shape=box,color=red];
		j3167926813 -> j2320033868;
	j305290460 [label="那么。你和昆西。你们有没有……你知道的？",shape=box,color=red];
		j305290460 -> j3270773103;
	j2320033868 [label="哦，我一直都很喜欢它们。小时候，我爸爸带我们去过拉德斯顿的创新博物馆，我和亚瑟都买了这些水晶种植套装。亚瑟种出来一坨屎，真的是长得跟屎一样的小石头。后来它被扔进了垃圾箱。我的是一颗美丽的蓝色钻石。从那天起，不知道为什么，我一直觉得水晶是属于我这边的。",shape=box,color=blue];
		j2320033868 -> j3484385383;
	j3270773103 [label="好，我今天感觉很大方，所以我免费告诉你这个。",shape=box,color=blue];
		j3270773103 -> j1242884617;
	j3484385383 [label="那么它们只是用来装饰吗？",shape=box,color=red];
		j3484385383 -> j3859622034;
	j1242884617 [label="我们从来没有。连一点苗头都没有。",shape=box,color=blue];
		j1242884617 -> j1241990928;
	j3859622034 [label="啊。不。",shape=box,color=blue];
		j3859622034 -> j3515032988;
	j1241990928 [label="我忍不住想到，昆西最喜欢的姿势可能是“伴侣在上面拿着一面镜子，这样他就能幻想自己正在和自己做爱”。",shape=box,color=blue];
		j1241990928 -> j2568032562;
	j3515032988 [label="我知道有很多关于水晶的文章都是废话。我对那种说法不感兴趣。我不相信水晶能净化我的气场或点亮我的脉轮。",shape=box,color=blue];
		j3515032988 -> j1014488525;
	j2568032562 [label="不谢谢。我只需要在远处欣赏就够了。",shape=box,color=blue];
		j2568032562 -> "Chat ends.";
	j1014488525 [label="但它们确实存在。",shape=box,color=blue];
		j1014488525 -> j2419194153;
	"Chat ends." [color=orange];
	j2419194153 [label="抱歉。我知道我听起来像个十足的疯子。",shape=box,color=blue];
		j2419194153 -> j2914831383;
		j2419194153 -> j2114639563;
		j2419194153 -> j328063072;
	j2914831383 [label="幸好说出这句话的人是你而不是我。它们只不过是一些漂亮的石头。",shape=box,color=red];
		j2914831383 -> j3142013492;
	j2114639563 [label="我有几个朋友是中枢。他们实际上是玻璃立方体形式的智慧生物。你怎么会觉得我会嘲笑你认为水晶确实存在的？",shape=box,color=red];
		j2114639563 -> j1190414543;
	j328063072 [label="我花了太多时间从地里挖水晶，所以它们在更深层次上的重要性不言而喻。你会惊讶地发现，有多少战甲的制作材料是某种或几种水晶。",shape=box,color=red];
		j328063072 -> j3375145070;
	j3142013492 [label="噢，有时候你真是让人抓狂。",shape=box,color=blue];
		j3142013492 -> j1463914348;
	j1190414543 [label="好吧，现在我因为觉得自己愚蠢真的感到蠢了。",shape=box,color=blue];
		j1190414543 -> j3912681251;
	j3375145070 [label="太神奇了，我很好奇！",shape=box,color=blue];
		j3375145070 -> j364861636;
	j1463914348 [label="我原本以为，在所有人中，你能够将自己的视角提升到高于纯粹物质的水平。",shape=box,color=blue];
		j1463914348 -> j2791866211;
	j3912681251 [label="我想你一定有些我梦寐以求的水晶。",shape=box,color=blue];
		j3912681251 -> j2747021315;
		j3912681251 -> j3108825858;
		j3912681251 -> j3309182963;
	j364861636 [label="请告诉我，智慧的旅行者，你们时代的水晶有什么特别之处？",shape=box,color=blue];
		j364861636 -> j3108825858;
	j2791866211 [label="去你的吧。让我和自己心爱的石头待在一起。",shape=box,color=blue];
		j2791866211 -> "Chat ends.";
	j2747021315 [label="氩结晶。只有在虚空中才能找到它，一旦你把它带出来它就会消散。",shape=box,color=red];
		j2747021315 -> j2210659726;
	j3108825858 [label="有一个完全由水晶构成的战甲。她叫 Citrine。",shape=box,color=red];
		j3108825858 -> j1231518421;
	j3309182963 [label="我们有一对被变成水晶的恋人。",shape=box,color=red];
		j3309182963 -> j1333218835;
	j2210659726 [label="你说“只有在虚空中才能找到它”，好像人们总是能随意进出虚空似的。我还以为这是一个让人疯狂的维度，而不是海滨度假胜地。",shape=box,color=blue];
		j2210659726 -> j2371685959;
	j1231518421 [label="+10 Chemistry",color=orange];
		j1231518421 -> j3485724078;
	j1333218835 [label="+10 Chemistry",color=orange];
		j1333218835 -> j1834525767;
	j2371685959 [label="我想我还没告诉你关于 Orokin 塔的事。它们就像虚空中的堡垒，是为了防止 Sentient 的攻击而建立的。",shape=box,color=red];
		j2371685959 -> j2985594831;
	j3485724078 [label="那是她的名字？我想我在你的脑海中瞥见过她！",shape=box,color=blue];
		j3485724078 -> j2978938562;
	j1834525767 [label="多么美丽又悲伤。成为爱情纪念的感觉如何呢？",shape=box,color=blue];
		j1834525767 -> j3374075526;
	j2985594831 [label="+20 Chemistry",color=orange];
		j2985594831 -> j3720351078;
	j2978938562 [label="那位漂亮的晶岩女士。我想我和她现在算是有点亲戚关系了。这是个令人欣慰的想法。",shape=box,color=blue];
		j2978938562 -> j293531678;
	j3374075526 [label="我希望没人会从它们晶体上弄下碎片来当作纪念品。在我们的时代，这肯定会发生。",shape=box,color=blue];
		j3374075526 -> j2810016293;
	j3720351078 [label="这听起来很熟悉。在拉德斯顿和曼彻尼亚地下有这些高度戒备的掩体。理论上，如果真的爆发了辐射战争，那些富人和权贵就可以躲进他们豪华避难所，边喝着含羞草鸡尾酒，边看着我们这些普通人在地上被烧成灰烬。",shape=box,color=blue];
		j3720351078 -> j481051865;
	j293531678 [label="那就，欢迎加入这个大家庭吧？",shape=box,color=red];
		j293531678 -> "Chat ends.";
	j2810016293 [label="人们确实试图这么做，但我们会保护他们。这是我们至少能做的事。",shape=box,color=red];
		j2810016293 -> "Chat ends.";
	j481051865 [label="历来如此，她叹了口气说道。",shape=box,color=blue];
		j481051865 -> j1269113306;
	j1269113306 [label="这很有意思。我小时候，大家都非常害怕辐射战争。我还记得那些公共服务广播。严肃地建议如何囤积物资、如何准备避难室，以及如何埋葬死去的亲人，即使你不想这么做。",shape=box,color=blue];
		j1269113306 -> j483126068;
	j483126068 [label="当我在你的脑海中看到那个反应堆爆炸的场景时，我仿佛又回到了那些噩梦中。我知道我们必须不惜一切代价阻止它。我会让其他人相信你的。",shape=box,color=blue];
		j483126068 -> j3518400451;
	j3518400451 [label="不过，等待这个星球的并不是辐射战争，对吧？而是科腐者。不管怎样，这个循环终将会结束，到那时，世界其他地方也会受到感染。",shape=box,color=blue];
		j3518400451 -> "Chat ends.";
}