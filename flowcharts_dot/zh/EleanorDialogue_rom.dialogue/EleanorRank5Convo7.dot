	j3809492293 [label="抱歉。这个问题更重要。",shape=box,color=blue];
		j3809492293 -> j375498270;
	"Chat ends." [color=orange];
	j375498270 [label="我认为“虚空无情之物”会试图以某种方式介入我们之间。",shape=box,color=blue];
		j375498270 -> j801018271;
	j801018271 [label="别笑。我能从骨髓深处感觉到。鲁莎卡里面的那个东西很想看到我们把对方撕成碎片。",shape=box,color=blue];
		j801018271 -> j3542042585;
	j3542042585 [label="我不知道为什么。我就是知道。",shape=box,color=blue];
		j3542042585 -> j2674170542;
		j3542042585 -> j2720666799;
	j2674170542 [label="是的。我觉得你是对的。",shape=box,color=red];
		j2674170542 -> j873179505;
	j2720666799 [label="也许这就是发生在英择谛家人身上的事。他们在他的实验室上方生活了多年，却一直不知情。当那孩子找到他们时，他们已经关系疏远，沉浸在怨恨之中。",shape=box,color=red];
		j2720666799 -> j873179505;
	j873179505 [label="我已经在你的脑海里看到了。阿尔布雷希特和洛德。“母亲”和“父亲”。你和你的父母。就我们所知，鲁莎卡和维克多也是如此。当然还有我们六个人。",shape=box,color=blue];
		j873179505 -> j3096471872;
	j3096471872 [label="这个东西的所作所为是冷酷的，扭曲的。但在某种意义上，它背后隐藏着动机。",shape=box,color=blue];
		j3096471872 -> j1919805817;
		j3096471872 -> j1184135413;
	j1919805817 [label="我觉得你说得对。在扎里曼号，父母是最先失去理智的。也许坚守者能坚持这么久，是因为他们都没有孩子。",shape=box,color=red];
		j1919805817 -> j2736884918;
	j1184135413 [label="我不喜欢假设去理解那些基本上可以称为*神*的事物。这个东西正在试图理解情感。它只是在玩弄自己的玩具，而且玩得很糟糕。",shape=box,color=red];
		j1184135413 -> j49302133;
	j2736884918 [label="就让它尝试来破坏我们之间的关系。我向太阳神发誓，我会结束它的。我会找到办法的。",shape=box,color=blue];
		j2736884918 -> j2515962289;
	j49302133 [label="这是个值得赞赏的理论，但我们并不确切知道，对吧？",shape=box,color=blue];
		j49302133 -> j1099560998;
	j2515962289 [label="宇宙中一定存在着那个东西无法对抗的力量。我选择全心全意相信这一点。",shape=box,color=blue];
		j2515962289 -> j2661274127;
	j1099560998 [label="如果你是对的，那么它是无辜的。几乎像个孩子。这和我们的认知不符——它是恶意的。",shape=box,color=blue];
		j1099560998 -> "Chat ends.";
	j2661274127 [label="如果它把我整个人吞下去，我会卡在它的喉咙里，让它窒息。",shape=box,color=blue];
		j2661274127 -> j4269066017;
		j2661274127 -> j3393137209;
	j4269066017 [label="这是一个很好的死法。",shape=box,color=red];
		j4269066017 -> j2004180830;
	j3393137209 [label="我还不知道我们该怎么做，但我支持你。我们必须赢。",shape=box,color=red];
		j3393137209 -> j683680096;
	j2004180830 [label="在这种情况下，我反正都要死了。",shape=box,color=blue];
		j2004180830 -> j915789063;
	j683680096 [label="如果这是个故事，你会想出一些巧妙的招数。你可能会挖出巨人的石心，用天鹅的羽毛敲碎它，或者诱骗它跳下悬崖。",shape=box,color=blue];
		j683680096 -> j2509806810;
	j915789063 [label="但至少我能把口水吐在它眼睛上。",shape=box,color=blue];
		j915789063 -> "Chat ends.";
	j2509806810 [label="你说过你是一名“记者”。这些不是你过去讲的那种故事，对吧？你讲的是真相，无论它有多丑陋。",shape=box,color=red];
		j2509806810 -> j683442497;
	j683442497 [label="如果那些古老的故事不是真实的，人们就不会一直讲述它们。",shape=box,color=blue];
		j683442497 -> j604341816;
	j604341816 [label="我并不是说巨人是真的。我只是说古老的故事讲述了另一种真相。",shape=box,color=blue];
		j604341816 -> j3331383126;
		j604341816 -> j791443976;
	j3331383126 [label="要度过难关，我们需要的不仅仅是童话故事，无论它们有多么鼓舞人心。",shape=box,color=red];
		j3331383126 -> j807735797;
	j791443976 [label="我生活在一个由故事构成的王国里。我知道故事的力量有多大。",shape=box,color=red];
		j791443976 -> j1748821022;
	j807735797 [label="我知道。我希望我有更多除了直觉之外的东西。",shape=box,color=blue];
		j807735797 -> j3793497428;
	j1748821022 [label="我认为所有那些打败巨人的故事都有一个共同点：它们都是关于学习规则并弄清楚如何巧妙地利用这些规则。",shape=box,color=blue];
		j1748821022 -> j152487790;
	j3793497428 [label="我们正在面对的是庞大、可怕并且超出我们理解的事物。通常只有孩子们才需要面对这种情况。这就是为什么童话存在的原因。",shape=box,color=blue];
		j3793497428 -> j4066625199;
		j3793497428 -> j891990754;
	j152487790 [label="有些怪物很蠢。有些怪物被迫只能按字面意义行事。有些怪物有秘密。但在你弄清楚怪物受限于什么规则之前，你是无法取胜的。",shape=box,color=blue];
		j152487790 -> j4066625199;
		j152487790 -> j891990754;
	j4066625199 [label="它被困在墙里。我甚至不确定那堵墙是什么。",shape=box,color=red];
		j4066625199 -> j4208442325;
	j891990754 [label="我不认为虚空无情之物受到任何规则的约束。",shape=box,color=red];
		j891990754 -> j2726454331;
	j4208442325 [label="没关系。如果它真的被困住了，那么我们可以推断出两件事，对吗？",shape=box,color=blue];
		j4208442325 -> j437664289;
	j2726454331 [label="一定会有规则约束它！即使是神也有规则！无论虚空无情之物看起来有多强大，它不是神。",shape=box,color=blue];
		j2726454331 -> j1172266293;
	j437664289 [label="一是，它可以被困住，甚至被囚禁。",shape=box,color=blue];
		j437664289 -> j3131772543;
	j1172266293 [label="你打败了士魂大帝。不管你失败了多少次，屁股多少次着地。最终你还是赢了。你为什么不能同样战胜墙中人呢？",shape=box,color=blue];
		j1172266293 -> j2816187737;
	j3131772543 [label="+15 Chemistry",color=orange];
		j3131772543 -> j861911811;
