		j2863781009 -> j2598050398;
	j2598050398 [label="是的。他很善良。英俊。强壮。也很容易被激起斗志，这是一个很大的优点。不过他一心想加入军队。",shape=box,color=blue];
		j2598050398 -> j510300136;
