		j800829110 -> j3461074769;
	j3461074769 [label="我们都不是软弱的人。我们习惯了艰苦的生活。相比普通的不列颠尼克兵营，这地方简直像五星级酒店。",shape=box,color=blue];
		j3461074769 -> j1725459704;
