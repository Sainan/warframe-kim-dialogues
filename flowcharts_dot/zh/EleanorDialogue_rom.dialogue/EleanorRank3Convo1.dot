		j2143123238 -> j2096373770;
	j2096373770 [label="有时候他们会冒出一个想法，然后立刻想着“她听到我在想什么了吗？”让他们保持猜测其实更好。只要他们不知道，就不会被伤害到。",shape=box,color=blue];
