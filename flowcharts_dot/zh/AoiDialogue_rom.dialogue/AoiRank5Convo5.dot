		j2653665234 -> j2493745446;
		j2653665234 -> j3550032501;
	j2086868518 [label="也许是这样，他们的女儿没能多陪陪他们，这让我心里很不是滋味。你知道，我们的人生时间有限。我们应该尽可能多地和我们爱的人在一起。",shape=box,color=red];
		j2086868518 -> j653398907;
