digraph {
	"AoiRank5Convo5 begins." [color=orange];
		"AoiRank5Convo5 begins." -> j3758532483;
		"AoiRank5Convo5 begins." -> j905787314;
	j3758532483 [label="我真的很佩服你对于建设更美好世界的坚定承诺。这种信念来自哪里？",shape=box,color=red];
		j3758532483 -> j1838400905;
	j905787314 [label="你的令人印象深刻的干劲。是什么激发了你的斗志？",shape=box,color=red];
		j905787314 -> j1838400905;
	j1838400905 [label="哦，哇，谢谢你这么说。呃……嗯。没有什么重大的决定性时刻影响了我的人生道路。",shape=box,color=blue];
		j1838400905 -> j2563542506;
	j2563542506 [label="据我所知，我从父母那里继承了这种精神。他们是务实的人，但也是梦想家。",shape=box,color=blue];
		j2563542506 -> j175757002;
	j175757002 [label="我的意思是，如果你不是一个梦想家，你就不会去造火箭探索太空的。",shape=box,color=blue];
		j175757002 -> j2330373803;
		j175757002 -> j1244476694;
		j175757002 -> j1545093567;
	j2330373803 [label="等等，你的父母是造火箭的？",shape=box,color=red];
		j2330373803 -> j1192248239;
	j1244476694 [label="这可真是个不得了的家族事业。听起来他们把标准定得很高。",shape=box,color=red];
		j1244476694 -> j3809113614;
	j1545093567 [label="这可不是普通的家族传统。他们有没有提到是什么激励了他们？",shape=box,color=red];
		j1545093567 -> j4101697277;
	j1192248239 [label="嗯，爸爸造。妈妈也懂得怎么造。",shape=box,color=blue];
		j1192248239 -> j51935087;
	j3809113614 [label="是的……当我成为一名士兵时，他们很明确地表示这并不是他们希望为我提供的生活。",shape=box,color=blue];
		j3809113614 -> j51935087;
	j4101697277 [label="说的不多。他们不喜欢公开分享他们的梦想。我觉得他们相信自己对世界的贡献足以说明一切。",shape=box,color=blue];
		j4101697277 -> j51935087;
	j51935087 [label="爸爸是一名航天工程师，妈妈在他们相识的大学里教空间系统设计。",shape=box,color=blue];
		j51935087 -> j1317558221;
		j51935087 -> j1725596891;
		j51935087 -> j3812606666;
	j1317558221 [label="哇，你们家一部分是智囊团，一部分是发射台。",shape=box,color=red];
		j1317558221 -> j497661844;
	j1725596891 [label="和他们交谈一定非常有趣。",shape=box,color=red];
		j1725596891 -> j2016587998;
	j3812606666 [label="听起来他们很聪明。在这样的环境中成长一定很不同寻常。",shape=box,color=red];
		j3812606666 -> j2016587998;
	j497661844 [label="哈哈，其实不是。大多数时间，我们家都很安静。",shape=box,color=blue];
		j497661844 -> j3121418564;
	j2016587998 [label="是啊，我想是这样",shape=box,color=blue];
		j2016587998 -> j4110572827;
	j3121418564 [label="我们大多数的对话是在碰到时说上几句的“嘿，宝贝”和“嗨”，他们会随便吃点东西，然后就回办公室继续熬夜工作",shape=box,color=blue];
		j3121418564 -> j387065905;
	j4110572827 [label="我们的交流大多是在厨房碰到时说上几句的“嘿，宝贝”和“嗨”，他们会随便吃点东西，然后就回办公室继续熬夜工作",shape=box,color=blue];
		j4110572827 -> j387065905;
	j387065905 [label="他们很忙",shape=box,color=blue];
		j387065905 -> j1995951952;
	j1995951952 [label="只要我不惹麻烦，他们就不太担心我。虽然我内心有很多激情，但我真的很擅长避免惹麻烦。",shape=box,color=blue];
		j1995951952 -> j3389527650;
		j1995951952 -> j968275628;
		j1995951952 -> j1285588260;
	j3389527650 [label="我能理解。我的父母也总是很忙。运行扎里曼号飞船需要每个人全身心投入。不过，至少我们总能抽出时间一起吃饭。",shape=box,color=red];
		j3389527650 -> j14066202;
	j968275628 [label="这很合理。当工作那么忙碌时，剩下的时间并不多，但我敢打赌他们一直在想你。",shape=box,color=red];
		j968275628 -> j1461551799;
	j1285588260 [label="我能理解他们。我自己就经历过，太空旅行几乎不留给人休息时间。看起来你的懂事让他们省了不少心",shape=box,color=red];
		j1285588260 -> j2384782799;
	j14066202 [label="+5 Chemistry",color=orange];
		j14066202 -> j3707578331;
	j1461551799 [label="+5 Chemistry",color=orange];
		j1461551799 -> j1506624159;
	j2384782799 [label="嗯，我想我确实有帮到他们。",shape=box,color=blue];
		j2384782799 -> j969299734;
	j3707578331 [label="那真好。我很高兴他们这么做。",shape=box,color=blue];
		j3707578331 -> j359877321;
	j1506624159 [label="可能吧",shape=box,color=blue];
		j1506624159 -> j2793158594;
	j969299734 [label="我希望是这样。",shape=box,color=blue];
		j969299734 -> j36892396;
	j359877321 [label="现在我意识到那些时刻有多珍贵，但在我成长的时候并没多少这样的时刻，却一点都不觉得奇怪。",shape=box,color=blue];
		j359877321 -> j36892396;
	j2793158594 [label="其实，我觉得确实是这样 :)",shape=box,color=blue];
		j2793158594 -> j36892396;
	j36892396 [label="我能理解为什么我的父母那么专注于他们的工作。在他们看来，这是他们表达爱的方式。他们希望为我提供生活保障，同时为人类能拥有更好的未来创造科技",shape=box,color=blue];
		j36892396 -> j128567510;
