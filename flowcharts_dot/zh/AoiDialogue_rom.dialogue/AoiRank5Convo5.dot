		j128567510 -> j3688422456;
		j128567510 -> j3131669919;
		j128567510 -> j246303314;
	j3688422456 [label="也许是这样，他们的女儿没能多陪陪他们，这让我心里很不是滋味。你知道，我们的人生时间有限。我们应该尽可能多地和我们爱的人在一起。",shape=box,color=red];
		j3688422456 -> j2454020870;
	j3131669919 [label="确实美好，但也很实际。他们做的是有意义的事情，没有什么比这更重要的了。",shape=box,color=red];
		j3131669919 -> j3553387211;
	j246303314 [label="这也是一种强大的爱，即使它不是那种会在围坐在餐桌上体现出来的爱。",shape=box,color=red];
		j246303314 -> j595020687;
	j2454020870 [label="+5 Chemistry",color=orange];
		j2454020870 -> j4137998164;
	j3553387211 [label="但我还是忍不住想到我们在一起度过的时间并不多。",shape=box,color=blue];
