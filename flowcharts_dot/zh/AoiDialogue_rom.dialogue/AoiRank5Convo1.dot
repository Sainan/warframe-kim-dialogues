		j2635583419 -> j2180542585;
	j4233798620 [label="有人怀疑埃莉诺？真的吗？",shape=box,color=red];
		j4233798620 -> j1827768429;
	j2180542585 [label="我很难相信亚瑟会容忍人们对他的姐姐存有戒心。",shape=box,color=red];
		j2180542585 -> j1827768429;
	j1827768429 [label="早些时候，莱蒂和昆西也对她有过怀疑，他们担心她会完全成为科腐者。亚瑟不愿意听，所以他们保持了沉默，但其实或多或少我们都感受到了。而如果*我们*感觉得到，你可以想象对于埃莉诺来说，这种感觉多么敏感和清晰。",shape=box,color=blue];
		j1827768429 -> j735818683;
		j1827768429 -> j949966555;
		j1827768429 -> j4006613475;
	j735818683 [label="感谢太阳神赐予我们像亚瑟这样的一位好弟弟。",shape=box,color=red];
		j735818683 -> j2219929114;
	j949966555 [label="总是知道别人在想什么，这对她来说一定是种煎熬。",shape=box,color=red];
		j949966555 -> j521316046;
	j4006613475 [label="我对莱蒂和昆西真的很失望。",shape=box,color=red];
		j4006613475 -> j521316046;
	j2219929114 [label="他是的，对。但我觉得，这让埃莉诺当时的处境更难。他不顾一切地想要保护她，而她也知道，如果他认为她因感染而饱受痛苦，他可能会亲自出手解决此事。",shape=box,color=blue];
		j2219929114 -> j2894129262;
		j2219929114 -> j3735811437;
	j521316046 [label="他们只是害怕。恐惧会使人做出奇怪的行为。",shape=box,color=blue];
		j521316046 -> j90383027;
		j521316046 -> j1542156962;
	j2894129262 [label="你是说他可能……杀了他自己的姐姐？",shape=box,color=red];
		j2894129262 -> j1030529962;
	j3735811437 [label="失去控制的恐惧一定非常可怕。尤其是在亚瑟那样看顾她的情况下。",shape=box,color=red];
		j3735811437 -> j1030529962;
	j90383027 [label="显然，从那时起这个团队已经走了很长的一段路。",shape=box,color=red];
		j90383027 -> j868058649;
	j1542156962 [label="我希望当时的他们能够克服恐惧。",shape=box,color=red];
		j1542156962 -> j1690190948;
	j1030529962 [label="我从没认为他会杀了她，但埃莉诺却这么想。她为什么会这么想，那是她和亚瑟之间的事。我只是很高兴她不再那样想了。",shape=box,color=blue];
		j1030529962 -> j1645528174;
		j1030529962 -> j553516824;
	j868058649 [label="我曾听人说过，真正让人们受到创伤的不是遭遇了什么，而是如何面对这些遭遇。在了解过莱蒂和昆西的内心世界后，我敢肯定，如果他们因为恐惧导致埃莉诺出事，他们绝对不会原谅自己。",shape=box,color=blue];
		j868058649 -> j844381658;
		j868058649 -> j2100428256;
