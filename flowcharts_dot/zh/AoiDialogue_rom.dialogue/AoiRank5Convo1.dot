		j2891134992 -> j3597410679;
		j2891134992 -> j4242058353;
		j2891134992 -> j4230535545;
	j3597410679 [label="真是太符合你的风格了，哈哈。我猜那次谈心进行得不错吧？",shape=box,color=red];
