		j3513295901 -> j1520134779;
		j3513295901 -> j3334475735;
		j3513295901 -> j2426533663;
	j1520134779 [label="真是太符合你的风格了，哈哈。我猜那次谈心进行得不错吧？",shape=box,color=red];
		j1520134779 -> j3105840375;
	j3334475735 [label="我很高兴你当时陪在她身边。那一晚可能改变了她的一切。",shape=box,color=red];
