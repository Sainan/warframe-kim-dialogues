		j4198752880 -> j3163857591;
	j3928691993 [label="J'étais impuissante, comme un bébé lapin. J'avais fondu comme une bougie. J'avais disparu. Je restais allongée là et je poussais cri après cri. Avant cela, je m'étais toujours considérée comme une dure à cuire.",shape=box,color=blue];
