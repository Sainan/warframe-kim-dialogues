		j3260021239 -> j1880533949;
	j950521767 [label="ILS ONT DES BLAGUES RINGARDES, DANS LE FUTUR",shape=box,color=blue];
		j950521767 -> j1740356241;
	"Chat ends." [color=orange];
	j1880533949 [label="Le vin me manque. Je n'ai pas bu de vrai vin depuis si longtemps. Tout le vin de Höllvanie a été réquisitionné par le commandement de la Scaldra quelques jours après le début de l'épidémie.",shape=box,color=blue];
		j1880533949 -> j1117599434;
	j1740356241 [label="+10 Chemistry",color=orange];
		j1740356241 -> j3633407037;
	j1117599434 [label="Quincy et Arthur ont essayé de faire de la gnôle de prison à partir de sachets de rations de fruits, mais ceux d'entre nous qui avaient encore un peu de dignité ont refusé. Un rare moment de solidarité avec Lettie, en fait.",shape=box,color=blue];
		j1117599434 -> j1238509549;
	j3633407037 [label="Je suis de meilleure humeur, maintenant. Merci.",shape=box,color=blue];
		j3633407037 -> "Chat ends.";
	j1238509549 [label="L’autre chose, la chose beaucoup plus importante, c’est le futur.",shape=box,color=blue];
		j1238509549 -> j1767008646;
	j1767008646 [label="Ce n'est pas de ta faute, mais... maintenant que j'ai vu le futur à travers tes yeux, je me sens misérable à un tel point que les méthodes habituelles ne peuvent rien y faire.",shape=box,color=blue];
		j1767008646 -> j3271333128;
		j1767008646 -> j1078914929;
	j3271333128 [label="Qu'est-ce que je peux faire pour te remonter le moral?",shape=box,color=red];
		j3271333128 -> j467525188;
	j1078914929 [label="Les méthodes habituelles?",shape=box,color=red];
		j1078914929 -> j3179874177;
	j467525188 [label="Je ne sais pas. Peut-être une danse exotique?",shape=box,color=blue];
		j467525188 -> j176171651;
	j3179874177 [label="Passer la soirée avec Amir à l'écouter s'enthousiasmer pour une série de bandes dessinées ou le jeu rétro sur lequel il joue actuellement.",shape=box,color=blue];
		j3179874177 -> j3113286970;
	j176171651 [label="Décris-moi la mort de Ballas avec plein de détails?",shape=box,color=blue];
		j176171651 -> j2611318720;
	j3113286970 [label="Je m'entoure de cristaux et je chante mentalement jusqu'à ce que Lettie appelle l'exorciste.",shape=box,color=blue];
		j3113286970 -> j1117937711;
	j2611318720 [label="C'est gentil de ta part de le proposer, mais je pense que je vais devoir supporter cette mélancolie.",shape=box,color=blue];
		j2611318720 -> j2542152696;
	j1117937711 [label="C'est une tristesse d'un autre genre. Elle m'alourdit comme une couverture mouillée.",shape=box,color=blue];
		j1117937711 -> j2534034588;
		j1117937711 -> j669918165;
	j2542152696 [label="Peut-être que je vais peindre.",shape=box,color=blue];
		j2542152696 -> j227430165;
	j2534034588 [label="La Höllvanie de 1999 est dure à vivre! La Scaldra, le Techrot, le Néant et bien d'autres choses encore. Est-ce que le futur semble vraiment s pire que ça?",shape=box,color=red];
		j2534034588 -> j3071973149;
	j669918165 [label="Oh, allez... tu es coriace. Tu peux gérer ça. Il y a tellement de trucs sympas ici.",shape=box,color=red];
		j669918165 -> j2397965338;
	j227430165 [label="+5 Chemistry",color=orange];
		j227430165 -> "Chat ends.";
	j3071973149 [label="Écoute, je n'ai jamais vraiment aimé la science-fiction. Arthur a traversé une phase comme ça, mais il a toujours préféré les histoires d'armée. Des hommes virils qui sont des héros, des histoires saines pour les garçons, des tonnes de sous-entendus, je suis sûre que tu connais ce genre de choses.",shape=box,color=blue];
		j3071973149 -> j4026989973;
	j2397965338 [label="Très bien. Prends-moi par la main et promène-moi dans les rues de Höllvanie.",shape=box,color=blue];
		j2397965338 -> j1006502820;
	j4026989973 [label="Mais j'en savais assez sur la science-fiction pour savoir que dans un futur lointain, nous sommes censés avoir notre vie en ordre. Tout est censé être brillant et aseptisé. Les robots font tout le travail dur, il n'y a pas de maladie, personne n'a besoin d'argent, tout est fait de tours argentées, de ciel bleu et de gens sexy.",shape=box,color=blue];
		j4026989973 -> j3555511233;
		j4026989973 -> j972295301;
	j1006502820 [label="Je connais un endroit où on peut manger des galettes de navet râpé et regarder le soleil se coucher sur les vestiges d’une école.",shape=box,color=blue];
		j1006502820 -> j3562284007;
	j3555511233 [label="On a des robots. Ils font une grande partie du travail pénible.",shape=box,color=red];
		j3555511233 -> j3529248239;
	j972295301 [label="Mais ce dont tu parles n'est que fiction, n'est-ce pas? Alors, pourquoi espères-tu que la réalité sera à la hauteur?",shape=box,color=red];
		j972295301 -> j2728283395;
	j3562284007 [label="Et plus tard on pourra siroter des cocktails à la lueur d'un char en feu.",shape=box,color=blue];
		j3562284007 -> "Chat ends.";
	j3529248239 [label="Bon, d'accord. Je veux bien te croire.",shape=box,color=blue];
		j3529248239 -> j4222800956;
	j2728283395 [label="Arf. Je ne... c'est compliqué. Laisse-moi t'expliquer.",shape=box,color=blue];
		j2728283395 -> j199603499;
	j4222800956 [label="Mais ces robots ont-ils rendu la vie plus facile aux gens ordinaires?",shape=box,color=blue];
		j4222800956 -> j950507476;
	j199603499 [label="Je ne dis pas que je m'attendais à ce que le futur ressemble à une bande dessinée pour enfants. Je veux juste dire qu'il y avait un certain optimisme qui y était associé.",shape=box,color=blue];
		j199603499 -> j3078763375;
	j950507476 [label="Ou alors sont-ils simplement devenus quelque chose de banal?",shape=box,color=blue];
		j950507476 -> j352828129;
		j950507476 -> j3600648266;
	j3078763375 [label="Je pense que nous avons tous pris pour acquis que, même si les choses étaient merdiques en ce moment, nous finirions par les régler. Notre civilisation est destinée à progresser. Nous avons commencé à nous frapper la tête à coups de gourdins et, après quelques milliers d'années, nous avons trouvé des moyens de régler nos différends sans passer par là.",shape=box,color=blue];
		j3078763375 -> j880619645;
		j3078763375 -> j65871905;
	j352828129 [label="Tu es déterminée à voir le côté négatif de chaque chose, n'est-ce pas?",shape=box,color=red];
		j352828129 -> j774147183;
	j3600648266 [label="Ok d'accord. On a pas résolu le problème de l’exploitation des gens en ayant des robots.",shape=box,color=red];
		j3600648266 -> j2811822244;
	j880619645 [label="Les gens restent des gens, même si la technologie évolue.",shape=box,color=red];
		j880619645 -> j4051329433;
	j65871905 [label="Excuse-moi de t'avoir déçue?",shape=box,color=red];
		j65871905 -> j2796407657;
	j774147183 [label="Évidemment.",shape=box,color=blue];
		j774147183 -> j3986122994;
	j2811822244 [label="Voilà. C'est ça qui est si décevant.",shape=box,color=blue];
		j2811822244 -> j1677503553;
	j4051329433 [label="Bingo. Tu as mis le doigt sur ce que je trouve si déprimant dans tout ça.",shape=box,color=blue];
		j4051329433 -> j1920287461;
	j2796407657 [label="Précisément.",shape=box,color=blue];
		j2796407657 -> j1920287461;
	j3986122994 [label="C'est comme ça que tu sais ce qui doit être changé.",shape=box,color=blue];
		j3986122994 -> "Chat ends.";
	j1677503553 [label="Vous êtes avancés, mais vous étiez censés être ÉCLAIRÉS aussi",shape=box,color=blue];
		j1677503553 -> j180792716;
	j1920287461 [label="Vous aviez à votre disposition toute cette incroyable technologie magique et tout ce que vous faisiez avec elle, c'était de trouver de nouvelles façons de vous exploiter les uns les autres. La science nous a rendus moins égaux, pas plus égaux.",shape=box,color=blue];
		j1920287461 -> j886092808;
		j1920287461 -> j2042409921;
	j180792716 [label="Merci de ta patience avec moi. Je sais que ça fait beaucoup.",shape=box,color=blue];
		j180792716 -> j4072819434;
	j886092808 [label="Euh, petit rappel poli: je n'ai RIEN fait de tout ça?",shape=box,color=red];
		j886092808 -> j3889905662;
	j2042409921 [label="Je transmettrai ta plainte aux Orokins.",shape=box,color=red];
		j2042409921 -> j3889905662;
	j4072819434 [label="+15 Chemistry",color=orange];
		j4072819434 -> "Chat ends.";
	j3889905662 [label="Oh par les Dieux, je ne parle pas de toi personnellement, mais de ton époque.",shape=box,color=blue];
		j3889905662 -> j854489283;
	j854489283 [label="Au lieu d’avancer, vous avez reculé. L'incarnation des Dieux-rois! De tout ce qui pouvait être recréé, vous LES avez choisis.",shape=box,color=blue];
		j854489283 -> j1939360513;
	j1939360513 [label="Les Tennos ont tué les Orokins. Tu as oublié cette partie?",shape=box,color=red];
		j1939360513 -> j1463763465;
	j1463763465 [label="Non, je n'ai pas oublié la partie où les orphelins, victimes de violences psychologiques, assassinent l'élite immortelle. J'adore ce passage. Si je ferme les yeux, je peux presque imaginer les cris.",shape=box,color=blue];
		j1463763465 -> j857134842;
		j1463763465 -> j357871880;
	j857134842 [label="Très bien. L'avenir est un désastre. Tu peux te concentrer là-dessus si tu veux.",shape=box,color=red];
		j857134842 -> j603265790;
	j357871880 [label="Ce n'est... pas quelque chose... que j'avouerais aux gens.",shape=box,color=red];
		j357871880 -> j3296340797;
	j603265790 [label="Je suis du côté des gens qui essaient d’améliorer les choses.",shape=box,color=red];
		j603265790 -> j2895199346;
	j3296340797 [label="... l’ironie sera-t-elle également obsolète dans le futur?",shape=box,color=blue];
		j3296340797 -> "Chat ends.";
	j2895199346 [label="+20 Chemistry",color=orange];
		j2895199346 -> "Chat ends.";
}