digraph {
	"EleanorRank5Convo6 begins." [color=orange];
		"EleanorRank5Convo6 begins." -> j4247012690;
	j4247012690 [label="Tu remarqueras peut-être que le centre commercial semble un peu différent ce matin.",shape=box,color=blue];
		j4247012690 -> j1796758346;
	j1796758346 [label="Rien de délirant. Juste quelques sourires de plus sur quelques visages. Un rire ici et là. Une touche de bonheur dans l'air.",shape=box,color=blue];
		j1796758346 -> j3892337265;
		j1796758346 -> j2766991930;
	j3892337265 [label="Tu y es pour quelque chose, n'est-ce pas?",shape=box,color=red];
		j3892337265 -> j2379780403;
	j2766991930 [label="Je re [Fin.]",shape=box,color=red];
		j2766991930 -> "Chat ends.";
	j2379780403 [label="C'est juste ma façon d'offrir quelque chose en retour. Une télépathe peut aussi bien envoyer que recevoir, comme tu le sais. Il n'est pas nécessaire que ce soit un jet d'eau orienté sur une seule personne. Cela peut plutôt ressembler à une fine brume pulvérisée sur une zone.",shape=box,color=blue];
		j2379780403 -> j633242609;
	"Chat ends." [color=orange];
	j633242609 [label="Y a-t-il des arroseurs de pelouse, à ton époque?",shape=box,color=blue];
		j633242609 -> j455081386;
		j633242609 -> j4069694311;
	j455081386 [label="Nous avons VERD-IE et des drones comme elle. Ils s'occupaient des plantes sur le Zariman. C'est à ce genre de choses que tu fais allusion?",shape=box,color=red];
		j455081386 -> j266591665;
	j4069694311 [label="Des arroseurs?",shape=box,color=red];
		j4069694311 -> j1049057164;
	j266591665 [label="Ça fera l'affaire.",shape=box,color=blue];
		j266591665 -> j1046863034;
	j1049057164 [label="Oui, des arroseurs. Des pulvérisateurs d'eau pour garder la pelouse bien humide.",shape=box,color=blue];
		j1049057164 -> j3786791796;
	j1046863034 [label="J'adorais les arroseurs de pelouse quand j'étais petite. Ils étaient magiques. Quand le soleil brillait à son apogée, on pouvait y attraper des arcs-en-ciel. On courait à travers et on ressentait un délicieux frisson.",shape=box,color=blue];
		j1046863034 -> j1240549541;
	j3786791796 [label="C'était également idéal pour faire de bonnes farcess à mon frère...",shape=box,color=blue];
		j3786791796 -> j1240549541;
	j1240549541 [label="\"Un arroseur de pelouse, c'est que du bonheur\". On pourrait inscrire ça sur une carte de vœux.",shape=box,color=blue];
		j1240549541 -> j2470232616;
	j2470232616 [label="Tu sembles de bonne humeur aujourd'hui. Je m'en réjouis.",shape=box,color=red];
		j2470232616 -> j4261689980;
	j4261689980 [label="Je le suis. Et je ne vois pas pourquoi je devrais la garder pour moi. Alors, j'en mets partout dans le centre commercial.",shape=box,color=blue];
		j4261689980 -> j3163911559;
		j4261689980 -> j2903021461;
		j4261689980 -> j126728993;
	j3163911559 [label="C'est pas un peu comme droguer les gens?",shape=box,color=red];
		j3163911559 -> j3324922355;
	j2903021461 [label="Diffuser des ondes positives? Pourquoi pas. Je ne vois pas en quoi c'est différent de jouer de la musique.",shape=box,color=red];
		j2903021461 -> j2665639845;
	j126728993 [label="Alors, aujourd'hui sera peut-être le jour où je pourrai voir Arthur sourire?",shape=box,color=red];
		j126728993 -> j2254980327;
	j3324922355 [label="... génial. J'essaie d'être la fée secrète du bonheur qui répand la joie, et au lieu de ça, je deviens le méchant gobelin de la drogue. Ma bonne humeur vient de chuter.",shape=box,color=blue];
		j3324922355 -> j1146777124;
	j2665639845 [label="Oh! Belle analogie!",shape=box,color=blue];
		j2665639845 -> j2413715732;
	j2254980327 [label="Peut-être. Je l'espère. Il a dû endurer tellement de choses.",shape=box,color=blue];
		j2254980327 -> j578033852;
	j1146777124 [label="Très bien, j'arrête.",shape=box,color=blue];
		j1146777124 -> "Chat ends.";
	j2413715732 [label="Si Aoi peut influencer l'ambiance à travers la musique, je peux le faire via les bonnes ondes, n'est-ce pas?",shape=box,color=blue];
		j2413715732 -> j1388781101;
	j578033852 [label="Tant de choses ont changé depuis ton arrivée. Je ne parle pas seulement de toi et de moi. Je parle de tout. Même du centre commercial.",shape=box,color=blue];
		j578033852 -> j1388781101;
	j1388781101 [label="Je suis contente qu'Aoi m'ait persuadée de rester. Qu'elle soit bénie.",shape=box,color=blue];
		j1388781101 -> j1177768368;
		j1388781101 -> j1413577589;
	j1177768368 [label="Attends. Elle a fait quoi?",shape=box,color=red];
		j1177768368 -> j2290934019;
	j1413577589 [label="Attends. J'ai raté quelque chose.",shape=box,color=red];
		j1413577589 -> j2290934019;
	j2290934019 [label="Je ne te l'ai pas dit?",shape=box,color=blue];
		j2290934019 -> j3737248482;
	j3737248482 [label="Ah. Ce n'était pas longtemps avant ton arrivée. Nous avions passé toute la journée dehors à essayer d'éradiquer une ruche de Techrot. Il y avait une tension meurtrière dans l'air. Les combats consomment généralement notre colère et nous laissent trop épuisés pour nous attaquer les uns aux autres, mais ce jour-là, c'était différent.",shape=box,color=blue];
		j3737248482 -> j1685807651;
	j1685807651 [label="Je me souviens qu'Arthur a dit quelque chose comme \"on recommence demain\" et ça nous a tous frappés comme un coup de massue. Personne n'a dit un mot sur le chemin du retour vers le centre commercial.",shape=box,color=blue];
		j1685807651 -> j1236263134;
		j1685807651 -> j3737414416;
		j1685807651 -> j230486890;
	j1236263134 [label="Waouh. Quelle belle façon de remonter le moral de l'équipe, Arthur.",shape=box,color=red];
		j1236263134 -> j1403075827;
	j3737414416 [label="Il peut être un tel charmeur...",shape=box,color=red];
		j3737414416 -> j1403075827;
	j230486890 [label="Il porte un lourd fardeau et il accuse le coup.",shape=box,color=red];
		j230486890 -> j1333570939;
	j1403075827 [label="Je pense qu'il essayait juste de nous concentrer sur le boulot. Mais je me sentais brisée. Arthur et Aoi ne parlaient pas, Amir tapait tellement sur les nerfs de Quincy que je commençais à craindre pour sa vie, et Lettie me regardait du coin de l'œil à chaque fois qu'elle en avait l'occasion.",shape=box,color=blue];
		j1403075827 -> j1235735750;
	j1333570939 [label="Oui. En général, il parvient à le cacher, même quand il est épuisé. Mais je suppose qu'il n'en pouvait plus.",shape=box,color=blue];
		j1333570939 -> j3738208196;
	j1235735750 [label="Ce jour-là, j'ai passé beaucoup de temps à me regarder dans le miroir. Je n'arrêtais pas d'imaginer mon reflet m'attraper par la gorge et me siffler: \"C'est TOI le problème. Sans toi, rien de tout cela ne se serait produit.\"",shape=box,color=blue];
		j1235735750 -> j1642394244;
		j1235735750 -> j415952835;
	j3738208196 [label="Il ne ménage pas ses efforts pour essayer de maintenir l'équipe soudée.",shape=box,color=blue];
		j3738208196 -> j1403075827;
	j1642394244 [label="On dirait que tout le monde est entré dans TA tête, pour une fois.",shape=box,color=red];
		j1642394244 -> j2744005191;
	j415952835 [label="Cela ressemble au genre de conneries que Lettie raconte.",shape=box,color=red];
		j415952835 -> j755116506;
	j2744005191 [label="Oh, ce n'était pas eux. C'était moi. Tous les doutes et la haine que j'avais refoulés en moi ont refait surface ce jour-là.",shape=box,color=blue];
		j2744005191 -> j388555329;
	j755116506 [label="Merci. Je sais que tu me soutiens et cela compte beaucoup pour moi.",shape=box,color=blue];
		j755116506 -> j482656802;
	j388555329 [label="Je me suis dit: ce dont ce groupe a besoin, c’est d’un bouc émissaire.",shape=box,color=blue];
		j388555329 -> j1788787570;
	j482656802 [label="J'avais entendu cette phrase de la bouche de Lettie tellement de fois que j'ai fini par me dire: peut-être qu'elle a raison. Je mets tout le monde en danger en restant ici.",shape=box,color=blue];
		j482656802 -> j1277123759;
	j1788787570 [label="Un monstre. Quelqu'un à blâmer.",shape=box,color=blue];
		j1788787570 -> j1277123759;
	j1277123759 [label="J'ai élaboré un plan complet: je m'éclipserais dans la nuit, je me cacherais dans la ville, là où Arthur ne me trouverait jamais, et - c'est la partie dont j'étais la plus fière - je continuerais à aider à distance.",shape=box,color=blue];
		j1277123759 -> j1322506556;
		j1277123759 -> j3514228306;
	j1322506556 [label="Je pense qu'il t'aurait trouvée. Il est assez obstiné.",shape=box,color=red];
		j1322506556 -> j3911099884;
	j3514228306 [label="Le scénario le plus probable est qu'il serait mort en essayant de te retrouver.",shape=box,color=red];
		j3514228306 -> j1731956195;
	j3911099884 [label="Comment? Amir ne peut pas tracer la télépathie.",shape=box,color=blue];
		j3911099884 -> j2888616493;
	j1731956195 [label="Tu essaies de me faire culpabiliser?",shape=box,color=blue];
		j1731956195 -> j3067949094;
	j2888616493 [label="J'allais être une noble exilée. Je m'accroupirais sur les toits, giflée par le vent et résolue. Je pourrais signaler l'accumulation de Techrot, informer l'Hexagone de l'endroit où les Scaldras installent des barrages routiers, et même servir de relais de communication d'urgence. Chaque fois que je sauverais les fesses de Lettie, je ferais en sorte qu'elle le sache. Mesquin? Certainement.",shape=box,color=blue];
		j2888616493 -> j162165157;
	j3067949094 [label="Pas besoin, je me sens déjà assez mal comme ça.",shape=box,color=blue];
		j3067949094 -> "Chat ends.";
	j162165157 [label="J'ai choisi mon moment. J'ai attendu qu'Amir s'endorme. Puis j'ai enfilé un long manteau et je me suis dirigée vers le garage aussi silencieusement que possible.",shape=box,color=blue];
		j162165157 -> j3132300954;
	j3132300954 [label="La grille de sécurité s'est ouverte toute seule. Je me suis retournée et j'ai vu Aoi.",shape=box,color=blue];
		j3132300954 -> j3239396695;
	j3239396695 [label="Nous avons parlé toute la nuit et pleuré dans les bras l'une de l'autre. Elle souffrait aussi. Profondément. Au lever du soleil, j'avais décidé de rester.",shape=box,color=blue];
		j3239396695 -> j3058728859;
		j3239396695 -> j82565097;
		j3239396695 -> j2769590069;
	j3058728859 [label="Il est encore temps de changer d’avis.",shape=box,color=red];
		j3058728859 -> j1171809209;
	j82565097 [label="Aoi est merveilleuse. Heureusement qu'elle a été là pour toi.",shape=box,color=red];
		j82565097 -> j3493220183;
	j2769590069 [label="Tu as eu de la chance.",shape=box,color=red];
		j2769590069 -> j3493220183;
	j1171809209 [label="Quelle que soit l'impulsion grotesque qui t'a poussé à écrire cela, j'espère que tu t'étoufferas avec.",shape=box,color=blue];
		j1171809209 -> "Chat ends.";
	j3493220183 [label="+20 Chemistry",color=orange];
		j3493220183 -> j3068101554;
	j3068101554 [label="Oui. Je crois que nous devions tous les six être là pour le réveillon du Nouvel An. Et toi, bien sûr. Le Voyageur qui sonne à la porte en apportant du bonheur.",shape=box,color=blue];
		j3068101554 -> "Chat ends.";
}