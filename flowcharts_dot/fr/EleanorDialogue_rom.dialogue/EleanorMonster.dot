		j3055069876 -> "Chat ends.";
	j2049894467 [label="Ce n'est pas que je ne sois pas extrêmement reconnaissante et honnêtement un peu choquée, mais... pourquoi? POURQUOI me laisserais-tu vivre? Alors que tous les autres ici seraient prêts à me tirer une balle dans la tête?",shape=box,color=blue];
