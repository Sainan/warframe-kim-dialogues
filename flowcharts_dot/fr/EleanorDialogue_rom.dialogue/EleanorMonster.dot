		j1977614069 -> "Chat ends.";
	j1002066507 [label="Et si le Techrot s'avérait être une autre aberration déclenchée par l'homme comme ton Homme dans le Mur? Et alors? Me traquerais-tu et me tuerais-tu?",shape=box,color=blue];
		j1002066507 -> j3095382189;
		j1002066507 -> j4074274796;
