		j4017983553 -> j4122001544;
	j4244230809 [label="Et comment je saurais s'il ne reste rien d'Eleanor? Tu parles de la façon dont le Techrot pense. De la façon dont il te parle. Il est clairement intelligent.",shape=box,color=red];
