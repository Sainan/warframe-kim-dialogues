		j3612273701 -> j2324112516;
	j2782013873 [label="Nous avons tous connu cela à un moment de notre vie... essayer de porter un masque pour impressionner quelqu'un et nous planter parce que nous ne voyions pas où nous allions. Et toi et moi, on venait de se rencontrer quand tu me l'as demandé. Très bien. Pardon accordé.",shape=box,color=blue];
		j2782013873 -> j3775595441;
	j1929630235 [label="Ah, je vois. Tu es le héraut envoyé par le Roi Tonnerre pour enrôler la belle Dame Eleanor. Nul doute que toutes sortes de vilenies m'attendent. Des histoires fantastiques et ainsi de suite.",shape=box,color=blue];
