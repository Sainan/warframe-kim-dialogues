	"Chat ends." [color=orange];
	j3854539825 [label="L'autre moi. Celle qui est devenue religieuse. J'ai essayé de la contacter.",shape=box,color=blue];
		j3854539825 -> j1095630595;
	j3839196059 [label="Depuis ton arrivée, une question me taraude. Une question à laquelle je crois avoir répondu. Et par Sol, je ne sais pas ce que j'ai fait.",shape=box,color=blue];
		j3839196059 -> j3386524946;
	j1095630595 [label="Je viens de le faire.",shape=box,color=blue];
		j1095630595 -> j638090625;
	j3386524946 [label="Commençons par le début. Je ne sais pas de quoi tu parles. Quelle question?",shape=box,color=red];
		j3386524946 -> j3206757554;
	j638090625 [label="Bien sûr, ça n'aurait jamais pu marcher tant que je restais dans ce monde. Je suis déjà \"moi\" dans ce monde. J'ai dû traverser cette blancheur rugissante. La fugue de l'étoile noire. Le gouffre des sangsues. C'est comme ça qu'on accède aux autres côtés. Les peut-être et les \"aurait pu être\".",shape=box,color=blue];
		j638090625 -> j843886923;
	j3206757554 [label="Toi! La question du foutu Toi. Tu n'existes pas une fois, mais deux.",shape=box,color=blue];
		j3206757554 -> j1293436167;
	j843886923 [label="Le Néant! J'ai vu le Néant!",shape=box,color=blue];
		j843886923 -> j2172312425;
		j843886923 -> j7724127;
	j1293436167 [label="Et si tu peux exister dans un autre monde, comme une autre version de toi-même, pourquoi ne pourrais-je pas le faire aussi?",shape=box,color=blue];
		j1293436167 -> j2448608140;
	j2172312425 [label="Es-tu... sûre de ne pas t'être servie dans la réserve spéciale de Lettie?",shape=box,color=red];
		j2172312425 -> j1797328307;
	j7724127 [label="Cela ne devrait pas être possible, Eleanor. Personne n'entre dans le Néant avant qu'Albrecht Entrati ne le fasse. C'est très loin dans le futur par rapport à où nous sommes maintenant.",shape=box,color=red];
		j7724127 -> j3351220615;
	j2448608140 [label="Quelque part, dans les vastes univers possibles, dont l’existence est désormais confirmée, se trouvait cette version de moi-même. Je me suis toujours demandé si je devais devenir Elle.",shape=box,color=blue];
		j2448608140 -> j4233698404;
	j1797328307 [label="Cher Voyageur, j'admire ta capacité à utiliser l'humour comme mécanisme d'adaptation, mais pour le moment, je suis tout à fait sérieuse.",shape=box,color=blue];
		j1797328307 -> j2784277517;
	j3351220615 [label="J'y suis allée avec mon esprit, pas avec mon corps. C'est ça, la différence.",shape=box,color=blue];
		j3351220615 -> j1138569904;
	j4233698404 [label="Celle qui s'est détournée du monde pour devenir nonne.",shape=box,color=blue];
		j4233698404 -> j1142324256;
	j2784277517 [label="J'ai fait ce que j'avais prévu de faire.",shape=box,color=blue];
		j2784277517 -> j2958618962;
	j1138569904 [label="Aïe, ce mal de tête, aïe, aïe, aïe.",shape=box,color=blue];
		j1138569904 -> j3014708669;
	j1142324256 [label="Une apôtre de Lua était assise au sommet d'une montagne quelque part, des nuages d'encens fleurissant autour d'elle.",shape=box,color=blue];
		j1142324256 -> j2732870240;
	j2958618962 [label="Je n’ai pas seulement vu le Néant, j’y suis entrée.",shape=box,color=blue];
		j2958618962 -> j1138569904;
	j3014708669 [label="Check boolean EleanorIndifference2",color=orange];
		j3014708669 -> j2571748697 [label=true,color=green];
		j3014708669 -> j1013599781 [label=true,color=green];
		j3014708669 -> j2275022909 [label=false,color=brown];
		j3014708669 -> j1013599781 [label=false,color=brown];
	j2732870240 [label="J'ai essayé de la contacter, de traverser le Néant pour la retrouver.",shape=box,color=blue];
		j2732870240 -> j3212129121;
	j2571748697 [label="Si tu es vraiment entrée dans le Néant, je ne peux pas te dire à quel point tu t'es mise en danger. Tu sais ce qu'il y a, là-bas et tu as quand même plongé dedans?",shape=box,color=red];
		j2571748697 -> j2293521745;
	j1013599781 [label="Tu es folle?!",shape=box,color=red];
		j1013599781 -> j642016661;
	j2275022909 [label="Qu'est-ce qie tu as vu? Il 'est passé quoi?",shape=box,color=red];
		j2275022909 -> j2958360397;
	j3212129121 [label="Boolean EleanorSister is now true.",color=orange];
		j3212129121 -> j3406798106;
		j3212129121 -> j1000945446;
	j2293521745 [label="C'est comme ça que je suis entrée dans le Néant. À travers ce souvenir. L'horreur n'était que de la fumée et à travers la fumée, il y avait une PORTE. Si je n'avais pas été si effrayée, je l'aurais vue clairement la première fois.",shape=box,color=blue];
		j2293521745 -> j866392418;
	j642016661 [label="Pas le moins \"des mondes\".",shape=box,color=blue];
		j642016661 -> j2561662054;
	j2958360397 [label="il y a un sentiment pour lequel il n'existe pas de mot. Tu regardes le monde et tu SAIS que toute la banalité - les bâtiments, les voitures, les gens, les emplois, le travail, la routine - n'est qu'une fine couche sur une horreur sans fond, et si les gens s'en rendaient compte, ils se mettraient à crier et n'arrêteraient jamais.",shape=box,color=blue];
		j2958360397 -> j1064226685;
	j3406798106 [label="Oh non.",shape=box,color=red];
		j3406798106 -> j1095630595;
	j1000945446 [label="Et...?",shape=box,color=red];
		j1000945446 -> j1095630595;
	j866392418 [label="C'était une erreur. Il a pris Rusalka. Il a failli prendre Albrecht. Il aurait pu facilement te prendre aussi.",shape=box,color=red];
		j866392418 -> j2441763609;
	j2561662054 [label="Écoute seulement. S'il te plaît. Et essaie de comprendre.",shape=box,color=blue];
		j2561662054 -> j2384742046;
		j2561662054 -> j935454589;
	j1064226685 [label="Je suis tombée à travers la croûte.",shape=box,color=blue];
		j1064226685 -> j502486834;
	j2441763609 [label="Il a essayé. Je l'ai senti. Je savais qu'il était curieux et qu'il essayait de m'atteindre.",shape=box,color=blue];
		j2441763609 -> j2384742046;
		j2441763609 -> j935454589;
	j2384742046 [label="Alors, parmi l’infinité de mondes possibles, tu es tombée par hasard sur le bon?",shape=box,color=red];
		j2384742046 -> j3585416874;
	j935454589 [label="Eleanor, l'Indifférence peut ressembler à ce qu'elle veut. Elle ressemblait à Albrecht lorsqu'il l'a rencontrée pour la première fois.",shape=box,color=red];
		j935454589 -> j236745896;
	j502486834 [label="Ce n'était pas le vide. J'aurais pu supporter ça. Cela a duré une éternité, mais ce n'était pas seulement un espace froid. Il y avait quelque chose là-dedans avec moi.",shape=box,color=blue];
		j502486834 -> j3789746628;
	j3585416874 [label="Ce n'est pas grave si je ne peux pas te convaincre. Je sais que ça a l'air fou. Écoute-moi, d'accord?",shape=box,color=blue];
		j3585416874 -> j4084634786;
	j236745896 [label="Je pense qu'il a peut-être senti ce que tu voulais et qu'il a pris la forme que tu espérais voir.",shape=box,color=red];
		j236745896 -> j4084634786;
	j3789746628 [label="Boolean EleanorIndifference2 is now true.",color=orange];
		j3789746628 -> j866392418;
	j4084634786 [label="Je vais faire un marché avec toi.",shape=box,color=blue];
		j4084634786 -> j458001967;
	j458001967 [label="Laisse-moi d'abord finir de te raconter ce qu'il s'est passé.",shape=box,color=blue];
		j458001967 -> j3085145036;
	j3085145036 [label="Une fois que j'aurai fini, tu pourras décider par toi-même si c'était l'Indifférence qui me jouait des tours ou non. Je te promets que je te prendrai au sérieux.",shape=box,color=blue];
		j3085145036 -> j838019867;
	j838019867 [label="Marché conclu?",shape=box,color=blue];
		j838019867 -> j1840391229;
		j838019867 -> j2280110249;
	j1840391229 [label="D'accord.",shape=box,color=red];
		j1840391229 -> j3344397722;
	j2280110249 [label="Je n'arrive pas à croire que tu aies fait ça. Ne fais plus jamais ça, Eleanor! JE NE VEUX PAS TE PERDRE À CAUSE DE CETTE CHOSE!",shape=box,color=red];
		j2280110249 -> j12440454;
	j3344397722 [label="Donc, comme je le disais, je l'ai trouvée.",shape=box,color=blue];
		j3344397722 -> j1083855420;
	j12440454 [label="Excuse-moi?",shape=box,color=blue];
		j12440454 -> j1725006915;
	j1083855420 [label="Je pouvais sentir l'Indifférence m'observer. Je pouvais la sentir faire une grimace comme l'une de ces momies desséchées du désert alors qu'elle tendait la main vers moi. Je l'ai tendue vers elle et elle m'a fait traverser.",shape=box,color=blue];
		j1083855420 -> j2847830884;
	j1725006915 [label="Je ne sais pas trop quoi dire. Je suis un peu inquiète parce que tu as l'air sous le choc, mais je ne savais pas que tu te souciais autant de moi.",shape=box,color=blue];
		j1725006915 -> j896154610;
	j2847830884 [label="Elle était assise en position du lotus dans une sorte de sanctuaire en plein air, à côté d'une fosse pleine de braises. C'était la nuit. La lune était faible, simple fantôme à travers les nuages défilants.",shape=box,color=blue];
		j2847830884 -> j3271423536;
	j896154610 [label="Je vais bien. Ne t'inquiète pas pour moi.",shape=box,color=blue];
		j896154610 -> j4266916958;
	j3271423536 [label="Elle a mis un doigt sur ses lèvres.",shape=box,color=blue];
		j3271423536 -> j2186229710;
	j4266916958 [label="+5 Chemistry",color=orange];
		j4266916958 -> j905519160;
	j2186229710 [label="Pendant une seconde, j'ai senti cette main géante qui tâtonnait toujours vers moi. Puis elle s'est rétractée, comme si une guillotine s'était abattue sur l'un de ses doigts.",shape=box,color=blue];
		j2186229710 -> j2442321274;
	j905519160 [label="Je ne peux rien promettre, mais je vais essayer de ne plus t'effrayer.",shape=box,color=blue];
		j905519160 -> j2179731796;
		j905519160 -> j521658734;
	j2442321274 [label="L'herbe bruissait. Les grillons chantaient. Nous étions seules.",shape=box,color=blue];
		j2442321274 -> j3645653948;
	j2179731796 [label="Très bien... Dis-moi... juste ce qu'il s'est passé.",shape=box,color=red];
		j2179731796 -> j3344397722;
	j521658734 [label="Je ne peux pas participer à ça. [Fin.]",shape=box,color=red];
		j521658734 -> "Chat ends.";
	j3645653948 [label="Tout cela me semble terriblement familier. Et ensuite?",shape=box,color=red];
		j3645653948 -> j556153100;
	j556153100 [label="Nous étions assise l'une en face de l'autre, près du feu. Elle ne parlait pas, mais elle me laissait lire dans son esprit, avec douceur et fraternité.",shape=box,color=blue];
		j556153100 -> j2680646499;
	j2680646499 [label="Elle était si heureuse, si parfaitement satisfaite. Une femme à l'endroit qui lui convenait. Des pieds calleux, des yeux clairs. Respirant le feu et buvant la lune.",shape=box,color=blue];
		j2680646499 -> j2917826614;
		j2680646499 -> j3340075463;
	j2917826614 [label="Tu as donc pensé que cela AURAIT été le bon choix pour toi?",shape=box,color=red];
		j2917826614 -> j3730250364;
	j3340075463 [label="As-tu été tentée de rester?",shape=box,color=red];
		j3340075463 -> j3730250364;
	j3730250364 [label="Non, au contraire!",shape=box,color=blue];
		j3730250364 -> j3241356094;
	j3241356094 [label="Je n'avais pas besoin de suivre ce chemin, car elle l'avait déjà fait. Tu ne vois pas? Je suis libre, maintenant. Je n'ai plus besoin de me demander \"et si\", parce qu'il y a déjà une sœur Eleanor. Elle me soutient. Est-ce que ce que je dis est logique?",shape=box,color=blue];
		j3241356094 -> j3219811035;
	j3219811035 [label="En quelque sorte? Tout ce que je veux savoir, c'est: est-ce que ça a aidé?",shape=box,color=red];
		j3219811035 -> j378383156;
	j378383156 [label="Oh, oui. Je n'ai jamais été aussi sûre de ce que je devais faire.",shape=box,color=blue];
		j378383156 -> j2844069530;
	j2844069530 [label="+15 Chemistry",color=orange];
		j2844069530 -> j3711060974;
	j3711060974 [label="Je vais prendre la vie qu'elle m'a offerte et la vivre jusqu'au bout.",shape=box,color=blue];
		j3711060974 -> "Chat ends.";
}