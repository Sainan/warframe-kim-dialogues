	j3003694146 [label="+5 Chemistry",color=orange];
		j3003694146 -> j1662587532;
	j835656451 [label="Une sorte de communion, je suppose. Pas nécessairement quelque chose de physique. Je veux juste un contact mental avec quelque chose qui n'est pas humain, ni même terrestre.",shape=box,color=blue];
		j835656451 -> j1352551803;
