	"Chat ends." [color=orange];
	j1662587532 [label="Excuse-toi. Je ne suis pas un \"monstre à tentacules\". Je suis un monstre qui a UNE TENTACULE. Une. Jusqu'à présent.",shape=box,color=blue];
		j1662587532 -> j3790643897;
	j1352551803 [label="J'ai essayé le contact mental avec l'un des rats de Lettie, une fois, juste pour voir ce qui se passerait.",shape=box,color=blue];
		j1352551803 -> j2077688313;
		j1352551803 -> j1246919907;
	j3091068537 [label="CETTE CHOSE? C'est la même chose que ton \"Indifférence\"?",shape=box,color=blue];
		j3091068537 -> j772831884;
	j3790643897 [label="Si tout ce que je voulais faire était de converser avec des choses qui ont des tentacules, j'irais au Parc Zoologique de la Princesse Lucina et je me lierais d'amitié avec une pieuvre.",shape=box,color=blue];
		j3790643897 -> j2725143136;
	j2077688313 [label="La pauvre petite créature n'a pas été blessée, n'est-ce pas?",shape=box,color=red];
