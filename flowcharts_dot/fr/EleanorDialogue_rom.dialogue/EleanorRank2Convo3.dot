		j789706876 -> j444186021;
	j3635069697 [label="Mais une fois que c'est fait, quel sera le défi? La tentation? Tu n'en verras jamais. L'idée est de te couper de tout ce qui pourrait te distraire du sacré.",shape=box,color=blue];
