	j1909914181 [label="Un homme coincé dans un mur?",shape=box,color=red];
		j1909914181 -> j3199000716;
	j1842313797 [label="Qu'est-ce que c'était?",shape=box,color=red];
		j1842313797 -> j2546801623;
	j3199000716 [label="Je ne sais pas. Mais il savait que j'étais là. Il était CONSCIENT de ma présence. Comme un éléphant serait conscient d'une mouche rampant dans son œil.",shape=box,color=blue];
		j3199000716 -> j2935781298;
	j2546801623 [label="+5 Chemistry",color=orange];
		j2546801623 -> j3199000716;
	j2935781298 [label="Je me souviens avoir pensé: \"Rien d’aussi grand ne devrait savoir que j’existe\".",shape=box,color=blue];
		j2935781298 -> j4173510143;
	j4173510143 [label="Je ressentais son incroyable mépris à mon égard, une indifférence si monumentale que j’avais envie de cesser d’exister sur-le-champ.",shape=box,color=blue];
		j4173510143 -> j2458554408;
		j4173510143 -> j219722515;
	j2458554408 [label="Ça... fait ça.",shape=box,color=red];
		j2458554408 -> j2895746499;
	j219722515 [label="Qu'est-ce que tu as fait?",shape=box,color=red];
		j219722515 -> j2895746499;
	j2895746499 [label="Je pense que j'aurais été capable de gérer s'il m'avait détestée. J'aurais alors eu une certaine valeur, une certaine importance. Mais il me considérait comme insignifiante, et j'ai vu ce qu'il a vu. J'étais une tache, une futilité.",shape=box,color=blue];
		j2895746499 -> j3572963508;
	j3572963508 [label="Tout ce que j’avais été, chaque moment de tendresse, chaque rire, chaque morceau d’Eleanor Nightingale a été emporté par cette tempête.",shape=box,color=blue];
		j3572963508 -> j4143056816;
	j4143056816 [label="Cette chose a senti ma lumière s'éteindre et, je le jure, un petit frisson de satisfaction l'a parcourue. Comme si elle était contente de m'avoir écrasée. Une puce sous son ongle.",shape=box,color=blue];
		j4143056816 -> j2874826552;
	j2874826552 [label="Mais tu es revenue, n'est-ce pas? Tu es toujours là. Tu as prouvé que cette chose se trompait.",shape=box,color=red];
