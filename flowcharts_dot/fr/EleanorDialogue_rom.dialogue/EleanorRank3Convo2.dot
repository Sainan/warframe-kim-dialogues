		j2845093527 -> j2946094802;
	j763282120 [label="il y a un sentiment pour lequel il n'existe pas de mot. Tu regardes le monde et tu SAIS que toute la banalité - les bâtiments, les voitures, les gens, les emplois, le travail, la routine - n'est qu'une fine couche sur une horreur sans fond, et si les gens s'en rendaient compte, ils se mettraient à crier et n'arrêteraient jamais.",shape=box,color=blue];
		j763282120 -> j3580349626;
	j2946094802 [label="J'avais à peine réussi à me préparer mentalement à ça, et maintenant, quoi? On ne mentionne pas le nom du Diable, au cas où il apparaîtrait?",shape=box,color=blue];
		j2946094802 -> j4087681430;
	j3580349626 [label="Je suis tombée à travers la croûte.",shape=box,color=blue];
		j3580349626 -> j4219432340;
	j4087681430 [label="Si tu entends des cris dans la nuit, c'est moi.",shape=box,color=blue];
		j4087681430 -> "Chat ends.";
	j4219432340 [label="Ce n'était pas le vide. J'aurais pu supporter ça. Cela a duré une éternité, mais ce n'était pas seulement un espace froid. Il y avait quelque chose là-dedans avec moi.",shape=box,color=blue];
		j4219432340 -> j1382391561;
		j4219432340 -> j1909914181;
		j4219432340 -> j1842313797;
	j1382391561 [label="Un œil?",shape=box,color=red];
