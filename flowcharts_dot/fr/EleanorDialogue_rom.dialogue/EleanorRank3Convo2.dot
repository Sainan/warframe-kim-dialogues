	j2187215426 [label="Oui, c'est vrai. Mais tu es encore là toi aussi.",shape=box,color=red];
		j2187215426 -> j3645442087;
	j2624572514 [label="Les étoiles ne cessent pas de brûler simplement parce que l’espace est grand, froid et sombre.",shape=box,color=red];
		j2624572514 -> j1928848222;
	j3645442087 [label="L'Indifférence finit par l'emporter, peu importe le nombre de platitudes que nous jetons dans l'abîme.",shape=box,color=blue];
		j3645442087 -> j2209694225;
	j1928848222 [label="Mais elles cesseront de brûler. Un jour. La mort thermique de l'Univers.",shape=box,color=blue];
		j1928848222 -> j3645442087;
	j2209694225 [label="C'est le problème quand on ne devient pas fou. On souffre de la clarté de notre pensée.",shape=box,color=blue];
		j2209694225 -> j4119836392;
	j4119836392 [label="Tu sais de quoi je parle. Tu aurais pu te briser comme un œuf de moineau ou te brûler le cerveau avec des drogues jusqu'à ne plus rien ressentir. Et ça aurait fait moins mal que ce... savoir.",shape=box,color=blue];
		j4119836392 -> j2613448580;
	j2613448580 [label="Je ne comprends pas pourquoi tu n’as pas encore quitté cette conversation.",shape=box,color=blue];
		j2613448580 -> j610736933;
		j2613448580 -> j724305479;
	j610736933 [label="Écoute. L'entropie existe. Je comprends. Peut-être que la Grande Indifférence est pareille. Mais si elle l'emporte à la fin, tu ne le sais pas et moi non plus.",shape=box,color=red];
		j610736933 -> j2112626958;
	j724305479 [label="J'attends que tu prennes ta décision, honnêtement. Préfères-tu choisir la solution de facilité et devenir folle? Ou préfères-tu rester debout et te battre?",shape=box,color=red];
		j724305479 -> j365830892;
	j2112626958 [label="Ce que je sais, c'est que je ne vais pas la laisser gagner MAINTENANT.",shape=box,color=red];
