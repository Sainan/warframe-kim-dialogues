		j573595948 -> j1503478012;
	j348571374 [label="Il est simplement juste de t'avertir que le bon navire Eleanor peut couler, soit dans la mer sanglante, soit dans le marais noir de la nuit, et tu ne voudras pas être à bord quand cela se produira.",shape=box,color=blue];
