		j846771013 -> j1356521212;
	j592607378 [label="À part Arthur et Aoi qui sortent ensemble? Non. Pas à ma connaissance, en tout cas... et je le SAURAIS.",shape=box,color=blue];
		j592607378 -> j2067941401;
	j2145061504 [label="Pas besoin de répondre à ça.",shape=box,color=blue];
		j2145061504 -> j2215623280;
	j1356521212 [label="Maintenant, elle agit comme si j'étais une autre personne. C'est pratique pour elle. Si je ne suis pas vraiment Eleanor, alors elle peut être aussi blessante envers moi qu'elle le souhaite, car elle ne fait pas de mal à la personne qu'elle connaissait. Je pense que c'est lâche.",shape=box,color=blue];
		j1356521212 -> j2235360891;
	j2067941401 [label="Et au cas où tu envisagerais de faire un pas vers l'un d'entre nous...",shape=box,color=blue];
		j2067941401 -> j3734539572;
	j2215623280 [label="Si je pensais qu'elle savait ce qu'elle faisait et qu'elle avait des sentiments sincères pour le gars, j'en serais ravie. Évidemment.",shape=box,color=blue];
		j2215623280 -> j20479924;
	j2235360891 [label="Mais elle veut quand même que tu te rétablisses, n'est-ce pas?",shape=box,color=red];
		j2235360891 -> j2352673604;
	j3734539572 [label="... tu pourrais faire pire que de gagner la confiance de Quincy. Il voit beaucoup de choses. Tu pourrais peut-être prendre des notes.",shape=box,color=blue];
		j3734539572 -> j227430165;
	j20479924 [label="Mais je voudrais m’assurer qu’elles savent à quel point il est facile de lui faire du mal.",shape=box,color=blue];
		j20479924 -> j423522072;
	j2352673604 [label="Oh, allez. J'aurai des ailes et une queue avant que ça n'arrive.",shape=box,color=blue];
