	j1429515838 [label="Mais oui, en effet.",shape=box,color=blue];
		j1429515838 -> j518518039;
	j225936334 [label="Lettie adore Arthur. C'est le grand frère qu'elle n'a jamais eu. Si Arthur n'était pas là, je me réveillerais avec un chiffon imbibé de chloroforme sur la bouche et je finirais au fond de la rivière.",shape=box,color=blue];
		j225936334 -> j3527857996;
	j518518039 [label="Je ne lui ai toujours pas pardonné ce qu'elle a dit à propos de laisser cette ville brûler.",shape=box,color=blue];
		j518518039 -> j1568149509;
	j3527857996 [label="Elle pense qu'il délire, qu'il s'accroche au passé. Mais elle aime ce type et c'est un bon soldat, donc elle ne s'opposera jamais à lui.",shape=box,color=blue];
		j3527857996 -> j323581111;
		j3527857996 -> j1791121166;
	j1568149509 [label="Même Aoi était choquée. On se demande qui sont les vrais monstres, n'est-ce pas?",shape=box,color=blue];
		j1568149509 -> j323581111;
		j1568149509 -> j1791121166;
	j323581111 [label="Pourquoi te déteste-t-elle autant, d'ailleurs?",shape=box,color=red];
		j323581111 -> j3891844404;
	j1791121166 [label="Sérieusement, c'est juste la langue qui la fait flipper? Ou est-ce le fait de lire dans les pensées?",shape=box,color=red];
		j1791121166 -> j3891844404;
	j3891844404 [label="C'est compliqué. Il y a plus d'une raison.",shape=box,color=blue];
		j3891844404 -> j2318995581;
	j2318995581 [label="Elle m'a dit en face que je ne devais pas fouiller dans sa tête. Si je n'étais pas d'accord, elle quitterait l'Hexagone et tenterait sa chance toute seule. C'est dingue.",shape=box,color=blue];
		j2318995581 -> j874090954;
	j874090954 [label="Donc, en partie, c'est juste qu'elle a peur que je lise son journal intime d'adolescente, en quelque sorte. Je vais jeter un œil à ses secrets les plus embarrassants et me moquer d'elle, et puis elle sera humiliée à jamais.",shape=box,color=blue];
		j874090954 -> j4294508521;
	j4294508521 [label="En vérité, je ne suis pas si curieuse que ça. Quel que soit le petit secret malsain qu'elle me cache, il est en sécurité.",shape=box,color=blue];
		j4294508521 -> j4184929528;
		j4294508521 -> j4145031616;
	j4184929528 [label="Avez-vous déjà eu une histoire... plus romantique, toutes les deux?",shape=box,color=red];
		j4184929528 -> j1524045603;
