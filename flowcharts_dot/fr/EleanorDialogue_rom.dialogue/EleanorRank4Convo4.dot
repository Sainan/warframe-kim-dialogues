		j423522072 -> j3268444514;
	j935325815 [label="Entrati a foutu le camp. S'il y avait un espoir de retrouver l'ancienne Eleanor, celle qui chantait sous la douche et souriait aux enfants dans la rue, cet espoir s'est envolé avec lui.",shape=box,color=blue];
