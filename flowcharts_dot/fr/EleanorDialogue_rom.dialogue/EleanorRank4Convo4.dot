		j333555056 -> j3620074596;
	j3620074596 [label="Par les Dieux, il parlait même de reprendre un pub, un jour. Je l'imagine toujours avec sa petite bande de clients habitués, tirant des pintes, préparant des rôtis le dimanche, plein de sagesse durement acquise.",shape=box,color=blue];
