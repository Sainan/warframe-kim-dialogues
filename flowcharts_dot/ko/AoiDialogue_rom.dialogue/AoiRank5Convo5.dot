		j1475794369 -> j3812287379;
	j2702776321 [label="우리 대화의 대부분은 \"안녕, 우리 보석\" 이랑 \"안녕\" 이 다였어. 부모님은 늦게 출근하거나 다른 일로 사무실에 가는 길에 저녁을 먹고 했거든.",shape=box,color=blue];
