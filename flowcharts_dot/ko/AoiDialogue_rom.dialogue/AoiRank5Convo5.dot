		j2243315763 -> j2693437187;
	j942549308 [label="이해가 되네. 일을 너무 열심히 할 때 남는 시간이 많지 않지. 그렇지만, 그분들이 너를 끊임없이 생각했을거라고 생각해.",shape=box,color=red];
