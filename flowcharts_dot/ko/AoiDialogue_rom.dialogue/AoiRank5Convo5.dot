	j4124916143 [label="부모님이 훌륭하신 분들이셨던 것 같네. 그런 환경에서 자란 건 정말 대단했을 거야.",shape=box,color=red];
		j4124916143 -> j3140867484;
	j1814169299 [label="하하 사실은 그렇지 않아, 그냥 조용했어.",shape=box,color=blue];
