		j1821513828 -> j4125068125;
	j2311546799 [label="Yes.",shape=box,color=blue];
		j2311546799 -> j1521230984;
	j2252822658 [label="I will never, ever forget this.",shape=box,color=blue];
		j2252822658 -> "Chat ends.";
