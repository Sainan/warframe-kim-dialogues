	"Chat ends." [color=orange];
	j4188924991 [label="So. Christopher.",shape=box,color=blue];
		j4188924991 -> j3093590447;
	j2391583421 [label="Check boolean EleanorChristopher1",color=orange];
		j2391583421 -> j794481829 [label=true,color=green];
		j2391583421 -> j2118753695 [label=false,color=brown];
	j3093590447 [label="I’ve been holding on to him for years. So has Arthur. We have a ritual every year in his memory. But not this year. I couldn't face it.",shape=box,color=blue];
		j3093590447 -> j2628363988;
	j794481829 [label="To... make a very long and tragic story very short... Christopher died in a roadside bomb before we were married.",shape=box,color=blue];
		j794481829 -> j757998797;
	j2118753695 [label="It's time I told you about Christopher.",shape=box,color=blue];
		j2118753695 -> j943199761;
	j2628363988 [label="I don’t know whether ghosts exist, but they might as well, because I’ve let him become one.",shape=box,color=blue];
		j2628363988 -> j1400279028;
	j757998797 [label="Perhaps I'll tell you the whole story another day. Perhaps I won't.",shape=box,color=blue];
		j757998797 -> j3217732700;
	j943199761 [label="Arthur's best friend. We were engaged. I thought I was in love with him.",shape=box,color=blue];
		j943199761 -> j2689644937;
	j1400279028 [label="Don’t be so hard on yourself. You were in love with the guy.",shape=box,color=red];
		j1400279028 -> j3451467983;
	j3217732700 [label="I'm telling you all this now because...",shape=box,color=blue];
		j3217732700 -> j3885160210;
	j2689644937 [label="Boolean EleanorChristopher1 is now true.",color=orange];
		j2689644937 -> j794481829;
	j3451467983 [label="Oh mate. I THOUGHT I was. I loved him, but I didn’t even know if I was IN love with him or not.",shape=box,color=blue];
		j3451467983 -> j2497184946;
	j3885160210 [label="Boolean EleanorChristopher2 is now true.",color=orange];
		j3885160210 -> j3093590447;
	j2497184946 [label="That sounds awful, doesn’t it? It’s true, though. At the time, it just didn’t seem important. Then he died, and that became what our story was all about.",shape=box,color=blue];
		j2497184946 -> j1556321708;
	j1556321708 [label="Everything that I admired about him has just been overshadowed by how he died. I can’t think about him without this knot tightening in my stomach.",shape=box,color=blue];
		j1556321708 -> j3845603313;
	j3845603313 [label="It’s not fair. TO HIM. He deserves better than that.",shape=box,color=blue];
		j3845603313 -> j408278987;
		j3845603313 -> j1182597970;
	j408278987 [label="I understand. Totally. I made a choice to remember my parents the way they deserved.",shape=box,color=red];
		j408278987 -> j3867262008;
	j1182597970 [label="What happened was terrible. There’s no getting around it. But we can’t let that be the only thing we hold on to.",shape=box,color=red];
		j1182597970 -> j3867262008;
	j3867262008 [label="So why do I feel so guilty?",shape=box,color=blue];
		j3867262008 -> j2062798612;
	j2062798612 [label="Why do I feel like letting him go is the same thing as throwing him away?",shape=box,color=blue];
		j2062798612 -> j1241938353;
		j2062798612 -> j4085525409;
	j1241938353 [label="Because you’ve carried him for so long you don’t know how to stop.",shape=box,color=red];
		j1241938353 -> j3488252712;
	j4085525409 [label="Because you're worried no one else would ever be capable of loving you.",shape=box,color=red];
		j4085525409 -> j1415583883;
	j3488252712 [label="Gods. You’re bloody right, you know.",shape=box,color=blue];
		j3488252712 -> j636325566;
	j1415583883 [label="Check boolean EleanorDating",color=orange];
		j1415583883 -> j3547883150 [label=true,color=green];
		j1415583883 -> j538790202 [label=false,color=brown];
	j636325566 [label="We’re useless at dealing with this, my lot. We bottle everything up. We put a brave face on and whistle in the trenches. It gets us through, just like it got our parents through. But we don’t know how to stop.",shape=box,color=blue];
		j636325566 -> j4155522801;
	j3547883150 [label="Is that it? Am I that shallow?",shape=box,color=blue];
		j3547883150 -> j1209278245;
	j538790202 [label="I doubt even Christopher would love me in my current condition.",shape=box,color=blue];
		j538790202 -> j2898155619;
	j4155522801 [label="What should I do? How do I let go?",shape=box,color=blue];
		j4155522801 -> j4017015120;
	j1209278245 [label="I don't feel I deserve EITHER of you right now",shape=box,color=blue];
		j1209278245 -> j1592776837;
	j2898155619 [label="... no. That's not fair. I'm not being fair to him at all, am I?",shape=box,color=blue];
		j2898155619 -> j952200052;
	j4017015120 [label="Check boolean LettieDiasdeMuertos",color=orange];
		j4017015120 -> j3278374217 [label=true,color=green];
		j4017015120 -> j2659687426 [label=true,color=green];
		j4017015120 -> j1964788180 [label=true,color=green];
		j4017015120 -> j2659687426 [label=false,color=brown];
		j4017015120 -> j1964788180 [label=false,color=brown];
	j1592776837 [label="I think... I want you to have ALL of me. I can't hold back part of myself in a state of endless mourning.",shape=box,color=blue];
		j1592776837 -> j4017015120;
	j952200052 [label="He wouldn't want to stand in the way of my happiness. He was kind.",shape=box,color=blue];
		j952200052 -> j4017015120;
	j3278374217 [label="I actually think Lettie could help.",shape=box,color=red];
		j3278374217 -> j3908753583;
	j2659687426 [label="You could write him a letter.",shape=box,color=red];
		j2659687426 -> j1236809632;
	j1964788180 [label="Talk to Arthur, maybe. He's been going through this grief with you from the start.",shape=box,color=red];
		j1964788180 -> j3850199622;
	j3908753583 [label="Lettie?",shape=box,color=blue];
		j3908753583 -> j3508486783;
	j1236809632 [label="Part of me thinks that's a stupid idea. He won't read it. He's dead. But...",shape=box,color=blue];
		j1236809632 -> j1242234357;
	j3850199622 [label="Yes, he has. Maybe that's part of the problem. I know he's upset that I didn't keep up our little ceremony of remembrance this year.",shape=box,color=blue];
		j3850199622 -> j3689205598;
	j3508486783 [label="Why her?",shape=box,color=blue];
		j3508486783 -> j4220368168;
	j1242234357 [label="... the more I think about it, the less stupid it seems.",shape=box,color=blue];
		j1242234357 -> j1677768533;
	j3689205598 [label="I just... have to say goodbye to Christopher MY way this time. Once and for all.",shape=box,color=blue];
		j3689205598 -> j2456843192;
	j4220368168 [label="She has a lot of experience. She knows all about commemorating the dead.",shape=box,color=red];
		j4220368168 -> j3119951474;
	j1677768533 [label="Yes. I'll do it. There are things I need to say to him. Things I never said when he was alive. Maybe then he can rest.",shape=box,color=blue];
		j1677768533 -> j1837973976;
	j2456843192 [label="To Arthur he'll always be a lost brother, a fallen soldier. That's the mourning he knows and understands. But it's not mine.",shape=box,color=blue];
		j2456843192 -> j1542561615;
	j3119951474 [label="I’m sure if you asked her for advice, respectfully, she wouldn’t turn you away.",shape=box,color=red];
		j3119951474 -> j1975711624;
	j1837973976 [label="Thank you for listening, and for the advice.",shape=box,color=blue];
		j1837973976 -> j2136805493;
	j1542561615 [label="I can't do it Arthur's way any more. I think... if I just tell him that, he'll get it.",shape=box,color=blue];
		j1542561615 -> j1821513828;
	j1975711624 [label="…",shape=box,color=blue];
		j1975711624 -> j2311546799;
