digraph {
	"EleanorRank4Convo5 begins." [color=orange];
		"EleanorRank4Convo5 begins." -> j3740728140;
	j3740728140 [label="Das Königreich, über das du einst geherrscht hast. Duviri. Du hast es nicht erfunden, oder?",shape=box,color=blue];
		j3740728140 -> j1812116947;
		j3740728140 -> j2586484516;
	j1812116947 [label="Habe ich und auch nicht. Der Ort und die Charaktere stammen aus einem Kinderbuch, das ich geliebt habe. 'Geschichten aus Duviri'. Ich habe es durch den Void zum Leben erweckt.",shape=box,color=red];
		j1812116947 -> j3254533561;
	j2586484516 [label="Ich habe es so satt, über Duviri zu reden ...",shape=box,color=red];
		j2586484516 -> j3081282367;
	j3254533561 [label="Wie hast du das geschafft?",shape=box,color=blue];
		j3254533561 -> j40816997;
		j3254533561 -> j750628614;
	j3081282367 [label="+5 Chemistry",color=orange];
		j3081282367 -> j4261357332;
	j40816997 [label="Ich war ganz allein. Ich schätze, die anderen Kinder waren tot oder gerettet. Die Einzelheiten sind mir nicht mehr ganz klar.",shape=box,color=red];
		j40816997 -> j3862321235;
	j750628614 [label="Es war nicht unbedingt Absicht. Ich erinnere mich nur an ...",shape=box,color=red];
		j750628614 -> j3862321235;
	j4261357332 [label="Ups. Mein Fehler.",shape=box,color=blue];
		j4261357332 -> j3461278743;
	j3862321235 [label="Die Engel kamen, brachen durch die Wände und ich konnte nirgendwohin. Ich hatte das Buch und ich weiß noch, dass ich dachte 'Dort wäre ich sicher.'",shape=box,color=red];
		j3862321235 -> j4165749664;
	j3461278743 [label="Das war rücksichtslos von mir, oder? Der Ort war nicht gerade ein Ferienlager für dich.",shape=box,color=blue];
		j3461278743 -> j559391296;
	j4165749664 [label="Engel? Heutzutage werden Engel als wohlwollende Wesen angesehen. Manchmal sind sie furchterregend, aber auf der Seite des Guten. Es sei denn, du meinst die Gefallenen.",shape=box,color=blue];
		j4165749664 -> j2812697861;
	j559391296 [label="Es tut mir aufrichtig leid. Ich werde meine Neugier von nun an unter Kontrolle halten.",shape=box,color=blue];
		j559391296 -> "Chat ends.";
	j2812697861 [label="Das hat sich zu meiner Zeit nicht wirklich geändert. Engel sind geflügelte, leuchtende Wesen aus den alten Geschichten, die guten Menschen helfen. Ich spreche von Void-Engeln. Das ist ein ganz anderes Kaliber.",shape=box,color=red];
		j2812697861 -> j795673283;
	"Chat ends." [color=orange];
	j795673283 [label="Ich verstehe. Ich kann mir nicht vorstellen, dass so etwas wie ein 'Void-Engel' herabstößt und ein neugeborenes Baby segnet.",shape=box,color=blue];
		j795673283 -> j649116720;
	j649116720 [label="Soweit ich es weiß, waren sie einmal Menschen. Wenn der Gesang der Engel sie erreicht, beginnen sie sich zu verändern. Wenn sie ihm nicht mehr widerstehen können, werden sie selbst zu Engeln.",shape=box,color=red];
		j649116720 -> j2384108246;
	j2384108246 [label="Die Zukunft, klingt wirklich furchterregend, so wie du sie beschreibst.",shape=box,color=blue];
		j2384108246 -> j1044694857;
	j1044694857 [label="Ich kann es dir nicht verübeln, dass du dich vor diesen Void-Engeln verstecken willst. Insbesondere, wenn sie ...",shape=box,color=blue];
		j1044694857 -> j2714259702;
	j2714259702 [label="Oh, mir kam gerade so ein Gedanke. Das waren nicht deine Eltern, oder?",shape=box,color=blue];
		j2714259702 -> j2336978949;
		j2714259702 -> j3193653458;
		j2714259702 -> j614161627;
	j2336978949 [label="Ich bin ziemlich sicher, dass meine Eltern tot sind.",shape=box,color=red];
		j2336978949 -> j2227702345;
	j3193653458 [label="Das hätte sein können. Andere Kinder hörten ihre Eltern rufen.",shape=box,color=red];
		j3193653458 -> j2227702345;
	j614161627 [label="Ich glaube schon, ja. Quinn warnte mich davor, nicht nach vertrauten Stimmen zu suchen. Das muss er gemeint haben.",shape=box,color=red];
		j614161627 -> j2227702345;
	j2227702345 [label="Es tut mir so leid. Ich bin froh, dass du einen sicheren Hafen gefunden hast, auch wenn es nicht dabei geblieben ist.",shape=box,color=blue];
		j2227702345 -> j2414788122;
	j2414788122 [label="Es war lange Zeit sicher. Lange genug, um es zu vergessen. Ich glaube, das war es, was ich wollte.",shape=box,color=red];
		j2414788122 -> j2919124433;
	j2919124433 [label="Aber wie um Himmels Willen hast du das gemacht? Du warst doch noch ein Kind. Was gab dir die Macht, aus dem Nichts ein ganzes Königreich zu erschaffen?",shape=box,color=blue];
		j2919124433 -> j4251754782;
		j2919124433 -> j4052713960;
		j2919124433 -> j2866025118;
	j4251754782 [label="Kinder sind faszinierend, weil sie noch nicht wissen, was sie nicht können. Ich wusste nicht, dass ich es nicht konnte. Also tat ich es.",shape=box,color=red];
		j4251754782 -> j782200343;
	j4052713960 [label="Ich hatte so viele Emotionen gleichzeitig, dass ich ... einfach ... etwas damit gemacht habe. Es ist schwer zu erklären.",shape=box,color=red];
		j4052713960 -> j2406203779;
	j2866025118 [label="Der Void ist nicht 'nichts'. Ich weiß, es klingt so, als ob es das ist, aber das ist es nicht. Albrecht hat das bewiesen.",shape=box,color=red];
		j2866025118 -> j2406203779;
	j782200343 [label="Mit fünf wollte ich eine Hexe sein. Ich schrie Abrakadabra und sprang von der obersten Stufe der Treppe. Ich wusste nicht, dass ich nicht fliegen konnte. Aber ich fand es schnell heraus.",shape=box,color=blue];
		j782200343 -> j1166166677;
	j2406203779 [label="Wir wissen bis heute nur sehr wenig darüber. Aber selbst die Orokin haben eines herausgefunden: Der Void reagiert auf menschliche Emotionen.",shape=box,color=red];
		j2406203779 -> j1221222736;
	j1166166677 [label="Die Macht, Duviri zu erschaffen, muss von woanders als von dir gekommen sein. Du hast sie genutzt, um etwas Unglaubliches zu tun, aber sie war nicht deine.",shape=box,color=blue];
		j1166166677 -> j1391898933;
	j1221222736 [label="ICH VERSTEHE. Wenn du also mit ... hmm, sagen wir 'Hunger' ... in den Void schauen würdest, könntest du einen herumstreunenden, halb verhungerten Wolf erschaffen. Oder vielleicht ein königliches Bankett. Oder einen Laib Brot, in dem Wolfszähne knirschen.",shape=box,color=blue];
		j1221222736 -> j1058155206;
	j1391898933 [label="Jedenfalls nicht am Anfang. Aber jetzt ist es in dir. Die wilde Energie. Die Zeitschleifen. Meine Güte, du bist ein Wunder.",shape=box,color=blue];
		j1391898933 -> j1537082287;
	j1058155206 [label="Was für eine erschreckende und aufregende Vorstellung.",shape=box,color=blue];
		j1058155206 -> j1537082287;
	j1537082287 [label="Warum dachten die Orokin, es wäre eine gute Idee, ein Schiff voller Kinder in den Void zu schicken? War das nicht eine emotionale Zeitbombe, die nur darauf wartete, hochzugehen?",shape=box,color=blue];
		j1537082287 -> j2838285486;
	j2838285486 [label="Dafür war dieses Geschichtenbuch gedacht. Oberflächlich betrachtet ging es um diese übertriebenen Charaktere am Hof eines verrückten Königs, dessen Launen jeden Tag wechselten.",shape=box,color=red];
		j2838285486 -> j2047202972;
	j2047202972 [label="Aber die wahre Lektion aus diesen Geschichten war, wie man seine Gefühle unter Kontrolle bringt. Wir sollten daraus eine wichtige Lehre lernen, damit die Reise sicher verläuft.",shape=box,color=red];
		j2047202972 -> j2830949554;
	j2830949554 [label="Urks. Tut mir leid, aber jetzt hast du es mir verdorben.",shape=box,color=blue];
		j2830949554 -> j2199703929;
	j2199703929 [label="Warum können Geschichten nicht einfach Geschichten sein? Ich HASSTE es, als ich herausfand, dass Rudolfo, der Leopardenkönig, der im Kampf mit der Tierkreisschlange heldenhaft starb, eigentlich Sol mit seinen Sonnenflecken sein sollte, der auf dem Sternenbaum leidet.",shape=box,color=blue];
		j2199703929 -> j3088441091;
	j3088441091 [label="Religion in Kinderbücher zu schmuggeln ist wie Medizin in Süßigkeiten zu verstecken. SAGT ES IHNEN EINFACH.",shape=box,color=blue];
		j3088441091 -> j2280846356;
		j3088441091 -> j293405612;
	j2280846356 [label="Ich habe noch nie von Rudolfo, dem Leopardenkönig, gehört.",shape=box,color=red];
		j2280846356 -> j2997863301;
	j293405612 [label="Ich möchte eines Tages die Geschichte von Rudolfo, dem Leopardenkönig, hören.",shape=box,color=red];
		j293405612 -> j611912090;
	j2997863301 [label="Ich werde als glückliche Frau sterben, wohl wissend, dass diese Bücher in Vergessenheit geraten. Ein Hoch auf die Zukunft!",shape=box,color=blue];
		j2997863301 -> j2716575924;
	j611912090 [label="Urks. Bitte nicht. Das ist keine echte Legende, sondern eine furchtbare Kinderbuchreihe.",shape=box,color=blue];
		j611912090 -> j1231168124;
	j2716575924 [label="Aber es gibt Geschichten, nicht wahr?",shape=box,color=blue];
		j2716575924 -> j117824814;
	j1231168124 [label="Wenn du willst, kann ich dir allerdings von einigen unserer wahren Legenden erzählen.",shape=box,color=blue];
		j1231168124 -> j3444367166;
		j1231168124 -> j601670378;
	j117824814 [label="Jede Menge. Sogar die Tenno und die Warframes haben Geschichten.",shape=box,color=red];
		j117824814 -> j4140468591;
	j3444367166 [label="Das wäre nett.",shape=box,color=red];
		j3444367166 -> j1048670224;
	j601670378 [label="Im Moment nicht. [Ende.]",shape=box,color=red];
		j601670378 -> "Chat ends.";
	j4140468591 [label="Da ist dieser Typ, Drusus Leverian. Er ist so etwas wie ein Museumskurator. Er führt Aufzeichnungen über alle Legenden der Warframes.",shape=box,color=red];
		j4140468591 -> j1191923029;
	j1048670224 [label="Mal sehen. Da ist natürlich Arthur. Der gute alte Arthur. Das Original. Britannias Bester.",shape=box,color=blue];
		j1048670224 -> j1134676129;
	j1191923029 [label="Nun, das bringt mich völlig aus der Fassung. Du kommst aus der fernen Zukunft, warst aber auch seit so vielen Jahren in Duviri und nicht auf dem Laufenden, dass Ereignisse, die zu deinen Lebzeiten stattfanden, Zeit hatten, zu Mythen zu werden?",shape=box,color=blue];
		j1191923029 -> j3287603589;
	j1134676129 [label="Und dann ist da noch die junge Loxley. Ich liebte sie. Ich wollte so sein wie sie. Sie war das Vorbild für einen bestimmten Typ junger Frau.",shape=box,color=blue];
		j1134676129 -> j2546285823;
	j3287603589 [label="Mir wird schwindlig.",shape=box,color=blue];
		j3287603589 -> j31405529;
		j3287603589 -> j1563276825;
	j2546285823 [label="Wie lautet ihre Geschichte?",shape=box,color=red];
		j2546285823 -> j3192955500;
	j31405529 [label="Glaube mir. Das Gefühl kenne ich.",shape=box,color=red];
		j31405529 -> j1634445336;
	j1563276825 [label="Dann erzähl mir von euren Mythen.",shape=box,color=red];
		j1563276825 -> j2595637637;
	j3192955500 [label="Okay. Kurzfassung: Sie war die Tochter eines Adligen in alten Zeiten, als das Land unter einem grausamen König litt.",shape=box,color=blue];
		j3192955500 -> j2672889572;
	j1634445336 [label="Du selbst bist eine Märchenfigur, nicht wahr?",shape=box,color=blue];
		j1634445336 -> j2611220349;
	j2595637637 [label="Verflixt. Das wird ein ganzes Jahr dauern, wenn ich erst einmal anfange.",shape=box,color=blue];
		j2595637637 -> j1048670224;
	j2672889572 [label="Ihr Bruder war im Kampf in der Fremde gefallen, aber die Nachricht hatte die Heimat noch nicht erreicht. Als das arme, unterdrückte Volk also einen Helden brauchte, war sie der Situation gewachsen.",shape=box,color=blue];
		j2672889572 -> j2215480408;
	j2611220349 [label="Gefangen in einem magischen Königreich, in dem die Zeit anders vergeht, gerufen an den Ort, an dem du am meisten gebraucht wirst ...",shape=box,color=blue];
		j2611220349 -> j3823787501;
	j2215480408 [label="Check boolean EleanorHeroVirus",color=orange];
		j2215480408 -> j2141426631 [label=true,color=green];
		j2215480408 -> j2307531875 [label=false,color=brown];
	j3823787501 [label="Um deine Geschichte abzurunden, brauchen wir nur noch eine Geliebte oder ein geiferndes, unmenschliches Monster.",shape=box,color=blue];
		j3823787501 -> j334933825;
	j2141426631 [label="Ich dachte, du hasst Helden.",shape=box,color=red];
		j2141426631 -> j4123931768;
	j2307531875 [label="Normalerweise kann ich Heldenfiguren nicht ausstehen. Sie sind schädlich. Ansteckend und deshalb gefährlich.",shape=box,color=blue];
		j2307531875 -> j3045441723;
	j334933825 [label="(Oder eine geifernde, unmenschliche Geliebte, je nachdem, was dein Geschmack ist.)",shape=box,color=blue];
		j334933825 -> j2221913471;
		j334933825 -> j233579631;
	j4123931768 [label="Sie kriegt einen Freifahrtschein. Halt die Klappe.",shape=box,color=blue];
		j4123931768 -> j3927647724;
	j3045441723 [label="Die junge Loxley ist anders.",shape=box,color=blue];
		j3045441723 -> j3545542281;
	j2221913471 [label="Ich fand die Monster sowieso immer interessanter. Helden sind langweilig. Vorhersehbar. ... Banal.",shape=box,color=red];
		j2221913471 -> j1936394532;
	j233579631 [label="Mm, ich bin nicht sicher, ob Monster wirklich mein Typ sind.",shape=box,color=red];
		j233579631 -> j1496438064;
	j3927647724 [label="Wie gesagt. Sie sah ihrem Bruder sehr ähnlich, also zog sie seine alten Kleider an, gab sich als er aus und zog los, um eine Revolution anzuführen. Wäre jemand zu nahe gekommen, wäre vielleicht aufgefallen, dass sie ein Mädchen war, also trug sie eine Kapuze. Das wurde ihr Markenzeichen.",shape=box,color=blue];
		j3927647724 -> j861195350;
	j3545542281 [label="Boolean EleanorHeroVirus is now true.",color=orange];
		j3545542281 -> j861195350;
	j1936394532 [label="Boolean EleanorFlirt is now true.",color=orange];
		j1936394532 -> j2855271987;
	j1496438064 [label="Und das ist auch in Ordnung.",shape=box,color=blue];
		j1496438064 -> j2206408011;
	j861195350 [label="Sie war unglaublich gut mit Pfeil und Bogen. Sie und ihre Leute wurden zu einer beliebten Bande von Gesetzlosen, die die Steuereintreiber des Königs beraubten und das Geld denen in Not gaben.",shape=box,color=blue];
		j861195350 -> j1715816360;
	j2855271987 [label="+15 Chemistry",color=orange];
		j2855271987 -> j3718206186;
	j2206408011 [label="Monster sind es gewohnt, allein zu sein.",shape=box,color=blue];
		j2206408011 -> j828574061;
	j1715816360 [label="Klingt, als hätte sie sich schon aus eigenem Recht Respekt verschafft. Warum musste sie so tun, als wäre sie ihr Bruder?",shape=box,color=red];
		j1715816360 -> j416474739;
	j3718206186 [label="Monstrosität liegt im Auge des Betrachters, mein Freund.",shape=box,color=blue];
		j3718206186 -> j4070264376;
	j828574061 [label="Wir sind Überlebenskünstler.",shape=box,color=blue];
		j828574061 -> "Chat ends.";
	j416474739 [label="Das ist Geschichte. Damals kämpften angeblich nur die Männer, also herrschten sie auch. Leute erwarteten Befehl von Männern entgegenzunehmen. Sie musste sich auf den Ruf ihres Bruders stützen, um das zu tun, was sie tun musste. Entweder das oder untergehen.",shape=box,color=blue];
		j416474739 -> j3317095650;
		j416474739 -> j2256518035;
		j416474739 -> j3199031517;
	j4070264376 [label="Wenn du dich aufrichtig zu den Eigenschaften einer anderen Person hingezogen fühlst, die du als 'monströs' bezeichnest, dann stelle sicher, dass diese Person deine Ansichten teilt und sich daran erfreut. Andernfalls begehst du die widerlichste Fetischisierung.",shape=box,color=blue];
		j4070264376 -> j2212969697;
	j3317095650 [label="Was ist am Ende mit ihr passiert?",shape=box,color=red];
		j3317095650 -> j3464153089;
	j2256518035 [label="Ich bin so froh, dass wir das in der Zukunft behoben haben.",shape=box,color=red];
		j2256518035 -> j2287925090;
	j3199031517 [label="Ich bin nicht sicher, ob wir das in der Zukunft *vollständig* behoben haben ...",shape=box,color=red];
		j3199031517 -> j1711471020;
	j2212969697 [label="Würde ich mich zum Beispiel als Monster bezeichnen? Ja. Mit Begeisterung. Ich nehme den Titel an, mit dem andere mich verurteilen würden, und frohlocke darüber. Falls du dich das fragst.",shape=box,color=blue];
		j2212969697 -> "Chat ends.";
	j3464153089 [label="+20 Chemistry",color=orange];
		j3464153089 -> j1214374917;
	j2287925090 [label="+20 Chemistry",color=orange];
		j2287925090 -> j623972259;
	j1711471020 [label="+20 Chemistry",color=orange];
		j1711471020 -> j3183863186;
	j1214374917 [label="Hängt davon ab, welche Version der Geschichte du liest. Entweder wurde sie direkt in die Halle der Helden berufen, oder sie heiratete die eigensinnige Tochter des Königs, oder sie wurde in einer Eiche gefangen, wo sie bis heute schläft und auf Britanniens Stunde der Not wartet.",shape=box,color=blue];
		j1214374917 -> j1033657120;
	j623972259 [label="Ich ebenfalls.",shape=box,color=blue];
		j623972259 -> j3213225494;
	j3183863186 [label="Nicht, wenn man nach Ballas geht.",shape=box,color=blue];
		j3183863186 -> j4202888503;
	j1033657120 [label="Daher das Sprichwort in meiner Heimat. Wenn es schlecht läuft, wie im Krieg zum Beispiel, ist es Tradition, die Achseln zu zucken und zu sagen 'Na ja, SO schlimm kann es nicht sein. Die junge Loxley ist noch nicht aufgetaucht.'",shape=box,color=blue];
		j1033657120 -> "Chat ends.";
	j3213225494 [label="Götter, ich würde deine Ära gerne mit eigenen Augen sehen. Vielleicht könntest du mir einen dieser Kühlsärge zurückschmuggeln und ich könnte ein paar Jahrtausende lang einfach schlafeeen.",shape=box,color=blue];
		j3213225494 -> "Chat ends.";
	j4202888503 [label="Wie lange zum Teufel brauchen wir dafür? Verdammt noch mal.",shape=box,color=blue];
		j4202888503 -> "Chat ends.";
}