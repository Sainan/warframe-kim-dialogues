		j3579983603 -> j1163131713;
	j1193240289 [label="Ich weiß nicht, ob du schon einmal so ein Tief hattest, bei dem du nichts anderes tun wolltest, als im Bett zu liegen, ein- und auszuatmen und einfach nur zu existieren, ohne etwas anderes zu tun, aber es war ganz ähnlich.",shape=box,color=blue];
