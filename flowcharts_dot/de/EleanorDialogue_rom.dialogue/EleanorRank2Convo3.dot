	j895135000 [label="+5 Chemistry",color=orange];
		j895135000 -> j2428926175;
	j172648062 [label="Eine Art Kommunion, denke ich. Nicht unbedingt etwas Physisches. Ich möchte einfach nur einen Geist berühren, der nicht nur nicht menschlich ist, sondern nicht einmal irdisch.",shape=box,color=blue];
