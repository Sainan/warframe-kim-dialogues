		j1484472100 -> j4034220724;
	j3064387139 [label="+5 Chemistry",color=orange];
		j3064387139 -> j2369076585;
	j124124774 [label="Nicht ganz.",shape=box,color=blue];
		j124124774 -> j4034220724;
	j2920831314 [label="+5 Chemistry",color=orange];
		j2920831314 -> j2561309674;
	j4034220724 [label="Sie ist der Tod. Je nachdem, welcher Tradition du folgst, ist sie auch einer der Aspekte von Lua. Sie ist diejenige, die kommt und dich ins Bett bringt, wenn der wilde Tag des Lebens vorbei ist, dir einen Gutenachtkuss gibt und dich in den Schlaf singt.",shape=box,color=blue];
		j4034220724 -> j1036830569;
		j4034220724 -> j987320365;
	j2369076585 [label="Ha. Du unterhältst dich also mit unserer lieben Lettie. Ja.",shape=box,color=blue];
		j2369076585 -> j4034220724;
	j2561309674 [label="Ja, tatsächlich.",shape=box,color=blue];
		j2561309674 -> j4034220724;
	j1036830569 [label="Das war kein Scherz, als du sagtest, du seist melancholisch, oder?",shape=box,color=red];
		j1036830569 -> j2358930263;
	j987320365 [label="Das wird eines dieser \"Mach es dir bequem, Drifter, das wird eine Minute dauern\"-Gespräche, nicht wahr?",shape=box,color=red];
		j987320365 -> j1513921060;
	j2358930263 [label="Mensch. Alles, was Entrati uns gegeben hat, die Macht, die Stärke, die gepanzerten Hintern ... würde ich gegen einen Krümelchen Gewissheit eintauschen, dass der Tod nicht das Ende ist.",shape=box,color=blue];
		j2358930263 -> j1261162781;
	j1513921060 [label="... das wäre es gewesen.",shape=box,color=blue];
		j1513921060 -> j2411198373;
	j1261162781 [label="Ich habe es versucht. Ich gebe es nicht gerne zu, aber manchmal, wenn wir auf Patrouille sind und es zu Kämpfen kommt, schaue ich in den Geist der Sterbenden und versuche, den Prozess mit ihnen durchzustehen. Wenn es am Ende des Tunnels ein Licht gibt, möchte ich es einfach sehen.",shape=box,color=blue];
		j1261162781 -> j3270332777;
		j1261162781 -> j537960523;
	j2411198373 [label="Vergiss es. Du hast sicher Wichtigeres zu tun.",shape=box,color=blue];
		j2411198373 -> "Chat ends.";
	j3270332777 [label="Eleanor, das ist makaber. Du hast kein Recht dazu.",shape=box,color=red];
		j3270332777 -> j4286474181;
	j537960523 [label="Versuchst du überhaupt, sie zu trösten?",shape=box,color=red];
		j537960523 -> j3479769882;
	j4286474181 [label="Ich weiß. ICH WEISS.",shape=box,color=blue];
		j4286474181 -> j4254670976;
	j3479769882 [label="Das habe ich einmal getan. Was soll's, richtig? Sie sterben sowieso. Es war diese junge Scaldra Soldatin. Sie kam auf mich zu und ich schoss ihr in die Brust.",shape=box,color=blue];
		j3479769882 -> j2546639601;
	j4254670976 [label="Ich sage mir, dass ich helfen wollte, aber in Wahrheit wollte ich nur sehen. Du hast recht. Es war grauenhaft.",shape=box,color=blue];
		j4254670976 -> "Chat ends.";
	j2546639601 [label="Ich kniete neben ihr nieder, als sie ihr Leben ausröchelte. Ich versuchte ihr zu sagen, dass alles in Ordnung sei, dass der Schmerz bald vorbei ist, dass sie bald bei ihrer Familie sein wird, die vor ihr gegangen war. All die ruhigen, besänftigenden Dinge, an die man denkt, wenn eine Trauerfeier ansteht.",shape=box,color=blue];
		j2546639601 -> j3958838683;
	j3958838683 [label="Sie hatte schreckliche Angst. Was von ihrem Bewusstsein übrig war, kroch wie eine Ratte in einem Wassertank umher. Als sie mich in Gedanken bei sich spürte, versuchte sie sich festzuhalten. Sie flehte mich an, ihr zu helfen. Unter all dem war ... Ich weiß nicht einmal, wie man es nennen würde ...",shape=box,color=blue];
		j3958838683 -> j3763740173;
		j3958838683 -> j4115939795;
		j3958838683 -> j1272372905;
	j3763740173 [label="Hoffnung?",shape=box,color=red];
		j3763740173 -> j1548460212;
	j4115939795 [label="Angst?",shape=box,color=red];
		j4115939795 -> j1548460212;
	j1272372905 [label="Verleugnung?",shape=box,color=red];
		j1272372905 -> j1548460212;
	j1548460212 [label="Verleugnung. Sie glaubte nicht, dass sie sterben könnte. Und warum sollte sie? Sie hatte das schon so oft durchgemacht. Wie konnte der Tod sie jetzt fortnehmen?",shape=box,color=blue];
		j1548460212 -> j3951899489;
	j3951899489 [label="Jeden Tag deines Lebens nimmst du DICH SELBST als selbstverständlich hin. Du bist da, wenn du aufwachst, und du bist da, wenn du schläfst. Du verbringst dein ganzes Leben in einem Haus, das aus dir besteht. Du gehst kein einziges Mal nach draußen, weil du es nicht kannst.",shape=box,color=blue];
		j3951899489 -> j238124553;
	j238124553 [label="Dann, eines Tages, hörst du, wie das Haus abgerissen wird.",shape=box,color=blue];
		j238124553 -> j2589917908;
		j238124553 -> j3383604712;
	j2589917908 [label="Warum hast du ihr nicht geholfen?",shape=box,color=red];
		j2589917908 -> j2986247952;
	j3383604712 [label="Bist du bis zum Ende bei ihr geblieben?",shape=box,color=red];
		j3383604712 -> j800201154;
	j2986247952 [label="Was hätte ich denn tun sollen? Lettie herbeirufen? Die klaffende Wunde auf der Brust heilen? Und das alles nur, weil ich aus nächster Nähe gesehen habe, was diese hier durchmachte?",shape=box,color=blue];
		j2986247952 -> j2459664783;
	j800201154 [label="Ja. Ich hatte eine Heidenangst. Als sie spürte, wie sie über die Kante rutschte, war ich sicher, dass sie mich mit nach unten ziehen würde. Ich konnte diese Unterströmung spüren, die an mir zerrte.",shape=box,color=blue];
		j800201154 -> j1095004572;
	j2459664783 [label="Bei den Scaldra heißt es töten oder getötet werden. Den Luxus der Gnade bieten sie uns nicht.",shape=box,color=blue];
		j2459664783 -> "Chat ends.";
	j1095004572 [label="Aber ich musste bleiben. Ich konnte sie nicht allein sterben lassen.",shape=box,color=blue];
		j1095004572 -> j2277987640;
	j2277987640 [label="Obwohl sie es am Ende doch allein getan hat. Ich konnte diesen letzten Schritt nicht mit ihr gehen.",shape=box,color=blue];
		j2277987640 -> j4168056476;
		j2277987640 -> j800989518;
	j4168056476 [label="Was hast du aus dieser Erfahrung gelernt?",shape=box,color=red];
		j4168056476 -> j2593143027;
	j800989518 [label="Hast du gesehen, was du gehofft hast zu sehen?",shape=box,color=red];
		j800989518 -> j2255043148;
	j2593143027 [label="Ich habe nichts daraus gelernt.",shape=box,color=blue];
		j2593143027 -> j1689457822;
	j2255043148 [label="Nein.",shape=box,color=blue];
		j2255043148 -> j1689457822;
	j1689457822 [label="Aber sie sah es. Sie sah das Gesicht ihrer Mutter, die Kinder, mit denen sie zur Schule gegangen war, ihre Katze, einen Cousin, der ertrunken war. Aber es waren keine Präsenzen oder Geister. Nur Erinnerungen, die von einem sterbenden Gehirn an die Oberfläche gezwungen wurden, das verzweifelt nach etwas suchte, an dem es sich festklammern konnte.",shape=box,color=blue];
		j1689457822 -> j29895072;
	j29895072 [label="Dann begann das Haus ihrer Identität um sie herum auseinanderzufallen. Kurz bevor sie starb, öffnete sie ihre Augen weit. Ich glaube, in dieser Sekunde verstand sie zum ersten Mal in ihrem Leben wirklich, was Tod eigentlich ist.",shape=box,color=blue];
		j29895072 -> j1726363001;
		j29895072 -> j167208749;
	j1726363001 [label="Sterbende verstehen, was Tod ist. Was für eine Erkenntnis, Eleanor.",shape=box,color=red];
		j1726363001 -> j3686963429;
	j167208749 [label="Wie meinst du das? Jeder weiß, was der Tod ist.",shape=box,color=red];
		j167208749 -> j1940164137;
	j3686963429 [label="Hör zu. Wenn du das nächste Mal das Bedürfnis verspürst, mich wegen meines Blödsinns zu kritisieren, brauchst du es dir nicht zuerst anzuhören.",shape=box,color=blue];
		j3686963429 -> j1773462813;
	j1940164137 [label="Nein. Wir glauben zu wissen, was es ist, aber alle unsere Erfahrungen stammen von außen.",shape=box,color=blue];
		j1940164137 -> j3721317226;
	j1773462813 [label="Niemand zwingt dich, mit mir zu reden. Tschüß.",shape=box,color=blue];
		j1773462813 -> "Chat ends.";
	j3721317226 [label="Ihr ganzes Leben lang lebte sie wie eine Frau in einem Haus ohne Licht, und alles, was sie hatte, war eine Taschenlampe. Sie richtete das Licht ihres Bewusstseins auf die eine oder andere Stelle. Eine Erinnerung, eine Aufgabe, eine Interaktion.",shape=box,color=blue];
		j3721317226 -> j2692216648;
	j2692216648 [label="Sie ging von Zimmer zu Zimmer und erkundete die Keller im Schlaf, doch es war immer ein abgeschlossener, endlicher Prozess. Kannst du mir folgen?",shape=box,color=blue];
		j2692216648 -> j3968282865;
		j2692216648 -> j2305752730;
	j3968282865 [label="Nicht wirklich, aber sprich weiter.",shape=box,color=red];
		j3968282865 -> j1124468149;
	j2305752730 [label="Ja.",shape=box,color=red];
		j2305752730 -> j1124468149;
	j1124468149 [label="Als dann das Haus zerstört wurde und ihre Persönlichkeit um sie herum in Stücke zerfiel, leuchtete die Taschenlampe plötzlich nicht mehr auf eine Wand, ein Buch oder ein Sofa. Sie leuchtete hinauf in den unendlichen Himmel. Ihr Bewusstsein strahlte ins Unendliche, ohne dass ihr eigenes ich als Hindernis im Weg stand.",shape=box,color=blue];
		j1124468149 -> j3171567617;
		j1124468149 -> j2036257425;
		j1124468149 -> j3164596931;
	j3171567617 [label="Aber das Bewusstsein stirbt, wenn der Körper stirbt. Was du erlebt hast, war nur eine Zuckung in ihrem sterbenden Gehirn.",shape=box,color=red];
		j3171567617 -> j3076165502;
	j2036257425 [label="Du denkst 'Bewusstsein' ist unsere wahre Identität und der Rest ist nur eine Hülle?",shape=box,color=red];
		j2036257425 -> j3350271538;
	j3164596931 [label="Wow. So tiefgründig. Du hast jemanden erschossen und dann ihre Privatsphäre verletzt.",shape=box,color=red];
		j3164596931 -> j3686963429;
	j3076165502 [label="Ich habe oft versucht, mir das selbst einzureden.",shape=box,color=blue];
		j3076165502 -> j1531264040;
	j3350271538 [label="+5 Chemistry",color=orange];
		j3350271538 -> j3586172223;
	j1531264040 [label="Ich kann nicht glauben, dass es nichts weiter war als ein Gehirn, das einfach abschaltet. Es fühlte sich an, als ob die Seele sich befreien würde.",shape=box,color=blue];
		j1531264040 -> "Chat ends.";
	j3586172223 [label="Ich weiß, das ergibt überhaupt keinen Sinn. Die Vorstellung, dass wir in erster Linie eine Sichtweise und erst in zweiter Linie eine Persönlichkeit sind, kommt mir so kontraintuitiv vor. Ich weiß nur, dass es sich so anfühlte, als diese menschliche Seele vom Leben in den Tod überging.",shape=box,color=blue];
		j3586172223 -> j844719076;
	j844719076 [label="Welches Gefühlt gibt dir diese ganze Sache?",shape=box,color=red];
		j844719076 -> j4230588934;
	j4230588934 [label="Trost? Mir gefällt die Vorstellung, dass etwas von uns überlebt. Der Gedanke, dass es beim Tod nicht darum geht, aus dem Universum gelöscht zu werden, sondern dass alles gelöscht wird, was einen vom Universum trennt.",shape=box,color=blue];
		j4230588934 -> j506575235;
	j506575235 [label="Aber was uns zu UNS macht, ist nicht nur reines Bewusstsein. Es sind Erinnerungen, Emotionen, Meinungen, alle möglichen Schichten. Wenn man all das verliert, verliert man einen echten Menschen, selbst wenn dieser Funke irgendwie überlebt.",shape=box,color=red];
		j506575235 -> j1353204973;
	j1353204973 [label="Ich weiß.",shape=box,color=blue];
		j1353204973 -> j1843040338;
	j1843040338 [label="Ehrlich gesagt, deshalb wollte ich mit dir über Void-Geister und Cephalons und all das Zeug aus deiner Zeit sprechen. Ich muss wissen, ob wir jemals die Chance bekommen, mit Menschen zu kommunizieren, die wir verloren haben.",shape=box,color=blue];
		j1843040338 -> j112052529;
	j112052529 [label="Echte Kommunikation, kein Hologramm-Mist. Die Art, die dich definitiv wissen lässt, was die Person gedacht hat.",shape=box,color=blue];
		j112052529 -> j455008060;
		j112052529 -> j2767482880;
	j455008060 [label="Nein, nicht wirklich. Selbst in der Zukunft sind die Menschen, die wir verloren haben ... im Guten wie im Schlechten? Einfach ... weg.",shape=box,color=red];
		j455008060 -> j3376336681;
	j2767482880 [label="Eleanor, komm zur Sache. Wen hast du verloren?",shape=box,color=red];
		j2767482880 -> j2955577633;
	j3376336681 [label="Ich verstehe. Vielleicht gibt es einige Türen, die wir niemals öffnen können. Vielleicht ist das auch besser so.",shape=box,color=blue];
		j3376336681 -> "Chat ends.";
	j2955577633 [label="Sein Name war Christopher. Er war Arthurs bester Freund.",shape=box,color=blue];
		j2955577633 -> j4049073643;
	j4049073643 [label="Wir waren verlobt. Ich dachte, ich wäre in ihn verliebt.",shape=box,color=blue];
		j4049073643 -> j3678079495;
	j3678079495 [label="Boolean EleanorChristopher1 is now true.",color=orange];
		j3678079495 -> j341740435;
	j341740435 [label="+10 Chemistry",color=orange];
		j341740435 -> j1820678469;
	j1820678469 [label="Es tut mir leid. Ich kann das nicht. Wir sprechen ein anderes Mal darüber.",shape=box,color=blue];
		j1820678469 -> "Chat ends.";
