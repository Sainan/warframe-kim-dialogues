digraph {
	"EleanorRank5Convo6 begins." [color=orange];
		"EleanorRank5Convo6 begins." -> j587943384;
	j587943384 [label="Du wirst möglicherweise bemerken, dass sich das Einkaufszentrum heute Morgen etwas anders anfühlt.",shape=box,color=blue];
		j587943384 -> j390519128;
	j390519128 [label="Nichts dramatisches und offensichtliches. Nur etwas mehr Lächeln auf ein paar mehr Gesichtern. Ein Lachen hier und da. Ein Hauch von Fröhlichkeit in der Luft.",shape=box,color=blue];
		j390519128 -> j3593075352;
		j390519128 -> j649132705;
	j3593075352 [label="Das liegt an dir, oder?",shape=box,color=red];
		j3593075352 -> j1576307391;
	j649132705 [label="Bin gleich zurück. [Ende.]",shape=box,color=red];
		j649132705 -> "Chat ends.";
	j1576307391 [label="Das ist nur meine Art, etwas zurückzugeben. Ein Telepath kann, wie du weißt, sowohl senden als auch empfangen. Es muss nicht unbedingt ein Wasserstrahl aus einem Gartenschlauch auf eine Person sein. Es kann eher wie ein feiner Nebel sein, der über einen ganzen Bereich gesprüht wird.",shape=box,color=blue];
		j1576307391 -> j1932004757;
	"Chat ends." [color=orange];
	j1932004757 [label="Gibt es bei dir zu Hause Rasensprenger?",shape=box,color=blue];
		j1932004757 -> j51035832;
		j1932004757 -> j2890365430;
	j51035832 [label="Wir haben VERD-IE und Drohnen ihrer Art. Sie haben sich um die Pflanzen auf der Zariman gekümmert. Meinst du so etwas?",shape=box,color=red];
		j51035832 -> j1053848758;
	j2890365430 [label="Rasen ...sprenger?",shape=box,color=red];
		j2890365430 -> j1015512264;
	j1053848758 [label="So in der Art.",shape=box,color=blue];
		j1053848758 -> j3947986261;
	j1015512264 [label="Ja, mein Schatz, Rasensprenger. Wassersprüher, um deinen Rasen schön feucht zu halten.",shape=box,color=blue];
		j1015512264 -> j1052093668;
	j3947986261 [label="Als kleines Mädchen liebte ich Rasensprenger. Sie waren magisch. An heißen Sommertagen konnte man Regenbögen darin sehen. Man konnte durch sie hindurchlaufen und spürte am ganzen Körper eine herrliche Abkühlung.",shape=box,color=blue];
		j3947986261 -> j3451728661;
	j1052093668 [label="Auch perfekt dafür geeignet, das Gesicht deines Bruders so lange hineinzuhalten, bis er schreit.",shape=box,color=blue];
		j1052093668 -> j3451728661;
	j3451728661 [label="'Fröhlichkeit ist ein Rasensprenger'. Schreib das auf eine Grußkarte.",shape=box,color=blue];
		j3451728661 -> j3595285600;
	j3595285600 [label="Du scheinst heute gut gelaunt zu sein. Das freut mich.",shape=box,color=red];
		j3595285600 -> j3110659031;
	j3110659031 [label="Das bin ich. Und ich sehe nicht ein, warum ich das für mich behalten soll. Also überflute ich das ganze Einkaufszentrum damit.",shape=box,color=blue];
		j3110659031 -> j639082514;
		j3110659031 -> j3161875734;
		j3110659031 -> j2113605460;
	j639082514 [label="Ist das nicht ein bisschen so, als würde man Menschen unter Drogen setzen?",shape=box,color=red];
		j639082514 -> j4185588340;
	j3161875734 [label="Verbreitest eine positive Stimmung? Warum nicht. Ich sehe da keinen Unterschied zum Abspielen von Musik.",shape=box,color=red];
		j3161875734 -> j267859826;
	j2113605460 [label="Heute ist also der Tag, an dem ich Arthur vielleicht lächeln sehe?",shape=box,color=red];
		j2113605460 -> j1939734228;
	j4185588340 [label="... großartig. Hier bin ich, versuche die heimliche Glücksfee zu sein, die Freude verbreitet, und stattdessen werde ich als der böse Drogenkobold abgestempelt. Meine gute Laune hat sich gerade verabschiedet.",shape=box,color=blue];
		j4185588340 -> j3515875485;
	j267859826 [label="Oh! Schöner Vergleich!",shape=box,color=blue];
		j267859826 -> j3214102089;
	j1939734228 [label="Vielleicht. Das hoffe ich. Er musste so viel ertragen.",shape=box,color=blue];
		j1939734228 -> j970796617;
	j3515875485 [label="Gut. Hör ich eben auf damit.",shape=box,color=blue];
		j3515875485 -> "Chat ends.";
	j3214102089 [label="Wenn Aoi die Stimmung durch Musik beeinflussen kann, kann ich das auch durch Schwingungen, oder?",shape=box,color=blue];
		j3214102089 -> j3845706387;
	j970796617 [label="So viel hat sich verändert, seit du gekommen bist. Ich meine nicht nur dich und mich. Ich meine alles. Sogar das Einkaufszentrum.",shape=box,color=blue];
		j970796617 -> j3845706387;
	j3845706387 [label="Ich bin froh, dass Aoi mich zum Bleiben überredet hat. Gesegnet sei sie.",shape=box,color=blue];
		j3845706387 -> j2722657710;
		j3845706387 -> j1224280913;
	j2722657710 [label="Warte. Sie hat was getan?",shape=box,color=red];
		j2722657710 -> j1935474410;
	j1224280913 [label="Zurück. Ich habe etwas verpasst.",shape=box,color=red];
		j1224280913 -> j1935474410;
	j1935474410 [label="Ich habe dir das nicht erzählt?",shape=box,color=blue];
		j1935474410 -> j1400472920;
	j1400472920 [label="Ah. Das war kurz vor deiner Ankunft. Wir waren den ganzen Tag unterwegs und haben versucht, ein Techrot-Hive auszumerzen. Es lag eine mörderische Spannung in der Luft. Beim Kämpfen lassen wir normalerweise Dampf ab und sind zu erschöpft, um aufeinander loszugehen, aber dieser Tag war anders.",shape=box,color=blue];
		j1400472920 -> j956114247;
	j956114247 [label="Ich erinnere mich, dass Arthur so etwas sagte wie 'das machen wir morgen alles noch einmal', und das traf uns alle wie ein Vorschlaghammer. Auf der Fahrt zurück zum Einkaufszentrum sagte niemand ein Wort.",shape=box,color=blue];
		j956114247 -> j1165675350;
		j956114247 -> j3371462027;
		j956114247 -> j1163266470;
	j1165675350 [label="Wow. So steigert man die Teammoral, Arthur.",shape=box,color=red];
		j1165675350 -> j1449107531;
	j3371462027 [label="Er kann so ein Charmeur sein ...",shape=box,color=red];
		j3371462027 -> j1449107531;
	j1163266470 [label="Er trägt eine schwere Last. Das fordert seinen Tribut.",shape=box,color=red];
		j1163266470 -> j2168280306;
	j1449107531 [label="Ich glaube, er wollte nur, dass wir uns auf die Arbeit konzentrieren. Aber ich fühlte mich einfach am Boden zerstört. Arthur und Aoi sprachen kein Wort, Amir ging Quincy so sehr auf die Nerven, dass ich anfing, um sein Leben zu fürchten, und Lettie warf mir bei jeder Gelegenheit einen Seitenblick zu.",shape=box,color=blue];
		j1449107531 -> j1337721779;
	j2168280306 [label="Ja. Normalerweise schafft er es, es zu verbergen, selbst wenn er fast am Ende ist. Aber ich denke, er war einfach am Ende.",shape=box,color=blue];
		j2168280306 -> j3290866024;
	j1337721779 [label="Ich habe an diesem Tag viel Zeit damit verbracht, in Spiegel zu schauen. Ich stellte mir immer wieder vor, wie mein Spiegelbild mich am Hals packte und zischte 'DU bist das Problem. Wenn du nicht wärst, würde das alles nicht passieren.'",shape=box,color=blue];
		j1337721779 -> j3207195916;
		j1337721779 -> j3265817038;
	j3290866024 [label="Er hatte sich beim Versuch, das Team zusammenzuhalten, völlig verausgabt.",shape=box,color=blue];
		j3290866024 -> j1449107531;
	j3207195916 [label="Hört sich an, als ob zur Abwechslung alle anderen in DEINEM Kopf rumgespukt haben.",shape=box,color=red];
		j3207195916 -> j1523751794;
	j3265817038 [label="Das klingt nach der Art von Mist, den Lettie von sich gibt.",shape=box,color=red];
		j3265817038 -> j4142841657;
	j1523751794 [label="Oh, das waren nicht die anderen. Das war ich. Jeder angestaute Rest an Selbstzweifeln und Selbsthass, den ich unterdrückt hatte, kam an diesem Tag hoch.",shape=box,color=blue];
		j1523751794 -> j4267180303;
	j4142841657 [label="Vielen Dank. Ich weiß, dass du mir den Rücken freihältst, und das bedeutet mir sehr viel.",shape=box,color=blue];
		j4142841657 -> j3223022096;
	j4267180303 [label="Ich dachte mir: Was dieses Team braucht, ist ein Sündenbock.",shape=box,color=blue];
		j4267180303 -> j2611623435;
	j3223022096 [label="Ich habe diesen Satz von Lettie schon so oft gehört, dass ich mir schließlich dachte: Vielleicht hat sie ja recht. Ich bringe alle in Gefahr, wenn ich hierbleibe.",shape=box,color=blue];
		j3223022096 -> j3364997127;
	j2611623435 [label="Ein Monster. Jemand, dem man die Schuld geben kann.",shape=box,color=blue];
		j2611623435 -> j3364997127;
	j3364997127 [label="Ich hatte einen umfassenden Plan gemacht. Ich würde mich nachts davonschleichen, mich irgendwo in der Stadt verstecken, wo Arthur mich nie finden würde, und - und darauf war ich besonders stolz - aus der Ferne weiterhelfen.",shape=box,color=blue];
		j3364997127 -> j535507312;
		j3364997127 -> j2324872288;
	j535507312 [label="Ich glaube, er hätte dich gefunden. Er ist ziemlich entschlossen.",shape=box,color=red];
		j535507312 -> j4180980070;
	j2324872288 [label="Die Wahrscheinlichkei ist hoch, dass er bei dem Versuch, dich zu finden, gestorben wäre.",shape=box,color=red];
		j2324872288 -> j334874777;
	j4180980070 [label="Wie? Amir kann Telepathie nicht aufspüren.",shape=box,color=blue];
		j4180980070 -> j2685202654;
	j334874777 [label="Versuchst du, mir ein schlechtes Gewissen zu machen?",shape=box,color=blue];
		j334874777 -> j1336845145;
	j2685202654 [label="Ich wollte ein edler Verbannter sein. Ich würde auf Dächern kauern und dabei ganz windgepeitscht und entschlossen aussehen. Ich könnte über die Ausbreitung von Techrot berichten, den Hex mitteilen, wo die Scaldra Straßensperren errichteten, und sogar als Notfallkommunikationsrelais fungieren. Jedes Mal, wenn ich Lettie den Arsch gerettet hätte, würde ich sie das wirklich spüren lassen. Gehässig? Auf jeden Fall.",shape=box,color=blue];
		j2685202654 -> j1318860572;
	j1336845145 [label="Mach dir keine Mühe. Ich fühle mich schon schrecklich genug.",shape=box,color=blue];
		j1336845145 -> "Chat ends.";
	j1318860572 [label="Ich wählte den richtigen Moment. Ich wartete, bis Amir eingeschlafen war. Dann zog ich einen langen Mantel an und ging so leise wie möglich in die Garage.",shape=box,color=blue];
		j1318860572 -> j1433119745;
	j1433119745 [label="Das Sicherheitsgitter senkte sich von selbst. Ich drehte mich um und da war Aoi.",shape=box,color=blue];
		j1433119745 -> j4155596754;
	j4155596754 [label="Wir redeten die ganze Nacht und weinten in den Armen der anderen. Auch sie litt. Sehr. Als die Sonne aufging, hatte ich beschlossen zu bleiben.",shape=box,color=blue];
		j4155596754 -> j3308229035;
		j4155596754 -> j2736043683;
		j4155596754 -> j3543700552;
	j3308229035 [label="Es ist noch Zeit, deine Meinung zu ändern.",shape=box,color=red];
		j3308229035 -> j173828569;
	j2736043683 [label="Aoi ist wunderbar. Ich bin froh, dass sie für dich da war.",shape=box,color=red];
		j2736043683 -> j1663431151;
	j3543700552 [label="Das war Glück.",shape=box,color=red];
		j3543700552 -> j1663431151;
	j173828569 [label="Welcher groteske Impuls dich auch immer dazu gebracht hat, das zu schreiben, ich hoffe, du erstickst daran.",shape=box,color=blue];
		j173828569 -> "Chat ends.";
	j1663431151 [label="+20 Chemistry",color=orange];
		j1663431151 -> j3869834257;
	j3869834257 [label="Ja. Ich glaube, wir alle sechs mussten an Silvester hier sein. Und du natürlich auch. Der Fremde an der Tür, der Glück bringt.",shape=box,color=blue];
		j3869834257 -> "Chat ends.";
}