	j2218247725 [label="ES GIBT PAPA-WITZE IN DER ZUKUNFT",shape=box,color=blue];
		j2218247725 -> j1877159754;
	"Chat ends." [color=orange];
	j1786860714 [label="Ich vermisse Wein. Ich habe schon so verdammt lange keinen richtigen Wein mehr getrunken. Der gesamte Wein in Höllvania wurde wenige Tage nach dem Ausbruch von Scaldra beschlagnahmt.",shape=box,color=blue];
		j1786860714 -> j3870206947;
	j1877159754 [label="+10 Chemistry",color=orange];
		j1877159754 -> j3315491629;
	j3870206947 [label="Quincy und Arthur versuchten, aus den Päckchen mit der Obstration Gefängnisschnaps zu machen, aber diejenigen von uns, die noch ein bisschen Würde hatten, lehnten ab. Das war ein seltener Moment der Solidarität mit Lettie.",shape=box,color=blue];
		j3870206947 -> j2169416161;
	j3315491629 [label="Bin jetzt fröhlich. Danke.",shape=box,color=blue];
		j3315491629 -> "Chat ends.";
	j2169416161 [label="Der andere, viel, viel größere Grund ist die Zukunft.",shape=box,color=blue];
		j2169416161 -> j50346477;
	j50346477 [label="Es ist nicht deine Schuld, aber ... nachdem ich die Zukunft durch deine Augen gesehen habe, fühle ich mich auf eine Art und Weise elend, die sich mit den üblichen Methoden nicht ändern lässt.",shape=box,color=blue];
		j50346477 -> j905755311;
		j50346477 -> j2960316056;
	j905755311 [label="Was kann ich tun, um dich aufzumuntern?",shape=box,color=red];
		j905755311 -> j163447900;
	j2960316056 [label="Übliche Methoden?",shape=box,color=red];
		j2960316056 -> j982385414;
	j163447900 [label="Ich weiß nicht. Vielleicht ein exotischer Tanz?",shape=box,color=blue];
		j163447900 -> j1638296289;
	j982385414 [label="Den Abend mit Amir verbringen und ihm zuhören, wie er von den Comicserien oder Retro-Spielen schwärmt, die ihn gerade faszinieren.",shape=box,color=blue];
		j982385414 -> j2581178248;
	j1638296289 [label="Beschreibe mir Ballas‘ Tod in allen Einzelheiten?",shape=box,color=blue];
		j1638296289 -> j3970442891;
	j2581178248 [label="Mich mit Kristallen umgeben und im Geiste singen, bis Lettie den Exorzisten ruft.",shape=box,color=blue];
		j2581178248 -> j2667779078;
	j3970442891 [label="Es ist nett von dir, es zu versuchen, aber ich glaube, diese Melancholie muss einfach ertragen werden.",shape=box,color=blue];
		j3970442891 -> j1258048691;
	j2667779078 [label="Das ist eine andere Art der Traurigkeit. Es belastet mich wie eine durchnässte Decke.",shape=box,color=blue];
		j2667779078 -> j1790903492;
		j2667779078 -> j125420240;
	j1258048691 [label="Vielleicht werde ich malen.",shape=box,color=blue];
		j1258048691 -> j227430165;
	j1790903492 [label="1999 Höllvania ist hart! Scaldra, Techrot, Void weiß was noch. Sieht die Zukunft wirklich so viel schlimmer aus?",shape=box,color=red];
		j1790903492 -> j2862885878;
	j125420240 [label="Ach komm schon ... du bist hart im Nehmen. Du schaffst das. Hier gibt es so viele coole Sachen.",shape=box,color=red];
		j125420240 -> j2329429738;
	j227430165 [label="+5 Chemistry",color=orange];
		j227430165 -> "Chat ends.";
	j2862885878 [label="Schau, ich war nie wirklich ein Science-Fiction-Fan. Arthur hatte eine Phase, aber er bevorzugte immer den harten Armeekram. Männliche Männer als Helden, nette Geschichten für Jungs, Unmengen an Subtext, ich bin sicher, du kennst das.",shape=box,color=blue];
		j2862885878 -> j1422679597;
	j2329429738 [label="Na schön. Nimm mich bei der Hand und führe mich durch die Straßen von Höllvania.",shape=box,color=blue];
		j2329429738 -> j3186260567;
	j1422679597 [label="Aber ich wusste genug über Science-Fiction, um zu wissen, dass wir in der fernen Zukunft alles im Griff haben werden. Alles soll glänzend und keimfrei sein. Roboter erledigen die ganze harte Arbeit, es gibt keine Krankheiten, niemand braucht Geld, alles ist silberne Türme, blauer Himmel und sexy Echsen.",shape=box,color=blue];
		j1422679597 -> j2675678545;
		j1422679597 -> j3581390214;
	j3186260567 [label="Ich kenne einen Ort, wo wir ein paar geriebene Rübenkuchen essen und zusehen können, wie die Sonne über den Überresten einer Schule untergeht.",shape=box,color=blue];
		j3186260567 -> j2929594420;
	j2675678545 [label="Wir haben Roboter. Sie erledigen einen Großteil der harten Arbeit.",shape=box,color=red];
		j2675678545 -> j2624536483;
	j3581390214 [label="Aber das, wovon du sprichst, ist doch bloß Fiktion, oder? Warum erwartest du also, dass die Realität dem gerecht wird?",shape=box,color=red];
		j3581390214 -> j3683155899;
	j2929594420 [label="Später können wir im Licht eines brennenden Panzers Cocktails schlürfen.",shape=box,color=blue];
		j2929594420 -> "Chat ends.";
	j2624536483 [label="Na gut. Dafür bekommst du einen Punkt.",shape=box,color=blue];
		j2624536483 -> j3007956779;
	j3683155899 [label="Äh. Erwarte ich nicht ... es ist kompliziert. Lass es mich erklären.",shape=box,color=blue];
		j3683155899 -> j225528536;
	j3007956779 [label="Aber haben diese Roboter das Leben der einfachen Leute leichter gemacht?",shape=box,color=blue];
		j3007956779 -> j3680647402;
	j225528536 [label="Ich sage nicht, dass ich erwartet habe, dass die Zukunft wie in einem Kindercomic aussehen würde. Ich meine nur, dass damit ein gewisser Optimismus verbunden war.",shape=box,color=blue];
		j225528536 -> j585492690;
	j3680647402 [label="Oder sind sie einfach zu etwas Selbstverständlichem geworden?",shape=box,color=blue];
		j3680647402 -> j3478566876;
		j3680647402 -> j1980867371;
	j585492690 [label="Ich glaube, wir haben es alle einfach als selbstverständlich hingenommen, dass wir, egal wie beschissen die Dinge gerade sind, es irgendwann auf die Reihe kriegen. Unsere Zivilisation sollte sich weiterentwickeln. Wir haben damit angefangen, uns gegenseitig mit Knüppeln auf den Kopf zu schlagen, und nach ein paar tausend Jahren sollten wir Wege finden, unsere Differenzen auch ohne das beizulegen.",shape=box,color=blue];
		j585492690 -> j2893119610;
		j585492690 -> j2785006252;
	j3478566876 [label="Du bist fest entschlossen, in allem das Schlechteste zu sehen, nicht wahr?",shape=box,color=red];
		j3478566876 -> j447613364;
	j1980867371 [label="Gut. Wir haben durch Roboter der Ausbeutung kein Ende gesetzt.",shape=box,color=red];
		j1980867371 -> j3037623437;
	j2893119610 [label="Menschen bleiben Menschen, egal wie sehr sich die Technologie ändert.",shape=box,color=red];
		j2893119610 -> j4285830235;
	j2785006252 [label="Es tut mir leid, dass wir dich enttäuscht haben?",shape=box,color=red];
		j2785006252 -> j3073317850;
	j447613364 [label="Offensichtlich.",shape=box,color=blue];
		j447613364 -> j2688999152;
	j3037623437 [label="Genau. Das ist das Enttäuschende.",shape=box,color=blue];
		j3037623437 -> j2760493388;
	j4285830235 [label="Bingo. Du hast auf den Punkt gebracht, was mich an der ganzen Sache so deprimiert.",shape=box,color=blue];
		j4285830235 -> j3310020743;
	j3073317850 [label="Genau.",shape=box,color=blue];
		j3073317850 -> j3310020743;
	j2688999152 [label="So weiß man, was geändert werden muss.",shape=box,color=blue];
		j2688999152 -> "Chat ends.";
	j2760493388 [label="Ihr seid fortgeschritten, aber ihr solltet auch ERLEUCHTET sein.",shape=box,color=blue];
		j2760493388 -> j865017260;
	j3310020743 [label="Ihr hattet all diese unglaubliche, magische Technologie zur Hand und alles, was ihr damit gemacht habt, war, neue Wege zu finden, sich gegenseitig auszubeuten. Die Wissenschaft hat uns weniger gleich gemacht, nicht mehr.",shape=box,color=blue];
		j3310020743 -> j136676150;
		j3310020743 -> j1908605360;
	j865017260 [label="Danke, dass du mich aushältst. Ich weiß, ich bin anstrengend.",shape=box,color=blue];
		j865017260 -> j4072819434;
	j136676150 [label="Ähm, eine höfliche Erinnerung, dass ich NICHTS davon getan habe?",shape=box,color=red];
		j136676150 -> j4155928173;
	j1908605360 [label="Ich werde deine Beschwerde an die Orokin weiterleiten.",shape=box,color=red];
		j1908605360 -> j4155928173;
	j4072819434 [label="+15 Chemistry",color=orange];
		j4072819434 -> "Chat ends.";
	j4155928173 [label="Oh Götter, ich meine nicht dich persönlich. Ich meine deine Ära.",shape=box,color=blue];
		j4155928173 -> j2442532246;
	j2442532246 [label="Wenn überhaupt, dann habt ihr euch rückwärts bewegt. Gottkönige in Menschengestalt! Von allen Dingen, die es neu zu erfinden galt, habt ihr SIE neu erfunden.",shape=box,color=blue];
		j2442532246 -> j4254412514;
	j4254412514 [label="Die Tenno haben die Orokin getötet. Hast du diesen Teil vergessen?",shape=box,color=red];
		j4254412514 -> j2123582975;
	j2123582975 [label="Nein. Ich habe den Teil nicht vergessen, in dem die emotional misshandelten Waisen die unsterbliche Elite ermordet haben. Ich liebe diesen Punkt einfach. Wenn ich die Augen schließe, kann ich mir das Geschrei fast vorstellen.",shape=box,color=blue];
		j2123582975 -> j3884806099;
		j2123582975 -> j1606516598;
	j3884806099 [label="Zugegeben. Die Zukunft ist ein Chaos. Du kannst dich darauf konzentrieren, wenn du willst.",shape=box,color=red];
		j3884806099 -> j2275338647;
	j1606516598 [label="Das ... ist ... etwas, was ich anderen gegenüber nur mit Vorsicht zugeben würde.",shape=box,color=red];
		j1606516598 -> j637957147;
	j2275338647 [label="Ich gehöre zu den Menschen, die versuchen, es besser zu machen.",shape=box,color=red];
		j2275338647 -> j2895199346;
	j637957147 [label="... ist Ironie in der Zukunft auch überholt?",shape=box,color=blue];
		j637957147 -> "Chat ends.";
